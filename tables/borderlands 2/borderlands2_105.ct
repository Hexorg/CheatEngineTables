  local iNewCount = 0; local iTotalCount = 0
  for i=1,DenCount do
   local offset  = foundlist_getAddress(DenResults,i)
   local address = getAddress(offset .. "+244")
   local count   = readInteger(address)

   iTotalCount = iTotalCount + 1
