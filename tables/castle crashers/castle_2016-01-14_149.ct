push edx
mov edx,[ebp-14]

cmp [esp+18],161 //Money
je ItsMoney
cmp [esp+18],4F3 //Bombs
je ItsBombs
cmp [esp+18],4F2 //Potions
je ItsPotions
cmp [esp+18],4F4 //Sandwiches
je ItsSandwiches
cmp [esp+18],4F1 //Stat Points
je ItsStatPoints
cmp [esp+18],6B4 //Sandwich Timer
je ItsSandwichTimer
cmp [esp+18],4E8 //Exp
je ItsExperience
//cmp [esp+18],1200
//cmp [esp+18],11C4
// ESP+18 value for mission timer seems to be depending on previously
//loaded missions
cmp [esp+58],110 //Mision Timer
je ItsMissionTimer
jmp RegularCode //If nothing matches, just go regular

FilterHere:
nop
jmp RegularCode

ItsMissionTimer:
cmp [UnlimitedMissionTimer],1
je SetMaxMissionTimer
jmp RegularCode

ItsExperience:
cmp [InstaLevel],1
je SetMaxEXP
jmp RegularCode

ItsSandwichTimer:
cmp [UnlimitedSandwichTime],1
je SetMaxSandwichTimer
jmp RegularCode

ItsStatPoints:
cmp [UnlimitedStatPoints],1
je SetMaxStuff
jmp RegularCode

ItsSandwiches:
cmp [UnlimitedSandwiches],1
je SetMaxStuff
jmp RegularCode

ItsPotions:
cmp [UnlimitedPotions],1
je SetMaxStuff
jmp RegularCode

ItsBombs:
cmp [UnlimitedBombs],1
je SetMaxStuff
jmp RegularCode

ItsMoney:
push ecx
mov ecx,esp
add ecx,10000
cmp edi,ecx //If edi close to/in stack its store, if away from stack its own money
pop ecx
jg SetMaxMoney //Its our money
jmp SetToZero //Its the store

SetMaxMoney:
cmp [MaxMoney],1
jne RegularCode
mov eax,#9999
jmp RegularCode

SetToZero:
cmp [FreeStore],1
jne RegularCode
mov eax,#0
jmp RegularCode

SetMaxStuff:
mov eax,#99
jmp RegularCode

SetMaxSandwichTimer:
{Sandwichtimer gives an indicator every whole second,
so just set it to a value that's not a whole second.
Sandwichtimer has 30 ticks per second so set it to a number not devidable by 30}
mov eax,#301
jmp RegularCode

SetMaxMissionTimer:
mov eax,#99
jmp RegularCode

SetMaxEXP:
mov eax,#200313
jmp RegularCode

RegularCode:
  mov [esi+08],eax
  mov al,01
pop edx
  jmp return

CodeHookOffset+C:
CodeHook:
  jmp Check
return:

