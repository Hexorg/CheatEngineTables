label(vles)
label(returnhere)
label(originalcode)
// Open Define
label(mechanical)
label(electricity)
label(fire)
label(cold)
label(acid)
label(energy)
label(bio)
// Actual Pointers
label(mch)
label(fre)
label(cld)
label(elt)
label(aci)
label(eng)
label(bie)
label(ret)
//
label(reset)


Pabs:


mch:
dd 0

fre:
dd 0

cld:
dd 0

elt:
dd 0

aci:
dd 0

eng:
dd 0

bie:
dd 0

ret:
dd 0


vles:
// Reset All Values To Get New Armor Piece
cmp [ret],1
je reset
cmp [eax+ebx+08],0
je mechanical
cmp [eax+ebx+08],1
je fire
cmp [eax+ebx+08],2
je cold
cmp [eax+ebx+08],3
je electricity
cmp [eax+ebx+08],4
je acid
cmp [eax+ebx+08],5
je energy
// The heck is 6? Unimplemented Psy?
cmp [eax+ebx+08],7
je bio
jmp originalcode


mechanical:
mov [mch],eax
add [mch],ebx
add [mch],08
jmp originalcode

fire:
mov [fre],eax
add [fre],ebx
add [fre],08
jmp originalcode

cold:
mov [cld],eax
add [cld],ebx
add [cld],08
jmp originalcode

electricity:
mov [elt],eax
add [elt],ebx
add [elt],08
jmp originalcode

acid:
mov [aci],eax
add [aci],ebx
add [aci],08
jmp originalcode

energy:
mov [eng],eax
add [eng],ebx
add [eng],08
jmp originalcode

bio:
mov [bie],eax
add [bie],ebx
add [bie],08
jmp originalcode


reset:
mov [mch],0
mov [fre],0
mov [cld],0
mov [elt],0
mov [aci],0
mov [eng],0
mov [bie],0
mov [ret],0
jmp vles

originalcode:
lea esi,[eax+ebx+08]
movq xmm0,[esi]
jmp returnhere


aob_armab:
jmp vles
nop
nop
nop
returnhere:


