    <CheatEntry>
      <ID>7</ID>
      <Description>"Gemcraft Zero 1.2k script"</Description>
      <Options moHideChildren="1"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
alloc(newmem,4096)
alloc(buffer,512)
aobscan(aobGrabber,F3 0F 7E 88 38 02 00 00 F3 0F 7E 80 60 01 00 00 F2 0F 5C C8)

label(aobGrabber_r)
label(lblGrabber)
label(lblGrabberSkipMaxMana)
label(lblGrabberSkipMana)
label(lblGrabberReturn)
label(bEnableManaHack)
label(bEnableMaxManaHack)
label(dDesiredMana)
label(dDesiredMaxMana)

registersymbol(aobGrabber_r)
registersymbol(bEnableManaHack)
registersymbol(bEnableMaxManaHack)
registersymbol(dDesiredMana)
registersymbol(dDesiredMaxMana)

newmem:
lblGrabber:
movq xmm1,[eax+00000238] // Original code
fxsave [buffer]
cmp dword ptr [bEnableManaHack],1
jne short lblGrabberSkipMana
movq xmm0,[eax+168]
movq xmm1,[eax+130]
movq xmm2,[dDesiredMana]
subsd xmm1,xmm0
comisd xmm2,xmm1
jp short lblGrabberSkipMana
jc short lblGrabberSkipMana
addsd xmm2,xmm0
movq [eax+130],xmm2
lblGrabberSkipMana:

cmp dword ptr [bEnableMaxManaHack],1
jne short lblGrabberSkipMaxMana
movq xmm0,[eax+160]
movq xmm1,[eax+238]
movq xmm2,[dDesiredMaxMana]
subsd xmm1,xmm0
comisd xmm2,xmm1
jp short lblGrabberSkipMaxMana
jc short lblGrabberSkipMaxMana
addsd xmm2,xmm0
movq [eax+238],xmm2
lblGrabberSkipMaxMana:
fxrstor [buffer]
jmp lblGrabberReturn

bEnableManaHack:
dd 1
bEnableMaxManaHack:
dd 1
dDesiredMana:
dq (double)50000.0
dDesiredMaxMana:
dq (double)200000.0

aobGrabber:
aobGrabber_r:
jmp lblGrabber
nop
nop
nop
lblGrabberReturn:

[DISABLE]
aobGrabber_r:
//Alt: movq xmm1,[eax+00000238]
db F3 0F 7E 88 38 02 00 00

unregistersymbol(aobGrabber_r)
unregistersymbol(bEnableManaHack)
unregistersymbol(bEnableMaxManaHack)
unregistersymbol(dDesiredMana)
unregistersymbol(dDesiredMaxMana)

dealloc(buffer)
dealloc(newmem)

</AssemblerScript>
