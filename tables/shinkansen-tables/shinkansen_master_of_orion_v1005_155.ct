//22F6096A - 48 83 EC 20           - sub rsp,20
//22F6096E - 83 38 00              - cmp dword ptr [rax],00
aobscan(Civilization_searchedAOB, 48 63 96 5C 01 00 00 48 8B C8)

alloc(Civilization_newmem, 2048, Civilization_searchedAOB)
alloc(Civilization_temp1, 8, Civilization_searchedAOB)
alloc(Civilization_temp2, 8, Civilization_searchedAOB)


