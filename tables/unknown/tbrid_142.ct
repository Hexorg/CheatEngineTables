GainXP:    //updates your xp and level and returns 1 if you gained a level
//when entering this function eax=&amp;PlayerData, edx = new xp, ecx=xp increment (with mod)
//First find which character we're handling
push ecx
push edi
push ebx

mov edi,ecx                  //edi=xp income
mov ecx,eax
sub ecx,TBRID.exe+E3CC4      //medal count for player 0 (Ryounosuke)
shr ecx,4                    //divide edx by the size of the Playerdata structure so now edx=player index (0 based)
cmp ecx,8                    //player index&gt;8 means it's an enemy
ja GainXP_Exit
imul ecx,ecx,74              //0x74=size in byte of a PlayerMech structure
mov ebx,dword [TBRID.exe+E0AF4+ecx] //reads what mech you're on
cmp ebx,1
jne short GainXP_NotInAPM1
  mov ebx, dword [eax+4]
  shr ebx,#18                //got pm1 xp
  add ebx,edi                //increase pm1 xp
  shl ebx,#18
  and dword [eax+4],3FFFF    //clear stored pm1 xp
  or dword [eax+4],ebx
  jmp GainXP_UpdateWeaponXp
GainXP_NotInAPM1:
cmp ebx,2
jne short GainXP_NotInAPM2
  mov ebx, dword [eax+C]
  and ebx, 3FFF              //got pm2 xp
  add ebx,edi                //increase pm2 xp
  and dword [eax+C],FFFFC000 //clear stored pm2 xp
  or dword [eax+C],ebx
  jmp GainXP_UpdateWeaponXp
GainXP_NotInAPM2:
cmp ebx,3
jne short GainXP_UpdateWeaponXp    //a bug
  mov ebx, dword [eax+C]
  shr ebx,#14
  and ebx, 3FFF              //got pm3 xp
  add ebx,edi                //increase pm3 xp
  shl ebx,#14
  and dword [eax+C],FFFC000 //clear stored pm3 xp
  or dword [eax+C],ebx
GainXP_UpdateWeaponXp:
mov bl,byte [ecx+TBRID.exe+E0AF8] //bl=primary weapon
mov bh,byte [ecx+TBRID.exe+E0AFC] //bh=secondary weapon
sub bl,1
sub bh,1                             //if your weapon was 0 (no weapon) bl and bh are now = FF
//search for grenades (item id: 1)
push ecx
push edi
push eax
lea edi,[TBRID.exe+E0b08+ecx]
mov eax,1
mov ecx,4
db F2 AF //REPNE SCAS DWORD PTR ES:[EDI]
pop eax
pop edi
pop ecx
  je short GainXP_GotAGrenade //jump if the repne found a grenade
cmp bl,2              //otherwise check if prim/sec weapons are weapon id 1 or 2
setb dl               //unsigned comparison, so no weapon=-1=FF is bigger than 3, so dl=0
cmp bh,2
setb dh
test dx,dx                          //here dx is non zero if weapon-3&lt;3
je short GainXP_NoShortRangeWeapon  //weapon IDs: 1=submachine gun, 2=shotgun
  GainXP_GotAGrenade:
  mov dx, word [eax+1]
  and edx, 3FFF              //got short range xp
  add edx,edi                //increase pm3 xp
  and word [eax+1],C000      //clear stored pm3 xp
  or word [eax+1],dx
GainXP_NoShortRangeWeapon:
sub bl,2
sub bh,2
cmp bl,2
setb dl
cmp bh,2
setb dh                   //here dx is non zero if 0&lt;=weapon-3&lt;2
test dx,dx                //which means we carry a medium range weapon
je short GainXP_NoMedRangeWeapon  //weapon IDs: 3=rockets 4=missile, 5=sniper
  mov edx, dword [eax+2]
  shr edx,6
  and edx, 3FFF              //got med range xp
  add edx,edi                //increase med range xp
  shl edx,6
  and dword [eax+2],FFF0003F //clear stored med range xp
  or dword [eax+2],edx
GainXP_NoMedRangeWeapon:     //weapon IDs: 6=144mm gun, 7=200mm gun, 8=long range missile, 9=long range rockets
sub bl,3
sub bh,3
cmp bl,4
setb dl
cmp bh,4
setb dh                   //here dx is non zero if 0&lt;=weapon-4-2&lt;5
test dx,dx                //which means we carry a long range weapon
je short GainXP_Exit
  mov edx, dword [eax+4]
  shr edx,4
  and edx, 3FFF              //got long range xp
  add edx,edi                //increase long range xp
  shl edx,4
  and dword [eax+4],FFFC000F //clear stored long range xp
  or dword [eax+4],edx
GainXP_Exit:
pop ebx
pop edi
pop ecx
retn


