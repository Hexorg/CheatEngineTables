NEWMEM1:
PUSH RCX
LEA RCX,[RAX]
MOV [BUMP_BASE],RCX
POP RCX
PUSH RDI
MOV RDI,[RAX+00000848]
MOV [BUMPER_VALUE],RDI
POP RDI
MOVZX EAX,WORD PTR [RAX+00000848]
CMP [BUMPER_VALUE],#16711744
JE TURN_ON_RAPIDFIRE
CMP [BUMPER_VALUE],#16711808
JE TURN_OFF_RAPIDFIRE
JMP RETURNHERE1

TURN_ON_RAPIDFIRE:
MOV BYTE PTR [RAPID_FIRE_ENABLE],1
JMP RETURNHERE1

TURN_OFF_RAPIDFIRE:
MOV BYTE PTR [RAPID_FIRE_ENABLE],0
JMP RETURNHERE1

BUMPER_VALUE:
DQ 0
BUMP_BASE:
DQ 0
RAPID_FIRE_ENABLE:
DB 0

AOB_BUMPER:
JMP NEWMEM1
NOP
NOP
RETURNHERE1:

{=====================
