      <GroupHeader>1</GroupHeader>
    </CheatEntry>
    <CheatEntry>
      <ID>9</ID>
      <Description>"Base Script 1.2 AOB"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>FF0000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{
===========================================
 Game Title    : Risen 3 - Titan Lords
 Game Version  : 1.0.90.0
 Process Name  : Risen3.exe
 Script Version: 1.2
 CE Version    : 6.4
 Release date  : 13-Aug-2014
 Author        : Matze500
 Features:
 - Godmode (better)
 - OneHit
 - Unlimited Item (Includes Ammo)
 - Minimum Glory
 - Minimum Gold
 - Pointer
===========================================
}
[ENABLE]

aobscan(aobHero,8B 44 81 10 C2 04 00 CC CC CC CC CC CC CC 8B 44 24 04)
aobscan(aobGold,0F AF 46 14 5F 5E 59 C3 5F 33 C0 5E 59 C3 83 EC 08)
aobscan(aobItem,8B 41 24 C3 CC CC CC CC CC CC CC CC CC CC CC CC 56 8B F1 E8 E8 16 00 00)
aobscan(aobOneHit,89 4C 86 10 8B16 8B 82 DC 00 00 00 53 57 8B CE FF D0 8B D8 83 FF 09)

alloc(Code,1024)

label(aobHero_r)
label(aobGold_r)
label(aobItem_r)
label(aobOneHit_r)
label(_Hero)
label(_Glory)
label(_BackH)
label(_ExitH)
label(_Gold)
label(_BackG)
label(_ExitG)
label(_Item)
label(_BackI)
label(_ExitI)
label(_OneHit)
label(_BackOH)
label(_ExitOH)
label(pHero)
label(pGold)
label(iEnableGM)
label(iEnableG)
label(iEnableH)
label(iEnableI)
label(iEnableOH)
label(iMinGold)
label(iMinGlory)
label(zero)

//Symbols
registersymbol(aobHero_r)
registersymbol(aobGold_r)
registersymbol(aobItem_r)
registersymbol(aobOneHit_r)
registersymbol(Code)
registersymbol(pHero)
registersymbol(pGold)
registersymbol(iEnableGM)
registersymbol(iEnableG)
registersymbol(iEnableH)
registersymbol(iEnableI)
registersymbol(iEnableOH)
registersymbol(iMinGold)
registersymbol(iMinGlory)
registersymbol(zero)

Code:
_Hero:
push ebx

cmp eax,0A
jne _ExitH

mov [pHero],ecx

cmp [iEnableGM],0
je _Glory

fild dword ptr[ecx+eax*4+0C]
fistp dword ptr [ecx+eax*4+10]

_Glory:

cmp [iEnableH],0
je _ExitH

mov ebx,[iMinGlory]
cmp ebx,[ecx+3c]
jle _ExitH

mov [ecx+3C],ebx

_ExitH:
pop ebx

mov eax,[ecx+eax*4+10]
ret 0004
jmp _BackH

_Gold:
 mov [pGold],ecx

 cmp [iEnableG],0
 je _ExitG

 mov edi,[iMinGold]
 cmp eax,edi
 jge _ExitG

 mov eax,edi
 mov [ecx+24],eax

_ExitG:
 imul eax,[esi+14]
 pop edi
 jmp _BackG

_Item:

 cmp [zero],ecx
 je _ExitI

 cmp [ecx+24],01
 jng _ExitI

 cmp [iEnableI],0
 je _ExitI

 cmp dword ptr [ecx+24],#99
 jge _ExitI

 mov [ecx+24],#99

_ExitI:

 mov eax,[ecx+24]
 ret
 int 3
 jmp _BackI

_OneHit:

cmp [iEnableOH],0
je _ExitOH

mov ecx,#0

_ExitOH:
mov [esi+eax*4+10],ecx
mov edx,[esi]
jmp _BackOH

//Variables
pHero:
