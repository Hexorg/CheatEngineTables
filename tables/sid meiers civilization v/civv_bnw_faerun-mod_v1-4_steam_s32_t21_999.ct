 jle _ExitMMSR

 mov [ecx+ebx*4],edi

_ExitMMSR:
 pop edi
 jmp _BackMMSR                  // Back to main code

//========================================= x2 new
// Activated when entering or leaving a city
_MonGamePace:
 mov [esp+30],eax               // Original code
 fild dword ptr [esp+30]        // Original code

 push eax
 mov [iFullBaseGA],eax
 shr eax,1
 mov [iHalfBaseGA],eax
 pop eax

 push ecx
 push edx

 mov ecx,5
 xor edx,edx
 div ecx

 mov [iBaseRH],eax

 pop edx
 pop ecx

_ExitMGP:
 jmp _BackMGP                   // Back to main code

