      <Description>"Set Inventory Item (slot 1)"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
aobscanmodule(inv_slot,darkest.exe,8B 41 24 83 38 00 7E)
alloc(newmem,$1000)

label(code)
label(return)
label(invslot1)

newmem:
  mov eax,[ecx+24]
  cmp edi,0
  jne code
  mov [invslot1],eax
  jmp code

invslot1:
  db 00 00 00 00

code:
  cmp dword ptr [eax],00
  jmp return

inv_slot:
  jmp newmem
  nop
return:
registersymbol(inv_slot)
registersymbol(invslot1)

[DISABLE]
inv_slot:
  db 8B 41 24 83 38 00
unregistersymbol(inv_slot)
unregistersymbol(invslot1)
dealloc(newmem)
</AssemblerScript>
