    <CheatEntry>
      <ID>15</ID>
      <Description>"Script: Gun"</Description>
      <LastState Activated="1" />
      <Color>FF0000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{ Game   : Teleglitch.exe
  Version: 
  Date   : 2015-10-02
  Author : Jason

  All credit to Shinkansen for finding this injection point

cmp dword ptr [playergunstatsset_enable], #0
je playergunstatsset_exit
mov [edx+108], #-5
mov [edx+104], (float)99
mov [edx+110], (float)9999
mov [edx+114], (float)9999
mov [edx+118], (float)9999
mov [edx+11c], (float)9999
mov [edx+120], (float)9999
mov [edx+124], (float)0
mov [edx+128], (float)10
mov [edx+12c], (float)9999
mov [edx+148], #1
mov [edx+14c], #99
playergunstatsset_exit:

cmp dword ptr [playergunclipammoset_enable], #0
je playergunclipammoset_exit
fld [edx+100]
fstp [edx+fc]

Offsets (order is 9mmpst1, autopst1, mg-3200, heavy rifle, shotgun, ag1-3, mg-hv)
  00FC: Current Clip Ammo
  0100: Max Clip Ammo
  0104: Spread (9mmpst1&amp;rifle=1,mg-3200=4,autopst1=5,shotgun&amp;ag1-3=6, mg-hv=3)
  0108: Delay, - is single shot, + is auto (9mmpst1=-5,rifle=-13,autpst1=2,mg-3200=3,mg-hv=5,ag1-3=10,shotgun=13)
  010C: RemainingDelay (when -, can fire)
  0110: ? Min Velocity ? - 14, 14, 18, 32, 12, 12, 40
  0114: DAMAGE: 26, 26, 20, 126, 17, 15, 50
  0118: 20 for all
  011C: 0.2, 0.2, 0.5, 0.7, 0.1, 0, 0.7 (pushed onto the stack along with DAMAGE)
  0120: 40, 40, 40, 160, 21, 1, 51  (pushed onto the stack along with DAMAGE)
  0124: 3, 1.5, 2, 6, 5, 5, 4
  0128: 1.2, 1.2, 1.2, 1.2, 1.2, 1.2, 1.5      // mg-hv = 1.5 (armor piercing?)
  012C: 6, 6, 6, 6, 6, 6, 8                    // mg-hv = 8 (armor piercing?)
  0148: (1 good?) 30, 35, 60, 60, 40, 40, 60
  014C: Shots per firing (1, 1, 1, 1, 5, 3, 1
  0150: Ammo per firing (1, 1, 1, 1, 1, 3, 1
  0154: ? (0, 0, 0, 0, 0, 5) - sticky grenade is only one with number


}

[ENABLE]

globalalloc(globals,$100000)

aobscanmodule(INJECT_GUN,teleglitch.exe,8B 8A FC 00 00 00 3B 88 00 01 00 00 0F) // should be unique
alloc(newmem,$1000)

label(code)
label(skip1)
label(skip2)
label(return)

newmem:

code:
  mov [globals+1004],edx
  cmp [globals+2004],1
  jne skip1
  mov ecx,[edx+00000100]
  mov [edx+000000FC],ecx

skip1:
  cmp [globals+2008],1
  jne skip2
//  mov dword ptr [edx+00000108],1 // fire delay (- numbers are 1 shot, + are auto)
//  mov dword ptr [edx+0120],1000  // damage?  Pistol 21, shotgun 40, rifle 160
  mov ecx,[edx+0000010C]
  cmp ecx,0
  jle skip2
  mov dword ptr [edx+0000010C],0  // remaining time to fire

skip2:
  mov ecx,[edx+000000FC]
  jmp return

INJECT_GUN:
  jmp code
  nop
return:
registersymbol(INJECT_GUN)

[DISABLE]

INJECT_GUN:
  db 8B 8A FC 00 00 00

unregistersymbol(INJECT_GUN)
dealloc(newmem)

