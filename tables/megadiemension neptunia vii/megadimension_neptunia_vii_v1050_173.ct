registersymbol(itemsstats_savedAOB2)

aobscanmodule(itemsstats_searchedAOB,NeptuniaVII.exe,33 C0 5E 5D C3 8B 40 04 5E) // should be unique

alloc(itemsstats_newmem,2048)

itemsstats_newmem:
  mov [itemsstats_basepointer],eax
  cmp dword ptr [itemsstats_update_enable],#0
  je itemsstats_update_exit
  push ebx
  mov ebx,[itemsstats_offset_value]
  lea ebx,[eax+ebx*8]
  mov [itemsstats_offset_basepointer],ebx
  pop ebx
itemsstats_update_exit:

  cmp dword ptr [itemsstats_add_enable],#0
  je itemsstats_add_exit
  push esi
  push edi
  //Adjust Number of Items
  mov edi,[eax-4]
  add [eax-4],1
  //Add Item ID and Quantity
  lea edi,[eax+edi*8]
  mov esi,[itemsstats_add_enable]
  mov [itemsstats_add_enable],#0
  mov word ptr [edi],si
  mov word ptr [edi+4],#99
  pop edi
  pop esi
itemsstats_add_exit:

itemsstats_originalcode:
  lea ecx,[eax+ecx*8]
  cmp eax,ecx
  jmp itemsstats_returnhere

itemsstats_newmem2:
  cmp dword ptr [itemsstats_set_enable],#0
  je itemsstats_set_exit
  cmp dword ptr [eax],#0
  je itemsstats_set_exit
  mov word ptr [eax+4],#99
itemsstats_set_exit:

itemsstats_originalcode2:
  mov eax,[eax+04]
  pop esi
  pop ebp
  jmp itemsstats_returnhere2

itemsstats_searchedAOB-13:
itemsstats_savedAOB:
  jmp itemsstats_newmem
itemsstats_returnhere:

itemsstats_searchedAOB+5:
itemsstats_savedAOB2:
jmp itemsstats_newmem2
itemsstats_returnhere2:

[DISABLE]

itemsstats_savedAOB:
  db 8D 0C C8 3B C1

itemsstats_savedAOB2:
  db 8B 40 04 5E 5D

unregistersymbol(itemsstats_savedAOB)
unregistersymbol(itemsstats_savedAOB2)

dealloc(itemsstats_newmem)
