    <CheatEntry>
      <ID>494</ID>
      <Description>"Lookup ID -&gt; String (only works w/o cheats)"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>/*
This is a simple script to lookup a string based on it's ID.
Yes I know I didn't need to copy the string like I did, a pointer would suffice
but I wanted to practice string work in assembly.
*/

[ENABLE]
aobscan(rwvariableAOB,DD 45 08 C7 46 04 00 00 00 00 DD 5E 08 5E 5D C3 CC CC 56)
registersymbol(rwvariableAOB)

alloc(codedata,2048)

label(convertID)
label(returnrwvariable)
label(originalrwvariable)
label(IDNotGameVariable)
label(IDNegIndex)
label(IDTooLarge)
label(memEr)
label(endConvertID)

label(copyString)
label(copyStringDone)
label(copyStringError)
label(copyStringErrorDone)

registersymbol(dLookupID)
label(dLookupID)
registersymbol(dStringBufferLen)
label(dStringBufferLen)
registersymbol(dStringBufferError)
label(dStringBufferError) // how many extra bytes of buffer are needed.
registersymbol(sBuffer)
label(sBuffer)
registersymbol(dLastID)
label(dLastID)

registersymbol(sErrorIDNotGameVariable)
label(sErrorIDNotGameVariable)
registersymbol(sErrorIDNegativeIndex)
label(sErrorIDNegativeIndex)
registersymbol(sErrorIDTooLarge)
label(sErrorIDTooLarge)
registersymbol(sErrorMemory)
label(sErrorMemory)

label(startvariables)
label(endcodedata)

// code/data region for new code to execute.
codedata:
db FF FF FF FF FF FF FF FF // just a helper for me to see start of a region.
convertID:
pushad
lea edi,[sBuffer]
xor edx,edx
mov eax,[dLookupID]
cmp eax,[dLastID]
je endConvertID
cmp eax,000186A0
jnge IDNotGameVariable  // is the ID a game variable?
add eax,FFFE7960
js IDNegIndex           // if it has a negative sign, the ID is too low.
cmp eax,[Valdis_Story_AC.exe+2DF414]
jnl IDTooLarge          // if the ID is larger than the index, it's out of bounds.
mov ecx,[Valdis_Story_AC.exe+2DF410]
test ecx,ecx
je memEr
mov eax,[ecx+eax*4]     // akin to multiply eax by 4 then adding result to ecx, THEN loading the pointer at that location.
test eax,eax
je memEr
mov esi,eax
mov eax,FFFFFFFF
call copyString
endConvertID:
mov eax,[dLookupID]
mov [dLastID],eax
popad
jmp originalrwvariable

// error strings...
IDNotGameVariable:
lea esi,[sErrorIDNotGameVariable]
call copyString
jmp endConvertID

IDNegIndex:
lea esi,[sErrorIDNegativeIndex]
call copyString
jmp endConvertID

IDTooLarge:
lea esi,[sErrorIDTooLarge]
call copyString
jmp endConvertID

memEr:
lea esi,[sErrorMemory]
call copyString
jmp endConvertID

originalrwvariable:
db C7 46 04 00 00 00 00
jmp returnrwvariable

// Function copyString
// NOTE: all registers mentioned will be used/left dirty, expects caller to
// take care of things.
// expects eax to be FF (in ah)
// expects edx to be nil.
// expects the string source to be pointed to by esi.
// expects the string dest to be pointed to by edi
copyString:
cmp ah,0
je copyStringDone
cmp edx,[dStringBufferLen]
ja copyStringError
mov al,byte ptr[esi]
mov byte ptr[edi],al
mov ah,al
inc edx
inc esi
inc edi
jmp copyString
ret
copyStringError: // calculate how many extra bytes of buffer are needed.
cmp byte ptr[esi],0
je copyStringErrorDone
inc edx
inc esi
jmp copyStringError
copyStringErrorDone: // record that count into memory.
sub edx,[dStringBufferLen]
inc edx
mov [dStringBufferError],edx
ret
copyStringDone:
ret

// Local Variables (how to define larger than 4 byte region?
startvariables:
db EE EE EE EE EE EE EE EE

dLookupID:
dd 00019007 // entity ID at the moment.

dStringBufferLen:
dd 0x14

dStringBufferError:
dd 0

dLastID:
dd 0

sErrorIDNotGameVariable:
db 'Not game variable.'\0

sErrorIDNegativeIndex:
db 'ID -&gt; Neg Index.'\0

sErrorIDTooLarge:
db 'ID &gt; max ID.'\0

sErrorMemory:
db 'Memory Error.'\0

sBuffer:
db 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00

endcodedata:
db FF FF FF FF FF FF FF FF
///


// injected code, usually just jumps and nops.
rwvariableAOB+3:
jmp convertID
nop
nop
returnrwvariable:

[DISABLE]
rwvariableAOB+3:
db C7 46 04 00 00 00 00

unregistersymbol(rwvariableAOB)

unregistersymbol(dLookupID)
unregistersymbol(dStringBufferLen)
unregistersymbol(dStringBufferError)
unregistersymbol(dLastID)
unregistersymbol(sBuffer)
unregistersymbol(sErrorIDNotGameVariable)
unregistersymbol(sErrorIDNegativeIndex)
unregistersymbol(sErrorIDTooLarge)

