 jne _ExitGMD

 mov rax,[rbx+000002e8]
 test rax,rax
 jz _ExitGMD

 cmp byte ptr [rax+00003a50],00
 jne _ExitGMD

 mov eax,[rcx]
 mov [rcx+04],eax

 xor edx,edx

_ExitGMD:
 mov eax,[rcx+04]                  // Original code (get cHP)
 cmp edx,eax                       // Original code
 jmp _BackGMD                      // Back to main code

