  <Comments>The Binding of Isaac +?? Cheat Table
Version: 1.032 Steam
Updated: 11/12/2014
====================================
1) Click "Search player structure", then expand Hearts, Items, etc...
 1a) In the Pill Effects section you can edit which pill effects are in the pool for the current run. Values that appear to have two effects (e.g. Range Down/Up) have the negative effect starting out but change to the positive one when you pick up an item which prevents negative pill effects (e.g. PhD)
2) Click "Pedestal Queue" then find a pedestal, pick the item you want to pick up in place of the item you see on the pedestal by editing the "Next Item" field. You can also build up a queue of items, that can be picke up from pedestals by using the "Add Item"/"Add Items" scripts.
3) Edit "Current Floor" number and beat current floor, then you will be teleported to that floor.
4) In the "Seed" section you can see the seed for the current run, set up seed logging (logs to "seeds.txt" by default; change that in the Lua script) and also edit the seed before you start a new run (see http://puu.sh/dlaMC/aefbef679b.webm)
5) Click "Bum Familiars" and edit the coins/half hearts collected by "Bum Friend"/"Dark Bum" or make them spawn items endlessly
6) Click "The Lost unlocker!" and go die/suicide, you will unlock The Lost character.
7) Click "Stats" to edit your scores like death counts, mom kills, eden tokens, etc...
</Comments>
  <LuaScript>function setup()
  local card_pill_ids = {52, 187, 185}
  local attack_style_ids = {136, 143, 142, 141, 140, 139, 138, 137, 144,
                            145, 146, 147, 148, 149, 150, 151, 165, 127,
                            126, 125, 124, 123, 166, 121, 128, 129, 130,
                            131, 132, 133, 134, 135}
  local invulnerability_ids = {252, 253}
  local seeds_filename = "seeds.txt"
  local bums_spawn_ids = {269, 275}
  local gold_key_id = {285}

  makeBitsToggle(card_pill_ids)
  makeBitsToggle(attack_style_ids)
  makeBitsToggle(invulnerability_ids)
  makeBitsToggle(bums_spawn_ids)
  makeBitsToggle(gold_key_id)
  enableSL,disableSL = setup_SeedLog(seeds_filename)
  enableWD,disableWD = setup_Watchdog()
  pedestalScripts()
end





function makeToggleable(mr)
  local closure = function()
    if mr.Value == "1" then
      mr.Value = "0"
    else
      mr.Value = "1"
    end
  end
  return closure
end

function makeBitsToggle(toggle_ids)
  local al = getAddressList()
  for i,id in ipairs(toggle_ids) do
    local mr = al:getMemoryRecordByID(id)
    mr.OnActivate = makeToggleable(mr)
  end
end



function setup_SeedLog(filename)
  local old_seed = ""
  function isSeed(s)
    if s:len() ~= 9 then
      return false
    elseif s:byte(5) ~= 32 then
      return false
    elseif s:sub(1,4):match("%W") then
      return false
    elseif s:sub(6,9):match("%W") then
      return false
    end
    return true
  end
  function logSeed()
    local seed = readString("[isaac-ng.exe+21E18C]+2ed44", 10)
    if seed == nil then return end
    if seed == old_seed then
      return
    end
    if isSeed(seed) then
      local f = io.open(filename, "a")
      f:write(seed, "\n")
      f:close()
    end
    old_seed = seed
  end

  local sl_timer = createTimer(nil, false)
  sl_timer:setInterval(1000)
  sl_timer:setOnTimer(logSeed)

  local enableSL = function()
    sl_timer:setEnabled(true)
  end

  local disableSL = function()
    sl_timer:setEnabled(false)
    old_seed = ""
  end

  return enableSL, disableSL
end

function setup_Watchdog()
  function doWatch()
    local wd1 = readInteger("pPlayer+4")
    writeInteger("pPlayer+4",  0)
    local wd2 = readInteger("pPlayer2+4")
    writeInteger("pPlayer2+4", 0)

    if wd1 == 0 then
      writeInteger("pPlayer", 0)
    end
    if wd2 == 0 then
      writeInteger("pPlayer2", 0)
    end
  end

  local wd_timer = createTimer(nil, false)
  wd_timer:setInterval(200)
  wd_timer:setOnTimer(doWatch)

  local enableWD = function()
    wd_timer:setEnabled(true)
  end

  local disableWD = function()
    wd_timer:setEnabled(false)
  end

  return enableWD, disableWD
end

function pedestalScripts()

  local al = getAddressList()
  local mr = al:getMemoryRecordByID(221)

  function joinList(list)
    if #list == 0 then
      return ""
    end
    local string = tostring(list[1])
    for i = 2,#list do
      string = string .. " " .. tostring(list[i])
    end
    return string
  end

  function splitList(text)
    local list = {}; local pos = 1; local delimiter = " "
    while 1 do
      local first, last = string.find(text, delimiter, pos)
      if first then
        table.insert(list, tonumber(string.sub(text, pos, first-1)))
        pos = last+1
      else
        table.insert(list, tonumber(string.sub(text, pos)))
        break
      end
    end
    return list
  end

  function pedestal_getQueue()
    local queue = {}
    local base = getAddress("pe_queue")
    local i = 1
    while true do
      local v = readInteger(base)
      if v == 0 then
        break
      end
      queue[i] = v
      i = i+1
      base = base+4
    end
    return queue
  end

  function pedestal_setQueue(queue)
    local base = getAddress("pe_queue")
    for i=1,#queue do
      writeInteger(base, queue[i])
      base = base + 4
    end
    writeInteger(base,0)
  end

  function pedestal_addItem()
    if not mr.Active then
      return false
    end
    local id = readInteger("pe_addid")
    if id == 0 then
      return false
    end
    local queue = pedestal_getQueue()
    queue[#queue+1] = id
    pedestal_setQueue(queue)
    writeInteger("pe_queue_len", readInteger("pe_queue_len")+1)
    return false
  end

  function pedestal_addItems()
    if not mr.Active then
      return false
    end
    local queue = pedestal_getQueue()
    local news = inputQuery("Add Items to Pedestal Queue", "queue:", joinList(queue))
    if news == nil then
      return false
    end
    local q2 = splitList(news)
    pedestal_setQueue(q2)
    return false
  end

  function pedestal_clearQueue()
    writeInteger("pe_queue", 0)
    writeInteger("pe_queue_len", 0)
  end

  al:getMemoryRecordByID(227).OnActivate = pedestal_addItem
  al:getMemoryRecordByID(228).OnActivate = pedestal_addItems
  al:getMemoryRecordByID(229).OnActivate = pedestal_clearQueue
  makeBitsToggle({224})
end


setup()
