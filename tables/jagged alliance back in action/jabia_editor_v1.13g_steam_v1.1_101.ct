      <AssemblerScript>[ENABLE]
alloc(MP_entry_militia_trade,256)
alloc(MP_exit_militia_trade,256)
alloc(MP_data_buffer,16)
alloc(MP_temp_inventory,1024)

label(MP_entry_militia_trade_return)
label(MP_exit_militia_trade_return)

registersymbol(MP_temp_inventory)
registersymbol(MP_data_buffer)

MP_entry_militia_trade:
// the call calls the copy inventory function which copies the inventory
// targetted to by ECX to the inventory targetted to by EAX
// ECX shall now point to a custom inventory which later on shall become
// editable from within the lua gui
// so it becomes kinda like the magic pocket function from Rhagic's table :)
mov [MP_data_buffer],ecx // save pointer to player's inventory and
mov ecx,MP_temp_inventory // copy a fake inventory instead

call GameJABiA.exe+138B10 // the original code only called this function
mov ecx,[MP_data_buffer] // restore the players inventory
mov ecx,[ecx+000000F0] // from original copy inventory function
mov [eax+000000F0],ecx // it happens at the end of copy_inventory and must be
                       // done otherwise the game will crash
jmp MP_entry_militia_trade_return // we good, leave now



MP_exit_militia_trade:
// do the pointer thing but omit the actual inventory copying
mov ecx,[ecx+000000F0] // from original copy inventory function
mov [eax+000000F0],ecx // it happens at the end of copy_inventory and must be
                       // done otherwise the game will crash
jmp MP_exit_militia_trade_return // we done, bai bai

// -----------------------------------------------------------------------------
// militia trade entry point:
// copy temporary inventory into trading screen instead of the selected mercs one
// -----------------------------------------------------------------------------
"GameJABiA.exe"+2F800:
jmp MP_entry_militia_trade
MP_entry_militia_trade_return:


// -----------------------------------------------------------------------------
// militia trade exit point:
// PREVENT copying temporary inventory back into the selected mercs one
// -----------------------------------------------------------------------------
"GameJABiA.exe"+20472:
jmp MP_exit_militia_trade
MP_exit_militia_trade_return:


MP_temp_inventory:
