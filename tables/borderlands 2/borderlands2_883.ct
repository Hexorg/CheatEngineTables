        <CheatEntry>
          <ID>3076</ID>
          <Description>"Weapon leveler script [from Rhagic]"</Description>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>/*
Rhagic:
The actual item level of a weapon is at +E3C.
Modifying the item level causes it's stats to be recalculate the next time you un/re-equip the item.
This script hooks the code that is called when you switch weapons.
So:
- enable script
- equip the weapon in game,
- then swap it in and back out of your back pack (do an unequip/equip cycle).
- close inventory
- pull out each weapon you want updated (just hit the 1, 2, 3, 4, 1 hotkeys)
- disable script
If you do this, the weapon will be upgraded to your current level.
The change is persistent between saves!
*/

[ENABLE]
// WeaponStart+1EC+3EC+258 // char level from weapon
// 50 8B CF E8 xx xx xx xx 8B 53 0C 6A 07 52 8B CF
// Borderlands2.exe+162B34 - 50                    - push eax
// Borderlands2.exe+162B35 - 8B CF                 - mov ecx,edi
// Borderlands2.exe+162B37 - E8 C4CC9E00           - call Borderlands2.GetOutermost+D9730
// Borderlands2.exe+162B3C - 8B 53 0C              - mov edx,[ebx+0C] // Read Real Level
// Borderlands2.exe+162B3F - 6A 07                 - push 07
// Borderlands2.exe+162B41 - 52                    - push edx
// Borderlands2.exe+162B42 - 8B CF                 - mov ecx,edi

aobscan(WeapLevel_Addr,50 8B CF E8 xx xx xx xx 8B 53 0C 6A 07 52 8B CF)
alloc(WeapLevel_Data,1024)
label(WeapLevel_Hook)
label(WeapLevel_Start)
label(WeapLevel_Exit)
label(WeapLevel_Return)
label(WeapLevel_Lvl)
registersymbol(WeapLevel_Hook)
registersymbol(WeapLevel_Lvl)

// Found Game code to hook
WeapLevel_Addr+8:
WeapLevel_Hook:
jmp WeapLevel_Start
WeapLevel_Return:

// Our Code
WeapLevel_Data:
WeapLevel_Lvl:
db 00 00 00 00
WeapLevel_Start:
mov edx,[ebx-0C44]
mov edx,[edx+03EC]
mov edx,[edx+0258]
mov [ebx+0c], edx
mov [WeapLevel_Lvl], edx
WeapLevel_Exit:
mov edx,[ebx+0c]
push 07
jmp WeapLevel_Return

[DISABLE]
WeapLevel_Hook:
db 8B 53 0C 6A 07 52 8B CF

unregistersymbol(WeapLevel_Hook)
unregistersymbol(WeapLevel_Lvl)

dealloc(WeapLevel_Data)

</AssemblerScript>
        </CheatEntry>
        <CheatEntry>
          <ID>3205</ID>
          <Description>"Item leveler script v2 [from Rhagic]"</Description>
          <Options moHideChildren="1"/>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>/*
Rhagic:
- Turn on option while in inventory
- Mousing over (loading tooltip) an item will update it's level to the current player level (if ItemLevel_DesLvl is 0) or ItemLevel_DesLvl.
- Swap items between backpack and equipped slots to see changes.
- Turn off option before leaving inventory

Option may crash your game (if turned on) while out of inventory.
*/

[ENABLE]
// ItemStart+1EC+3EC+258 // char level from weapon
// CC CC CC CC 8B 81 D0 01 00 00 C3 CC CC CC CC
// Borderlands2.exe+DAF4C - CC                    - int 3
// Borderlands2.exe+DAF4D - CC                    - int 3
// Borderlands2.exe+DAF4E - CC                    - int 3
// Borderlands2.exe+DAF4F - CC                    - int 3
// Borderlands2.exe+DAF50 - 8B 81 D0010000        - mov eax,[ecx+000001D0]
// Borderlands2.exe+DAF56 - C3                    - ret
// Borderlands2.exe+DAF57 - CC                    - int 3
// Borderlands2.exe+DAF58 - CC                    - int 3
// Borderlands2.exe+DAF59 - CC                    - int 3
// Borderlands2.exe+DAF5A - CC                    - int 3
//

aobscan(ItemLevel_Addr,CC CC CC CC 8B 81 D0 01 00 00 C3 CC CC CC CC)
alloc(ItemLevel_Data,4096)
label(ItemLevel_Hook)
label(ItemLevel_Start)
label(ItemLevel_Exit)
label(ItemLevel_Return)

label(ItemLevel_ItemAddr)
label(ItemLevel_TargetLvl)
label(ItemLevel_DesLvl)

label(ItemLevel_GetPlayerLvl)
label(ItemLevel_GotLvl)
label(ItemLevel_NotGun)
label(ItemLevel_MakeChange)

registersymbol(ItemLevel_Hook)
registersymbol(ItemLevel_ItemAddr)
registersymbol(ItemLevel_DesLvl)

// Found Game code to hook
ItemLevel_Addr+4:
ItemLevel_Hook:
jmp ItemLevel_Start
nop
ItemLevel_Return:

// Our Data
ItemLevel_Data:
ItemLevel_ItemAddr:
db 00 00 00 00
ItemLevel_TargetLvl:
db 00 00 00 00
ItemLevel_DesLvl:
db 00 00 00 00
// Our Code
ItemLevel_Start:
pushfd
pushad
mov [ItemLevel_ItemAddr], ecx // Store Item Location for convenience.
// Check if desired Level Specified:
mov eax, [ItemLevel_DesLvl]
cmp eax, 0
je ItemLevel_GetPlayerLvl
mov [ItemLevel_TargetLvl], eax
jmp ItemLevel_GotLvl
// Check Player Level
ItemLevel_GetPlayerLvl:
mov eax, [ecx+1EC]
cmp eax, 0
je ItemLevel_Exit
mov eax, [eax+3EC]
cmp eax, 0
je ItemLevel_Exit
mov eax, [eax+258]
cmp eax, 0
je ItemLevel_Exit
mov [ItemLevel_TargetLvl], eax
ItemLevel_GotLvl:
// Current Item Display Level is at +1E0
mov ebx, [ecx+1E0]
cmp ebx, 0
je ItemLevel_Exit
cmp ebx, [ecx+E3C] // Compare Current Level with Weapon Actual Level Offset +E3C
jne ItemLevel_NotGun
// Matches gun level
lea eax, [ecx+E3C]
jmp ItemLevel_MakeChange
ItemLevel_NotGun:
cmp ebx, [ecx+8B4] // Compare Current Level with Item Actual Level Offset +8B4
jne ItemLevel_Exit // No Match, exit.
lea eax, [ecx+8B4] // Matches, assign address to EAX
ItemLevel_MakeChange:
mov ebx, [ItemLevel_TargetLvl]
mov [eax], ebx // Assign New Value to Actual Level
lea eax, [ecx+1E4] // Additional Level Specification at +1E4, not auto updated
mov [eax], ebx // Update Additional Level Specification.
ItemLevel_Exit:
popad
popfd
mov eax,[ecx+000001D0]
jmp ItemLevel_Return

[DISABLE]
ItemLevel_Hook:
db 8B 81 D0 01 00 00

unregistersymbol(ItemLevel_Hook)
unregistersymbol(ItemLevel_ItemAddr)
unregistersymbol(ItemLevel_DesLvl)

dealloc(ItemLevel_Data)

</AssemblerScript>
