define(MOCT,H5_Game.exe+008d45b5)

//=========================================
// Constants used in the script (that can be changed)
define(ctMMON,#63000)  // Minimum Money
define(ctMRES,#6300)   // Minimum Resources
define(ctMTSZ,#2063)   // Minimum Size

//=========================================
[ENABLE]
//=========================================
//LuaCall(CheckVersion())
//=========================================
// Check if script is compatible to this game version
// If false the script will not be loaded

assert(MHPR,ff 90 84 01 00 00 84 c0 ?? ?? b3 01 ?? ?? 32 db 8d 4c 24 18 8d)
assert(MOAM,89 81 2c 01 00 00 83 c1 e4 e8 ?? ?? ?? ?? c2 04 00 ?? ?? ?? ??)
assert(MOCT,8b 40 74 8b 2c 88 85 ed ?? ?? 8b 4d 04 8b 51 04 f6 44 2a 0b 80)

//=========================================
alloc(MyCode,4096,LUDO)
//=========================================
// Declaration section
label(_MonHumanPlayer)
label(_BackMHP)
label(_ExitMHP)
label(_MonArmyMovement)
label(_MonAM00)
label(_MonAM01)
label(_BackMAM)
label(_ExitMAM)
