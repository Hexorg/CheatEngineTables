 cmp esi,[pOwner]
 jne _GodM00

_GodM0E:
 cmp dword ptr [iEnableGM],0
 je _ExitGM                        // Jump if feature is disabled

 mov ecx,[eax+20]
 mov [eax],ecx

 xor edx,edx                       // Value2decrease = 0
 jmp _ExitGM
//==
