  <LuaScript>function Employee()
    counterValue = readInteger("[[[bGSM]+C]+10]+10")
    if counterValue == nil then
      return
    end
    for _o=1, counterValue do
      _o= 16 + 4*(_o-1)
      --Professionalism
      if readInteger("bPro") &gt; 0 then
         writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+34", readInteger("bPro"))
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+10", readInteger("bPro"))
      end
      --Energy
      if readInteger("bEne") &gt; 0 then
         writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+2C", readInteger("bEne"))
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+18", readInteger("bEne"))
      end
      --Viewable Political Affiliation
      writeBytes("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+48", 1, 1)
      --Political Affiliation
      if readInteger("bPol") &gt; 0 then
         writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+44", readInteger("bPol"))
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+24", readInteger("bPol"))
      end
      --Drunk
      writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+58", readInteger("bDru"))
      --Alcoholism
      if readInteger("bAlc") &gt; 0 then
         writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+20", 0)
         writeInteger("[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+24", 0)
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+1C", 0)
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+20", 0)
      end
      --Sex
      if readInteger("bGen") &gt; 0 then
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+28", readInteger("bGen")-1)
      end
      --Age
      if readInteger("bAge") &gt; 0 then
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+30", readInteger("bAge")-1)
      end
      --Race
      if readInteger("bRac") &gt; 0 then
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+2C", readInteger("bRac")-1)
      end
      --Laziness
      if readInteger("bLaz") &gt; 1 then
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+3C", 100)
      elseif readInteger("bLaz") &gt; 0 then
         writeInteger("[[[[[[bGSM]+C]+10]+8]+"..string.format("%X",_o).."]+18]+3C", 0)
      end
    end
end

function PrintReports()
  counterValue = readInteger("[[[[bGSM]+18]+C]+8]+C")
  if counterValue == nil then
    return
  end
  print("")
  print("")
  print("")
  print("**Investigation Reports**")
  print("")
  for _o=1, counterValue do
    __o= 16 + 4*(_o-1)
    if readBytes("[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+90",1) == 1 then
       print("Investigation: " .._o.." is solved.")
    else
       print("Investigation: " .._o.." is pending...")
       cntValue = readInteger("[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+60]+C")
       if cntValue == nil then
          print("    Frames Unlocked: None")
       else
          s=""
          for _p=1, cntValue do
            _p= 16 + 4*(_p-1)
            if _p == 16 then
               s=s..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+60]+8]+10")
            else
               s=s..", "..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+60]+8]+"..string.format("%X",_p))
            end
          end
          print("    Frames Unlocked: "..s)
       end
       cntValue = readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+C")
       if cntValue == nil then
          print("    Frame Solution:   None")
       else
          s=""
          for _p=1, cntValue do
            _p= 16 + 4*(_p-1)
            if _p == 16 then
               s=s..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+"..string.format("%X",_p))
            else
               s=s..", "..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+"..string.format("%X",_p))
