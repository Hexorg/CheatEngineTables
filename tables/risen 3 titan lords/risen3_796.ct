function FormShow(sender)
  if ItemBase == "" then
    local IScan = AOBScan(ItemBaseAOB)
    ItemBase = IScan[0]
    ItemPoint = "[[" .. ItemBase .. "+1]+c4]+2d8"
    ItemAddr = getAddress(ItemPoint)
    IScan = AOBScan(BaseBaseAOB)
    BaseAddr = getAddress("[[" .. IScan[0] .. "+8]+10]")
    IScan = nil

  end
  local IList = ItemEdit_LVItems:getItems()
  if IList:getCount() == 0 then
    for k,v in pairs(ItemTable) do
      local ListAdd = IList:add()
      ListAdd:setCaption(BaseGet(v.ID))
      local ListSub = ListAdd:getSubItems()
      ListSub:add(v.ItemName)
    end
  end
end

function BItemUpdateClick(sender)
  ItemEdit_LVInventory:clear()
  local List = ItemEdit_LVInventory:getItems()
  local ICount = readInteger(ItemAddr+4)
  local IStart = readInteger(ItemAddr)
  for i = 0, ICount -1 do
    local ListEntry = List:add()
    ItemGet(IStart+i*0x28,ListEntry)
  end

end

function BQuantUpdateClick(sender)
  local Quant = tonumber(ItemEdit_EQuant.Text)
  local Index = ItemEdit_LVInventory:getItemIndex()
  if Quant == nil then showMessage("Please enter a valid number.") return end
  if Index == -1 then showMessage("Please select an item first.") return end
  writeInteger(readInteger(ItemAddr)+(Index*0x28)+0x24,Quant)
  BItemUpdateClick()
end

function BItemSwapClick(sender)
  local Index = ItemEdit_LVInventory:getItemIndex()
  if Index == -1 then showMessage("Please select an item first.") return end
  local CBIndex = ItemEdit_CBItems:getItemIndex()
  local CBID = ItemTable[CBIndex+1].ID
  local SwapPoint = readInteger(readInteger(BaseAddr+0xC*CBID+8)+0xC)
  writeInteger(readInteger(ItemAddr)+Index*0x28+4,SwapPoint)
  BItemUpdateClick()
end

function ItemGet(IAddr,RowEntry)
  local IID = readInteger(readInteger(readInteger(IAddr+4))+0x10)
  local DefaultName = IID
  local DefaultBase = BaseGet(IID)
  for k,v in pairs(ItemTable) do
    if IID == v.ID then
    DefaultName = v.ItemName
    break
    end
  end
  RowEntry:setCaption(DefaultBase)
  local RowSub = RowEntry:getSubItems()
  RowSub:add(DefaultName)
  RowSub:add(readInteger(IAddr+0x24))
end

function BaseGet(BID)
  return readString(readInteger(BaseAddr+0xC*BID))
end
</LuaScript>
</CheatTable>
