          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>//=========================================
[ENABLE]
//=========================================
label(CHP)
label(CHP2)
label(CHP3)
registersymbol(CHP)
registersymbol(CHP2)
registersymbol(CHP3)

//aobscan(aob1, A1 20 FB ?? 01 80 B8 58 04 00 00 00 75 ??) //show console       OLD
//aobscan(aob2, 8B 1D 20 FB ?? 01 39 BB 4C 04 00 00 74 ??) //enable console     OLD

//=========================================
// Actual script
//=========================================

//Removes the check for showing the console
//aob1+5:                                                                       OLD
CK2game.CConsole::Show+5:
CHP:
 nop
 nop
 nop
 nop
 nop
 cmp eax,eax

//Removes the check for ironman mode
//aob2+6:                                                                       OLD
CK2game.CConsoleCmdManager::Execute+3C:
CHP2:
 nop
 nop
 nop
 nop
 cmp eax,eax

CK2game.CConsoleCmdManager::Execute+379:
CHP3:
 mov byte ptr [eax+41],01

[DISABLE]
CHP:
//cmp byte ptr [eax+00000468],00
 db 80 B8 68 04 00 00 00
CHP2:
//cmp [ebx+0000045C],edi
 db 39 BB 5C 04 00 00
CHP3:
//mov byte ptr [eax+41],00
 db C6 40 41 00

unregistersymbol(CHP)
unregistersymbol(CHP2)
</AssemblerScript>
