    <CheatEntry>
      <ID>10</ID>
      <Description>"Inf. Ammo (hold R to temp disable)"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat
alloc(newmem2,2048) //2kb should be enough
label(returnhere2)
label(originalcode2)
label(exit2)
aobscan(AOB2,8B 45 08 01 03 8B 1B 8B 4D 0C 5E 89 19 5B 8B E5)
label(AOB2JMP)
registersymbol(AOB2JMP)

newmem2: //this is allocated memory, you have read,write,execute access
//place your code here
//cmp [edx+68],00000C28
pushad
push 'R'      //key R
call GetAsyncKeyState
shr ax,#15
cmp ax,1
popad
je originalcode2

mov ecx,[ebp+68]
or ecx,ecx
je originalcode2
cmp [ecx-38],00000C28
jne originalcode2
cmp dword ptr [ecx-3C],00000000
jne originalcode2
cmp dword ptr [ecx-40],00000000
jne originalcode2
mov ecx,[ebp+84]
sub ecx,1
cmp [ebx],ecx
jl originalcode2
mov eax,0

originalcode2:
add [ebx],eax
mov ebx,[ebx]
mov ecx,[ebp+0C]

exit2:
jmp returnhere2

AOB2+3:                              //"MassEffect3.exe"+449AD:
AOB2JMP:
jmp newmem2
nop
nop
returnhere2:


 
 
[DISABLE]
//code from here till the end of the code will be used to disable the cheat
dealloc(newmem2)
AOB2JMP:                             //"MassEffect3.exe"+449AD:
db 01 03 8B 1B 8B 4D 0C
//Alt: add [ebx],eax
//Alt: mov ebx,[ebx]
//Alt: mov ecx,[ebp+0C]
unregistersymbol(AOB2JMP)
</AssemblerScript>
    </CheatEntry>
    <CheatEntry>
      <ID>4</ID>
