//=========================================
MyCode:
//=========================================
// Resources: Food, Wood and Metal (when increasing)
_MonPlayerRes:
 push ecx
 push esi

 add [eax],ebx                     // Original code

 cmp edi,1
 jne _ExitMPR

 cmp dword ptr [iEnableMPR],0
 je _ExitMPR                       // Jump if feature is disabled

 mov ecx,[edx+10]
 cmp ecx,1
 je _MonPR10

 cmp ecx,2
 je _MonPR10

 cmp ecx,3
 je _MonPR10

 cmp ecx,4
 je _MonPR10

 cmp ecx,6
 jne _ExitMPR

 mov ecx,#60
 cmp ecx,[edx+14]
 jle _ExitMPR

 mov [edx+14],ecx
 jmp _ExitMPR
//----
_MonPR10:
 mov ecx,[iMinRes]
 cmp ecx,[edx+14]
 jle _ExitMPR

 mov [edx+14],ecx

_ExitMPR:
 pop esi
 pop ecx

 cmp byte ptr [ebp+18],00          // Original code
 jmp _BackMPR                      // Back to main code

//=========================================
// Upon selecting building or units
_MonSelObj:
 mov [pUnit],eax

_ExitMSO:
 mov [ecx+esi*4],eax               // Original code
 mov esi,edi                       // Original code
 jmp _BackMSO                      // Back to main code

//=========================================
// Upon combat
_GodMode:
 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov eax,[esi+28]
 test eax,eax
 jz _ExitGMD

 cmp dword ptr [eax+000000fc],1
 jne _ExitGMD

 mov ebx,[eax+00000088]
 mov [esi+30],ebx

 xor eax,eax
 mov [esp+58],eax

_ExitGMD:
 sub ebx,[esp+58]                  // Original code
 mov [esi+30],ebx                  // Original code
 jmp _BackGMD                      // Back to main code

//=========================================
// On Main Screen
_MonPlayerXP:
 mov ecx,[esi+000000fc]
 cmp ecx,1
 jne _ExitMPX

 cmp dword ptr [iEnableMMX],0
 je _MonPX00                       // Jump if feature is disabled

 mov ecx,[iMinXP]
 cmp ecx,[esi+20]
 jle _MonPX00

 mov [esi+20],ecx

_MonPX00:
 cmp dword ptr [iEnableMSX],0
 je _ExitMPX                       // Jump if feature is disabled

 xor ecx,ecx
 mov [eax+2c],ecx

_ExitMPX:
 mov ecx,[esi+20]                  // Original code
 mov eax,[eax+2C]                  // Original code
 jmp _BackMPX                      // Back to main code

//=========================================
// Resources: Population (when decreasing)
_MonPop:
 push ecx

 mov edx,[ebp+14]                  // Original code
 sub [eax],edx                     // Original code

 cmp esi,6
 jne _ExitMPN

 mov ecx,[ebp-10]
 cmp ecx,1
 jne _ExitMPN

 cmp dword ptr [iEnableMPR],0
 je _ExitMPN                       // Jump if feature is disabled

 mov ecx,#60
 cmp ecx,[eax]
 jle _ExitMPN

 mov [eax],ecx
 jmp _ExitMPN

_ExitMPN:
 pop ecx
 jmp _BackMPN                      // Back to main code

//=========================================
 db '=====================================&gt;'
 db 'CE6.3 Script by Recifense 010214'
//=========================================
// Variables
