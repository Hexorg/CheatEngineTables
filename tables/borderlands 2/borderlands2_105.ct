  object_destroy(timer)
  DebugLog("@Function: GetRarityChancesMemoryRecordsDelayed()")

  DebugLog("-Rarity Mod Status: Scanning... Please wait!")
  processMessages()

    local address = nil

    --RarityChances
    for symbol,groupscancommand in pairs(RarityChances_Grouped) do
