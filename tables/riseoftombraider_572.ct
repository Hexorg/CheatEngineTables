  {===============}
  {RBX here (health calculation) is equal to RDI from Player}
  {===============}
  cmp [playerCheck],rbx {Filter}
  jne healthCalc_orig
  movd xmm0,[rcx+2B4] {Max Health in stead of New Health into xmm0 before cvtdq2ps}
  movd [rax+28],xmm0 {Move Max Health (dword) into New Health (dword). Using xmm0 since the original uses it anyway.}
  cvtdq2ps xmm0,xmm0
  movss [rax+2C],xmm0 {Move Max Health (float) into New Health (float).}
  jmp healthCalc_exit

healthCalc_orig:
  movd xmm0,[rax+28]
  cvtdq2ps xmm0,xmm0
  jmp healthCalc_exit

