//code from here to '[DISABLE]' will be used to enable the cheat
label(SeinHealthController_savedAOB)
label(SeinHealthController_returnhere)
label(SeinHealthController_originalcode)
label(SeinHealthController_exit)

label(SeinHealthController_update_exit)
label(SeinHealthController_set_exit)
label(SeinHealthController_healthset_exit)
label(SeinHealthController_jumpset_exit)
label(SeinHealthController_skillset_exit)
label(SeinHealthController_energyset_exit)
label(SeinHealthController_immortalset_exit)

registersymbol(SeinHealthController_savedAOB)

aobscan(SeinHealthController_searchedAOB,D9 47 24 DD 5D E0 D9 47 1C)

alloc(SeinHealthController_newmem,2048) //2kb should be enough
alloc(SeinHealthController_temp1,4)


SeinHealthController_newmem:
mov [SeinHealthController_temp1], ebx

cmp dword ptr [SeinHealthController_update_enable], #0
je SeinHealthController_update_exit
mov [SeinHealthController_basepointer], edi
SeinHealthController_update_exit:

cmp dword ptr [SeinHealthController_set_enable], #0
je SeinHealthController_set_exit
SeinHealthController_set_exit:

cmp dword ptr [SeinHealthController_healthset_enable], #0
je SeinHealthController_healthset_exit
fild [edi+20]
fstp [edi+1c]
SeinHealthController_healthset_exit:

cmp dword ptr [SeinHealthController_jumpset_enable], #0
je SeinHealthController_jumpset_exit
mov ebx, [edi+18]
mov ebx, [ebx+10]
mov ebx, [ebx+c]
mov [ebx+50], (float)60
mov [ebx+54], (float)60
mov [ebx+5c], (float)60
mov [ebx+60], (float)60
mov [ebx+68], (float)60
mov [ebx+6c], (float)60
SeinHealthController_jumpset_exit:

cmp dword ptr [SeinHealthController_skillset_enable], #0
je SeinHealthController_skillset_exit
mov ebx, [edi+18]
mov ebx, [ebx+38]
mov [ebx+24], #999
SeinHealthController_skillset_exit:

cmp dword ptr [SeinHealthController_energyset_enable], #0
je SeinHealthController_energyset_exit
mov ebx, [edi+18]
mov ebx, [ebx+3c]
fld [ebx+24]
fstp [ebx+20]
SeinHealthController_energyset_exit:

cmp dword ptr [SeinHealthController_immortalset_enable], #0
je SeinHealthController_immortalset_exit
mov ebx, [edi+18]
mov ebx, [ebx+40]
mov ebx, [ebx+8]
mov byte ptr [ebx+4c], #1
SeinHealthController_immortalset_exit:

mov ebx, [SeinHealthController_temp1]


SeinHealthController_originalcode:
db D9 47 24 DD 5D E0
//fld dword ptr [edi+24]
//fstp qword ptr [ebp-20]


SeinHealthController_exit:
jmp SeinHealthController_returnhere


SeinHealthController_searchedAOB:
SeinHealthController_savedAOB:
jmp SeinHealthController_newmem
nop
SeinHealthController_returnhere:


[DISABLE]
SeinHealthController_savedAOB:
db D9 47 24 DD 5D E0
//fld dword ptr [edi+24]
//fstp qword ptr [ebp-20]

unregistersymbol(SeinHealthController_savedAOB)

dealloc(SeinHealthController_newmem)
dealloc(SeinHealthController_temp1)

