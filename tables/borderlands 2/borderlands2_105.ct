function GetBasePointer(pointer)
     local scanObj = createMemScan()

     memscan_returnOnlyOneResult(scanObj,true)
     memscan_firstScan(scanObj, soExactValue, vtDword, "+W-X-C",
                       pointer,"0","0x00000000","0x70000000",'',fsmAligned,
                       "4" ,false ,false, true, false);
     memscan_waitTillDone(scanObj)

     local result = memscan_getOnlyResult(scanObj)
     object_destroy(scanObj)

     if result==nil then return nil end
     return result
end

function GetPointerNames()
  if getOpenedProcessID() == 0 and readInteger("pBasePointer") == nil then return end
  DebugLog("@Function: GetPointerNames()")

  local bp = getNameFromAddress(GetBasePointer(readInteger(getAddress('[pBasePointer]'))))
  local kp = getNameFromAddress(GetBasePointer(readInteger(getAddress('pKeyBasePointer'))))
  if bp == nil then return end

  DebugLog("-Base Pointer Name: " .. bp)
  DebugLog("-Key Pointer Name: " .. kp)
end
