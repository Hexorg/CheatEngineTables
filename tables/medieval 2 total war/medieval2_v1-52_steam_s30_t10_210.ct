//=========================================
// TBS - Main Screen (all the time)
_MonHumanPlayer:
 mov [pPlayer],eax

 cmp dword ptr [iEnableMMG],0
 je _ExitMHP                       // Jump if feature is disabled

 cmp dword ptr [eax+00000aec],ctMMON
 jge _ExitMHP

 mov dword ptr [eax+00000aec],ctMMON

_ExitMHP:
