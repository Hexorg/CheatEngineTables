function AddFrames()
  --Collect Case
  Case=readInteger("bCase")
  if Case == 0 then
    return
  end
  --Establish Case Offset
  __o= 16 + 4*(Case-1)
  --Read Answer Frames
  cntValue = readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+C") --Answer Frames Cnt Value
  if cntValue == nil then
     return
  else
     --Set Number of Frames
     writeInteger("[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+60]+C",cntValue)--Unlocked Frames Cnt Value
     for _p=1, cntValue do
       _p= 16 + 4*(_p-1)
  --Set Answer Frames into Unlocked Frames
       writeInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+60]+8]+"..string.format("%X",_p),readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+"..string.format("%X",_p)))
     end
  end
end

