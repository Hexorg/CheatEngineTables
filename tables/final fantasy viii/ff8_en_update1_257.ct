selphieSpellAd =  0x01CFE3F1



function GodMode(sender)

-- GodMode

  -- Setting the middle character's HP to 1/4-1 of maximum

  charMidMaxHp = readShortint(charMidMaxHpAd)
  charMidCurHp = readShortint(charMidCurHpAd)

  if ( charMidMaxHp and charMidCurHp ) then
    if charMidCurHp &lt; (charMidMaxHp / 4 - 1)
      then writeShortint(charMidCurHpAd, charMidMaxHp / 4 - 1)
    end
  end

  -- Setting the right character's HP to 1/4-1 of maximum

  charRigMaxHp = readShortint(charRigMaxHpAd)
  charRigCurHp = readShortint(charRigCurHpAd)

  if ( charRigMaxHp and charRigCurHp ) then
    if charRigCurHp &lt; (charRigMaxHp / 4 - 1)
      then writeShortint(charRigCurHpAd, charRigMaxHp / 4 - 1)
    end
  end

  -- Setting the left character's HP to 1/4-1 of maximum

  charLefMaxHp = readShortint(charLefMaxHpAd)
  charLefCurHp = readShortint(charLefCurHpAd)

  if ( charLefMaxHp and charLefCurHp ) then
    if charLefCurHp &lt; (charLefMaxHp / 4 - 1)
      then writeShortint(charLefCurHpAd, charLefMaxHp / 4 - 1)
    end
  end

  -- Setting the middle character's GF's HP to maximum

  charMidGfMaxHp = readShortint(charMidGfMaxHpAd)
  charMidGfCurHp = readShortint(charMidGfCurHpAd)

  if ( charMidGfMaxHp and charMidGfCurHp ) then
    if charMidGfCurHp &lt; (charMidGfMaxHp)
      then writeShortint(charMidGfCurHpAd, charMidGfMaxHp)
    end
  end

  -- Setting the right character's GF's HP to maximum

  charRigGfMaxHp = readShortint(charRigGfMaxHpAd)
  charRigGfCurHp = readShortint(charRigGfCurHpAd)

  if ( charRigGfMaxHp and charRigGfCurHp ) then
    if charRigGfCurHp &lt; (charRigGfMaxHp)
      then writeShortint(charRigGfCurHpAd, charRigGfMaxHp)
    end
  end

  -- Setting the left character's GF's HP to maximum

  charLefGfMaxHp = readShortint(charLefGfMaxHpAd)
  charLefGfCurHp = readShortint(charLefGfCurHpAd)

  if ( charLefGfMaxHp and charLefGfCurHp ) then
    if charLefGfCurHp &lt; (charLefGfMaxHp)
      then writeShortint(charLefGfCurHpAd, charLefGfMaxHp)
    end
  end

-- Infinite Items (if you have at least one)

  if readBytes(firstItemAd) then
    maxThings(firstItemAd, 198)
  end

-- Infinite Spells (if you have at lest one)

  if readBytes(firstItemAd) then
    maxThings(squallSpellAd, 32)
    maxThings(zellSpellAd, 32)
    maxThings(quistisSpellAd, 32)
    maxThings(selphieSpellAd, 32)
  end

-- Infinite Cards (if you have at lest one)

--  if readBytes(firstItemAd) then
--    maxThings(firstCardAd, 110)
--  end

end


