    <CheatEntry>
      <Description>"GalCiv2_V10_Script"</Description>
      <Color>000000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{
===========================================
 Galactic Civilization 2
 Game Version  : 1.0
 Script Version: 1.0
 CE Version    : 6.0
 Empire Treasure, Research Tech, Move Points, God Mode, Colonists
 14-Jan-2011
===========================================
}

[ENABLE]
//=========================================
// Check if script is compatible to this game version
// If false the script will not be loaded

assert(galciv2.exe+07ebad,8b af f0 04 00 00)

//=========================================

alloc(MyCode,1024)

label(_MonET)
label(_BackME)
label(_ExitME)
label(_MonRT)
label(_BackMR)
label(_ExitMR)
label(_MonSelShip)
label(_BackMSS)
label(_ExitMSS)
label(_MonSS0)
label(_MonSS1)
label(_MonMovesLeft)
label(_BackMML)
label(_ExitMML)
label(_GodMode)
label(_BackGM)
label(_ExitGM)
label(_MonInvation)
label(_BackMI)
label(_ExitMI)
label(_MonI0)
label(pPlayer)
label(pSelShip)
label(iPlayerID)
label(iEnableME)
label(iEnableMR)
label(iEnableMM)
label(iEnableGM)
label(iEnableMC)
label(iRTCount)

registersymbol(MyCode)
registersymbol(pPlayer)
registersymbol(pSelShip)
registersymbol(iPlayerID)
registersymbol(iEnableME)
registersymbol(iEnableMR)
registersymbol(iEnableMM)
registersymbol(iEnableGM)
registersymbol(iEnableMC)
registersymbol(iRTCount)

//=========================================
MyCode:
//=========================================

_MonET:
 mov dword ptr [iRTCount],2

 mov [pPlayer],edi

 mov ebp,[edi+04]
 mov [iPlayerID],ebp

 cmp dword ptr [iEnableME],0
 je _ExitME

 mov ebp,#10000

 cmp ebp,[edi+000004f0]
 jle _ExitME

 mov [edi+000004f0],ebp

_ExitME:
 mov ebp,[edi+000004f0]
 jmp _BackME

//=========================================
_MonRT:
 cmp dword ptr [iRTCount],0
 je _ExitMR

 cmp dword ptr [iEnableMR],0
 je _ExitMR

 cmp esi,[pPlayer]
 jne _ExitMR

 mov edx,[esi+00000504]
 mov [edx+edi*4],eax

 dec dword ptr [iRTCount]

_ExitMR:
 mov edx,[esi+00000504]
 jmp _BackMR

//=========================================

_MonSelShip:
 push ebx
 pushfd

 or esi,esi
 jz _ExitMSS

 mov [pSelShip],esi

 mov ebx,[esi+000000c8]

 cmp ebx,[iPlayerID]
 jne _ExitMSS

 cmp dword ptr [iEnableMM],0
 je _MonSS0

 cmp dword ptr [esi+000001f0],0
 jne _MonSS0

 mov dword ptr [esi+000001f0],2

_MonSS0:
 cmp dword ptr [iEnableGM],0
 je _MonSS1

 cmp dword ptr [esi+00000318],#1000
 jge _MonSS1

 mov dword ptr [esi+00000318],#1000

_MonSS1:
 cmp dword ptr [iEnableMC],0
 je _ExitMSS

 cmp dword ptr [esi+00000204],0
 je _ExitMSS

 cmp dword ptr [esi+00000204],#1500
 jge _ExitMSS

 mov dword ptr [esi+00000204],#1500

_ExitMSS:
 popfd
 pop ebx

 mov [edi+0001d15c],esi
 jmp _BackMSS

//=========================================

_MonMovesLeft:
 push ebx

 cmp dword ptr [iEnableMM],0
 je _ExitMML

 cmp esi,[pSelShip]
 jne _ExitMML

 mov ebx,[esi+000000c8]
 cmp ebx,[iPlayerID]
 jne _ExitMML

 mov eax,00000000

_ExitMML:
 pop ebx

 sub [esi+000001f0],eax
 jmp _BackMML

//=========================================

_GodMode:
 push ebx
 pushfd

 cmp dword ptr [iEnableGM],0
 je _ExitGM

 mov ebx,[esi+000000c8]
 cmp ebx,[iPlayerID]
 jne _ExitGM

 xor eax,eax

_ExitGM:
 popfd
 pop ebx

 mov [esi+000001e4],eax
 jmp _BackGM

//=========================================

_MonInvation:
 cmp dword ptr [iEnableGM],0
 je _ExitMI

 mov eax,[esi+2c]           // Get pointer to attacker information
 mov eax,[eax+04]           // Get attacker ID
 cmp eax,[iPlayerID]        // Is it a player's Troop?
 jne _MonI0                 // Jump if false

 mov dword ptr [esi+60],0   // Make the defender's advantage = 0
 jmp _ExitMI                // Go to original code

_MonI0:
 mov eax,[esi+54]           // Get pointer to Defender information
 mov eax,[eax+04]           // Get defender ID
 cmp eax,[iPlayerID]        // Is it a player's Planet?
 jne _ExitMI                // Jump if false

 mov dword ptr [esi+38],0   // Make the attacker's advantage = 0

_ExitMI:
 mov eax,[esi+60]           // Original code (get defender's advantage...)
 lea edx,[eax+eax*4]        // Original code (...and multiply it by 5)
 jmp _BackMI                // Go back to the original code

//=========================================

pPlayer:
 dd 0
pSelShip:
 dd 0
iPlayerID:
 dd #999
iEnableME:
 dd 1
iEnableMR:
 dd 1
iEnableMM:
 dd 1
iEnableGM:
 dd 1
iEnableMC:
 dd 1
iRTCount:
 dd 2

//=========================================
// Hacking Points

galciv2.exe+07ebad:
 jmp _MonET
 nop
_BackME:

galciv2.exe+18f0f2:
 jmp _MonRT
 nop
_BackMR:

galciv2.exe+1ce76b:
 jmp _MonSelShip
 nop
_BackMSS:

galciv2.exe+167198:
 jmp _MonMovesLeft
 nop
_BackMML:

galciv2.exe+165f6c:
 jmp _GodMode
 nop
_BackGM:

galciv2.exe+217b24:
 jmp _MonInvation
 nop
_BackMI:

//=========================================
// - Restore the original codes
// - Unregister the symbols
// - Deallocate (free) the memory

[DISABLE]
galciv2.exe+07ebad:
 mov ebp,[edi+000004f0]

galciv2.exe+18f0f2:
 mov edx,[esi+00000504]

galciv2.exe+1ce76b:
 mov [edi+0001d15c],esi

galciv2.exe+167198:
 sub [esi+000001f0],eax

galciv2.exe+165f6c:
 mov [esi+000001e4],eax

galciv2.exe+217b24:
 mov eax,[esi+60]
 lea edx,[eax+eax*4]

unregistersymbol(MyCode)
unregistersymbol(pPlayer)
unregistersymbol(pSelShip)
unregistersymbol(iPlayerID)
unregistersymbol(iEnableME)
unregistersymbol(iEnableMR)
unregistersymbol(iEnableMM)
unregistersymbol(iEnableGM)
unregistersymbol(iEnableMC)
unregistersymbol(iRTCount)

dealloc(MyCode)

