 jz _ExitMS

 mov edi,[edi+14]                  // Get pInfo
 test edi,edi
 jz _ExitMS

 mov edi,[edi+00000224]            // Get pOwner
 cmp edi,[pOwner]
 jne _MonS00

 mov dword ptr [ebx+14],1
 mov eax,#100
 jmp _ExitMS
