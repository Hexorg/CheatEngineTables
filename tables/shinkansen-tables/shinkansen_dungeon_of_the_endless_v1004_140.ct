</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>110451</ID>
          <Description>"Health Base Address Scan (To Activate: Battle, To Update: Battle)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(Health_savedAOB)
label(Health_returnhere)
label(Health_originalcode)
label(Health_exit)

label(Health_update_exit)
label(Health_set_exit)
label(Health_invincibleset_exit)
label(Health_unlockset_exit)

registersymbol(Health_savedAOB)

//1B397CEE - 39 00                 - cmp [eax],eax
//1B397CF0 - E8 53F30100           - call 1B3B7048
// Health:GetHealth+25
//1B397CF5 - 83 C4 10              - add esp,10
//1B397CF8 - EB 09                 - jmp 1B397D03
//1B397CFA - 8B 45 08              - mov eax,[ebp+08]
//1B397CFD - D9 80 84000000        - fld dword ptr [eax+00000084]
//1B397D03 - C9                    - leave
//1B397D04 - C3                    - ret
aobscan(Health_searchedAOB, 83 C4 10 EB 09 8B 45 08 D9 80 84 00 00 00 C9)

alloc(Health_newmem, 2048)
alloc(Health_temp1, 4)
alloc(Health_temp2, 4)


Health_newmem:
mov [Health_temp1], ebx
mov [Health_temp2], ecx

// Is enemy?
cmp dword ptr [eax+40], #0
je Health_unlockset_exit

cmp dword ptr [Health_update_enable], #0
je Health_update_exit
mov [Health_basepointer], eax
Health_update_exit:

cmp dword ptr [Health_set_enable], #0
je Health_set_exit
Health_set_exit:

cmp dword ptr [Health_invincibleset_enable], #0
je Health_invincibleset_exit
mov byte ptr [eax+b4], #1
Health_invincibleset_exit:

cmp dword ptr [Health_unlockset_enable], #0
je Health_unlockset_exit
cmp dword ptr [eax+2c], #0
je Health_unlockset_exit
mov ebx, [eax+2c]
// Already unlocked
cmp dword ptr [ebx+290], #0
je Health_unlockset_exit
mov ecx, [ebx+270]
mov [ebx+290], ecx
Health_unlockset_exit:

mov ebx, [Health_temp1]
mov ecx, [Health_temp2]


Health_originalcode:
db D9 80 84 00 00 00
//fld dword ptr [eax+00000084]


Health_exit:
jmp Health_returnhere


Health_searchedAOB + 8:
Health_savedAOB:
jmp Health_newmem
nop
Health_returnhere:


[DISABLE]
Health_savedAOB:
db D9 80 84 00 00 00
//fld dword ptr [eax+00000084]

unregistersymbol(Health_savedAOB)

dealloc(Health_newmem)
dealloc(Health_temp1)
dealloc(Health_temp2)

</AssemblerScript>
