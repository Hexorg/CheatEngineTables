          <AssemblerScript>//Dark Souls II (ver: 1.0.1)
//Enemy Damage Multiplyer (Simple bit shift)

//Original by Steve Andrews modiefied by Matze500 (removed health cheat)

[ENABLE]
alloc(PlayerHealth,1024)
label(NotPlayer)
label(DamageMultiplyer)
label(PlayerHealthRet)
registersymbol(DamageMultiplyer)

PlayerHealth:
cmp byte [esi+18],1
jne NotPlayer

mov [esi+fc],ecx

jmp PlayerHealthRet

NotPlayer: //Do Normal Damage To Enemy Or Apply Simple Damage Multiplyer
push edx   //default: 3x damage
push ebx
mov edx,ecx //new enemy health val (with default damage taken)
mov ebx,[esi+fc] //old enemy health val
sub ebx,edx //old - new == (EBX) normal damage done
push ecx
mov cl,[DamageMultiplyer] //cl == {damage multiplyer}
shl ebx,cl//{normal damage} &lt;&lt; {damage multiplyer} == New Multiplyed Damage Delt
sub [esi+fc],ebx //New multiplyed damage delt subtracted from enemy health!
pop ecx
pop ebx
pop edx
jmp PlayerHealthRet

DamageMultiplyer: //00 == normal damage to enemies; 01 == x2; 02 == x3; etc...
db 00

DarkSoulsII.exe+1bba81:
jmp PlayerHealth
nop
PlayerHealthRet:

[DISABLE]

DarkSoulsII.exe+1bba81:
mov [esi+fc],ecx

dealloc(PlayerHealth)
unregistersymbol(DamageMultiplyer)
</AssemblerScript>
