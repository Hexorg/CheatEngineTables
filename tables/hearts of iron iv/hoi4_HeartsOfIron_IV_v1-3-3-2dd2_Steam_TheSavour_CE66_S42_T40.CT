 mov edx,[ebx+000001b4]
 mov [ebp+0c],edx

 mov edx,[ebx+000001b0]
 shr edx,1
 cmp [ebp+08],edx
 jns _ExitGMD

 mov [ebp+08],edx

_ExitGMD:
 pop ebx

 mov edx,[ebp+08]                  // Original code
 jmp _BackGMD                      // Back to main code

