CodeBuffer:
//when entering this function eax=&amp;PlayerData and edx=1 PM1,2 for PM2, 3 for PM3
GetDrivingSkill:
//vanilla PlayerData format
//mov ebx, dword [eax]      //ebx= medal count
//mov ecx, dword [eax+04]    //ecx= xp
//mov esi, dword [eax+0C]    //edx= current level

//new PlayerData format:
//ebx 0:7              medal count
//ebx 8:21             short range xp
//ebx 22:31 + ecx 0:3  med range xp
//ecx 4:17             long range xp
//ecx 18:31            PM01 xp
//esi 0:13             PM02 xp
//esi 14:27            PM03 xp
cmp edx, 1
jne short GetDrivingSkill_NotLookingForPM1
  mov ax, word [eax+4]
  shr eax,#18
  and eax,3FFF         //only take 14 bits
  jmp short GetDrivingSkill_Exit
GetDrivingSkill_NotLookingForPM1:
cmp edx, 2
jne short GetDrivingSkill_NotLookingForPM2
  mov ax, word [eax+C]   //medal count
  and eax,3FFF           //only take 14 bits
  jmp short GetDrivingSkill_Exit
GetDrivingSkill_NotLookingForPM2:
cmp edx, 3
jne short GetDrivingSkill_Error
  mov eax, dword [eax+C]   //xp
  shr eax,#14              //thash part of the PM2 xp
  and eax,3FFF             //only take 14 bits
  jmp short GetDrivingSkill_Exit
GetDrivingSkill_Error:
xor eax,eax
GetDrivingSkill_Exit:
xor edx,edx
push ecx
mov ecx,HP_DIVISOR         //mech hp bonus = chassis xp/100
div ecx
pop ecx
retn

///////////////////////////////////////////////
//when entering this function eax=&amp;PlayerData
GetShortRangeSkill://damage bonus is now weapon xp/50
push ecx         //save ecx, may not be needed

mov eax,dword [eax+1]
and eax,3FFF              //now eax=short range xp
//now lets' do eax=eax/50
xor edx,edx
mov ecx,WEAPON_SKILL_DIVISOR
div ecx

pop ecx
retn


///////////////////////////////////////////////
//when entering this function eax=&amp;PlayerData
GetMedRangeSkill://damage bonus is now weapon xp/50 =
push ecx         //save ecx, may not be needed

mov eax,dword [eax+2]
shr eax,6
and eax, 3FFF        //now eax=med range xp
xor edx,edx
mov ecx,WEAPON_SKILL_DIVISOR
div ecx

pop ecx
retn

///////////////////////////////////////////////
//when entering this function eax=&amp;PlayerData
GetLongRangeSkill://mech skill bonus is now mech xp/100 =
push ecx         //save ecx, may not be needed

mov eax,dword [eax+4]
shr eax,4
and eax, 3FFF            //now eax=long range xp
xor edx,edx
mov ecx,WEAPON_SKILL_DIVISOR
div ecx

pop ecx
retn

