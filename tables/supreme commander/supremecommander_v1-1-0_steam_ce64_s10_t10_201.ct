alloc(MyCode,4096,LUDO)
//=========================================
// Declaration section
label(_FindHumanPlayer)
label(_BackFHP)
label(_ExitFHP)
label(_GodMode)
label(_GodMD00)
label(_BackGMD)
label(_ExitGMD)
label(_MonPlayerRes)
label(_BackMPR)
label(_ExitMPR)
label(iEnableGMD)
label(iEnableMBP)
label(iEnableMPR)
label(iPlayerID)
label(pPlayer)
label(pPlayerList)

//=========================================
// Registering Symbols
registersymbol(MyCode)
registersymbol(iEnableGMD)
registersymbol(iEnableMBP)
registersymbol(iEnableMPR)

//=========================================
MyCode:
//=========================================
// Main game screen
_FindHumanPlayer:
 push ebx
 push esi

 mov esi,eax

 mov eax,[esi+00000AA8]
 mov [iPlayerID],eax
 mov ebx,[esi+00000920]
 mov [pPlayerList],ebx
 mov ebx,[ebx+eax*4]
 mov [pPlayer],ebx

 mov eax,esi

_ExitFHP:
 pop esi
 pop ebx

 mov edx,[eax+00000920]            // Original code (Get Index)
 jmp _BackFHP                      // Back to main code

//=========================================
// During Combat/Production/Construction
_GodMode:
 push eax
 push ebx

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov eax,[esi+0000012c]
 cmp eax,[pPlayer]
 jne _ExitGMD

 cvtss2si eax,xmm2
 cvtss2si ebx,[esi+00000090]
 cmp eax,ebx
 jns _GodMD00

 movss xmm2,[esi+00000090]

_GodMD00:
 cmp dword ptr [iEnableMBP],0
 je _ExitGMD                       // Jump if feature is disabled

 movss xmm2,[esi+00000094]

_ExitGMD:
 pop ebx
 pop eax

 movss [esi+00000090],xmm2         // Original code (upd HP)
 jmp _BackGMD                      // Back to main code

//=========================================
// Main game screen - Full Player Resources
_MonPlayerRes:
 cmp dword ptr [iEnableMPR],0
 je _ExitMPR                       // Jump if feature is disabled

 mov ecx,[ebx+04]
 cmp ecx,[iPlayerID]
 jne _ExitMPR

 fild dword ptr [ebx+40]           // Get max Energy
 fstp dword ptr [ebx+18]
 fild dword ptr [ebx+48]           // Get max Mass
 fstp dword ptr [ebx+1c]

_ExitMPR:
 mov ecx,[ebx+18]                  // Original code (rd Energy)
 mov edx,[ebx+1C]                  // Original code (rd Mass)
 jmp _BackMPR                      // Back to main code

//=========================================
 db '=======================================&gt;'
 db 'CE6.4 Script by Recifense 090114'
//=========================================
// Variables
iEnableGMD:
 dd 0
iEnableMBP:
 dd 0
iEnableMPR:
 dd 0
iPlayerID:
 dd #99
pPlayer:
 dd MyCode
pPlayerList:
 dd 0

//=========================================
// Hacking Points
FHPR:
 jmp _FindHumanPlayer
 nop
_BackFHP:

GDMD:
 jmp _GodMode
 nop
 nop
 nop
_BackGMD:

MOPR:
 jmp _MonPlayerRes
 nop
_BackMPR:

//=========================================
// Script for Restoring Original Codes
[DISABLE]
FHPR:
// mov edx,[eax+00000920]
 db 8b 90 20 09 00 00

GDMD:
// movss [esi+00000090],xmm2
 db f3 0f 11 96 90 00 00 00

MOPR:
// mov ecx,[ebx+18]
// mov edx,[ebx+1C]
 db 8b 4b 18 8b 53 1c

//=========================================
// Unregistering Symbols
unregistersymbol(MyCode)
unregistersymbol(iEnableGMD)
unregistersymbol(iEnableMBP)
unregistersymbol(iEnableMPR)

//=========================================
dealloc(MyCode)
//============= Scripts End ===============

// ****************************************
// NOTES
// ****************************************
{
}

