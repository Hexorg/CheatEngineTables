//4AD92CB6 - 8B 4D 08              - mov ecx,[ebp+08]
//4AD92CB9 - 8B 41 0C              - mov eax,[ecx+0C]
// BBI.Game.Simulation:Commander:get_PopCap+c
//4AD92CBC - 8B 89 BC000000        - mov ecx,[ecx+000000BC]
//4AD92CC2 - 83 EC 04              - sub esp,04
//4AD92CC5 - 6A 00                 - push 00
//4AD92CC7 - 51                    - push ecx
aobscan(Commander_searchedAOB, 8B 89 BC 00 00 00 83 EC 04)

alloc(Commander_newmem, 2048, Commander_searchedAOB)
alloc(Commander_temp1, 4, Commander_searchedAOB)


Commander_newmem:
mov [Commander_temp1], ebx

cmp byte ptr [ecx+60], #1
jne Commander_resource3set_exit

cmp dword ptr [Commander_update_enable], #0
je Commander_update_exit
mov [Commander_basepointer], ecx
Commander_update_exit:

cmp dword ptr [Commander_set_enable], #0
je Commander_set_exit
Commander_set_exit:

cmp dword ptr [Commander_populationset_enable], #0
je Commander_populationset_exit
mov [ecx+bc], #9999
mov [ecx+c0], #9999
Commander_populationset_exit:

cmp dword ptr [Commander_resource1set_enable], #0
je Commander_resource1set_exit
cmp dword ptr [ecx+14], #0
je Commander_resource1set_exit
mov ebx, [ecx+14]
cmp dword ptr [ebx+8], #0
je Commander_resource1set_exit
mov ebx, [ebx+8]
cmp dword ptr [ebx+14], #0
je Commander_resource1set_exit
mov ebx, [ebx+14]
cmp dword ptr [ebx+10], #0
je Commander_resource1set_exit
mov ebx, [ebx+10]
mov [ebx+18], #9999999
Commander_resource1set_exit:

cmp dword ptr [Commander_resource2set_enable], #0
je Commander_resource2set_exit
cmp dword ptr [ecx+14], #0
je Commander_resource2set_exit
mov ebx, [ecx+14]
cmp dword ptr [ebx+8], #0
je Commander_resource2set_exit
mov ebx, [ebx+8]
cmp dword ptr [ebx+14], #0
je Commander_resource2set_exit
mov ebx, [ebx+14]
cmp dword ptr [ebx+14], #0
je Commander_resource2set_exit
mov ebx, [ebx+14]
mov [ebx+18], #9999999
Commander_resource2set_exit:

cmp dword ptr [Commander_resource3set_enable], #0
je Commander_resource3set_exit
cmp dword ptr [ecx+14], #0
je Commander_resource3set_exit
mov ebx, [ecx+14]
cmp dword ptr [ebx+8], #0
je Commander_resource3set_exit
mov ebx, [ebx+8]
cmp dword ptr [ebx+14], #0
je Commander_resource3set_exit
mov ebx, [ebx+14]
cmp dword ptr [ebx+18], #0
je Commander_resource3set_exit
mov ebx, [ebx+18]
mov [ebx+18], #9999999
Commander_resource3set_exit:

mov ebx, [Commander_temp1]


Commander_originalcode:
db 8B 89 BC 00 00 00
//mov ecx,[ecx+000000BC]


Commander_exit:
jmp Commander_returnhere


Commander_searchedAOB:
Commander_savedAOB:
jmp Commander_newmem
nop
Commander_returnhere:


