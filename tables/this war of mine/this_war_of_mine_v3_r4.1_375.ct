////////////////////////////////////////////////////////////////////////////////
label(codeHealth)
label(lblHealthDamage)
label(lblHealthExit)
label(returnHealth)

codeHealth:
  push eax
  mov eax,[edi+1C]
  cmp dword ptr [eax+4],7     // scavenger? (7=scavenger 6=enemy)
  pop eax
  jne lblHealthDamage         // no - it is enemy

  cmp dword ptr [bMaxHealth],0
  je lblHealthDamage

  movss xmm2,[fMax]
  movss [edi+24],xmm2         // set max health to scavenger
  movss xmm1,[fMin]           // set no damage to scavenger
  jmp lblHealthExit

lblHealthDamage:
  cmp dword ptr [bMaxDamage],0
  je lblHealthExit

  movss xmm1,[fMax]           // set max damage to enemy

lblHealthExit:
//  movss xmm2,[edi+24]
  db F3 0F 10 57 24
  jmp returnHealth

