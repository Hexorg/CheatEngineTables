  <LuaScript>function setup()
  local card_pill_ids = {52, 187, 185}
  local attack_style_ids = {136, 143, 142, 141, 140, 139, 138, 137, 144,
                            145, 146, 147, 148, 149, 150, 151, 165, 127,
                            126, 125, 124, 123, 166, 121, 128, 129, 130,
                            131, 132, 133, 134, 135}
  local seeds_filename = "seeds.txt"

  enableMTM,disableMTM = setup_MoveToMouse()
  makeBitsToggle(card_pill_ids)
  makeBitsToggle(attack_style_ids)
  enableSL,disableSL = setup_SeedLog(seeds_filename)
end





function setup_MoveToMouse()
  function wasdKeys(s,a,w,d)
    if w then keyDown(VK_W) else keyUp(VK_W) end
    if a then keyDown(VK_A) else keyUp(VK_A) end
    if s then keyDown(VK_S) else keyUp(VK_S) end
    if d then keyDown(VK_D) else keyUp(VK_D) end
  end

  function moveToMouse()
    local sx = 2*readFloat("[pPlayer]+35c")
    local sy = 2*readFloat("[pPlayer]+360")
    local mx, my = getMousePos()
    local vx = mx-sx
    local vy = my-sy-30

    local len = math.sqrt(vx*vx+vy*vy)
    if len &lt; 20 then
      wasdKeys(false, false, false, false)
      return
    end

    local phi = math.atan2(vy, vx)
    local quad = 4*phi/math.pi + 0.5
    if quad &lt; 0 then quad = quad + 8 end

    if     quad &lt; 1 then wasdKeys(false, false, false,  true)
    elseif quad &lt; 2 then wasdKeys( true, false, false,  true)
    elseif quad &lt; 3 then wasdKeys( true, false, false, false)
    elseif quad &lt; 4 then wasdKeys( true,  true, false, false)
    elseif quad &lt; 5 then wasdKeys(false,  true, false, false)
    elseif quad &lt; 6 then wasdKeys(false,  true,  true, false)
    elseif quad &lt; 7 then wasdKeys(false, false,  true, false)
    else                 wasdKeys(false, false,  true,  true) end

  end

  local mtm_timer = createTimer(nil, false)
  mtm_timer:setInterval(50)
  mtm_timer:setOnTimer(moveToMouse)

  local enableMTM = function()
    mtm_timer:setEnabled(true)
  end

  local disableMTM = function()
    mtm_timer:setEnabled(false)
    wasdKeys(false, false, false, false)
  end

  return enableMTM, disableMTM
end




function makeToggleable(mr)
  local closure = function()
    if mr.Value == "1" then
      mr.Value = "0"
    else
      mr.Value = "1"
    end
  end
  return closure
end

function makeBitsToggle(toggle_ids)
  local al = getAddressList()
  for i,id in ipairs(toggle_ids) do
    local mr = al:getMemoryRecordByID(id)
    mr.OnActivate = makeToggleable(mr)
  end
end



function setup_SeedLog(filename)
  local old_seed = ""
  function isSeed(s)
    if s:len() ~= 9 then
      return false
    elseif s:byte(5) ~= 32 then
      return false
    elseif s:sub(1,4):match("%W") then
      return false
    elseif s:sub(6,9):match("%W") then
      return false
    end
    return true
  end
  function logSeed()
    local seed = readString("[isaac-ng.exe+21E18C]+2ed44", 10)
    if seed == old_seed then
      return
    end
    if isSeed(seed) then
      local f = io.open(filename, "a")
      f:write(seed, "\n")
      f:close()
    end
    old_seed = seed
  end

  local sl_timer = createTimer(nil, false)
  sl_timer:setInterval(1000)
  sl_timer:setOnTimer(logSeed)

  local enableSL = function()
    sl_timer:setEnabled(true)
  end

  local disableSL = function()
    sl_timer:setEnabled(false)
    old_seed = ""
  end

  return enableSL, disableSL
end


setup()
</LuaScript>
