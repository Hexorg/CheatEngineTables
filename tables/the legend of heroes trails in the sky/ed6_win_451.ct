  local Quant = tonumber(ItemSwap_EQuant.Text)
  if Quant == nil then showMessage("Please enter a valid number.") end
  for i = 0, ItemSwap_LItems.Items.Count-1 do
    writeBytes(ItemBase+i*4+2,Quant)
  end
  BUpdateListClick()
end

function BQuantSelClick(sender)
  local Quant = tonumber(ItemSwap_EQuant.Text)
  local Index = ItemSwap_LItems:getItemIndex()
  if Index == -1 then showMessage("Please select an item first.") return end
  if Quant == nil then showMessage("Please enter a valid number.") return end
  writeBytes(ItemBase+Index*4+2,Quant)
  BUpdateListClick()
end

function BSwapItemClick(sender)
  local Index = ItemSwap_LItems:getItemIndex()
  if Index == -1 then showMessage("Please select an item first.") return end
  --ItemTable[ItemSwap_CBItems.ItemIndex+1].ItemName
  local CBIndex = ItemSwap_CBItems.ItemIndex+1
  writeShortint(ItemBase+Index*4,ItemTable[CBIndex].ItemID)
  BUpdateListClick()
end
