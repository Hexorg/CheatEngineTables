_MonMaxPop:
 mov [ebp-08],eax                  // Original code

 cmp edx,[pPlayer]
 jne _ExitMMP

 xor eax,[edx+38]
 mov [iMaxPop],eax

_ExitMMP:
 mov eax,[edx+38]                  // Original code (get Used Pop)
 jmp _BackMMP                      // Back to main code

