      <Description>"Modify FOV"</Description>
      <Options moHideChildren="1"/>
      <LastState/>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
aobscanmodule(aob_fov,ShadowOfMordor.exe,48 8B 83 40 03 00 00 f3 0f11 45 ef)
alloc(mem_fov,256,ShadowOfMordor.exe)
registersymbol(mem_fov)
label(_fov)
registersymbol(_fov)

registersymbol(new_fov)
label(new_fov)

label(return_fov)
label(prereturn_fov)

mem_fov:
movss xmm6,[rax+60]//real value

movss xmm0,[new_fov] //new max value
ucomiss xmm6,xmm0
ja return_fov

movss xmm0,[new_fov+8]//old max value
movss xmm2,[new_fov+4]//min value

subss xmm6,xmm2
subss xmm0,xmm2

divss xmm6,xmm0

movss xmm0,[new_fov] //new max value
subss xmm0,xmm2

mulss xmm6,xmm0
addss xmm6,xmm2

movss xmm0,[new_fov] //new max value

ucomiss xmm6,xmm2
ja prereturn_fov
movss xmm6,[rax+60]

prereturn_fov:
ucomiss xmm6,xmm0
jb return_fov
movss xmm6,xmm0

jmp return_fov


new_fov:
dd (float)1.3//new max
dd (float)0.34//min
dd (float)0.7 //max

aob_fov+c:
_fov:
jmp mem_fov
return_fov:

[DISABLE]
_fov:
movss xmm6,[rax+60]

unregistersymbol(_fov)
unregistersymbol(mem_fov)
unregistersymboL(new_fov)
dealloc(mem_fov)
</AssemblerScript>
