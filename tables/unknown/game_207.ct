      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>/*

*** 8B 56 08 89 10 5F ***
  *** [esi] Filters ***

2ADA = Y Coordinate Filter
2AD2 = X Coordinate Filter
2ACA = Move Filter
31F2 = EXP Filter
392A = Current Health Filter
3962 = Attack Filter (CANNOT EDIT)
397A = Held Item Filter
867A = Speed Filter (CANNOT EDIT)
A89A = Status Fitler
A99A = Species Filter
A9BA = Defense Filter (CANNOT EDIT)
A9C2 = Special Attack Filter (CANNOT EDIT)
A9CA = Special Defense Filter (CANNOT EDIT)
C822 = Gender Filter
C88A = Ability Filter
CAB2 = Happiness Filter
C7FA = Eggsteps Filter
C8D2 = Nature Filter
C8EA = Shininess Filter
E95A = Money Filter
14E6A = Heart Gauge Filter

*/
[Enable]
[Disable]
</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>125</ID>
          <Description>"----&gt; Pokemon Editor"</Description>
          <Options moHideChildren="1"/>
          <LastState Activated="0"/>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[Enable]

alloc(pe_1,128,RGSS102E.dll) //Pokemon Editor
aobscanmodule(pe_aob1,RGSS102E.dll,8B 56 08 89 10 5F)
registersymbol(pe_aob1)
label(returnhere_pe_1)


label(isMove)
label(updateMove_status) //Update Move
registersymbol(updateMove_status)
label(oldMove) //Old Move
registersymbol(oldMove)
label(newMove) //New Move
registersymbol(newMove)

label(isEXP)
globalalloc(p_isEXP,4) //EXP Pointer

label(isCurrentHealth)
globalalloc(p_isCurrentHealth,4) //Current Health Pointer

label(isHeldItem)
globalalloc(p_isHeldItem,4) //Held Item Pointer

label(isSpecies)
globalalloc(p_isSpecies,4) //Species Pointer

label(isSomething) //Don't Know Yet

label(pe_orig)

//--------------------------------------------------//
pe_1:
cmp [esi],2ACA //Move Filter
je isMove
cmp [esi],31F2 //EXP Filter
je isEXP
cmp [esi],392A //Current Health Filter
je isCurrentHealth
cmp [esi],397A //Held Item Filter
je isHeldItem
cmp [esi],A99A //Species Filter
je isSpecies
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
isMove:
cmp [updateMove_status],1
jne pe_orig
push eax
mov eax,[oldMove]
cmp [esi+08],eax
pop eax
jne pe_orig
push [newMove]
pop [esi+08]
mov edx,[esi+08]
mov [eax],edx
mov [updateMove_status],0
jmp returnhere_pe_1

updateMove_status:
dd 0

oldMove:
dd 0

newMove:
dd 0
//--------------------------------------------------//
isEXP:
mov [p_isEXP],esi
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
isCurrentHealth:
mov [p_isCurrentHealth],esi
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
isHeldItem:
mov [p_isHeldItem],esi
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
isSpecies:
mov [p_isSpecies],esi
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
isSomething:
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//
pe_orig:
mov edx,[esi+08]
mov [eax],edx
jmp returnhere_pe_1
//--------------------------------------------------//

pe_aob1:
jmp pe_1
returnhere_pe_1:

[Disable]

pe_aob1:
db 8B 56 08 89 10
unregistersymbol(pe_aob1)
dealloc(pe_1)

unregistersymbol(updateMove_status)
unregistersymbol(oldMove)
unregistersymbol(newMove)

unregistersymbol(p_isEXP)

unregistersymbol(p_isCurrentHealth)

unregistersymbol(p_isHeldItem)

unregistersymbol(p_isSpecies)
</AssemblerScript>
