  pop rbx
  jne isplayer
  inc rcx
  movss xmm6,[rax+rcx*4]
  dec rcx
isplayer:
  movss [rax+rcx*4],xmm6
  jmp return2
code2:
  movss [rax+rcx*4],xmm6
  jmp return2
player_stats:
  jmp newmem2
return2:

registersymbol(player)
registersymbol(player_ptr)
registersymbol(player_stats)
registersymbol(player_hack)

[DISABLE]
