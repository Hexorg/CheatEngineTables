//=========================================
// Definitions
define(LUDO,"TheMoonProject.exe")
define(MHPR,TheMoonProject.exe+0007f492)
define(GDMD,TheMoonProject.exe+0014a3b5)
define(MUAO,TheMoonProject.exe+00051704)
define(MPRH,TheMoonProject.exe+00070d13)
define(MPPN,TheMoonProject.exe+000258b1)

//=========================================
// Constants used in the script (that can be changed)
define(ctMMMY,#6300)   // Minimum Money

//=========================================
[ENABLE]
//=========================================
//LuaCall(CheckVersion())
//=========================================
// Check if script is compatible to this game version
// If false the script will not be loaded

assert(MHPR,8b 8b 6c 03 00 00 8b 40 48 3b c1 ?? ?? 50 68 ?? ?? ?? ?? 68 ??)
assert(GDMD,66 29 51 3c 66 8b 41 3c 66 85 c0 ?? ?? 53 6a 00 66 c7 41 3c 00)
assert(MUAO,89 b3 80 00 00 00 8b 70 7c e8 ?? ?? ?? ?? 8b c8 b8 1f 85 eb 51)
assert(MPRH,03 d6 89 53 08 8b 08 2b fe 03 ce 89 7d 08 89 08 ?? ?? 57 e8 ??)
assert(MPPN,89 83 ec 01 00 00 3b d1 ?? ?? 8b b3 f0 01 00 00 8b 13 8b cb 2b)

//=========================================
alloc(MyCode,4096,LUDO)
//=========================================
// Declaration section
label(_MonHumanPlayer)
label(_BackMHP)
label(_ExitMHP)
label(_GodMode)
label(_BackGMD)
label(_ExitGMD)
label(_MonUnitAmmo)
label(_BackMUA)
label(_ExitMUA)
label(_MonPlayerRes)
label(_BackMPR)
label(_ExitMPR)
label(_MonPlayerProd)
label(_BackMPP)
label(_ExitMPP)
label(iEnableMMM)
label(iEnableGMD)
label(iEnableMUA)
label(iEnableMPR)
label(iEnableMPP)
label(iMinMoney)
label(pPlayer)
label(pSel)

//=========================================
// Registering Symbols
registersymbol(MyCode)
registersymbol(iEnableMMM)
registersymbol(iEnableGMD)
registersymbol(iEnableMUA)
registersymbol(iEnableMPR)
registersymbol(iEnableMPP)
registersymbol(iMinMoney)
registersymbol(pPlayer)
registersymbol(pSel)

//=========================================
MyCode:
//=========================================
// Main game screen
_MonHumanPlayer:
 mov [pPlayer],eax

 cmp dword ptr [iEnableMMM],0
 je _ExitMHP                       // Jump if feature is disabled

 mov ecx,[iMinMoney]
 cmp ecx,[eax+48]
 js _ExitMHP

 mov [eax+48],ecx
 mov [ebx+0000036C],ecx

_ExitMHP:
 mov ecx,[ebx+0000036C]            // Original code (rds Money Copy)
 jmp _BackMHP                      // Back to main code

//=========================================
// During Combat
_GodMode:
 push eax

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov eax,[ecx+68]
 cmp eax,[pPlayer]
 jne _ExitGMD

 mov ax,[ecx+3e]
 mov [ecx+3C],ax

 xor edx,edx

_ExitGMD:
 pop eax

 sub [ecx+3C],dx                   // Original code (upd cHP)
 mov ax,[ecx+3C]                   // Original code (rd cHP)
 jmp _BackGMD                      // Back to main code

//=========================================
// When shooting
// ECX = Unit
_MonUnitAmmo:
 push eax

 cmp dword ptr [iEnableMUA],0
 je _ExitMUA                       // Jump if feature is disabled

 test ecx,ecx
 jz _ExitMUA

 mov eax,[ecx+68]
 cmp eax,[pPlayer]
 jne _ExitMUA

 mov esi,[ebx+00000080]

_ExitMUA:
 pop eax

 mov [ebx+00000080],esi            // Original code (rds Money Copy)
 jmp _BackMUA                      // Back to main code

//=========================================
// When researching
// ECX = Player
_MonPlayerRes:
 cmp dword ptr [iEnableMPR],0
 je _ExitMPR                       // Jump if feature is disabled

 cmp ecx,[pPlayer]
 jne _ExitMPR

 mov edx,[ebx+1c]
 sub edx,esi

 mov dword ptr [ebx+20],#50

_ExitMPR:
 add edx,esi                       // Original code
 mov [ebx+08],edx                  // Original code
 jmp _BackMPR                      // Back to main code

//=========================================
// When Producing Units
_MonPlayerProd:
 push ecx

 cmp dword ptr [iEnableMPP],0
 je _ExitMPP                       // Jump if feature is disabled

 mov ecx,[ebx+68]
 cmp ecx,[pPlayer]
 jne _ExitMPP

 mov eax,[ebx+000001F0]
 mov dword ptr [ebx+000001a0],#50

_ExitMPP:
 pop ecx

 mov [ebx+000001EC],eax            // Original code
 jmp _BackMPP                      // Back to main code

//=========================================
 db '=====================================&gt;'
 db 'CE6.3 Script by Recifense 110713'
//=========================================
// Variables
iEnableMMM:
 dd 0
iEnableGMD:
 dd 0
iEnableMUA:
 dd 0
iEnableMPR:
 dd 0
iEnableMPP:
 dd 0
iMinMoney:
 dd ctMMMY
pPlayer:
 dd MyCode
pSel:
 dd 0

//=========================================
// Hacking Points
MHPR:
 jmp _MonHumanPlayer
 nop
_BackMHP:

GDMD:
 jmp _GodMode
 nop
 nop
 nop
_BackGMD:

MUAO:
 jmp _MonUnitAmmo
 nop
_BackMUA:

MPRH:
 jmp _MonPlayerRes
_BackMPR:

MPPN:
 jmp _MonPlayerProd
 nop
_BackMPP:

//=========================================
// Script for Restoring Original Codes
[DISABLE]
MHPR:
// mov ecx,[ebx+0000036C]
 db 8b 8b 6c 03 00 00

GDMD:
// sub [ecx+3C],dx
// mov ax,[ecx+3C]
 db 66 29 51 3c 66 8b 41 3c

MUAO:
// mov [ebx+00000080],esi
 db 89 b3 80 00 00 00

MPRH:
// add edx,esi
// mov [ebx+08],edx
 db 03 d6 89 53 08

MPPN:
// mov [ebx+000001EC],eax
 db 89 83 ec 01 00 00

//=========================================
// Unregistering Symbols
unregistersymbol(MyCode)
unregistersymbol(iEnableMMM)
unregistersymbol(iEnableGMD)
unregistersymbol(iEnableMUA)
unregistersymbol(iEnableMPR)
unregistersymbol(iEnableMPP)
unregistersymbol(iMinMoney)
unregistersymbol(pPlayer)
unregistersymbol(pSel)

//=========================================
dealloc(MyCode)
//============= Scripts End ===============

// ****************************************
// NOTES
// ****************************************
{
}

