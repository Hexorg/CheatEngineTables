 mov esi,[ebx+10]                  // Get pCapital

 mov esi,[esi+34]                  // Get pCounty
 test esi,esi
 jz _ExitMS

 mov edi,[esi+00000678]            // Get ppArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi]                     // Get p1stArmy
 test edi,edi
 jz _ExitMS

 mov ecx,[edi+0000010c]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonS01

 mov edi,[esi+00000678]            // Get ppArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi]                     // Get pLastArmy
 test edi,edi
 jz _ExitMS

 mov ecx,[edi+0000010c]            // Get pOwner
 cmp ecx,[pOwner]
