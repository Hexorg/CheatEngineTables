function findObjectsViaVTable(vtable) 
  local memScan = createMemScan() 
  memscan_firstScan(memScan, soExactValue, vtDword, '', vtable, '0', '0x00000000', '0x70000000', '', fsmAligned, '4', false, false, true, false) 
  memscan_waitTillDone(memScan) 

  local results = createFoundList(memScan) 
  foundlist_initialize(results) 
  local count = foundlist_getCount(results) - 1 

  local i 
  local objects = {} 
  for i = 1, foundlist_getCount(results) - 1 do 
    local address = getAddress(foundlist_getAddress(results, i)) 
    table.insert(objects, address) 
  end 

  foundlist_deinitialize(results) 

  return objects 
end 

function patchFastTravel()
  local stationVTable = getAddress('"Borderlands2.exe"+013A6808') 
  local stations = findObjectsViaVTable(stationVTable) 

  for i, address in ipairs(stations) do 
    writeInteger(address + 0x74, 1) 
  end 

  showMessage('Patched ' .. table.getn(stations) .. ' stations.')
end
createHotkey("patchFastTravel",114) --F3

