alloc(newmem2,$1000,player_stats)
alloc(player_ptr,8)
alloc(player_hack,256)

label(code)
label(return)
label(code2)
label(return2)
label(isplayer)
label(ishorse)

newmem:
  mov [player_ptr],rax
code:
  mov ecx,[rax+rdx*4]
  mov [rsi],ecx
  jmp return
