MyCode:
//========================================= v2.2* ok
// When setting point to move to or Entering in the status screen
// RSI = pHero
_MonHumanPlayer:
 mov dword ptr [aLast],'_MHP'

 mov [lSaveRAX],rax
 mov [lSaveRCX],rcx
 mov [lSaveRDX],rdx

 mov rax,[rsi+00000928]
 test rax,rax
 jz _ExitMHP

 cmp byte ptr [rax+00000250],0
 jne _ExitMHP

 mov [pPlayer],rax
 mov [pHero],rsi

 mov rcx,[rax+000002b4]
 test rcx,rcx
 jz _ExitMHP

 mov rdx,[rcx+000000a0]
 cmp rdx,rax
 jne _ExitMHP

 cmp dword ptr [iEnableMMP],0
 je _MonHPZZ                       // Jump if feature is disabled

 mov edx,(float)20.0
 cmp edx,[rsi+00000970]
 js _MonHPZZ

 mov [rsi+00000970],edx

_MonHPZZ:
 cmp dword ptr [iEnableMPG],0
 je _MonHP00                       // Jump if feature is disabled

 mov edx,ctMGLD
 cmp edx,[rcx+00000088]
 js _MonHP00

 mov [rcx+00000088],edx

_MonHP00:
 cmp dword ptr [iEnableMPR],0
 je _ExitMHP                       // Jump if feature is disabled

 movsxd rdx,dword ptr [rcx+78]
 cmp edx,[rcx+7c]
 jne _ExitMHP

 test edx,edx
 jz _ExitMHP

 mov rax,[rcx+70]
 test rax,rax
 jz _ExitMHP

 mov rcx,rdx
 mov edx,ctMRES

_MonHP01:
 cmp edx,[rax+08]
 js _MonHP02

 mov [rax+08],edx

_MonHP02:
 add rax,10
 loop _MonHP01

_ExitMHP:
 mov rdx,[lSaveRDX]
 mov rcx,[lSaveRCX]
 mov rax,[lSaveRAX]

 comiss xmm0,[rsi+00000970]        // Original code (get MP)
 jmp _BackMHP                      // Back to main code

//========================================= v2.2 ok
// When moving a Hero
_MonMovePoints:
 mov dword ptr [aLast],'_MMP'

 mov [lSaveRAX],rax
 mov [lSaveRCX],rcx
 mov [lSaveRDX],rdx

 mov [pHero2],r14

 test r14,r14
 jz _ExitMMP

 cmp r14,[pHero]
 jne _ExitMMP

 cmp dword ptr [iEnableMMP],0
 je _MonMP00                       // Jump if feature is disabled

 mov rax,[r14+00000928]
 cmp rax,[pPlayer]
 jne _ExitMMP

 xor rax,rax
 mov [rsp+60],eax

_MonMP00:
 cmp dword ptr [iEnableMTS],0
 je _ExitMMP                       // Jump if feature is disabled

 mov rax,[r14+00000540]
 test rax,rax
 jz _ExitMMP

 cmp rax,[r14+00000548]
 jne _ExitMMP

 cmp r14,[rax+000000d0]
 jne _ExitMMP

 movsxd rcx,dword ptr [rax+00000310]
 test rcx,rcx
 jz _ExitMMP

 mov rax,[rax+00000368]
 test rax,rax
 jz _ExitMMP

_MonMP01:
 mov rdx,[rax]
 test rdx,rdx
 jz _ExitMMP

 cmp dword ptr [rdx+68],ctSIZE
 jns _MonMP02

 mov dword ptr [rdx+68],ctSIZE

_MonMP02:
 add rax,8
 loop _MonMP01

_ExitMMP:
 mov rdx,[lSaveRDX]
 mov rcx,[lSaveRCX]
 mov rax,[lSaveRAX]

 subss xmm0,[rsp+60]               // Original code (dec MP)
 jmp _BackMMP                      // Back to main code

//========================================= v2.2*
// Troop Size During Battle
_MonTroopBattle:
 mov dword ptr [aLast],'_MTB'

 mov [lSaveRBX],rbx

 mov rax,[rcx+0000031c]
 test rax,rax
 jnz _ExitMTB

 mov rbx,[rcx+000000c8]
 test rbx,rbx
 jz _ExitMTB

 cmp rbx,[rcx+00000540]
 jne _ExitMTB

 mov rax,[rbx+00000380]
 test rax,rax
 jz _ExitMTB

 cmp byte ptr [rax+00000250],0
 jne _ExitMTB

 mov [pPlayer],rax

 movsxd rax,[rbx+000003ac]
 test rax,rax
 jz _MonTB12

 cmp rax,#10
 jns _MonTB12

 mov [iArmySize],eax

 mov rbx,[rbx+000003a4]
 test rbx,rbx
 jz _MonTB12

 xor rax,rax

_MonTB10:
 cmp rcx,[rbx+rax*8]
 je _MonTB11

 inc eax
 cmp eax,[iArmySize]
 js _MonTB10
 jmp _MonTB12

_MonTB11:
 mov rbx,pTroopList
 lea rbx,[rbx+rax*8]
 mov [rbx],rcx

_MonTB12:
 cmp dword ptr [iEnableMTS],0
 je _MonTB00                       // Jump if feature is disabled

 mov eax,ctSIZE
 cmp eax,[rcx+00000744]
 js _MonTB00

 mov [rcx+00000744],eax

_MonTB00:
 cmp dword ptr [iEnableMTB],0
 je _MonTB01                       // Jump if feature is disabled

 mov eax,#20
 mov ebx,[rcx+00000458]            // Initiative
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000458],ebx

 mov ebx,[rcx+0000045c]            // Attack
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+0000045c],ebx

 mov ebx,[rcx+00000464]            // Morale
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000464],ebx

 mov ebx,[rcx+00000468]            // Luck
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000468],ebx

 mov ebx,[rcx+0000046c]            // Minimum Damage
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+0000046c],ebx

 mov eax,#30
 mov ebx,[rcx+00000460]            // Defence
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000460],ebx

 mov ebx,[rcx+00000470]            // Maximum Damage
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000470],ebx

 mov eax,#18
 mov ebx,[rcx+00000474]            // Walk
 cmp ebx,eax
 cmovs ebx,eax
 mov [rcx+00000474],ebx

_MonTB01:
 cmp dword ptr [iEnableGMD],0
 je _ExitMTB                       // Jump if feature is disabled

 cmp dword ptr [rcx+00000748],0
 je _ExitMTB

 mov eax,[rcx+00000744]
 mov [rcx+00000748],eax

_ExitMTB:
 mov rbx,[lSaveRBX]

 xor eax,eax                       // Original code
 cmp [rcx+00000748],eax            // Original code (chk if size = 0)
 jmp _BackMTB                      // Back to main code

//========================================= v2.2*
// Troop Size During Battle
// Warning: This code is used for many other things
_GodMode:
 mov [lSaveRBX],rbx

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 movsxd rax,dword ptr [iArmySize]
 test rax,rax
 jz _ExitGMD

 cmp rax,#10
 jns _ExitGMD

 mov rbx,pTroopList

_GodMD00:
 cmp rcx,[rbx]
 je _GodMD01

 add rbx,8
 dec eax
 test rax,rax
 jz _ExitGMD
 jmp _GodMD00

_GodMD01:
 mov edx,[rcx+00000744]

_ExitGMD:
 mov rbx,[lSaveRBX]

 mov [rcx+00000748],edx            // Original code (upd size)
 jmp _BackGMD                      // Back to main code

//========================================= v2.2
// When a castle is selected
_MonSelCastle:
 mov dword ptr [aLast],'_MSC'
 mov [lSaveRAX],rax
 mov [lSaveRBX],rbx

 mov rbx,[rsi+000002b4]
 cmp rbx,[pPlayer]
 jne _ExitMSC

 mov rbx,[rsi+000000C8]
 test rbx,rbx
 jz _ExitMSC

 cmp rbx,[rsi+000000d0]
 jne _ExitMSC

 cmp rbx,[rsi+000002c4]
 jne _ExitMSC

 cmp rsi,[rbx+0000049c]
 jne _ExitMSC

 mov rax,[rbx+000000d0]
 test rax,rax
 jnz _ExitMSC

 mov [pCastle],rsi

 cmp dword ptr [iEnableMFB],0
 je _ExitMSC                       // Jump if feature is disabled

 mov eax,fffffeff
 and [rbx+00000688],eax

_ExitMSC:
 mov rbx,[lSaveRBX]
 mov rax,[lSaveRAX]

 cmp qword ptr [rsi+000000C8],00   // Original code (get pCastleInfo)
 jmp _BackMSC                      // Back to main code

//========================================= v2.2 NHP
// When an army is under cursor
_MonArmyUC:
 mov dword ptr [aLast],'_MAU'

 mov rdx,[rcx+000000d0]
 cmp rdx,[pHero]
 je _ExitMAU

 mov [pArmy],rcx                   // actually, pArmy

_ExitMAU:
 mov rdx,[rcx+00000380]            // Original code (get pPlayer)
 jmp _BackMAU                      // Back to main code

