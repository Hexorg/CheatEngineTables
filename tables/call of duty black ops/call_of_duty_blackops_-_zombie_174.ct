    <CheatEntry>
      <ID>40</ID>
      <Description>"Call of Duty BlackOps - Zombie"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>FF8000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{
=====================================================
 Game Title    : Call of Duty BlackOps - Zombie Mode
 Proces Name   : BlackOps.exe
 CE Version    : 6.4
 Author        : XByte07
 Features:
 - God Mode
 - One Hit Kill
 - Infinite Cost
 - Infinite Ammo
 - Infinite Grenade
 - No Reload
 - Rapid Fire
=====================================================
}
[ENABLE]
aobscanmodule(GodMode,BlackOps.exe,89 85 84 01 00 00)
aobscanmodule(OHK,BlackOps.exe,29 98 84 01 00 00)
aobscanmodule(Cost,BlackOps.exe,89 BB 88 1B 00 00)
aobscanmodule(Ammo,BlackOps.exe,01 4E 04 79 07)
aobscanmodule(Grenade,BlackOps.exe,8B 8C CE C0 03 00 00 5F)
aobscanmodule(NoReload,BlackOps.exe,89 50 04 5F C3 * * 10)
aobscanmodule(RapidFire,BlackOps.exe,89 10 80 BB 4F 05 00 00 00)

alloc(mem,2048)

label(_GodMode)
label(_OHK)
label(_Cost)
label(_Ammo)
label(_Grenade)
label(_NoReload)
label(_RapidFire)

label(Back_GodMode)
label(Back_OHK)
label(Back_Cost)
label(Back_Ammo)
label(Back_Grenade)
label(Back_NoReload)
label(Back_RapidFire)

label(Exit_GodMode)
label(Exit_OHK)
label(Exit_Cost)
label(Exit_Ammo)
label(Exit_Grenade)
label(Exit_NoReload)
label(Exit_RapidFire)

label(Flag_GodMode)
label(Flag_OHK)
label(Flag_Cost)
label(Flag_Ammo)
label(Flag_Grenade)
label(Flag_NoReload)
label(Flag_RapidFire)

registersymbol(GodMode)
registersymbol(OHK)
registersymbol(Cost)
registersymbol(Ammo)
registersymbol(Grenade)
registersymbol(NoReload)
registersymbol(RapidFire)

registersymbol(Flag_GodMode)
registersymbol(Flag_OHK)
registersymbol(Flag_Cost)
registersymbol(Flag_Ammo)
registersymbol(Flag_Grenade)
registersymbol(Flag_NoReload)
registersymbol(Flag_RapidFire)

mem:

Flag_GodMode:
dd 0
Flag_OHK:
dd 0
Flag_Cost:
dd 0
Flag_Ammo:
dd 0
Flag_Grenade:
dd 0
Flag_NoReload:
dd 0
Flag_RapidFire:
dd 0

// God Mode
_GodMode:
cmp [Flag_GodMode],01
jne Back_GodMode
mov [ebp+00000184],#9999
jmp Exit_GodMode

Back_GodMode:
mov [ebp+00000184],eax
jmp Exit_GodMode

// One Hit Kill
_OHK:
cmp [Flag_OHK],01
jne Back_OHK
sub [eax+00000184],#9999
jmp Exit_OHK

Back_OHK:
sub [eax+00000184],ebx
jmp Exit_OHK

// Infinite Cost
_Cost:
cmp [Flag_Cost],01
jne Back_Cost
mov [ebx+00001B88],#100000
jmp Exit_Cost

Back_Cost:
mov [ebx+00001B88],edi
jmp Exit_Cost

// Infinite Ammo
_Ammo:
cmp [Flag_Ammo],01
jne Back_Ammo
mov [esi+04],#999
jns BlackOps.exe+F91E5
jmp Exit_Ammo

Back_Ammo:
add [esi+04],ecx
jns BlackOps.exe+F91E5
jmp Exit_Ammo

// Infinite Grenade
_Grenade:
cmp [Flag_Grenade],01
jne Back_Grenade
mov [esi+ecx*8+000003C8],04
jmp Exit_Grenade

Back_Grenade:
mov ecx,[esi+ecx*8+000003C0]
jmp Exit_Grenade

// No Reload
_NoReload:
cmp [Flag_NoReload],01
jne Back_NoReload
db 90 90 90
pop edi
ret
jmp Exit_NoReload

Back_NoReload:
mov [eax+04],edx
pop edi
ret
jmp Exit_NoReload

// Rapid Fire
_RapidFire:
cmp [Flag_RapidFire],01
jne Back_RapidFire
mov [eax],00
cmp byte ptr [ebx+0000054F],00
jmp Exit_RapidFire

Back_RapidFire:
mov [eax],edx
cmp byte ptr [ebx+0000054F],00
jmp Exit_RapidFire

GodMode:
jmp _GodMode
nop

Exit_GodMode:

OHK:
jmp _OHK
nop

Exit_OHK:

Cost:
jmp _Cost
nop

Exit_Cost:

Ammo:
jmp _Ammo
nop

Exit_Ammo:

Grenade:
jmp _Grenade
nop
nop

Exit_Grenade:

NoReload:
jmp _NoReload

Exit_NoReload:

RapidFire:
jmp _RapidFire
nop
nop
nop
nop

Exit_RapidFire:

[DISABLE]
dealloc(mem)
GodMode:
db 89 85 84 01 00 00
OHK:
db 29 98 84 01 00 00
Cost:
db 89 BB 88 1B 00 00
Ammo:
db 01 4E 04 79 07
Grenade:
db 8B 8C CE C0 03 00 00 5F
NoReload:
db 89 50 04 5F C3
RapidFire:
db 89 10 80 BB 4F 05 00 00 00
unregistersymbol(GodMode)
unregistersymbol(OHK)
unregistersymbol(Cost)
unregistersymbol(Ammo)
unregistersymbol(Grenade)
unregistersymbol(NoReload)
unregistersymbol(RapidFire)

unregistersymbol(Flag_GodMode)
unregistersymbol(Flag_OHK)
unregistersymbol(Flag_Cost)
unregistersymbol(Flag_Ammo)
unregistersymbol(Flag_Grenade)
unregistersymbol(Flag_NoReload)
unregistersymbol(Flag_RapidFire)
</AssemblerScript>
