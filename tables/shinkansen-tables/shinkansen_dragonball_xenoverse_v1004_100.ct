fstp [ebx+d0]
battlestats_kiset_exit:

cmp dword ptr [battlestats_staminaset_enable], #0
je battlestats_staminaset_exit
fld [ebx+134]
fstp [ebx+130]
battlestats_staminaset_exit:

jmp battlestats_originalcode

