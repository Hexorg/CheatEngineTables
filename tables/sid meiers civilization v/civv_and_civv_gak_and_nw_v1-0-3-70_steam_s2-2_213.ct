 cmp edi,[pPlayer]
 jne _ExitMMSR

 mov esi,[eax+ebx*4]
 test esi,esi
 jz _ExitMMSR

 mov esi,[iMinSRes]
 cmp esi,[eax+ebx*4]
 jle _ExitMMSR

 mov [eax+ebx*4],esi

_ExitMMSR:
 jmp _BackMMSR                  // Back to main code

//========================================= x0 new
// Activated when entering or leaving a city
_MonGamePace:
 mov [esp+28],eax               // Original code
 fild dword ptr [esp+28]        // Original code

 mov [iFullBaseGA],eax
 shr eax,1
 mov [iHalfBaseGA],eax

_ExitMGP:
 jmp _BackMGP                   // Back to main code

