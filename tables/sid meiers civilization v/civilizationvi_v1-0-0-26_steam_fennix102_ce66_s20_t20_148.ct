MyCode:
//=========================================
// When GOLD is changed (Gold / Faith)
_MonPlayerGold:
 push rbx

 movzx eax,[rcx+40]
 cmp eax,[iPlayerID]
 jne _ExitMPG

 mov rbx,[rcx+78]
 test rbx,rbx
 jz _ExitMPG

 mov [pGold],rcx
 mov [pPlayer],rbx

 cmp dword ptr [iEnableMPG],0
 je _MonPG00                       // Jump if feature is disabled

 mov eax,ctMGLD
 shl eax,08

 cmp eax,[rcx+000000A0]
 js _MonPG00

 mov [rcx+000000A0],eax

_MonPG00:
 cmp dword ptr [iEnableMPF],0
 je _ExitMPG                       // Jump if feature is disabled

 mov eax,ctMFAI
 shl eax,08

 cmp eax,[rbx+00006b30]
 js _ExitMPG

 mov [rbx+00006b30],eax

_ExitMPG:
 pop rbx

 mov eax,[rcx+000000A0]            // Original code (get Gold)
 jmp _BackMPG                      // Back to main code

//=========================================
align 10,cc
//=========================================
// When a unit is selected (move points/Act Again/Works/Points)
_MonUnitMP:
 push rax
 push rbx

 movzx eax,[rdi+40]
 cmp eax,[iPlayerID]
 jne _ExitMUM

 mov [pUnit],rdi

 mov rbx,[rdi+00001240]
 test rbx,rbx
 jz _ExitMUM

 cmp dword ptr [iEnableMMP],0
 je _MonUM00                       // Jump if feature is disabled

 mov eax,ctMUMP
 shl eax,08

 cmp eax,[rbx+00000148]
 js _MonUM00

 mov [rbx+00000148],eax
 mov [rdi+00000608],eax

_MonUM00:
 cmp dword ptr [iEnableFCA],0
 je _MonUM01                       // Jump if feature is disabled

 mov eax,#1
 cmp [rdi+00000640],eax
 jns _MonUM01

 mov [rdi+00000640],eax

_MonUM01:
 cmp dword ptr [iEnableQLU],0
 je _MonUM02                       // Jump if feature is disabled

 cmp dword ptr [rdi+000006b0],00
 jne _MonUM02

 mov eax,[rdi+00000e88]
 cmp eax,#8
 jns _MonUM02

 mov eax,[rdi+00000e80]
 or eax,00010101
 mov [rdi+00000e80],eax

_MonUM02:
 cmp dword ptr [iEnableMNW],0
 je _ExitMUM                       // Jump if feature is disabled

 cmp dword ptr [rdi+000006b0],00
 je _ExitMUM

 mov eax,#3
 cmp [rdi+000006b0],eax
 jns _ExitMUM

 mov [rdi+000006b0],eax

_ExitMUM:
 pop rbx
 pop rax

 mov r14d,[rdi+00000220]           // Original code (get MP)
 jmp _BackMUM                      // Back to main code

//=========================================
align 10,cc
//=========================================
// During Battle
_MonUnitDamage:
 cmp dword ptr [iEnableGMD],0
 je _ExitMUD                       // Jump if feature is disabled

 movzx r8d,[rdi+40]
 cmp r8d,[iPlayerID]
 jne _ExitMUD

 xor r8d,r8d
 mov [rdi+00000528],r8d

_ExitMUD:
 mov r8d,[rdi+00000528]            // Original code (get Damage)
 jmp _BackMUD                      // Back to main code

//=========================================
align 10,cc
//=========================================
// During Turn
_MonPlayerResearch:
 shl eax,08                        // Original code

 movzx edx,[rdi+40]
 cmp edx,[iPlayerID]
 jne _ExitMPR

 mov [pResearch],rdi

 cmp dword ptr [iEnableMPR],0
 je _ExitMPR                       // Jump if feature is disabled

 mov rdx,[rdi+00000290]

 cmp eax,[r14+rdx]
 js _ExitMPR

 mov [r14+rdx],eax

_ExitMPR:
 mov rdx,[rdi+00000290]            // Original code (get pArray)
 jmp _BackMPR                      // Back to main code

//=========================================
align 10,cc
//=========================================
// During Turn (All prods or Wonders Only)
_MonPlayerProd:
 shl r15d,08

 mov rax,[rdi+000000c8]            // Get pCity
 test rax,rax
 jz _ExitMPP

 movzx eax,[rax+40]
 cmp eax,[iPlayerID]
 jne _ExitMPP

 cmp dword ptr [iEnableMPP],0
 je _ExitMPP                       // Jump if feature is disabled

 cmp dword ptr [iEnablePWO],0
 je _MonPP00                       // Jump if feature is disabled

 mov eax,[edi+24]
 test eax,eax
 js _ExitMPP

_MonPP00:
 mov rax,[rdi+000000F0]
 lea rcx,[r14*4+00000000]

 cmp r15d,[rcx+rax]
 js _ExitMPP

 mov [rcx+rax],r15d

_ExitMPP:
 shr r15d,08

 mov rax,[rdi+000000F0]            // Original code (get pArray)
 jmp _BackMPP                      // Back to main code

