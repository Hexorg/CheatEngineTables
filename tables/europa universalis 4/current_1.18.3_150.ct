 mov eax,#66000000            // Get minimum value (66000)

 cmp eax,[ecx+000004e8]       // Currrent Ducats &gt;= 66000?
 jle _MonPR00                 // Jump if true

 mov [ecx+000004e8],eax       // Make Ducats = 65000

