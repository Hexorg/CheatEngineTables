 mov ebx,[esi+00000170]
 cmp ebx,ecx
 cmovs ebx,ecx
 mov [esi+00000170],ebx

 mov ebx,[esi+00000174]
 cmp ebx,ecx
 cmovs ebx,ecx
 mov [esi+00000174],ebx

_MonHPZZ:
 mov ecx,[esi+0000089C]
 test ecx,ecx
 jz _ExitMHP

 cmp dword ptr [iEnableMMR],0
 je _ExitMHP                       // Jump if feature is disabled

 mov ebx,[ecx+28]
 test ebx,ebx
 jz _ExitMHP

 mov edx,[ecx+2c]
 test edx,edx
 jz _ExitMHP

_MonHP00:
 cmp ebx,edx
 jns _ExitMHP

 mov ecx,[ebx]
 mov eax,[ecx+9c]
 test eax,eax
 js _MonHP01

 shr eax,1

 cmp eax,[ebx+04]
 js _MonHP01

 mov [ebx+04],eax

_MonHP01:
 lea ebx,[ebx+10]
 jmp _MonHP00

_ExitMHP:
 pop edx
 pop ebx
 pop eax

 mov ecx,[esi+0000089C]            // Original code (get pResourcesInfo)
 jmp _BackMHP                      // Back to main code

