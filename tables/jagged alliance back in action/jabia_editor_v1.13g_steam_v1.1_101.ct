function getMercAddr(base, num)
  -- num == number, 0 .. 5
  for i = 1, num do
      base = MERC_LIST_PRE .. base .. MERC_LIST_INC;
  end
  return MERC_DATA_PRE .. base .. MERC_DATA_BASE;
end

function getMercName(base, num)
  return readString(getMercAddr(base, num) .. MERC_NAME_OFFS);
end

function getNumMercs(base, num)
  -- starting at MERC_BASE, check
  c = 0; -- start at first merc, one MUST exist, and get names until name is NIL
  while getMercName(base, c) ~= nil do
    c = c +1;
  end;
  return c;
end

function setMercItem(base, num, slot, item)
  -- print("inv base\t"  .. INVENTORY_BASE);  -- "0x178"
  -- print("slot.high\t" .. slot.ah);         -- "12"  !!!
  -- print("slot.low\t"  .. slot.al);         -- "10"  !!!

  local targetH = getMercAddr(base, num) .. "+" .. hexAdd(INVENTORY_BASE, slot.ah);
  local targetL = getMercAddr(base, num) .. "+" .. hexAdd(INVENTORY_BASE, slot.al);

  -- print("low\t" .. targetL);
  -- print("high\t" .. targetH);
  -- print("id\t" .. item.id);
  -- print("amount\t" .. item.amount);
  -- print("dura\t" .. item.dura);
  -- print("uses\t" .. item.uses);

  -- print(readInteger(targetH));
  -- print(readInteger(targetL));

  writeInteger(targetH, (item.uses*65536)+item.dura);  -- uses + durability
  writeInteger(targetL, (item.amount*65536)+item.id);  -- amount + id
end;

function setInventoryItem(slot, item)
  local targetH = string.format("%X+%X", getAddress(MP_INV_IDENTIFIER), tonumber(slot.ah));
  local targetL = string.format("%X+%X", getAddress(MP_INV_IDENTIFIER), tonumber(slot.al));
  -- print("low\t" .. targetL);
  -- print("high\t" .. targetH);
  -- print("id\t" .. item.id);
  -- print("amount\t" .. item.amount);
  -- print("dura\t" .. item.dura);
  -- print("uses\t" .. item.uses);

  writeInteger(targetH, (item.uses*65536)+item.dura);  -- uses + durability
  writeInteger(targetL, (item.amount*65536)+item.id);  -- amount + id
end;

function getMPAddress(offset)
  return string.format("%X+%X", getAddress(MP_INV_IDENTIFIER), tonumber(offset));
end;




