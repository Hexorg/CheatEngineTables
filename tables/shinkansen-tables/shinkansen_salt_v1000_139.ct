mov [eax+10], (float)40
mov [eax+14], (float)30
mov [eax+18], (float)200
mov [eax+1c], (float)50
PlayerMovement_movementset_exit:

cmp dword ptr [PlayerMovement_sprintset_enable], #0
je PlayerMovement_sprintset_exit
mov [edi+54], (float)50
mov [edi+58], (float)100
mov [edi+64], (float)0
