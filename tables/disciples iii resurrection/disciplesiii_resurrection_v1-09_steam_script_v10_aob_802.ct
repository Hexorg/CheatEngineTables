//========================================= 1.09
// Player Resources at Minimum
_MonPlayerRes:
 mov [pGold],ebp                // Save pointer for debugging

 cmp dword ptr [iEnableMPR],0
 je _ExitMPR                    // Jump if feature is disabled

 cmp dword ptr [ebp],1          // Is it the player's resource structure?
