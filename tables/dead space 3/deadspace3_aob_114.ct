 jmp botslimitRet

getbots: //Get Max Bots
 cmp [getbotsToggle],1
 jne short getbotsOrg
 mov [eax+24],5
 mov [getbotsToggle],0
getbotsOrg:
 mov ecx,[eax+24]
 mov eax,[esi+60]
 jmp getbotsRet

_gravity: //Gravity
