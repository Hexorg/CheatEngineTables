alloc(cyclistseffortstats_newmem, 2048)


cyclistseffortstats_newmem:

// Check Normal Effort which should be set to (float)99
// AI seems to be alwaya (float)65
cmp dword ptr [ecx+194], #1120272384
je cyclistseffortstats_update_entry

// Check Individual Time-Trial Effort which should be set to (float)99
// AI seems to be alwaya (float)70
cmp dword ptr [ecx+1c0], #1120272384
je cyclistseffortstats_update_entry

// Check Team Time-Trial Effort which should be set to (float)99
// AI seems to be alwaya (float)70
cmp dword ptr [ecx+1d4], #1120272384
jne cyclistseffortstats_barsset_exit

cyclistseffortstats_update_entry:
cmp dword ptr [cyclistseffortstats_update_enable], #0
je cyclistseffortstats_update_exit
mov [cyclistseffortstats_basepointer], ecx
cyclistseffortstats_update_exit:

cmp dword ptr [cyclistseffortstats_set_enable], #0
je cyclistseffortstats_set_exit
cyclistseffortstats_set_exit:

cmp dword ptr [cyclistseffortstats_heartrateset_enable], #0
je cyclistseffortstats_heartrateset_exit
mov eax, [ecx+144]
fld [cyclistseffortstats_heartrateset_value]
fstp [eax+14]
cyclistseffortstats_heartrateset_exit:

cmp dword ptr [cyclistseffortstats_barsset_enable], #0
je cyclistseffortstats_barsset_exit
mov eax, [ecx+144]
// Individual Time-Trial
fld [eax+28]
fstp [eax+20]
// Team Time-Trial
fld [eax+30]
fstp [eax+24]
// Normal
fld [eax+50]
fstp [eax+40]
fld [eax+58]
fstp [eax+44]
fld [eax+58]
fstp [eax+48]
mov [eax+5c], (float)0
cyclistseffortstats_barsset_exit:


cyclistseffortstats_originalcode:
db 8B 89 44 01 00 00
//mov ecx,[ecx+00000144]


cyclistseffortstats_exit:
jmp cyclistseffortstats_returnhere


cyclistseffortstats_searchedAOB:
cyclistseffortstats_savedAOB:
