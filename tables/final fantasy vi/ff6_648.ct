    <CheatEntry>
      <ID>95</ID>
      <Description>" &lt;based on vng21092 and cake-san's work, this just a rearrangement&gt;"</Description>
      <LastState Value="" Activated="0" RealAddress="00000000"/>
      <Color>4080FF</Color>
      <GroupHeader>1</GroupHeader>
