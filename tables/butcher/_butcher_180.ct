define(playerMul,cave)
define(enemyMul,cave+04)

playerMul:
dd (float)0.25
enemyMul:
dd (float)4

define(Enabled,cave+40)
Enabled:
dd 1 // Enabled == true

cave+300:
readmem(hack,5)

cmp   byte ptr[Enabled],0
jne   short @f
jmp   hack+05
@@:
cmp   byte ptr[rcx+pa.isPlayer],0
jne   short @f
mulss xmm1,[enemyMul]
jmp   hack+05
@@:
mulss xmm1,[playerMul]
jmp  hack+05



hack:
jmp  cave+300


[DISABLE]

