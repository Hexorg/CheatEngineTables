label(creditstats_returnhere)
label(creditstats_originalcode)
label(creditstats_exit)

label(creditstatsupdate_exit)
label(creditstatsset_exit)

registersymbol(creditstats_savedAOB)

aobscan(creditstats_searchedAOB,8B 40 40 3B C3 0F 9C C0)

alloc(creditstats_newmem,2048) //2kb should be enough


creditstats_newmem:
cmp dword ptr [creditstatsupdate_enable], #0
je creditstatsupdate_exit
mov [creditstats_basepointer], eax
creditstatsupdate_exit:

cmp dword ptr [creditstatsset_enable], #0
je creditstatsset_exit
mov [eax+40], #9999999
creditstatsset_exit:


creditstats_originalcode:
db 8B 40 40 3B C3
//mov eax,[eax+40]
//cmp eax,ebx


creditstats_exit:
jmp creditstats_returnhere


creditstats_searchedAOB:
creditstats_savedAOB:
jmp creditstats_newmem
creditstats_returnhere:


[DISABLE]
creditstats_savedAOB:
db 8B 40 40 3B C3
//mov eax,[eax+40]
//cmp eax,ebx

unregistersymbol(creditstats_savedAOB)

dealloc(creditstats_newmem)

