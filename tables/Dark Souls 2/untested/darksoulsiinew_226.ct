    local addr = flight_getPointer()
    flight_set(addr, FLIGHT_OFFSET_X_MODEL, FLIGHT_OFFSET_X_GHOST)
    flight_set(addr, FLIGHT_OFFSET_Y_MODEL, FLIGHT_OFFSET_Y_GHOST)
    flight_set(addr, FLIGHT_OFFSET_Z_MODEL, FLIGHT_OFFSET_Z_GHOST)
  end
end

if flight_modify == nil then
  function flight_modify(addr, offset, value)
    addr = addr + offset
    local modify = readFloat(addr)
    modify = modify + value
    writeFloat(addr, modify)
  end
end

