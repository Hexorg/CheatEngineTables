===========================================
 Game Title    : Divinity Dragon Commander
 Game Version  : 1.0.12.0
 Game Sponsor  : FairLight (Release #1108)
 Proces Name   : DCApp.exe
 Script Version: 1.0
 CE Version    : 6.3
 Release date  : 07-Aug-2013
 Author        : Recifense
 History:
 07-Aug-13: First Release
 Features:
 - God Mode
 - Minimum Population   (630)
 - Full Gold            (50)
 - Full Research Points (30)
 - Some Pointers
===========================================
}

//=========================================
// Definitions
define(LUDO,"DCApp.exe")
define(MSUN,DCApp.exe+00165eae)
define(GDMD,DCApp.exe+00013fee)
define(MPPP,DCApp.exe+00090b69)
define(MPP1,DCApp.exe+00090a73)
define(MPRS,DCApp.exe+001ffd0f)

//=========================================
[ENABLE]
//=========================================
//LuaCall(CheckVersion())
//=========================================
// Check if script is compatible to this game version
// If false the script will not be loaded

assert(MSUN,83 be 10 02 00 00 00 ?? ?? 8b ce e8 ?? ?? ?? ?? 84 c0 ?? ?? 8b)
assert(GDMD,89 88 10 02 00 00 59 c2 04 00 8d 0c 24 8b 11 89 90 10 02 00 00)
assert(MPPP,8b 84 be 80 04 00 00 3b c1 ?? ?? 8b 0d ?? ?? ?? ?? e8 ?? ?? ??)
assert(MPP1,83 bc b1 80 04 00 00 00 a1 ?? ?? ?? ?? 8b 78 0c ?? ?? 3b f7 ??)
assert(MPRS,8b 47 3c 50 6a 01 8d 8c 24 b0 04 00 00 ff 15 ?? ?? ?? ?? 8b 3d)

//=========================================
alloc(MyCode,4096,LUDO)
//=========================================
// Declaration section
label(_MonSelUnit)
label(_BackMSU)
label(_ExitMSU)
label(_GodMode)
label(_BackGMD)
label(_ExitGMD)
label(_MonPlayerPop)
label(_BackMPP)
label(_ExitMPP)
label(_MonPlayerPop1)
label(_BackMP1)
label(_ExitMP1)
label(_MonPlayerResources)
label(_MonPR00)
label(_BackMPR)
label(_ExitMPR)
label(pPlayer)
label(pPop)
label(pUnit)
label(iEnableGMD)
label(iEnableMPP)
label(iEnableMGD)
label(iEnableMRP)

//=========================================
// Registering Symbols
registersymbol(MyCode)
registersymbol(pPlayer)
registersymbol(pPop)
registersymbol(pUnit)
registersymbol(iEnableGMD)
registersymbol(iEnableMPP)
registersymbol(iEnableMGD)
registersymbol(iEnableMRP)

//=========================================
MyCode:
//=========================================
// Selected Unit (all) (4debugging)
_MonSelUnit:
 mov [pUnit],esi

_ExitMSU:
 cmp dword ptr [esi+00000210],00   // Original code (rds cHP)
 jmp _BackMSU                      // Back to main code

//=========================================
// During Combat
_GodMode:
 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 cmp dword ptr [eax+00000228],0
 jne _ExitGMD

 mov ecx,[eax+00000214]

_ExitGMD:
 mov [eax+00000210],ecx            // Original code (upds HP)
 jmp _BackGMD                      // Back to main code

//=========================================
// In Main Screen
_MonPlayerPop:
 mov [pPop],esi

 cmp dword ptr [iEnableMPP],0
 je _ExitMPP                       // Jump if feature is disabled

 mov eax,#630
 cmp eax,[esi+00000480]
 jle _ExitMPP

 mov [esi+00000480],eax

_ExitMPP:
 mov eax,[esi+edi*4+00000480]      // Original code (rds cPop)
 jmp _BackMPP                      // Back to main code

//=========================================
// In Main Screen
_MonPlayerPop1:
 push eax

 mov [pPop],ecx

 cmp dword ptr [iEnableMPP],0
 je _ExitMP1                       // Jump if feature is disabled

 mov eax,#630
 cmp eax,[ecx+00000480]
 jle _ExitMP1

 mov [ecx+00000480],eax

_ExitMP1:
 pop eax

 cmp dword ptr [ecx+esi*4+00000480],00 // Original code (rds cPop)
 jmp _BackMP1                      // Back to main code

//=========================================
// At Empire Overview
_MonPlayerResources:
 mov [pPlayer],edi

 cmp dword ptr [edi+10],0
 jne _ExitMPR

 cmp dword ptr [iEnableMGD],0
 je _MonPR00                      // Jump if feature is disabled

 mov eax,#50
 cmp eax,[edi+3C]
 jle _MonPR00

 mov [edi+3c],eax

_MonPR00:
 cmp dword ptr [iEnableMRP],0
 je _ExitMPR                       // Jump if feature is disabled

 mov eax,#30
 cmp eax,[edi+44]
 jle _ExitMPR

 mov [edi+44],eax

_ExitMPR:
 mov eax,[edi+3C]                  // Original code (rds Gold)
 push eax                          // Original code
 db 6a 01                          // Original code (push 01)
 jmp _BackMPR                      // Back to main code

//=========================================
 db '============================================&gt;'
 db 'CE6.2 Script by Recifense 080713'
//=========================================
// Variables
iEnableGMD:
 dd 0
iEnableMPP:
 dd 0
iEnableMGD:
 dd 0
iEnableMRP:
 dd 0
pPop:
 dd 0
pPlayer:
 dd 0
pUnit:
 dd 0

//=========================================
// Hacking Points
MSUN:
 jmp _MonSelUnit
 nop
 nop
_BackMSU:

GDMD:
 jmp _GodMode
 nop
_BackGMD:

MPPP:
 jmp _MonPlayerPop
 nop
 nop
_BackMPP:

MPP1:
 jmp _MonPlayerPop1
 nop
 nop
 nop
_BackMP1:

MPRS:
 jmp _MonPlayerResources
 nop
_BackMPR:

//=========================================
// Script for Restoring Original Codes
[DISABLE]
MSUN:
// cmp dword ptr [esi+00000210],00
 db 83 be 10 02 00 00 00

GDMD:
// mov [eax+00000210],ecx
 db 89 88 10 02 00 00

MPPP:
// mov eax,[esi+edi*4+00000480]
 db 8b 84 be 80 04 00 00

MPP1:
// mcmp dword ptr [ecx+esi*4+00000480],00
 db 83 bc b1 80 04 00 00 00

MPRS:
// mov eax,[edi+3C]
// push eax
// push 01
 db 8b 47 3c 50 6a 01

//=========================================
// Unregistering Symbols
unregistersymbol(MyCode)
unregistersymbol(pPlayer)
unregistersymbol(pPop)
unregistersymbol(pUnit)
unregistersymbol(iEnableGMD)
unregistersymbol(iEnableMPP)
unregistersymbol(iEnableMGD)
unregistersymbol(iEnableMRP)

//=========================================
dealloc(MyCode)
//============= Scripts End ===============

// ****************************************
// NOTES
// ****************************************
{
}

