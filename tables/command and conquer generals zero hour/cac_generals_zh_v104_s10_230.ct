 mov dword ptr [eax+00000190],3

_MonR1:
 cmp dword ptr [eax+00000194],#5000
 jg _MonR2

 mov ebx,[eax+00000194]
 dec ebx
 mov [eax+0000018c],ebx

_MonR2:
 cmp dword ptr [iEnableME],0
 je _ExitMR                     // Jump if feature is disabled

 xor ebx,ebx
