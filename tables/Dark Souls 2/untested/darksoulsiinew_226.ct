  local base = readPointer("DarkSoulsII.exe+160DC90")
  base = readPointer(base + 0x100 + 0x1D8)
  local names = readInteger(base + 0x14) + base
  local maxIndex = readInteger(base + 0xC)
  local i
  for i = 0, maxIndex do
    -- get the item id range
    local index = i * 3
    local addr = base + index * 4
    local startId = readInteger(addr + 0x20)
    local endId = readInteger(addr + 0x24)
    -- get the item name
    local offset = readInteger(addr + 0x1C)
    offset = offset - startId
    for itemId = startId, endId do
      local nameIndex = offset + itemId
      nameIndex = readInteger(names + nameIndex * 4)
      local itemName = readString(base + nameIndex, 500, true)
      -- list the item
      print(string.format("%-9s : %s", itemId, itemName))
    end
  end
end)()
