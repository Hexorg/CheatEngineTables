code:
  cmp dword ptr [globals+2004],1
  je skip
  sub eax,[edx+00000150]

skip:
  jmp return

INJECT_GunAmmoUse:
  jmp code
  nop
return:
registersymbol(INJECT_GunAmmoUse)

