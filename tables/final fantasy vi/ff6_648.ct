      <CheatEntries>
        <CheatEntry>
          <ID>91</ID>
          <Description>"Choose Hero (2nd) &lt;Guest Included &gt;"</Description>
          <LastState Activated="0"/>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>{$lua}
if syntaxcheck then return end

local function ceupd(t,o)for k,v in next,o do t[k]=v end return t end
local base = readInteger('[[__+10]]')

if base==nil then
  showMessage("Can't locate Character Base,sry~")
  return
end

local choice = createStringlist()
for i=0,15 do
  local addr = readInteger(base +i*0x30+0xc)
  local name = readString(addr,8) or ''
  if name:len()&gt;0 then choice.add(string.format('%X:%s',i,name)) end
end
local f = ceupd(createForm(),{Caption='Choose',Width=100,Height=120,Position='poOwnerFormCenter',
            ShowInTaskBar='stAlways',BorderStyle='bsDialog',OnClose=function(s)s.Destroy()end})

local ls = ceupd(createListBox(f),{Items=choice,Align=alClient})

ls.OnDblClick = function(sender)
  local idx = sender.ItemIndex
  if idx &gt;= 0 then
    idx = sender.Items[idx]:match('^%x+')
    if idx~=nil and tonumber(idx,16)~=nil then
      idx = tonumber(idx,16)
      local al = getAddressList()
      local mr = al.getMemoryRecordByDescription('*HERO*')
      mr.Offset[1]=0x8+idx*0x30
      mr = al.getMemoryRecordByDescription('*NAME*')
      mr.Offset[1]=0xc+idx*0x30
    end
  end
  sender.Destroy()
  f:OnClose()
end
f.show()

[Enable]

[Disable]

</AssemblerScript>
