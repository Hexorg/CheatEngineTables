mov dword ptr [ebx],ecx
mov eax,0

originalcode:
sub [ebx],eax
mov ebx,[ebx]
mov ecx,[ebp+0C]

exit:
jmp returnhere

newmem1: //this is allocated memory, you have read,write,execute access
//place your code here
or edx,edx
je originalcode1
mov [pO1],edx

originalcode1:
mov eax,[ecx]
mov edx,[AK::IAkStreamMgr::m_pStreamMgr+16385C]

exit1:
jmp returnhere1

