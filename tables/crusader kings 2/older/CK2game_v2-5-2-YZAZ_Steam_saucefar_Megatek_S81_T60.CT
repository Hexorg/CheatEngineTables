//========================================= v2.5.1 (s8.1)
// During combat
_GodMode:
 push esi

 mov esi,[ebp+08]                  // Get Base_2
 test esi,esi
 jz _ExitGM

 lea esi,[esi-b4]
 mov ecx,[esi+34]
 test ecx,ecx
 jz _GodM0D

 mov ecx,[ecx+0000010c]
 cmp ecx,[pOwner]
 jne _GodM00
 jmp _GodM0E
