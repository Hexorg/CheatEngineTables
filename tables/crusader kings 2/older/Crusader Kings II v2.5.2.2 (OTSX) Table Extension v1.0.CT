      <CheatEntries>
        <CheatEntry>
          <ID>3153</ID>
          <Description>"[X] &lt;== Enable Console/Developer Mode in Ironman"</Description>
          <Options moHideChildren="1"/>
          <LastState/>
          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>//=========================================
[ENABLE]
//=========================================
label(CHP)
label(CHP2)
registersymbol(CHP)
registersymbol(CHP2)

aobscan(aob1, A1 20 FB 4A 01 80 B8 58 04 00 00 00 ?? ??) //show console
aobscan(aob2, 8B 1D 20 FB 4A 01 39 BB 4C 04 00 00 ?? ??) //enable console

//=========================================
// Actual script
//=========================================

//Removes the check for showing the console
aob1+5:
CHP:
 nop
 nop
 nop
 nop
 nop
 cmp eax,eax

//Removes the check for ironman mode
aob2+6:
CHP2:
 nop
 nop
 nop
 nop
 cmp eax,eax

[DISABLE]
CHP:
//cmp byte ptr [eax+00000458],00
 db 80 B8 58 04 00 00 00
CHP2:
//cmp [ebx+0000044C],edi
 db 39 BB 4C 04 00 00

unregistersymbol(CHP)
unregistersymbol(CHP2)
</AssemblerScript>
