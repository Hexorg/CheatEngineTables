function readShortint(address)
  local b1,b2 = readBytes(address,2)
  if (b1 and b2) then
    return b2*256+b1
  end
  return nil
end

function writeShortint(address, value)
  local b1 = value % 256
  local b2 = value / 256
  writeBytes(address, b1, b2)
  return nil
end

function maxThings(address, max)
  if readBytes(address,1) then
    for i = 1, max do
      local curItemAmount = readBytes(address,1)
      if curItemAmount &gt; 0 then
        writeBytes(address, 100)
      end
      address = address + 2
    end
  end
  return nil
end


