      <CheatEntries>
        <CheatEntry>
          <ID>21</ID>
          <Description>"Invincibility + 1 Hit Kills"</Description>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
aobscan(dmg_aob, D9 C0 D8 ?? D9 ?? C7 85 ?? ?? FF FF 00 00 00 00 D9 ?? 04 D9 C9 DB E9 DB D1 DD D9 D9 EE DB E9 0F 87 ?? ?? 00 00 D9 C9 D9 ?? 0F 83 ?? ?? 00 00 DF)
label(dmg_hack)
registersymbol(dmg_hack)
label(dmg_ret)
label(isnt_me)
alloc(dmg_mem, 1024)

dmg_aob:
dmg_hack:
jmp dmg_mem
nop
dmg_ret:

dmg_mem:
fstp st(0)
cmp esi,[player+8]
jne isnt_me

fstp st(0)
fldz // life before hit
fld dword ptr [esi] // life after hit
jmp dmg_ret

isnt_me:
fld dword ptr [esi] // life before hit
fldz // life after hit
fst dword ptr [esi] // store new life
jmp dmg_ret


[DISABLE]
dmg_hack:
db D9 C0 D8 2E D9 16
dealloc(dmg_mem)
</AssemblerScript>
