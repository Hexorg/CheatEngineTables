// Hack points

// Reduce Clothes Durability
//Game.exe+1167CC - 23 C2               - and eax,edx
//Game.exe+1167CE - 66 89 01            - mov [ecx],ax
//Game.exe+1167D1 - 8B 5C 24 18         - mov ebx,[esp+18]
//Game.exe+1167D5 - 85 DB               - test ebx,ebx
// 23C26689018B5C241885DB
aobscan(aob_clothes,23C26689018B5C241885DB)

// Reduce Weapon Durability
// GameJABiA.exe+122376 - 66 89 86 88010000  - mov [esi+00000188],ax
// GameJABiA.exe+12237D - 8B 53 08       - mov edx,[ebx+08]
// GameJABiA.exe+122380 - 8B 72 30       - mov esi,[edx+30]
// GameJABiA.exe+122383 - 66 39 BE C4010000  - cmp [esi+000001C4],di
// 668986880100008B53088B7230

aobscan(aob_weapon,668986880100008B53088B7230)



// Game.exe+11501C: // Clothes Durability Reduction max always 10?
aob_clothes:
_Dura1_Hook:
  jmp _Dura1_Start
_Dura1_Back:

// Game.exe+120FF2: // Weapon Durability Reduction
aob_weapon:
_Dura2_Hook:
