//=========================================
// Province undercursor or selected
_MonProvinceGarrison:
 push ebx

 cmp dword ptr [iEnableMPG],0
 je _ExitMPG                  // Jump if feature is disabled

 mov ebx,[eax+00000a88]
 cmp ebx,[iPlayerID]
 jne _ExitMPG

