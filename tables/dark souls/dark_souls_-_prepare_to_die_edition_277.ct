//==================================================
// MEMORY CODES
//==================================================
MEMORYCODES:
//==================================================
_MAIN:
  PUSHAD
  MOV ESI,[BASE]
  MOV ESI,[ESI]
  TEST ESI,ESI
  JZ _MAIN_EXIT
  MOV ESI,[ESI+0000003C]
  TEST ESI,ESI
  JZ _MAIN_EXIT
  PUSHAD
  MOV EDX,[ESI]
  MOV EDX,[EDX+000000AC]
  MOV ECX,ESI
  CALL EDX
  MOV [PLAYER.PSTATUS],EAX
  MOV [PLAYER.PCHARACTER],ESI
  POPAD

//--------------------------------------------------

_MAIN_GODMODE:
  CMP BYTE PTR [PLAYER.GODMODE.ENABLE],01
  JNE SHORT _MAIN_GODMODE_OFF
  OR BYTE PTR [ESI+000003C4],40
  MOV EAX,[ESI+00000028]
  MOV EAX,[EAX+0000001C]
  MOV DWORD PTR [EAX+00000190],0
  MOV DWORD PTR [EAX+00000194],0
  JMP SHORT _MAIN_RESTOREHEALTH

_MAIN_GODMODE_OFF:
  CMP BYTE PTR [PLAYER.GODMODE.ENABLE],00
  JNE SHORT _MAIN_RESTOREHEALTH
  MOV AL,[ESI+000003C4]
  NOT AL
  AND BYTE PTR [ESI+000003C4],AL

//--------------------------------------------------

_MAIN_RESTOREHEALTH:
  CMP BYTE PTR [PLAYER.RESTOREHEALTH.ENABLE],01
  JNE _MAIN_KILLYOURSELF
  MOV BYTE PTR [PLAYER.RESTOREHEALTH.ENABLE],00
  MOV EAX,[ESI+000002D8]
  INC EAX
  PUSH EAX
  PUSH ESI
  CALL HEALTH.HOOKADR

//--------------------------------------------------

_MAIN_KILLYOURSELF:
  CMP BYTE PTR [PLAYER.KILLYOURSELF.ENABLE],01
  JNE SHORT _MAIN_KILLALLMONSTER
  MOV BYTE PTR [PLAYER.KILLYOURSELF.ENABLE],00
  XOR EAX,EAX
  MOV [ESI+000002D4],EAX

//--------------------------------------------------

_MAIN_KILLALLMONSTER:
  CMP BYTE PTR [NPCMONSTER.KILLS.ENABLE],01
  JNE _MAIN_KILLALLHUMAN
  MOV BYTE PTR [NPCMONSTER.KILLS.ENABLE],00
  CMP DWORD PTR [NPCMONSTER.VSIZECHA],-1
  JE _MAIN_KILLALLHUMAN
  MOV ECX,NPCMONSTER.PCHA
  MOV EDI,[NPCMONSTER.VSIZECHA]
  XOR EAX,EAX
  DEC EAX

_MAIN_KILLALLMONSTER_LOOP:
  CMP DWORD PTR [ECX],0
  JZ _MAIN_KILLALLHUMAN
  PUSH EAX
  PUSH 00
  PUSH [ECX]
  CALL _CHECKCHA
  TEST AL,AL
  POP EAX
  JE SHORT _MAIN_KILLALLMONSTER_LOOPSKIP
  MOV EBX,[ECX]
  CMP BYTE PTR [EBX+000001FF],20
  JA SHORT _MAIN_KILLALLMONSTER_LOOPSKIP
  OR BYTE PTR [EBX+000001FF],20
  MOV DWORD PTR [EBX+000002D4],0

_MAIN_KILLALLMONSTER_LOOPSKIP:
  ADD ECX,04
  INC EAX
  CMP EAX,EDI
  JB SHORT _MAIN_KILLALLMONSTER_LOOP

//--------------------------------------------------

_MAIN_KILLALLHUMAN:
  CMP BYTE PTR [NPCHUMAN.KILLS.ENABLE],01
  JNE _MAIN_HUMANHIDE
  MOV BYTE PTR [NPCHUMAN.KILLS.ENABLE],00
  CMP DWORD PTR [NPCHUMAN.VSIZECHA],-1
  JE _MAIN_HUMANHIDE
  MOV ECX,NPCHUMAN.PCHA
  MOV EDI,[NPCHUMAN.VSIZECHA]
  XOR EAX,EAX
  DEC EAX

_MAIN_KILLALLHUMAN_LOOP:
  CMP DWORD PTR [ECX],0
  JZ _MAIN_HUMANHIDE
  PUSH EAX
  PUSH 00
  PUSH [ECX]
  CALL _CHECKCHA
  TEST AL,AL
  POP EAX
  JE SHORT _MAIN_KILLALLHUMAN_LOOPSKIP
  MOV EBX,[ECX]
  CMP BYTE PTR [EBX+000001FF],20
  JA SHORT _MAIN_KILLALLHUMAN_LOOPSKIP
  OR BYTE PTR [EBX+000001FF],20
  MOV DWORD PTR [EBX+000002D4],0

_MAIN_KILLALLHUMAN_LOOPSKIP:
  ADD ECX,04
  INC EAX
  CMP EAX,EDI
  JB SHORT _MAIN_KILLALLHUMAN_LOOP

//--------------------------------------------------

_MAIN_HUMANHIDE:
  PUSHAD
  CMP BYTE PTR [NPCHUMAN.INVIS.ENABLE],01
  JNE _MAIN_HUMANSHOW
  MOV BYTE PTR [NPCHUMAN.INVIS.ENABLE],00
  CMP DWORD PTR [NPCHUMAN.VSIZECHA],-1
  JE _MAIN_MONSTERHIDE
  MOV ECX,NPCHUMAN.PCHA
  MOV EDI,[NPCHUMAN.VSIZECHA]
  XOR EAX,EAX
  DEC EAX

_MAIN_HUMANHIDE_LOOP:
  CMP DWORD PTR [ECX],0
  JZ _MAIN_MONSTERHIDE
  PUSH EAX
  PUSH 00
  PUSH [ECX]
  CALL _CHECKCHA
  TEST AL,AL
  POP EAX
  JE SHORT _MAIN_HUMANHIDE_LOOPSKIP
  MOV EBX,[ECX]
  MOV EDX,[EBX+0000000C]
  MOV BYTE PTR [EBX+00000014],01

_MAIN_HUMANHIDE_LOOPSKIP:
  ADD ECX,04
  INC EAX
  CMP EAX,EDI
