teleport:
cmp [save_status],1
je save
cmp [gotosave_status],1
je gotosave
movss [edi+08],xmm3
jmp returnhere_teleport
//--------------------------------------------------//
save:
fld [edi]
fstp [save_X]
fld [edi+4]
fstp [save_Y]
fld [edi+8]
fstp [save_Z]
