cmp dword ptr [PlayerMovement_jumpset_enable], #0
je PlayerMovement_jumpset_exit
cmp dword ptr [edi+1c], #0
je PlayerMovement_jumpset_exit
mov eax, [edi+1c]
cmp dword ptr [eax+14], #0
je PlayerMovement_jumpset_exit
mov eax, [eax+14]
mov [eax+c], (float)7.5
PlayerMovement_jumpset_exit:

cmp dword ptr [PlayerMovement_movementset_enable], #0
je PlayerMovement_movementset_exit
cmp dword ptr [edi+20], #0
je PlayerMovement_movementset_exit
mov eax, [edi+20]
mov [eax+c], (float)50
mov [eax+10], (float)40
mov [eax+14], (float)30
mov [eax+18], (float)200
mov [eax+1c], (float)50
PlayerMovement_movementset_exit:

cmp dword ptr [PlayerMovement_sprintset_enable], #0
je PlayerMovement_sprintset_exit
mov [edi+54], (float)50
mov [edi+58], (float)100
mov [edi+64], (float)0
PlayerMovement_sprintset_exit:

cmp dword ptr [PlayerMovement_sneakset_enable], #0
je PlayerMovement_sneakset_exit
mov [edi+50], (float)0
mov [edi+5c], (float)100
mov [edi+68], (float)0
PlayerMovement_sneakset_exit:

cmp dword ptr [PlayerMovement_dodgeset_enable], #0
je PlayerMovement_dodgeset_exit
mov [edi+6c], (float)60
mov [edi+70], (float)100
mov [edi+74], (float)0
PlayerMovement_dodgeset_exit:


PlayerMovement_originalcode:
db D9 5F 7C 8B 47 1C
//fstp dword ptr [edi+7C]
//mov eax,[edi+1C]


PlayerMovement_exit:
jmp PlayerMovement_returnhere


PlayerMovement_searchedAOB:
PlayerMovement_savedAOB:
jmp PlayerMovement_newmem
nop
