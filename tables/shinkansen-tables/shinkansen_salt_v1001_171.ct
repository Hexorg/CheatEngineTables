//0521F427 - E8 FC86FAFF           - call 051C7B28
//0521F42C - 83 C4 10              - add esp,10
// PlayerMovement:Update+67
//0521F42F - D9 5F 7C              - fstp dword ptr [edi+7C]
//0521F432 - 8B 47 1C              - mov eax,[edi+1C]
//0521F435 - 0FB6 40 39            - movzx eax,byte ptr [eax+39]
//0521F439 - 8B F0                 - mov esi,eax
//0521F43B - 0FB6 87 8D000000      - movzx eax,byte ptr [edi+0000008D]
aobscan(PlayerMovement_searchedAOB, D9 5F 7C 8B 47 1C 0F B6 40 39)

alloc(PlayerMovement_newmem, 2048, PlayerMovement_searchedAOB)


PlayerMovement_newmem:
cmp dword ptr [PlayerMovement_update_enable], #0
je PlayerMovement_update_exit
mov [PlayerMovement_basepointer], edi
PlayerMovement_update_exit:

cmp dword ptr [PlayerMovement_set_enable], #0
je PlayerMovement_set_exit
