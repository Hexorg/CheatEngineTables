          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(Sail_savedAOB)
label(Sail_returnhere)
label(Sail_originalcode)
label(Sail_exit)

label(Sail_update_exit)
label(Sail_set_exit)

registersymbol(Sail_savedAOB)

// SailInfoGUI:ShowSailInfo+32
aobscan(Sail_searchedAOB,0F B6 47 2C 85 C0 * * * * * * 8B 47 20)

alloc(Sail_newmem,2048)


Sail_newmem:
cmp dword ptr [Sail_update_enable], #0
je Sail_update_exit
mov [Sail_basepointer], edi
Sail_update_exit:

cmp dword ptr [Sail_set_enable], #0
je Sail_set_exit
mov [edi+24], (float)75
Sail_set_exit:


Sail_originalcode:
db 0F B6 47 2C 85 C0
//movzx eax,byte ptr [edi+2C]
//test eax,eax


Sail_exit:
jmp Sail_returnhere


Sail_searchedAOB:
Sail_savedAOB:
jmp Sail_newmem
nop
Sail_returnhere:


[DISABLE]
Sail_savedAOB:
db 0F B6 47 2C 85 C0
//movzx eax,byte ptr [edi+2C]
//test eax,eax

unregistersymbol(Sail_savedAOB)

dealloc(Sail_newmem)

