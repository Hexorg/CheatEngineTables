  cmp [titanhealth_set],#0
  je @f
  push ecx
  movd ecx,xmm0 //Backup xmm0
  movd xmm0,[rbx+17F0]
  cvtdq2ps xmm0,xmm0
  movss [rbx+17F4],xmm0
  movd xmm0,ecx //Restore xmm0
  pop ecx
@@:
