local al = getAddressList()
local headerRecord = al.getMemoryRecordByDescription("Challenges")
local characterList = createStringlist()
characterList.Text = character
local challengeList = createStringlist()
challengeList.Text = challenge
for i=0,characterList.Count-1,1 do
    local characterRecord = al.createMemoryRecord()
    characterRecord.Description = characterList[i]
    characterRecord.IsGroupHeader = true
    characterRecord.Options = 'moHideChildren'
    for j=0,challengeList.Count-1,1 do
        local k = i
        if k &gt; 0xF then k = k + 3 end --Gap between K-Sha &amp; Nepgya
        local challengeRecord = al.createMemoryRecord()
        challengeRecord.Description = challengeList[j]
        challengeRecord.Address = 'partystats_basepointer'
        challengeRecord.OffsetCount = 1
        challengeRecord.Offset[0] = 0xE790+(j*0x4)+(k*0x60)
        challengeRecord.Options = 'moHideChildren'
        challengeRecord.appendToEntry(characterRecord)
    end
    characterRecord.appendToEntry(headerRecord)
end
local inventoryHeader = al.createMemoryRecord()
inventoryHeader.Description = "Inventory"
inventoryHeader.IsGroupHeader = true
inventoryHeader.Options = "moHideChildren"
headerRecord = al.getMemoryRecordByDescription("Items Stats")
for i=0,500-1,1 do
    local itemSlotHeader = al.createMemoryRecord()
    itemSlotHeader.Description = "Slot "..i+1
    itemSlotHeader.IsGroupHeader = true
    itemSlotHeader.Options = 'moHideChildren'
    local id = al.createMemoryRecord()
    id.Description = "ID"
    id.Type = vtWord
    id.Address = 'itemsstats_offset_basepointer'
    id.OffsetCount = 1
    id.Offset[0] = i*8
    id.ShowAsHex = true
    id.appendToEntry(itemSlotHeader)
    local quantity = al.createMemoryRecord()
    quantity.Description = "Quantity"
    quantity.Type = vtWord
    quantity.Address = 'itemsstats_offset_basepointer'
    quantity.OffsetCount = 1
    quantity.Offset[0] = (i*8)+4
    quantity.appendToEntry(itemSlotHeader)
	itemSlotHeader.appendToEntry(inventoryHeader)
end
inventoryHeader.appendToEntry(headerRecord)

function searchInventory(tmp,itemPtr,itemCount)
	local itemID
	local quantity
	for i=0,itemCount,1 do
		itemID = readBytes(itemPtr+(i*8),2,true)
		itemID = byteTableToWord(itemID)
		quantity = readBytes(itemPtr+(i*8)+4,2,true)
		quantity = byteTableToWord(quantity)
		if itemID == tonumber(tmp) then
			if  quantity ~= 0 then
				return 1,i
			else
				return 2,i
			end
		end
	end
	return 0,i
end
function itemAdd(startIndex,endIndex)
	local itemID = createStringlist()
	if myDropDownList_Items == nil then return end
	itemID.Text = myDropDownList_Items
	local itemPtr = readInteger(getAddress('itemsstats_basepointer'))
	local itemCount = readInteger(itemPtr-4)
	local itemBlockEnd = itemPtr+(itemCount*8)
	local addCount = 0
	local tmp,searchResult,searchIndex
	for i=startIndex,endIndex,1 do
		tmp = "0x"..itemID[i]:match("(.-):")
		searchResult,searchIndex = searchInventory(tmp,itemPtr,itemCount)
		if searchResult == 0 then
			tmp = dwordToByteTable(tmp)
			writeBytes(itemBlockEnd+(addCount*8),tmp)
			writeInteger(itemBlockEnd+(addCount*8)+4,99)
			addCount = addCount+1
		elseif searchResult == 2 then
			tmp = dwordToByteTable(tmp)
			writeBytes(itemPtr+(searchIndex*8),tmp)
			writeInteger(itemPtr+(searchIndex*8)+4,99)
		end
	end
	writeInteger(itemPtr-0x04,itemCount+addCount)
end

addDropDownList(myDropDownList_Scouts,myDropDownListName_Scouts,myDropDownListOptions_Scouts)
addDropDownList(myDropDownList_Toggle,myDropDownListName_Toggle,myDropDownListOptions_Toggle)
addDropDownList(myDropDownList_Items,myDropDownListName_Items,myDropDownListOptions_Items)
</LuaScript>
