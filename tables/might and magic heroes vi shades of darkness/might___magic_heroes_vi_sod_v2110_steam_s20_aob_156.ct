MyCode:
//========================================= 210
// On main game screen
_MonPlayerResources:
 mov esi,[ebp-40]                  // Original code
 lea edi,[esi-28]                  // Adjust pointer
 mov [pRes],edi                    // Save it for further use

 cmp dword ptr [iEnableMR],0
 je _ExitMPR                       // Jump if feature is disabled

 mov ecx,#63000
 mov edi,[esi+04]
 xor edi,[esi+08]
 cmp ecx,edi                       // Gold
 jle _MonPR00

 mov edi,ecx
 xor edi,[esi+08]
 mov [esi+04],edi

_MonPR00:
 mov ecx,#6300

 mov edi,[esi+0c]
 xor edi,[esi+10]
 cmp ecx,edi                       // Wood
 jle _MonPR01

 mov edi,ecx
 xor edi,[esi+10]
 mov [esi+0c],edi

_MonPR01:
 mov edi,[esi+14]
 xor edi,[esi+18]
 cmp ecx,edi                       // Ore
 jle _MonPR02

 mov edi,ecx
 xor edi,[esi+18]
 mov [esi+14],edi

_MonPR02:
 mov edi,[esi+1c]
 xor edi,[esi+20]
 cmp ecx,edi                       // Blood Crystal
 jle _ExitMPR

 mov edi,ecx
 xor edi,[esi+20]
 mov [esi+1c],edi

_ExitMPR:
 mov esi,[ebp-40]                  // Original code
 add esi,04                        // Original code
 jmp _BackMPR                      // Back to main code

//========================================= 210
// While moving army
_MonMovePoints:
 push edx
 push ebx
 push ecx
 push edi
 push esi

 mov ecx,eax

 mov eax,[ecx+10]

 mov ebx,[eax+0000011c]
 cmp ebx,00020000
 jne _ExitMMP

 mov [pHero],eax

 mov edx,[eax+70]                  // Get pArmy
 test edx,edx
 jz _MonMP02

 mov ebx,[edx+0c]                  // Get # troops
 test ebx,ebx
 jz _MonMP02

 cmp dword ptr [iEnableMS],0       // Troop Size
 je _MonMP02                       // Jump if feature is disabled

 mov edx,[edx+04]                  // get pTroopList
 test edx,edx
 jz _MonMP02

 mov eax,[iMinSize]

_MonMP00:
 dec ebx

 mov esi,[edx+ebx*4]               // Get Troop
 mov [pUnit],esi                   // Save ptr for debugging
 mov edi,[esi+54]
 xor edi,[esi+58]
 cmp eax,edi
 jle _MonMP01

 mov edi,eax
 xor edi,[esi+58]
 mov [esi+54],edi

_MonMP01:
 test ebx,ebx
 jnz _MonMP00

_MonMP02:
 cmp dword ptr [iEnableMX],0
 je _MonMP03                      // Jump if feature is disabled

 mov eax,[pHero]
 mov esi,[eax+00000134]           // Get pXP
 test esi,esi
 jz _MonMP03

 mov eax,[iMinXP]
 mov edi,[esi+0c]
 xor edi,[esi+10]
 cmp eax,edi
 jle _MonMP03

 mov edi,eax
 xor edi,[esi+10]
 mov [esi+0c],edi

_MonMP03:
 mov eax,[pHero]

 cmp dword ptr [iEnableMM],0
 je _MonMP04                       // Jump if feature is disabled

 mov edx,[eax+00000678]
 test edx,edx
 jz _MonMP04

 movss xmm0,[eax+00000678]

_MonMP04:
_ExitMMP:
 mov eax,ecx

 pop esi
 pop edi
 pop ecx
 pop ebx
 pop edx

 movss [eax+0C],xmm0               // Original code
 jmp _BackMMP                      // Back to main code

//========================================= 210
// During Battle
_MonHeroMana:
 push eax
 push ebx

 cmp dword ptr [iEnableMMa],0
 je _ExitMHM                       // Jump if feature is disabled

 mov ebx,[ebp-0c]                  // Get pHero
 cmp ebx,[pHero]
 jne _ExitMHM

 mov eax,[ebp-04]
 mov [ebx+000006e8],eax

_ExitMHM:
 pop ebx
 pop eax

 fld dword ptr [ebp-04]            // Original code
 mov esp,ebp                       // Original code
 jmp _BackMHM                      // Back to main code

//========================================= 211
// During Battle
_GodMode:
 push eax

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov eax,[edx+18]
 test eax,eax
 jz _ExitGMD

 mov eax,[eax+10]
 test eax,eax
 jnz _ExitGMD

  mov eax,[edx+48]
 mov [edx+44],eax
 mov [edx+40],eax
 mov [ebp-000000d8],eax
 mov [ebp-0000016c],eax
 xor eax,eax
 mov [edx+00000990],eax
 mov [edx+00000994],eax

 movss xmm0,[edx+000004a4]

_ExitGMD:
 pop eax

 movss [edx+50],xmm0               // Original code
 jmp _BackGMD                      // Back to main code

