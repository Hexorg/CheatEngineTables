  Table  : +7 Cheat Table

  This script hooks the main instruction
  and search for Health, Weapon and Ammunition (5 types)

  BUGS: Every time you beat a level, you must deactivate and reactivate
  the main script again or the healt + weapon pointer will stop working :(

  Thanks to SteveAndrew: For showing how to use StackView
  Thanks to dXPacmanXb:  For the weapon ID list }

[ENABLE]

aobscanmodule(FindReadAmmo,nuclearthrone.exe,DD 03 DD 1E 5E 5B C3 8B 0B)
alloc(newmem,$1000)

label(code)
label(return)
label(original)                 //Original instruction
label(tryAmmo1)                 //Dissect bullets
label(tryAmmo2)                 //Dissect shells
label(tryAmmo3)                 //Dissect bolts
label(tryAmmo4)                 //Dissect explosives
label(tryAmmo5)                 //Dissect energy

label(testAmmo)                 //Dissect ammo
label(testHealth)               //Dissect healt &amp; weapons

label(setHealth)                //Dump health
label(setWeapon)                //Dump weapons

globalalloc(pAmmunition1,4)     //Set bullets
globalalloc(pAmmunition2,4)     //Set shells
globalalloc(pAmmunition3,4)     //Set bolts
globalalloc(pAmmunition4,4)     //Set explosives
globalalloc(pAmmunition5,4)     //Set energy

globalalloc(pHealth,4)          //Set health
globalalloc(pWeapon,4)          //Set weapon

newmem:

code:
  cmp eax,0
  jne original
  cmp edx,edi
  je testAmmo
  cmp ecx,esi
  je testHealth
  jmp original

testAmmo:
  cmp ebp,ecx
  jne original
  cmp ecx,1
  je tryAmmo1
  cmp ecx,2
  je tryAmmo2
  cmp ecx,3
  je tryAmmo3
  cmp ecx,4
  je tryAmmo4
  cmp ecx,5
  je tryAmmo5
  jmp original

testHealth:
  cmp ebp,80000000
  jne original
  cmp edi,6E
  jne original
  cmp [esp+4],0
  jne original
  cmp [esp+2C],0
  jne original
  cmp [esp+4C],6E
  jne original
  cmp [esp+74],6E
  jne original
  cmp [esp+14],39
  je setHealth
  cmp [esp+14],32
  je setWeapon
  jmp original

setHealth:
  mov [pHealth],ebx
  jmp original

setWeapon:
  mov [pWeapon],ebx
  jmp original

tryAmmo1:
  cmp [esp+18],0
  jne original
  cmp [esp+1C],1E
  jne original
  cmp [esp+2C],1
  jne original
  cmp [esp+34],1
  jne original
  cmp [esp+40],1
  jne original
  cmp [esp+54],1
  jne original
  cmp [esp+68],1
  mov [pAmmunition1],ebx
  jmp original

tryAmmo2:
  cmp [esp+18],0
  jne original
  cmp [esp+1C],1E
  jne original
  cmp [esp+2C],2
  jne original
  cmp [esp+34],1
  jne original
  cmp [esp+40],2
  jne original
  cmp [esp+54],2
  jne original
  cmp [esp+68],1
  mov [pAmmunition2],ebx
  jmp original

tryAmmo3:
  cmp [esp+18],0
  jne original
  cmp [esp+1C],1E
  jne original
  cmp [esp+2C],3
  jne original
  cmp [esp+34],1
  jne original
  cmp [esp+40],3
  jne original
  cmp [esp+54],3
  jne original
  cmp [esp+68],1
  mov [pAmmunition3],ebx
  jmp original

tryAmmo4:
  cmp [esp+18],0
  jne original
  cmp [esp+1C],1E
  jne original
  cmp [esp+2C],4
  jne original
  cmp [esp+34],1
  jne original
  cmp [esp+40],4
  jne original
  cmp [esp+54],4
  jne original
  cmp [esp+68],1
  mov [pAmmunition4],ebx
  jmp original

tryAmmo5:
  cmp [esp+18],0
  jne original
  cmp [esp+1C],1E
  jne original
  cmp [esp+2C],5
  jne original
  cmp [esp+34],1
  jne original
  cmp [esp+40],5
  jne original
  cmp [esp+54],5
  jne original
  cmp [esp+68],1
  mov [pAmmunition5],ebx
  jmp original

original:
  fld qword ptr [ebx]
  fstp qword ptr [esi]
  pop esi
  jmp return

FindReadAmmo:
  jmp code
return:
registersymbol(FindReadAmmo)

[DISABLE]

FindReadAmmo:
  db DD 03 DD 1E 5E

unregistersymbol(FindReadAmmo)
dealloc(newmem)

