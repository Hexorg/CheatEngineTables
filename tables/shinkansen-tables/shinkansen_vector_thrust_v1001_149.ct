          <ID>112305</ID>
          <Description>"Battle Stats Base Address Scan (To Update: Battle)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(battlestats_savedAOB)
label(battlestats_returnhere)
label(battlestats_originalcode)
label(battlestats_exit)

label(battlestats_update_exit)
label(battlestats_set_exit)
label(battlestats_damagereceivedset_exit)
label(battlestats_offensiveset_exit)
label(battlestats_offensivegunset_exit)
label(battlestats_offensiveweaponset_exit)
label(battlestats_offensiveweaponspset_exit)
label(battlestats_defensiveset_exit)
label(battlestats_fuelset_exit)
label(battlestats_missionset_exit)

registersymbol(battlestats_savedAOB)

//006C08EA - FF D2                 - call edx
//006C08EC - 89 44 24 1C           - mov [esp+1C],eax
//006C08F0 - 8B 83 14020000        - mov eax,[ebx+00000214]
//006C08F6 - 8B 00                 - mov eax,[eax]
//006C08F8 - 8B 48 44              - mov ecx,[eax+44]
//006C08FB - 8A 90 C4000000        - mov dl,[eax+000000C4]
//006C0901 - 89 4C 24 20           - mov [esp+20],ecx
aobscan(battlestats_searchedAOB, 8B 83 14 02 00 00 8B 00 8B 48 44)

alloc(battlestats_newmem, 2048)
alloc(battlestats_temp1, 4)


battlestats_newmem:
mov [battlestats_temp1], ecx

cmp dword ptr [battlestats_update_enable], #0
je battlestats_update_exit
mov [battlestats_basepointer], ebx
battlestats_update_exit:

cmp dword ptr [battlestats_set_enable], #0
je battlestats_set_exit
battlestats_set_exit:

cmp dword ptr [battlestats_damagereceivedset_enable], #0
je battlestats_damagereceivedset_exit
cmp dword ptr [ebx+5c], #0
je battlestats_damagereceivedset_exit
mov ecx, [ebx+5c]
cmp dword ptr [ecx+18], #0
je battlestats_damagereceivedset_exit
mov ecx, [ecx+18]
mov [ecx+1c], (float)-1000000
battlestats_damagereceivedset_exit:

cmp dword ptr [battlestats_offensiveset_enable], #0
je battlestats_offensiveset_exit
// Gun
cmp dword ptr [ebx+20c], #0
je battlestats_offensivegunset_exit
mov ecx, [ebx+20c]
cmp dword ptr [ecx], #0
je battlestats_offensivegunset_exit
mov ecx, [ecx]
mov eax, [ecx+40]
mov [ecx+44], eax
mov [ecx+c0], #0
battlestats_offensivegunset_exit:
// Weapon
cmp dword ptr [ebx+214], #0
je battlestats_offensiveweaponset_exit
mov ecx, [ebx+214]
cmp dword ptr [ecx], #0
je battlestats_offensiveweaponset_exit
mov ecx, [ecx]
mov eax, [ecx+40]
mov [ecx+44], eax
battlestats_offensiveweaponset_exit:
// Weapon SP
cmp dword ptr [ebx+214], #0
je battlestats_offensiveweaponspset_exit
mov ecx, [ebx+214]
cmp dword ptr [ecx+4], #0
je battlestats_offensiveweaponspset_exit
mov ecx, [ecx+4]
mov eax, [ecx+40]
mov [ecx+44], eax
battlestats_offensiveweaponspset_exit:
battlestats_offensiveset_exit:

cmp dword ptr [battlestats_defensiveset_enable], #0
je battlestats_defensiveset_exit
mov [ebx+1c4], #30
mov [ebx+1d0], #30
battlestats_defensiveset_exit:

cmp dword ptr [battlestats_fuelset_enable], #0
je battlestats_fuelset_exit
mov [ebx+cc], (float)1000
battlestats_fuelset_exit:

cmp dword ptr [battlestats_missionset_enable], #0
je battlestats_missionset_exit
cmp dword ptr [ebx+3e4], #0
je battlestats_missionset_exit
mov ecx, [ebx+3e4]
mov [ecx+38], (float)9999999
mov [ecx+88], (float)0
battlestats_missionset_exit:

mov ecx, [battlestats_temp1]


battlestats_originalcode:
db 8B 83 14 02 00 00
//mov eax,[ebx+00000214]


battlestats_exit:
jmp battlestats_returnhere


battlestats_searchedAOB:
battlestats_savedAOB:
jmp battlestats_newmem
nop
battlestats_returnhere:


[DISABLE]
battlestats_savedAOB:
db 8B 83 14 02 00 00
//mov eax,[ebx+00000214]

unregistersymbol(battlestats_savedAOB)

dealloc(battlestats_newmem)
dealloc(battlestats_temp1)

