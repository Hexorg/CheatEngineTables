label( UnlimitedHealth_o )
registersymbol( UnlimitedHealth_o )
alloc( UnlimitedHealthHook, 256, Dishonored2.exe )
label( back )

UnlimitedHealthHook:
push rax
mov rax,[dqPlayerPtr]
cmp [rcx+10],rax
pop rax
jne short UnlimitedHealth_o
  cmp [rcx+A8],FFFFFFFF   // if switch not enabled OR you performed a special action kill
  je short @f
    movss xmm14,xmm0
    movss xmm15,xmm1
    push r8
    push r9
    push rax
    push rcx
    push rdx
	mov rax,[rcx]
	call [rax+D8]         // get Max Health
	movss [rbx+20],xmm0   // write it to Current Health
    pop rdx
    pop rcx
	pop rax
    pop r9
    pop r8
    movss xmm1,xmm15
    movss xmm0,xmm14
	mov [rcx+A8],FFFFFFFF // set flag
UnlimitedHealth_o:
readmem( UnlimitedHealth, 5 )
jmp back

UnlimitedHealth:
jmp UnlimitedHealthHook
back:

