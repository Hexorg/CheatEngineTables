 cmp [LUDO+005e68e0],ebx        // Original code
 jmp _BackMR                    // Back to main code

//=========================================
_GodMode:
 fst dword ptr [fGarbage]
 mov ecx,[fGarbage]
 test ecx,ecx
 js _GodM00

 cmp ecx,[esi+08]
 jge _ExitGM

 mov ecx,[esi+10]
 cmp ecx,47c34f80
 je _ExitGM

_GodM00:
 cmp dword ptr [iEnableGM],0
 je _ExitGM                     // Jump if feature is disabled

 mov ecx,[esi-08]               // Get ptr to Unit/Building
 or ecx,ecx
