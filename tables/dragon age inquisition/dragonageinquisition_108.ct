  <LuaScript>-- convert DEC numbers to HEX representation strings --
function dectohex(num)
  return string.format('%02X', num)
end

-- aob scan --
function rangedAOBScan(pStart, pEnd, flags, alignment, AOB)
  local scan = createMemScan()
  memscan_returnOnlyOneResult(scan, true)
  memscan_firstScan(scan, soExactValue, vtByteArray, 0, AOB, nil, pStart, pEnd, flags, 1, alignment, true, false, false, false)
  memscan_waitTillDone(scan)
  return memscan_getOnlyResult(scan)
end

-- set a label to an address --
function changeLabel(label, address)
  if (address ~= nil) then
    autoAssemble([[
      label(]]..label..[[)
      registersymbol(]]..label..[[)
      ]]..dectohex(address)..[[:
      ]]..label..[[:
    ]])
  else
    showMessage(label..": Bad Address")
  end
end

function AOBSimpleScan(label, AOB)
  changeLabel(label, rangedAOBScan(0x0000000000000000, 0x000000FFFFFFFFFF, "+W*X", 1, AOB))
end

function AOBExecScan(label, AOB)
  changeLabel(label, rangedAOBScan(0x0000000000000000, 0x000000FFFFFFFFFF, "+W+X", 1, AOB))
end

function AOBDataScan(label, AOB)
  changeLabel(label, rangedAOBScan(0x0000000000000000, 0x000000FFFFFFFFFF, "+W-X", 8, AOB))
end

function AOBRangeScan(pStart, pEnd, label, AOB)
  changeLabel(label, rangedAOBScan(pStart, pEnd, "+W*X", 1, AOB))
end

---------- party pointer script ----------

reinitializeSymbolhandler()
addresslist = getAddressList()

function start_party()
  update_party()
  timer_setEnabled(timer_party, true)
end

function update_party_member(label, offset)
  local address = addresslist.getMemoryRecord(offset).getValue()
  if (address ~= "??") then
    changeLabel(label, getAddress(address))
  end
end

function update_party()
  update_party_member("pHero0", 1)
  update_party_member("pHero1", 2)
  update_party_member("pHero2", 3)
  update_party_member("pHero3", 4)
  update_party_member("pHero4", 5)
end

function stop_party()
  timer_setEnabled(timer_party, false)
  autoAssemble([[
    unregistersymbol(pHero0)
    unregistersymbol(pHero1)
    unregistersymbol(pHero2)
    unregistersymbol(pHero3)
    unregistersymbol(pHero4)
  ]])
end

if (timer_party ~= nil) then
  timer_setEnabled(timer_party, false)
  object_destroy(timer_party)
  timer_party = nil
end

timer_party = createTimer(nil, false)
timer_setInterval(timer_party, 1000)
timer_onTimer(timer_party, update_party)
timer_setEnabled(timer_party, false)

