function waitForGame()
  local waitForm = createForm(false);
  local waitLabel = createLabel(waitForm);
  waitLabel.Caption = "waiting for process to spawn..."
  waitLabel.Top = SPACING;
  waitLabel.Left = SPACING;
  waitForm.Width = SPACING + waitLabel.Width*2.5 + SPACING;
  waitForm.Height = SPACING + waitLabel.Height + SPACING;
  waitForm.Caption = "";
  waitForm.centerScreen();
  waitForm.show();
  waitForm.OnClose = function()
    waitForm.destroy();
  end;

  local t = createTimer(nil);
  t.setInterval(250); -- milliseconds
  t.OnTimer = function()
    if getProcessIDFromProcessName(JABIA_EXE) ~= nil then
      t.destroy();
      waitForm.destroy();
	  openProcess(JABIA_EXE);
	  initializeComponents();
    end;
  end;
end;




