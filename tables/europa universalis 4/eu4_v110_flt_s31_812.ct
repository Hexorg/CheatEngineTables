MyCode:
//=========================================
// This code finds out Player ID
_MonHumanPlayer:
 mov ecx,[esi+0000176C]       // Get player ID

_ExitMHP:
 mov [iPlayerID],ecx          // Save it for further use
 jmp _BackMHP                 // Back to main code

//=========================================
// main game window
_MonPlayerResources:
 push eax
 push ebx
 push edx
 push esi
 push edi

 mov ecx,eax

 mov eax,[ecx+10]             // Get Owner ID
 cmp eax,[iPlayerID]          // Is it the playerÂ´s resources?
 jne _ExitMPR                 // jump if false

 mov [pPlayer],ecx            // Save pointer for debugging

 mov eax,[ecx+0000231c]       // Get Religion
 mov [pReligion],eax          // Save pointer for debugging
 mov eax,[ecx+00002304]       // Get Culture
 mov [pCulture],eax           // Save pointer for debugging

// Ducats
 cmp dword ptr [iEnableMMD],0
 je _MonPR00                  // Jump if feature is disabled

 mov eax,#63000000            // Get minimum value (63000)

 cmp eax,[ecx+000005E4]       // Currrent Ducats &gt;= 63000?
 jle _MonPR00                 // Jump if true

 mov [ecx+000005E4],eax       // Make Ducats = 63000

// Manpower
_MonPR00:
 cmp dword ptr [iEnableFMP],0
 je _MonPR01                  // Jump if feature is disabled

 mov eax,[ecx+00001294]       // Get maximum Manpower
 mov [ecx+00001290],eax       // Make Manpower = Max

// Prestige
_MonPR01:
 cmp dword ptr [iEnableMMP],0
 je _MonPR02                  // Jump if feature is disabled

 mov eax,#90000               // Minimum Prestige
 cmp eax,[ecx+00000d3c]
 js _MonPR02

 mov [ecx+00000d3c],eax       // Update Prestige

// Research Powers
_MonPR02:
 cmp dword ptr [iEnableMRP],0
 je _MonPR03                  // Jump if feature is disabled

 mov eax,[iMaxPower]          // Minimum RP

 dec eax
 dec eax
 dec eax
 dec eax

 mov ebx,[ecx+00000080]       // Administrative Power
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+00000080],ebx

 mov ebx,[ecx+00000084]       // Diplomatic Power
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+00000084],ebx

 mov ebx,[ecx+00000088]       // Military Power
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+00000088],ebx

// Morale
_MonPR03:
 cmp dword ptr [iEnableANM],0
 je _MonPR04                  // Jump if feature is disabled

 mov ebx,[ecx+000023f8]
 test ebx,ebx
 jz _MonPR04

 mov esi,[ecx+00001460]
 test esi,esi
 jz _MonPR04

 mov edi,[ecx+00001510]
 test edi,edi
 jz _MonPR04

 mov eax,#63000               // Minimum Morale
 cmp eax,[ebx+000000e8]       // Total Army Morale
 js _MonPR13

 mov edx,eax
 sub edx,[ebx+000000e8]
 add [ebx+000000e8],edx
 add [esi+000000e8],edx

_MonPR13:
 cmp eax,[ebx+000000f0]       // Total Navy Morale
 js _MonPR04

 mov edx,eax
 sub edx,[ebx+000000f0]
 add [ebx+000000f0],edx
 add [edi+000000f0],edx

// Tradition
_MonPR04:
 cmp dword ptr [iEnableANT],0
 je _MonPR05                  // Jump if feature is disabled

 mov eax,#63000               // Minimum value

 mov ebx,[ecx+000017ac]       // Army
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+000017ac],ebx

 mov ebx,[ecx+000017b0]       // Naval
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+000017b0],ebx

// Mercantilism
_MonPR05:
 cmp dword ptr [iEnableMER],0
 je _MonPR06                  // Jump if feature is disabled

 mov eax,#300                 // Minimum value

 mov ebx,[ecx+00001c9c]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+00001c9c],ebx

// Legitimacy
_MonPR06:
 cmp dword ptr [iEnableLEG],0
 je _MonPR07                  // Jump if feature is disabled

 mov eax,#90000               // Minimum value

 mov ebx,[ecx+00001d50]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+00001d50],ebx

// National revolt Risk
_MonPR07:
 cmp dword ptr [iEnableMRR],0
 je _MonPR08                  // Jump if feature is disabled

 mov ebx,[ecx+000023f8]
 test ebx,ebx
 jz _MonPR08

 mov esi,[ecx+00000ed8]
 test esi,esi
 jz _MonPR08

 mov eax,ffff3cb0             // NRR = -50

 cmp eax,[ebx+18]             // National RR
 jns _MonPR08

 mov edx,eax
 sub edx,[ebx+18]
 add [ebx+18],edx
 add [esi+18],edx

_MonPR08:
_ExitMPR:
 pop edi
