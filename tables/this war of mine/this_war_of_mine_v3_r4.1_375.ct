label(codeDayTime)
label(lblDayTimeSetStop)
label(lblDayTimeStop)
label(lblDayTimeLoop)
label(lblDayTimeExit)
label(returnDayTime)

codeDayTime:
  cmp dword ptr [bTimeStop],0
  je lblDayTimeLoop

  push eax
  mov eax,[fTime]

  cmp eax,(float)1.0                  // stop time already set during night?
  jge lblDayTimeSetStop               // yes - adjust stop time to day time

  cmp eax,(float)0.0                  // stop time already set?
  jne lblDayTimeStop                  // yes - stop at set time

lblDayTimeSetStop:
  mov eax,[ecx+10]                    // no - set stop time
  mov [fTime],eax

lblDayTimeStop:
  mov [ecx+10],eax                    // stop day time
  pop eax

  jmp lblDayTimeExit

lblDayTimeLoop:
  mov [fTime],(float)0.0              // reset stop time

  cmp dword ptr [bTimeLoop],0
  je lblDayTimeExit

  cmp dword ptr [ecx+10],(float)0.99  // day time end?
  jl lblDayTimeExit                   // no - let the time flow

  mov [ecx+10],(float)0.0             // endo of day time - reset day time

lblDayTimeExit:
//  fld dword ptr [ecx+10]
//  push esi
//  fld1
  db D9 41 10 56 D9 E8
  jmp returnDayTime

