 je _ExitMPT                       // Jump if feature is disabled

 mov eax,[ebx+10]
 test eax,eax
 jz _ExitMPT

 mov ecx,[ebx+04]
 test ecx,ecx
 jz _ExitMPT

 mov ecx,[ecx+000001d4]
 cmp ecx,[iOwnerID]
 jne _ExitMPT

 mov ecx,[ebx+1c]
 test ecx,ecx
 jz _ExitMPT

 mov edx,[ebx+20]
 test edx,edx
 jz _ExitMPT

 mov eax,#10000

_MonPT00:
 cmp ecx,edx
 jge _ExitMPT

 cmp eax,[ecx+04]
 js _MonPT01

 mov [ecx+04],eax

_MonPT01:
 lea ecx,[ecx+10]
 jmp _MonPT00
//--
_ExitMPT:
 pop edx

 mov ecx,[ebx+5C]                  // Original code (get pType)
 mov eax,[ecx]                     // Original code
 jmp _BackMPT                      // Back to main code

