  mov rcx,[rdi+000000D8]
  mov [playerCheck],rdi {Update: OnDamage Taken}
  jmp getPlayer_exit

rottr_getPlayer:
  jmp rottr_healthAlloc+128
  db 90 90
getPlayer_exit:

rottr_healthCalc:
  jmp rottr_healthAlloc
  db 90 90 90
healthCalc_exit:


