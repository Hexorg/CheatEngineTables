//MegaTagmension Blanc.exe+D8780 - 8B 10                 - mov edx,[eax]
//MegaTagmension Blanc.exe+D8782 - 89 11                 - mov [ecx],edx
// Accesses Exp
//MegaTagmension Blanc.exe+D8784 - 8B 50 04              - mov edx,[eax+04]
//MegaTagmension Blanc.exe+D8787 - 89 51 04              - mov [ecx+04],edx
//MegaTagmension Blanc.exe+D878A - 8B 40 08              - mov eax,[eax+08]
//MegaTagmension Blanc.exe+D878D - 89 41 08              - mov [ecx+08],eax
//MegaTagmension Blanc.exe+D8790 - 8B 4D 0C              - mov ecx,[ebp+0C]
//MegaTagmension Blanc.exe+D8793 - 8B 51 04              - mov edx,[ecx+04]
//MegaTagmension Blanc.exe+D8796 - 52                    - push edx
//MegaTagmension Blanc.exe+D8797 - 8D 45 E4              - lea eax,[ebp-1C]
//MegaTagmension Blanc.exe+D879A - 50                    - push eax
aobscan(characterstats_searchedAOB, 8B 50 04 89 51 04 8B 40 08 89 41 08 8B 4D 0C 8B 51 04 52)

alloc(characterstats_newmem, 2048, characterstats_searchedAOB)

characterstats_newmem:
cmp dword ptr [characterstats_update_enable], #0
je characterstats_update_exit
mov [characterstats_basepointer], eax
characterstats_update_exit:

cmp dword ptr [characterstats_set_enable], #0
je characterstats_set_exit
characterstats_set_exit:

cmp dword ptr [characterstats_apset_enable], #0
je characterstats_apset_exit
mov [eax+c], #9999999
characterstats_apset_exit:


characterstats_originalcode:
db 8B 50 04 89 51 04
//mov edx,[eax+04]
//mov [ecx+04],edx


characterstats_exit:
jmp characterstats_returnhere


characterstats_searchedAOB:
characterstats_savedAOB:
jmp characterstats_newmem
nop
characterstats_returnhere:


[DISABLE]
characterstats_savedAOB:
db 8B 50 04 89 51 04
//mov edx,[eax+04]
//mov [ecx+04],edx

unregistersymbol(characterstats_savedAOB)

dealloc(characterstats_newmem)

