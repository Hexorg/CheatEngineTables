alloc(PedScript,256)

// Declaration section
registersymbol(AOB_pedInj)
registersymbol(PedScript)
label(_Code)
label(_ExitPS)
label(_BackPS)

//=========================================
// Hacking Point

AOB_pedInj+9:
 jmp _Code
_BackPS:

//=========================================
// Var Section

PedScript:
 dd 0  // Item ID to pick up

//=========================================
// Code Section
_Code:
 mov eax,[PedScript]
 cmp eax,0
 jz short _ExitPS

 mov [esi+2C],eax
 xor eax,eax
 mov [PedScript],eax

_ExitPS:
 mov eax,[esi+2C]
 test eax,eax
 jmp _BackPS


