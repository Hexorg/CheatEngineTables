aobscanmodule(battlestats_team_aob,Conception2.exe,66 0F 6E 93 CC 05 00 00)
registersymbol(battlestats_team_aob)
alloc(battlestats_team_newmem,1024,Conception2.exe)
label(battlestats_team_enemy)
label(battlestats_team_originalcode)
label(battlestats_team_return)

battlestats_team_newmem:
  push eax
  push ecx
  cmp word ptr [ebx+62E],6400 //Enemy Check
  jne battlestats_team_enemy
  cmp [battlestats_set_health],#0
  je @f
  mov eax,[ebx+80+114]
  mov [ebx+80+11C],eax //Team Member 1 Health
  mov eax,[ebx+80+1C0+114]
  mov [ebx+80+1C0+11C],eax //Team Member 2 Health
  mov eax,[ebx+80+380+114]
  mov [ebx+80+380+11C],eax //Team Member 3 Health
  mov eax,[ebx+5C4]
  mov [ebx+5CC],eax //Team Total Health
@@:
  cmp [battlestats_set_mana],#0
  je @f
  mov eax,[ebx+80+118]
  mov [ebx+80+120],eax //Team Member 1 Mana
  mov eax,[ebx+80+1C0+118]
  mov [ebx+80+1C0+120],eax //Team Member 2 Mana
  mov eax,[ebx+80+380+118]
  mov [ebx+80+380+120],eax //Team Member 3 Mana
  mov eax,[ebx+5C8]
  mov [ebx+5D0],eax //Team Total Mana
@@:
  jmp battlestats_team_originalcode
battlestats_team_enemy:
  cmp [battlestats_set_enemy],#0
  je battlestats_team_originalcode
  xor ecx,ecx
  cmp [ebx+80+11C],#1
  jbe @f //Skip if already low health
  mov [ebx+80+11C],#1 //Team Member 1 Health
@@:
  add ecx,[ebx+80+11C]
  cmp [ebx+80+1C0+11C],#1
  jbe @f //Skip if already low health
  mov [ebx+80+1C0+11C],#1 //Team Member 2 Health
@@:
  add ecx,[ebx+80+1C0+11C]
  cmp [ebx+80+380+11C],#1
  jbe @f //Skip if already low health
  mov [ebx+80+380+11C],#1 //Team Member 3 Health
@@:
  add ecx,[ebx+80+380+11C]
  mov [ebx+5CC],ecx
battlestats_team_originalcode:
  pop ecx
  pop eax
  movd xmm2,[ebx+000005CC]
  jmp battlestats_team_return

battlestats_team_aob:
  jmp battlestats_team_newmem
  nop
  nop
  nop
battlestats_team_return:

