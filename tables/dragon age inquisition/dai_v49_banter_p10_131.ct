      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>// Tree Node Struct
// +00: pointer to left child (8 byte)
// +08: pointer to right child (8 byte)
// +10: weird pointer to an address with bad alignment (+1 byte from regular alignment)
// +18: some kind of id value (8 byte) (determines which stat this value represents?)
// +20: some kind of id value (8 bute) (continuation of above - the two together might be a GUID?)
// +28: value (8 byte)

[ENABLE]
alloc(memory,20000) // there can be *a lot* of pointers to store... (I saw mem usage ~4KB, so this is for safety)
LuaCall(AOBExecScan("onSetInquisition", "48 8B B9 18 01 00 00 4C 8D B1 08 01 00 00 48 89 D6 48 89 CD 4C 89 F3"))
label(code)
label(return)
label(process)
label(record)
label(skip)
label(done)
label(exit)

label(pInquisitionLeafOnly)
registersymbol(pInquisitionLeafOnly)

label(skipPerks)
label(pInquisitionPerks)
registersymbol(pInquisitionPerks)

label(skipPower)
label(pInquisitionPower)
registersymbol(pInquisitionPower)

label(skipInfluence)
label(pInquisitionInfluence)
registersymbol(pInquisitionInfluence)

label(skipGold)
label(pInquisitionGold)
registersymbol(pInquisitionGold)

label(skipShards)
label(pInquisitionShards)
registersymbol(pInquisitionShards)

label(skipCarryCapacity)
label(pInquisitionCarryCapacity)
registersymbol(pInquisitionCarryCapacity)

label(skipCourtInfluence)
label(pInquisitionCourtInfluence)
registersymbol(pInquisitionCourtInfluence)

label(pInquisitionBase)
registersymbol(pInquisitionBase)

onSetInquisition:
  jmp code // 14
  db 90 90 90 90 90 90 90 90 90 // 9
return:

memory:
code:
  // run original code
  db 48 8B B9 18 01 00 00 4C 8D B1 08 01 00 00 48 89 D6 48 89 CD 4C 89 F3

  // store base pointer
  add rbp,118
  mov [pInquisitionBase],rbp
  sub rbp,118

  // save values
  push rax // temporary value
  push rbx // processing stack size
  push rcx // next free location
  push rdx // processing stack of nodes to examine

  // initialize vars
  mov rax,0
  mov rbx,0
  mov rcx,pInquisitionBase
  add rcx,8 // first address is reserved for pInquisitionBase
  mov rdx,[pInquisitionBase] // root tree node
  mov rdx,[rdx] // root tree node only has one child, so start there
  push rdx // push child (root tree node single child)
  inc rbx // increment processing stack size

  mov rdx,[pInquisitionBase]
  mov rdx,[rdx+20]
  push rdx
  inc rbx
  mov rdx,[pInquisitionBase]
  mov rdx,[rdx+28]
  push rdx
  inc rbx
  mov rdx,[pInquisitionBase]
  mov rdx,[rdx+30]
  push rdx
  inc rbx

process:
  // if no more nodes on the processing stack, we're done
  cmp rbx,0
  je done
  // if the processing stack is too big, stop early
  cmp rbx,10000
  je done
  // pop a node from the processing stack and process it
  pop rdx
  dec rbx
  // if this is not a valid node, skip it
  cmp rdx,0
  je skip
  // prepare to push children unto the processing stack
  mov rax,rdx
  // push the left child unto the processing stack
  mov rdx,[rax+0]
  push rdx
  inc rbx
  // push the right child unto the processing stack
  mov rdx,[rax+8]
  push rdx
  inc rbx

  // check if this node is one of the important ones
  // pInquisitionPerks : 12 CC 1F 98 | 6F 63 5A 4C | AB 38 DF C6 | CB 39 03 93 : 981FCC12 4C5A636F C6DF38AB 930339CB
  mov edx,[rax+18]
  cmp edx,981FCC12
  jne skipPerks
  mov edx,[rax+1C]
  cmp edx,4C5A636F
  jne skipPerks
  mov [pInquisitionPerks],rax
skipPerks:
  // pInquisitionPower : DA F3 9D C1 | 6F 51 B2 45 | 82 74 9F E6 | 33 92 03 A8 : C19DF3DA 45B2516F
  mov edx,[rax+18]
  cmp edx,C19DF3DA
  jne skipPower
  mov edx,[rax+1C]
  cmp edx,45B2516F
  jne skipPower
  mov [pInquisitionPower],rax
skipPower:
  // pInquisitionInfluence : 74 40 41 E3 | E2 D6 61 4F | 81 AC 93 D8 | 46 AE 03 85 : E3414074 4F61D6E2
  mov edx,[rax+18]
  cmp edx,E3414074
  jne skipInfluence
  mov edx,[rax+1C]
  cmp edx,4F61D6E2
  jne skipInfluence
  mov [pInquisitionInfluence],rax
skipInfluence:
  // pInquisitionGold : 54 CF A2 69 | FA EE 88 46 | 9D ED A7 94 | CB C5 03 4F : 69A2CF54 4688EEFA
  mov edx,[rax+18]
  cmp edx,69A2CF54
  jne skipGold
  mov edx,[rax+1C]
  cmp edx,4688EEFA
  jne skipGold
  mov [pInquisitionGold],rax
skipGold:
  // pInquisitionShards : AB 1E 9A 19 | EA 75 6F 4F | 9C C7 A7 70 | 80 C0 03 A3 : 199A1EAB 4F6F75EA
  mov edx,[rax+18]
  cmp edx,199A1EAB
  jne skipShards
  mov edx,[rax+1C]
  cmp edx,4F6F75EA
  jne skipShards
  mov [pInquisitionShards],rax
skipShards:
  // pInquisitionCarryCapacity : C3 94 7C F6 | CB 44 EA 4E | B4 2B EA B3 | F2 83 03 59 : F67C94C3 4EEA44CB
  mov edx,[rax+18]
  cmp edx,F67C94C3
  jne skipCarryCapacity
  mov edx,[rax+1C]
  cmp edx,4EEA44CB
  jne skipCarryCapacity
  mov [pInquisitionCarryCapacity],rax
skipCarryCapacity:
  // pInquisitionCourtInfluence : 13 C5 1B 4A | B2 5C FF BC : 4A1BC513 BCFF5CB2
  mov edx,[rax+18]
  cmp edx,4A1BC513
  jne skipCourtInfluence
  mov edx,[rax+1C]
  cmp edx,BCFF5CB2
  jne skipCourtInfluence
  mov [pInquisitionCourtInfluence],rax
skipCourtInfluence:

  // if pInquisitionLeafOnly is disabled, always record node
  mov rdx,[pInquisitionLeafOnly]
  cmp rdx,0
  je record
  // otherwise, only record the node if it's a leaf node
  mov rdx,[rax+0]
  cmp rdx,0
  jne skip
  mov rdx,[rax+8]
  cmp rdx,0
  jne skip
record:
  // record the node
  mov [rcx],rax
  // increment the location to store next found address
  add rcx,8
skip:
  // process the next node
  jmp process

done:
  // if something is still on the processing stack, get rid of it
  cmp rbx,0
  je exit
  pop rdx
  dec rbx
  jmp done

exit:
  // restore values
  pop rax
  pop rbx
  pop rcx
  pop rdx

  // return to game code
  jmp return

pInquisitionLeafOnly:
  db 00 00 00 00 00 00 00 00

pInquisitionPerks:
  db 00 00 00 00 00 00 00 00

pInquisitionPower:
  db 00 00 00 00 00 00 00 00

pInquisitionInfluence:
  db 00 00 00 00 00 00 00 00

pInquisitionGold:
  db 00 00 00 00 00 00 00 00

pInquisitionShards:
  db 00 00 00 00 00 00 00 00

pInquisitionCarryCapacity:
  db 00 00 00 00 00 00 00 00

pInquisitionCourtInfluence:
  db 00 00 00 00 00 00 00 00

pInquisitionBase:
  db 00 00 00 00 00 00 00 00 // it'll all be nulled out anyway

[DISABLE]
onSetInquisition:
  db 48 8B B9 18 01 00 00 4C 8D B1 08 01 00 00 48 89 D6 48 89 CD 4C 89 F3 // 23

unregistersymbol(onSetInquisition)
unregistersymbol(pInquisitionBase)
unregistersymbol(pInquisitionNonLeaf)
