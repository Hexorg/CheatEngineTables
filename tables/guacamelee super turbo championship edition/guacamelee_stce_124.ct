-- check version/hash of process
function VersionCheck(value)
  file = enumModules()[1].PathToFile
  return (md5file(file) == value)
end
if (VersionCheck("8e31520186bc1eb7406e862830ebbc46") == false) then
  showMessage("Version does not match!")
  error()
end
-- set record onactivate/ondeactivate
local activate = function(memoryrecord, before, currentstate)
  memoryrecord.Value = 1
  return true
end
local deactivate = function(memoryrecord, before, currentstate)
  memoryrecord.Value = 0
  return true
end
local ids = {57, 58, 59, 60, 61, 62, 63, 64, 65, 66}
for i = 1, #ids do
  mr = getAddressList().getMemoryRecordByID(ids[i])
  mr.OnActivate = activate
  mr.OnDeactivate = deactivate
end
