      <ID>68894</ID>
      <Description>"Armament Power Base Address Scan Initialization (Activate This First)"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>800000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat

registersymbol(armamentpower_basepointer)
registersymbol(armamentpowerupdate_enable)
registersymbol(armamentpowerset_enable)

alloc(armamentpower_basepointer,4)
alloc(armamentpowerupdate_enable,4)
alloc(armamentpowerset_enable,4)


[DISABLE]

unregistersymbol(armamentpower_basepointer)
unregistersymbol(armamentpowerupdate_enable)
unregistersymbol(armamentpowerset_enable)

dealloc(armamentpower_basepointer)
dealloc(armamentpowerupdate_enable)
dealloc(armamentpowerset_enable)

</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>68895</ID>
          <Description>"Armament Power Base Address Scan (To Update: Use Armament)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat
label(armamentpower_savedAOB)
label(armamentpower_returnhere)
label(armamentpower_originalcode)
label(armamentpower_exit)

label(armamentpowerupdate_exit)
label(armamentpowerset_exit)

registersymbol(armamentpower_savedAOB)

aobscan(armamentpower_searchedAOB,8B 48 20 89 4D F8 8B 4D F0)

alloc(armamentpower_newmem,2048) //2kb should be enough
alloc(armamentpower_temp,4)


armamentpower_newmem:
mov [armamentpower_temp], ebx

cmp dword ptr [armamentpowerupdate_enable], #0
je armamentpowerupdate_exit
mov [armamentpower_basepointer], ecx
armamentpowerupdate_exit:

cmp dword ptr [armamentpowerset_enable], #0
je armamentpowerset_exit
mov [ecx+20], #300
armamentpowerset_exit:

mov ebx, [armamentpower_temp]


armamentpower_originalcode:
db 8B 48 20 89 4D F8
//mov ecx,[eax+20]
//mov [ebp-08],ecx


armamentpower_exit:
jmp armamentpower_returnhere


armamentpower_searchedAOB:
armamentpower_savedAOB:
jmp armamentpower_newmem
nop
armamentpower_returnhere:


[DISABLE]
armamentpower_savedAOB:
db 8B 48 20 89 4D F8
//mov ecx,[eax+20]
//mov [ebp-08],ecx

unregistersymbol(armamentpower_savedAOB)

dealloc(armamentpower_newmem)

