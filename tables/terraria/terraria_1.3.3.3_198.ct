jne legendary_ret
mov ebx,51
jmp legendary_ret

godly_mem:
cmp [flag],0
jne godly_ret
add [flag],1
mov eax,b
jmp godly_ret

unreal_mem:
cmp [flag],0
jne @f
mov eax,23
mov [flag],1
@@: //if random re-rolled unreal, even though flag was set
cmp eax,23
jne unreal_ret
mov ebx,52
jmp unreal_ret

