  <LuaScript>-- script starts here
slotMax = 5

--processName = 'MetroLL.exe'
--processName = 'xrEngine.exe'
processName = 'DX9_in_C#.exe'


function num2hex(num) return string.format('%X',num) end
function smrtDestroy(o) if o and o.destroy then o.destroy() end end

function tryLock(o) local t=true; t,o.locked = o.locked,true return not t end
function lock(o) while tryLock(o)==false do sleep(1) end end
function unlock(o) o.locked = false end

function getDirect(structure,a) local T={}
for k,v in pairs(structure) do T[k]=num2hex(a+v) end return T
end

function createDirectTables()
  textContainerDirect = {}
  sprDirect = {}

  for i=1, slotMax do
   textContainerDirect[i]= getDirect(textContainerStruct, commandList+0x28*(i*3-3))
   sprDirect[i]={}
   sprDirect[i].StalkerSlider   = getDirect(SpriteStruct, commandList+0x28*(i*3-2))
   sprDirect[i].AnimatedSprite  = getDirect(SpriteStruct, commandList+0x28*(i*3-1))
  end
end

function SaveInfoAndCreateDirectTables()
  commandList = allocateSharedMemory('CED3D_'..getOpenedProcessID())+0x47C
  createDirectTables()

  -- save some info, for new CE instance
  fullAccess(getAddress(processName..'+600'),64)
  writeString(processName..'+600','hooked')
  writeQword( processName..'+610',commandList)
end

function RestoreInfoAndCreateDirectTables()
  commandList = readQword(processName..'+610')
  createDirectTables()
end

function createTextureFromAttachedFile(h,name)
 local pic = createPicture(); pic.loadFromStream( findTableFile(name).Stream )
 local texture = h.createTexture(pic); pic.destroy()
 return texture
end

function createRenderObjectsAndEtc(thread)
  thread.name='"createRenderObjectsAndEtc"'

  -- wait for right moment, some games will give hook.Width==0 and hook.Height==0
  -- and..., some games start a small window first
  local W = hook.Width
  local H = hook.Height
  while (W&lt;400) or (H&lt;300) do sleep(100); W = hook.Width; H = hook.Height end


  reinitializeSymbolhandler()
  autoAssemble(AAScript_fixAlphablendForTextContainers) -- fix for D3D9 32bit (Alphablend for TextContainer)


  local Spr = {}
  local TextContainer = {}

  local font = createFont()
        font.Name = 'MS Sans Serif'
        font.Height = -16
        font.Color = 0x00FFFFFF -- white
      --font.Color = 0x00FF0000 -- blue

  local TxStalkerSlider  = createTextureFromAttachedFile(hook, 'StalkerSlider' )
  local TxAnimatedSprite = createTextureFromAttachedFile(hook, 'AnimatedSprite' )
  local TxFontMap        = hook.createFontmap(font)


  hook.beginUpdate()
  do

    for i=1, slotMax do
     TextContainer[i] = hook.createTextContainer(TxFontMap,0,0, string.rep (" ", 255) ) -- will preserve 255 bytes
     TextContainer[i].Text = ' '

     Spr[i] = {}
     Spr[i].StalkerSlider = hook.createSprite( TxStalkerSlider )
     Spr[i].AnimatedSprite = hook.createSprite( TxAnimatedSprite )
    end


    -- X=-1 will center in viewport
    -- X=-2 will move to mouse cursor

    -- center it
    local X = (W - 300) / 2
    for i=1, slotMax do
     TextContainer[i].X = X + 40
     TextContainer[i].Visible = false -- "Visible = false" must be at the end

     Spr[i].StalkerSlider.X = X
     Spr[i].StalkerSlider.Width = 300
     Spr[i].StalkerSlider.Visible = false

     Spr[i].AnimatedSprite.X = X - 20 + 12
     Spr[i].AnimatedSprite.CenterX = 12
     Spr[i].AnimatedSprite.CenterY = 12
     Spr[i].AnimatedSprite.Visible = false
    end

  end
  hook.endUpdate()

  SaveInfoAndCreateDirectTables()

  isHooked = true
end



function initializeD3DHook()
  if isHooked then return true end                    -- if already hooked, just quit

  local  status = readString(processName..'+600',6)
  if status == nil then return false end              -- process doesn't exist

  if status == 'hooked' then                          -- already hooked (previous CE instance)
    RestoreInfoAndCreateDirectTables()
    isHooked = true
    return true
  end

  -- things below should be executed only once for target process
  hook = createD3DHook()
  if (hook==nil) then
    return false                   --I can't see shit, captain
  else
    createNativeThread(createRenderObjectsAndEtc)
  end

  return true
end


-- Command is enum type
-- rcEndOfCommandlist=0,  // Stop going through the list
-- rcIgnored=1,           // Ignore (Being updated)
-- rcDrawSprite=2,		  // Render the sprite at the given position
-- rcDrawFont=3

-- size 0x28
SpriteStruct = {Visible = 0x0,                      -- 1 not visible, 2 visible ( COMMAND )
                X = 0x4, Y = 0x8, Alphablend = 0xC, -- float
                CenterX  = 0x10, CenterY = 0x14,    -- float
                Rotation = 0x18,                    -- radians (Lua class property uses degrees)
                Width = 0x1C, Height = 0x20,        -- integer
                TextureId = 0x24                    -- integer
               }

-- size 0x28
textContainerStruct = {Visible = 0x0,                      -- 1 not visible, 3 visible ( COMMAND )
                       X = 0x4, Y = 0x8, Alphablend = 0xC, -- float
                       CenterX  = 0x10, CenterY = 0x14,    -- NOT USED
                       Rotation = 0x18,                    -- NOT USED
                       TextPointer = 0x1C,                 -- pointer to text (UINT64)
                       FontId = 0x24                       -- INTEGER
                      }

slotTable = {}

animationThreadCriticalSection = {}
animationThreadCriticalSection.locked = false

function animationThread(thread)
  thread.name='"animationThread"'

  local slotNumber = 0

  repeat -- wait for free slot
   lock(animationThreadCriticalSection)
    for i = 1,slotMax do
      if slotTable[i]~=1 then slotTable[i]=1; slotNumber=i; break; end
    end
   unlock(animationThreadCriticalSection)
   sleep(1) -- prevent 50%-90% cpu usage
  until slotNumber~=0

  local TextContainer = textContainerDirect[slotNumber]
  local StalkerSlider = sprDirect[slotNumber].StalkerSlider
  local AnimatedSprite = sprDirect[slotNumber].AnimatedSprite

  writeFloat(TextContainer.Y, -80)
  writeFloat(StalkerSlider.Y, -80)
  writeFloat(AnimatedSprite.Y, -80)

  local text = 'animationThread thread, slot '..slotNumber

  writeInteger('['..TextContainer.TextPointer..']+'..num2hex(#text),0)
  writeString('['..TextContainer.TextPointer..']', text)

  local endY = 50.5 + (slotNumber-1) * 70

  writeInteger(TextContainer.Visible  , 3) -- 1 not visible, 3 visible ( COMMAND ) (textCont)
  writeInteger(StalkerSlider.Visible  , 2) -- 1 not visible, 2 visible ( COMMAND ) (sprite)
  writeInteger(AnimatedSprite.Visible , 2)

  writeFloat(TextContainer.Alphablend  , 1)
  writeFloat(StalkerSlider.Alphablend  , 1)
  writeFloat(AnimatedSprite.Alphablend , 1)

  local A,V,X = 0,0,0

  local getTickCount = getTickCount
  local writeFloat = writeFloat
  local readFloat = readFloat

  local StartTime = getTickCount()
  local TimeElapsed
  local prevTickCount, adaptableT, currentTickCount = 0,0,0

  X = endY - ( readFloat(StalkerSlider.Y) or 0 )
  while isHooked do

   currentTickCount = getTickCount()
   TimeElapsed = currentTickCount - StartTime

   adaptableT = (currentTickCount - prevTickCount) / 500 -- / 1000
   adaptableT = adaptableT &lt; 0.020 and adaptableT or 0.020

   prevTickCount = currentTickCount

   ---[[--
   A = -100*X-3*V
   V = V + A * adaptableT
   X = X + V * adaptableT
   --]]--

   writeFloat(TextContainer.Y,  endY - X + 22)
   writeFloat(StalkerSlider.Y,  endY - X)
   writeFloat(AnimatedSprite.Y, endY - X + 32)

   local newRot = ((readFloat(AnimatedSprite.Rotation) or 0) + 0.02) % 6.283
   writeFloat( AnimatedSprite.Rotation , newRot )

   if TimeElapsed &gt; 7000 then
     break;  -- end "animationThread"
   end

   if TimeElapsed &gt; 6000 then
     local alph = 1.0 - ( TimeElapsed - 6000 ) / 1000
     writeFloat( TextContainer.Alphablend  , alph)
     writeFloat( StalkerSlider.Alphablend  , alph)
     writeFloat( AnimatedSprite.Alphablend , alph)
   end

   sleep(1) -- wait
  end

  writeInteger(TextContainer.Visible  , 1) -- 1 not visible, 3 visible ( COMMAND ) (textCont)
  writeInteger(StalkerSlider.Visible  , 1) -- 1 not visible, 2 visible ( COMMAND ) (sprite)
  writeInteger(AnimatedSprite.Visible , 1)

  -- free slot
  lock(animationThreadCriticalSection)
    slotTable[slotNumber]=0
  unlock(animationThreadCriticalSection)
end


function showInfo(o)
  if isHooked then
    createNativeThread(animationThread)
  end
end

smrtDestroy(hk1); hk1=nil
hk1 = createHotkey(showInfo, VK_F4) --, VK_LSHIFT, VK_LCONTROL)

smrtDestroy(OpenProcessTimer); OpenProcessTimer=nil
OpenProcessTimer = createTimer(MyForm)
OpenProcessTimer.Interval = 1000
OpenProcessTimer.onTimer =
  function (timer)
    local processNotExist = readInteger(processName)==nil
    if processNotExist then
      isHooked=false
      if openProcess(processName) then
        initializeD3DHook()
      end
    else

    end
  end

function addDebugMsg()
  if not message then return end

  if type(message)=='string' then message = message:gsub('\n','\r\n') end

  if not showforthefirst then  UDF1.show();  showforthefirst=true end
  UDF1.CEMemo1.Align = alClient
  UDF1.CEMemo1.append( message )
end




AAScript_fixAlphablendForTextContainers = [[
alloc(newmem,64)
label(returnhere)

newmem:
// free to use registers: XMM0, EDX
mov edx,FF
cvtsi2ss xmm0,edx

mov edx,[edi]     // address of RenderCommandsList
add edx,[esp+D8]  // [ESP+D8] keeps offset to Alpha for current RenderCommand (luckily)

mulss xmm0,[edx]   // muliply 255 by Alpha
cvttss2si edx,xmm0 // convert to int

shl edx,18
or edx,00FFFFFF
push edx

//push FF //orig
push 00
push 00

jmp returnhere

CED3D9Hook.dll+1653:
jmp newmem
nop
returnhere:
]]
</LuaScript>
