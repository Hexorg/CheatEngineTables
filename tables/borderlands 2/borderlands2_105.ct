                  <AssemblerScript>[ENABLE]
aobscan(FixSlotsSignature,8B 7D D8 8B 3F 8B C8 03 C9 F3 0F 10 54 CF 04 F3 0F 58 D0)
alloc(FixSlots,1024)
alloc(Data,40)
alloc(CountArray,1024)
label(Exit)
label(Return)
label(FixSlotsInjectionLocation)
label(FixSlotsCounter)
label(FixSlotsValue)
label(FixVendValue)
label(FixValue)
registersymbol(CountArray)
registersymbol(FixSlotsInjectionLocation)
registersymbol(FixSlotsCounter)
registersymbol(FixSlotsValue)
registersymbol(FixVendValue)

label(Counter)
label(Loop_Begin)
label(Loop_Next)
label(Loop_Exit)
label(Inner_NotEmpty)
label(Inner_IncValue)
label(Next1)

Data:
FixSlotsValue:
db 0C 30 00 00
FixSlotsCounter:
db 00 00 00 00
FixVendValue:
db 00 7d 00 00

FixSlots:
cmp [esi],0x434347AE
jnz Next1
add [FixSlotsCounter],1
mov eax,[FixSlotsValue]

// Comment next line to enable result fixing attempts.
jmp Counter

Next1:
cmp [esi],0x43AF0000
jz FixValue
cmp [esi],0x449B0000
jz FixValue
cmp [esi],0x42FA0000
jz FixValue
cmp [esi],0x42200000
jz FixValue
cmp [esi],0x447A0000
jz FixValue
cmp [esi],0x45250000
jz FixValue
cmp [esi],0x43160000
jz FixValue
cmp [esi],0x430C0000
jz FixValue
cmp [esi],0x43A28000
jz FixValue
cmp [esi],0x43E60000
jz FixValue
cmp [esi],0x43870000
jz FixValue
cmp [esi],0x44480000
jz FixValue
cmp [esi],0x42200000
jz FixValue
cmp [esi],0x43160000
jz FixValue
cmp [esi],0x444A8000
jz FixValue

jmp Counter
FixValue:
mov eax, [FixVendValue]
inc [FixSlotsCounter]

Counter:
pushad
cmp [esi], 0
je Exit // If [ESI] is empty, exit.
mov ebx, 0
mov ecx, [esi]
Loop_Begin:
mov edx, [CountArray+ebx*8]
cmp edx, 0
jne Inner_NotEmpty   // Empty Array Entry, Store Key and Inc Count.
// Empty Array Entry and we've not found the Key
mov [CountArray+ebx*8], ecx
Inner_IncValue:
inc [CountArray+ebx*8+4]
jmp Loop_Exit
Inner_NotEmpty: // Check whether key in array matches ours.
cmp ecx, edx
je Inner_IncValue
Loop_Next:
inc ebx
cmp ebx, 32  // How big we let it get.
jle Loop_Begin
Loop_Exit:

Exit:
popad
movss xmm1,[esi]
xorps xmm0,xmm0
jmp Return

FixSlotsSignature-63:
FixSlotsInjectionLocation:
jmp FixSlots
nop
nop
Return:

