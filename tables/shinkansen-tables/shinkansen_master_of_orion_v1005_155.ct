mov [Civilization_temp1], rbx
mov [Civilization_temp2], rcx

cmp dword ptr [Civilization_update_enable], #0
je Civilization_update_exit
mov [Civilization_basepointer], rsi
Civilization_update_exit:

cmp dword ptr [Civilization_set_enable], #0
je Civilization_set_exit
Civilization_set_exit:

mov rbx, [Civilization_temp1],
mov rcx, [Civilization_temp2],


Civilization_originalcode:
db 48 63 96 5C 01 00 00
//movsxd  rdx,dword ptr [rsi+0000015C]


Civilization_exit:
jmp Civilization_returnhere


Civilization_searchedAOB:
Civilization_savedAOB:
jmp Civilization_newmem
nop
nop
Civilization_returnhere:


[DISABLE]
Civilization_savedAOB:
db 48 63 96 5C 01 00 00
//movsxd  rdx,dword ptr [rsi+0000015C]

unregistersymbol(Civilization_savedAOB)

dealloc(Civilization_newmem)
dealloc(Civilization_temp1)
dealloc(Civilization_temp2)

