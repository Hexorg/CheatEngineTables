 mov ecx,[eax+00000284]            // Get pArmy
 test ecx,ecx
 jz _ExitMMT

 mov ecx,[ecx+4c]
 cmp ecx,[pPlayer]
 jne _ExitMMT

 cmp dword ptr [iEnableMMP],0
