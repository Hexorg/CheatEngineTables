      <ID>95783</ID>
      <Description>"Death Stats Base Address Scan Initialization (Activate This First)"</Description>
      <Options moHideChildren="1"/>
      <LastState/>
      <Color>800000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
registersymbol(deathstats_basepointer)
registersymbol(deathstats_update_enable)
registersymbol(deathstats_set_enable)
registersymbol(deathstats_idset_value)
registersymbol(deathstats_expset_enable)
registersymbol(deathstats_expset_value)
registersymbol(deathstats_creditsset_enable)
registersymbol(deathstats_creditsset_value)

alloc(deathstats_basepointer, 4)
alloc(deathstats_update_enable, 4)
alloc(deathstats_set_enable, 4)
alloc(deathstats_idset_value, 4)
alloc(deathstats_expset_enable, 4)
alloc(deathstats_expset_value, 4)
alloc(deathstats_creditsset_enable, 4)
alloc(deathstats_creditsset_value, 4)

deathstats_expset_value:
dd #1

deathstats_creditsset_value:
dd #1


[DISABLE]
unregistersymbol(deathstats_basepointer)
unregistersymbol(deathstats_update_enable)
unregistersymbol(deathstats_set_enable)
unregistersymbol(deathstats_idset_value)
unregistersymbol(deathstats_expset_enable)
unregistersymbol(deathstats_expset_value)
unregistersymbol(deathstats_creditsset_enable)
unregistersymbol(deathstats_creditsset_value)

dealloc(deathstats_basepointer)
dealloc(deathstats_update_enable)
dealloc(deathstats_set_enable)
dealloc(deathstats_idset_value)
dealloc(deathstats_expset_enable)
dealloc(deathstats_expset_value)
dealloc(deathstats_creditsset_enable)
dealloc(deathstats_creditsset_value)

</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>95784</ID>
          <Description>"Death Stats Base Address Scan (To Update: Kill Enemy)"</Description>
          <LastState/>
          <Color>800000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(deathstats_savedAOB)
label(deathstats_returnhere)
label(deathstats_originalcode)
label(deathstats_exit)

label(deathstats_update_exit)
label(deathstats_set_exit)
label(deathstats_idset_exit)
label(deathstats_expset_exit)
label(deathstats_creditsset_exit)

registersymbol(deathstats_savedAOB)

aobscanmodule(deathstats_searchedAOB,NeptuniaVII.exe,8B 82 B0 00 00 00) // should be unique

alloc(deathstats_newmem, 2048)

deathstats_newmem:
//Check for battle to not trigger in library
  cmp ax,#2
  jne deathstats_originalcode

  push ebx

  cmp dword ptr [deathstats_update_enable], #0
  je deathstats_update_exit
  mov [deathstats_basepointer], edx
deathstats_update_exit:

  cmp dword ptr [deathstats_set_enable], #0
  je deathstats_set_exit
  mov dword ptr [edx+c2], 0xFFFFFFFF
  mov dword ptr [edx+ca], 0xFFFF
deathstats_set_exit:

  cmp dword ptr [deathstats_idset_value], #0
  je deathstats_idset_exit
  mov ebx, [deathstats_idset_value]
  mov word ptr [edx+c6], bx
  mov word ptr [edx+c8], bx
deathstats_idset_exit:

  cmp dword ptr [deathstats_expset_enable], #0
  je deathstats_expset_exit
  mov ebx, [edx+b0]
  imul ebx, [deathstats_expset_value]
  mov [edx+b0], ebx
deathstats_expset_exit:

  cmp dword ptr [deathstats_creditsset_enable], #0
  je deathstats_creditsset_exit
  mov ebx, [edx+b8]
  imul ebx, [deathstats_creditsset_value]
  mov [edx+b8], ebx
deathstats_creditsset_exit:

  pop ebx

deathstats_originalcode:
  mov eax,[edx+000000B0]

deathstats_exit:
  jmp deathstats_returnhere


deathstats_searchedAOB:
deathstats_savedAOB:
jmp deathstats_newmem
nop
deathstats_returnhere:


[DISABLE]
deathstats_savedAOB:
  db 8B 82 B0 00 00 00

unregistersymbol(deathstats_savedAOB)

dealloc(deathstats_newmem)

</AssemblerScript>
