function labelParty_start()
  labelParty_update()
  timer_setEnabled(timer_labelParty, true)
end

function labelParty_updateHero(label, address)
  if (address ~= nil) then
    changeLabel(label, address)
  end
end

function labelParty_update()
  errorOnLookupFailure(false)
  labelParty_updateHero("pHero0", getAddress("[[[[[pHeroBase]+70]+18]+d8]+20]")) -- 70 18 d8 20
  labelParty_updateHero("pHero1", getAddress("[pHeroBase1]"))
  labelParty_updateHero("pHero2", getAddress("[pHeroBase2]"))
  labelParty_updateHero("pHero3", getAddress("[pHeroBase3]"))
  labelParty_updateHero("pHero4", getAddress("[pHeroBase4]"))
  errorOnLookupFailure(true)
end

function labelParty_stop()
