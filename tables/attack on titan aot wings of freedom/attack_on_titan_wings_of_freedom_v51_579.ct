  mov [rcx+184],4 //Set last slot to Bertholdt
  jmp long setTimer
@@:
  cmp byte ptr [rcx+184],A //Check if last slot is Ymir
  jne long @f
  mov [rcx+184],5 //Set last slot to Annie
