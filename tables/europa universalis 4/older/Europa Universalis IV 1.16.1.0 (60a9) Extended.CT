//========================================= new 1.12.0.0
// Find Status Address
// 1st = ppStructureAddress
// 2nd = ID
// EDI = Result if NZ = ON
_FindStatusAddress:
 push ebp
 mov ebp,esp
 push eax
 push ecx
 push esi

 xor edi,edi

 mov esi,[ebp+0c]
 mov ecx,[esi+04]
 sub ecx,[esi]
 shr ecx,3
 test ecx,ecx
 jz _ExitFSA

 mov eax,[ebp+08]
 mov esi,[esi]

_FindSA0:
 cmp eax,[esi]
 je _FindSA1
 add esi,8
 loop _FindSA0

 jmp _ExitFSA
//==
_FindSA1:
 mov edi,esi

_ExitFSA:
 test edi,edi

 pop esi
 pop ecx
 pop eax
 pop ebp
 ret 0008

