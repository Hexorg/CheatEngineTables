 js _ExitMPG

 imul ebx,ebx,000003e8
 mov [eax+00000a60],ebx

_ExitMPG:
 pop ebx

 mov ecx,[ecx+00000280]       // Original code (Fort Level)
 jmp _BackMPG                 // Back to main code

