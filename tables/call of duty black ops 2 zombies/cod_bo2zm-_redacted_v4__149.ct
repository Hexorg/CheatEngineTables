    <CheatEntry>
      <ID>39</ID>
      <Description>"Teleport"</Description>
      <LastState Activated="0"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
aobscanmodule(Teleport,t6zm.exe,F3 0F 58 40 30 F3)

alloc(Teleport_Mem,2048)

label(_Teleport)
label(save_coords)
label(load_coords)
label(undo_coords)
label(Bus_Depot)
label(Town)
label(Farm)
label(Back_Teleport)
label(Exit_Teleport)
label(xpos)
label(ypos)
label(zpos)
label(xpos_undo)
label(ypos_undo)
label(zpos_undo)
label(s_enable)
label(l_enable)
label(u_enable)
label(enable_SpawntoBusDepot)
label(enable_SpawntoTown)
label(enable_SpawntoFarm)

registersymbol(Teleport)
registersymbol(s_enable)
registersymbol(l_enable)
registersymbol(u_enable)
registersymbol(enable_SpawntoBusDepot)
registersymbol(enable_SpawntoTown)
registersymbol(enable_SpawntoFarm)

Teleport_Mem:
xpos:
dd 0
ypos:
dd 0
zpos:
dd 0
xpos_undo:
dd 0
ypos_undo:
dd 0
zpos_undo:
dd 0
s_enable:
dd 0
l_enable:
dd 0
u_enable:
dd 0
enable_SpawntoBusDepot:
dd 0
enable_SpawntoTown:
dd 0
enable_SpawntoFarm:
dd 0

_Teleport:
cmp [s_enable],1
je save_coords
cmp [l_enable],1
je load_coords
cmp [u_enable],1
je undo_coords
cmp [enable_SpawntoBusDepot],1
je Bus_Depot
cmp [enable_SpawntoTown],1
je Town
cmp [enable_SpawntoFarm],1
je Farm
jmp Back_Teleport

// Save Position
save_coords:
mov [s_enable],0
push edx
mov edx,[eax+28]
mov [xpos],edx
mov edx,[eax+2C]
mov [ypos],edx
mov edx,[eax+30]
mov [zpos],edx
pop edx
jmp Back_Teleport

// Load Position
load_coords:
mov [l_enable],0
push edx
mov edx,[eax+28]
mov [xpos_undo],edx
mov edx,[eax+2C]
mov [ypos_undo],edx
mov edx,[eax+30]
mov [zpos_undo],edx
pop edx

push edx
mov edx,[xpos]
mov [eax+28],edx
mov edx,[ypos]
mov [eax+2C],edx
mov edx,[zpos]
mov [eax+30],edx
pop edx
jmp Back_Teleport

// Undo Teleport
undo_coords:
mov [u_enable],0
push edx
mov edx,[xpos_undo]
mov [eax+28],edx
mov edx,[ypos_undo]
mov [eax+2C],edx
mov edx,[zpos_undo]
mov [eax+30],edx
pop edx
jmp Back_Teleport

// Spawn to Bus Depot
Bus_Depot:
mov [enable_SpawntoBusDepot],0
mov [eax+28],(float)-7113.944336
mov [eax+2C],(float)5270.697266
mov [eax+30],(float)-55.875
jmp Back_Teleport

// Spawn to Town
Town:
mov [enable_SpawntoTown],0
mov [eax+28],(float)1561.57019
mov [eax+2C],(float)213.9562988
mov [eax+30],(float)-61.875
jmp Back_Teleport

// Spawn to Farm
Farm:
mov [enable_SpawntoFarm],0
mov [eax+28],(float)8195.208008
mov [eax+2C],(float)-5281.258789
mov [eax+30],(float)48.125
jmp Back_Teleport

Back_Teleport:
addss xmm0,[eax+30] // Original Code
jmp Exit_Teleport

Teleport:
jmp _Teleport

Exit_Teleport:

[DISABLE]
dealloc(Teleport_Mem)
Teleport:
db F3 0F 58 40 30
unregistersymbol(Teleport)
unregistersymbol(s_enable)
unregistersymbol(l_enable)
unregistersymbol(u_enable)
unregistersymbol(enable_SpawntoBusDepot)
unregistersymbol(enable_SpawntoTown)
unregistersymbol(enable_SpawntoFarm)
</AssemblerScript>
