          <AssemblerScript>// This information was obtained by Hextator of the CE forums.
// http://forum.cheatengine.org/viewtopic.php?t=568669&amp;start=90

[ENABLE]

alloc(newmem_players, 2048)
label(return_players)
label(player1)
registersymbol(player1)
label(player2)
registersymbol(player2)
label(playerOffset1)
registersymbol(playerOffset1)
label(playerOffset2)
registersymbol(playerOffset2)
label(players)
registersymbol(players)
aobscan(players_aob, 8B 44 90 0C 8B 40 3C 3B 70 04 0F 83 38 1E 00 00 8B 4C B0 0C A1 ?? ?? ?? ?? 8B 40 3C 3B 70 04 0F 83 23 1E 00 00 8B 54 B0 0C 39 09 FF 15 ?? ?? ?? ?? 85 C0 0F 84 A4 00 00 00)

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~

newmem_players:

add eax, 0C

push ebx
mov ebx, eax
add ebx, [playerOffset1]
mov [player1], ebx
mov ebx, eax
add ebx, [playerOffset2]
mov [player2], ebx
pop ebx

mov eax,[eax+edx*4]
mov eax,[eax+3C]

jmp return_players

player1:
db 00 00 00 00

playerOffset1:
db 00 00 00 00

player2:
db 00 00 00 00

playerOffset2:
db 00 00 00 00

players_aob:
players:
jmp newmem_players
nop
nop
return_players:

//~~~~~~~~~~~~~~~~~~~~~~~~~~~~


[DISABLE]

dealloc(newmem_players)
players:
mov eax,[eax+edx*4+0C]
mov eax,[eax+3C]

</AssemblerScript>
          <CheatEntries>
            <CheatEntry>
              <ID>296</ID>
              <Description>"Make Player 1 Chase Player 2"</Description>
              <Color>000000</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
globalalloc(newmem_playerChase,4096)
CREATETHREAD(newmem_playerChase);
label(playerChase)
registersymbol(playerChase)
label(originalStackAddress)
label(invalidPlayer)
label(end_playerChase)
registersymbol(end_playerChase)

newmem_playerChase:

// Awkward 32/64 bit portability, round 1
mov [originalStackAddress], esp

playerChase:

mov eax, [player2]
mov eax, [eax]
cmp eax, 0
je invalidPlayer

mov ebx, [player1]
mov ebx, [ebx]
cmp ebx, 0
je invalidPlayer

// Mimic X pos
mov ecx, [eax+380]
mov [ebx+380], ecx
// Mimic Y pos
mov ecx, [eax+384]
mov [ebx+384], ecx

invalidPlayer:

push #1
call sleep
// Awkward 32/64 bit portability, round 2
mov esp, [originalStackAddress]

cmp [end_playerChase], 1
jne newmem_playerChase

ret

originalStackAddress:
dd 0

end_playerChase:
dd 0

[DISABLE]
end_playerChase:
dd 1

</AssemblerScript>
