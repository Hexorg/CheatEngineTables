MyCode:
//========================================= v2.2.0
// On main screen
_MonPlayer:
 push ebx
 push ecx
 push esi

 mov esi,edi
 test esi,esi
 jz _ExitMP

 mov [pPlayer],esi

 mov eax,[esi+04]
 test eax,eax
 jz _MonP01

 mov eax,[eax+04]
 test eax,eax
 jz _MonP01

 mov eax,[eax+0c]
 mov [pOwner],eax

 test eax,eax
 jz _MonP01

 cmp eax,[eax+000000e4]
 jne _MonP01

 cmp dword ptr [iEnableSR],0
 je _MonP00                        // Jump if feature is disabled

 xor ecx,ecx
 mov ebx,#70

 mov cl,[eax+000000f4]             // Diplomacy (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+000000f4],cl

 mov cl,[eax+000000f5]             // Martial (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+000000f5],cl

 mov cl,[eax+000000f6]             // Stewardship (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+000000f6],cl

 mov cl,[eax+000000f7]             // Intrigue (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+000000f7],cl

 mov cl,[eax+000000f8]             // Learning (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+000000f8],cl

//Other structure

 mov ebx,#1000
 mov ecx,[esi+00000080]            // Fertility (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000080],ecx

 mov ecx,[esi+00000084]            // Fertility (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000084],ecx

 mov ebx,#9900
 mov ecx,[esi+00000088]            // Health (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000088],ecx

 mov ebx,#100000
 mov ecx,[esi+00000090]            // Prestige
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000090],ecx

 mov ecx,[esi+00000094]            // Piety
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000094],ecx

// Other Structure

 mov ebx,[iDate]
 mov ecx,0006aef0
 sub ebx,ecx
 js _MonP00

 mov ecx,[eax+00000098]            // Age
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+00000098],ecx

_MonP00:
 cmp dword ptr [iEnableMDe],0
 je _MonP01                        // Jump if feature is disabled

 mov ecx,[eax+000000a0]            // pDinasty
 test ecx,ecx
 jz _MonP01

 mov ebx,[ecx+5c]
 test ebx,ebx
 js _MonP01

 mov [ecx+58],0
 mov [ecx+5c],0

 mov ebx,[ecx+64]
 test ebx,ebx
 js _MonP01

 mov [ecx+60],0
 mov [ecx+64],0

 mov ebx,[ecx+6c]
 test ebx,ebx
 js _MonP01

 mov [ecx+68],0
 mov [ecx+6c],0

 mov ebx,[ecx+74]
 test ebx,ebx
 js _MonP01

 mov [ecx+70],0
 mov [ecx+74],0

_MonP01:
 cmp dword ptr [iEnableMG],0
 je _ExitMP                        // Jump if feature is disabled

 mov eax,[esi+0000009c]
 test eax,eax
 jnz _ExitMP

 mov eax,0c800000
 cmp eax,[esi+00000098]
 jle _ExitMP

 mov [esi+00000098],eax

_ExitMP:
 pop esi
 pop ecx
 pop ebx

 lea eax,[edi+00000098]            // Original code (pMoney)
 jmp _BackMP                       // Back to main code

//========================================= v2.2.0
// On Selecting the Capital of a County
_MonCapital:
 mov eax,[esi+20]

 mov [pCapital],eax

 mov eax,[eax+34]
 mov [pProvince],eax

 cmp dword ptr [iEnableRR],0
 je _ExitMC                        // Jump if feature is disabled

 mov eax,[eax+000007e8]
 test eax,eax
 jz _ExitMC

 mov eax,[eax+0000020c]
 cmp eax,[pOwner]
 jne _ExitMC

 mov eax,[pCapital]
 mov [eax+68],0

 mov eax,[pProvince]
 mov [eax+00000358],0

_ExitMC:
 mov eax,[esi+20]                  // Original code (pCap)
 mov eax,[eax+14]                  // Original code (pCapInfo)
 jmp _BackMC                       // Back to main code

//========================================= v2.2.0
// On Main Screen
_MonDate:
 mov edi,[eax+00000248]            // Original code (Get Date)

 mov [iDate],edi

_ExitMD:
 jmp _BackMD                       // Back to main code

//========================================= v2.2.0
// When changing date
// Building and Upgrading in county's regions
// Building Port in Republic Counties
_MonUpgrade:
 push ebx
 push edi
 push esi

 cmp dword ptr [iEnableMU],0
 je _ExitMU                        // Jump if feature is disabled

 mov edi,esi

 mov ecx,[edi+28]                  // Get pCapital
 test ecx,ecx
 jz _ExitMU

 mov ecx,[edi+20]                  // Get pCounty
 test ecx,ecx
 jz _ExitMU

 mov ebx,[ebp+04]
 cmp ebx,UPNL
 je _MonU00

 cmp ebx,UPRB
 je _MonU00

 cmp ebx,UPRX
 jne _ExitMU

 mov ebx,[edi+28]                  // Get pCapital
 mov ecx,[ebx+1c]                  // Get pManager
 cmp ecx,[pOwner]
 je _MonU02

 cmp dword ptr [iEnableMUV],0
 je _ExitMU                        // Jump if feature is disabled

 mov ebx,[ebx+1c]                  // Get pManager
 mov ecx,[ebx+0000012b]            // Get pLieage
 cmp ecx,[pOwner]
 je _MonU02
 jmp _ExitMU
//==
_MonU00:
 mov ebx,[ecx+000007e8]            // Get pCountyName
 test ebx,ebx
 jz _ExitMU

 mov ecx,[ebx+0000020c]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonU02

 cmp dword ptr [iEnableMUV],0
 je _ExitMU                        // Jump if feature is disabled

 mov ebx,[ebx+00000174]
 test ebx,ebx
 jz _ExitMU

 mov ecx,[ebx+0000020c]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonU02
 jmp _ExitMU
//==
_MonU02:
 cmp ecx,[pOwner]
 jne _ExitMU

 mov ecx,[eax]                     // Get current date
 mov [edi+14],ecx                  // Update finishing Date
 mov ecx,[edi+18]
 imul ecx,ecx,000003e8
 mov [edi+1c],ecx

_ExitMU:
 pop esi
 pop edi
 pop ebx

 mov ecx,[eax]                     // Original code
 cmp ecx,[esi+14]                  // Original code
 jmp _BackMU                       // Back to main code

//========================================= v2.2.0
// When changing date (construction)
_MonBuilding:
 push ebx

 cmp dword ptr [iEnableMB],0
 je _ExitMB                        // Jump if feature is disabled

 mov ecx,[edi+20]                  // Get pCounty
 test ecx,ecx
 jz _MonB01

 mov ebx,[ecx+000007e8]            // Get pCountyName
 test ebx,ebx
 jz _MonB01

 mov ecx,[ebx+0000020c]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonB02

_MonB00:
 cmp dword ptr [iEnableMBV],0
 je _MonB01                        // Jump if feature is disabled

 mov ebx,[ebx+00000174]
 test ebx,ebx
 jz _MonB01

 mov ecx,[ebx+0000020c]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonB02

_MonB01:
 cmp dword ptr [iEnableMBV],0
 je _ExitMB                        // Jump if feature is disabled

 mov ecx,[edi+28]
 cmp ecx,[pOwner]
 jne _ExitMB

_MonB02:
 mov ecx,[eax]                     // Get current date
 mov [edi+14],ecx                  // Update finishing Date

_ExitMB:
 pop ebx

 mov ecx,[eax]                     // Original code
 cmp ecx,[edi+14]                  // Original code
 jmp _BackMB                       // Back to main code

//========================================= v2.2.0
// When changing date
_MonMovement:
 cmp dword ptr [iEnableMM],0
 je _ExitMM                        // Jump if feature is disabled

 mov eax,[ebx+00000128]            // Get pOwner
 cmp eax,[pOwner]
 jne _ExitMM

 mov eax,[esp+14]
 inc eax
 mov [ebx+00000088],eax

_ExitMM:
 mov eax,[ebx+00000088]            // Original code
 jmp _BackMM                       // Back to main code

//========================================= v2.2.0
// During combat
_GodMode:
 push esi

 mov esi,[ebp+08]
 test esi,esi
 jz _ExitGM

 lea esi,[esi-b4]
 mov esi,[esi+50]                  // Get pOwner
 cmp esi,[pOwner]
 jne _GodM00

 cmp dword ptr [iEnableGM],0
 je _ExitGM                        // Jump if feature is disabled

 mov ecx,[ebx+20]
 mov [ebx],ecx

 xor eax,eax                       // Value2decrease = 0
 jmp _ExitGM

_GodM00:
 cmp dword ptr [iEnableMD],0
 je _ExitGM                        // Jump if feature is disabled

 shl eax,4                         // Damage * 16
 cmp ecx,eax
 cmovl eax,ecx

_ExitGM:
 pop esi

 sub ecx,eax                       // Original code
 mov [ebx],ecx                     // Original code
 mov ecx,[ebp-14]                  // Original code
 jmp _BackGM                       // Back to main code

//========================================= v2.2.0
// When in combat
_MonMorale:
 push edi

 cmp dword ptr [iEnableMMo],0
 je _ExitMMo                       // Jump if feature is disabled

 mov edi,[eax+0c]                  // Get pOwner
 cmp edi,[pOwner]
 jne _ExitMMo

 mov edi,#1000
 mov [eax],edi

 xor ecx,ecx                       // value2decrease = 0

_ExitMMo:
 pop edi

 sub [eax],ecx                     // Original code
 cmp dword ptr [eax],00            // Original code
 jmp _BackMMo                      // Back to main code

//========================================= v2.2.0
// Selected County in Siege
// If a player's county, set value to 1 day
// If a Enemy's county, set value to maximum (Enable Assault)
_MonSiege:
 push ecx
 push edi
 push esi

 add eax,edx                       // Original code

 cmp dword ptr [iEnableMS],0
 je _ExitMS                        // Jump if feature is disabled

 mov edi,[ebx+10]                  // Get pCapital
 test edi,edi
 jz _ExitMS

 mov edi,[edi+14]                  // Get pInfo
 test edi,edi
 jz _ExitMS

 mov edi,[edi+0000020c]            // Get pOwner
 cmp edi,[pOwner]
 jne _MonS00

 mov dword ptr [ebx+14],1
 mov eax,#100
 jmp _ExitMS
// Enemy Province's Capital
_MonS00:
 mov esi,[ebx+10]                  // Get pCapital

 mov esi,[esi+34]                  // Get pCounty
 test esi,esi
 jz _ExitMS

 mov edi,[esi+000007a8]            // Get ppArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi]                     // Get p1stArmy
 test edi,edi
 jz _ExitMS

 mov ecx,[edi+00000128]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonS01

 mov edi,[esi+000007ac]            // Get ppArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi]                     // Get pLastArmy
 test edi,edi
 jz _ExitMS

 mov ecx,[edi+00000128]            // Get pOwner
 cmp ecx,[pOwner]
 jne _ExitMS

_MonS01:
 cmp ebx,[edi+64]                  // Sieged by the player?
 jne _ExitMS

 mov [ebx+14],eax                  // Enable Assault

_ExitMS:
 pop esi
 pop edi
 pop ecx

 cmp [ebx+14],eax                  // Original code
 jmp _BackMS                       // Back to main code

//========================================= v2.2.0
// Army During Siege
_GodModeS:
 push esi
 push edi
 push ecx

 mov [eax+000000D8],ecx            // Original code

 cmp dword ptr [iEnableGM],0
 je _ExitGMS                       // Jump if feature is disabled

 mov esi,[eax+50]                  // Get pOwner
 cmp esi,[pOwner]
 jne _ExitGMS

 pushfd
 cld

 lea esi,[eax+000000dc]
 lea edi,[eax+000000bc]
 mov ecx,8

 rep movsd

 popfd

 mov [eax+44],#1000                // Morale

_ExitGMS:
 pop ecx
 pop edi
 pop esi
 jmp _BackGMS                      // Back to main code

//========================================= v2.2.0
// Looking at the Technology Screen
// ESI = province or demesne part, EDI = Index
_MonTechLevel:
 push edx
 push ebx

 cmp dword ptr [iEnableMTL],0
 je _ExitMTL                       // Jump if feature is disabled

 mov ebx,[edi+000007e8]            // Get pCountyName
 test ebx,ebx
 jz _ExitMTL

 mov ebx,[ebx+0000020c]            // Get pOwner
 cmp ebx,[pOwner]
 jne _ExitMTL

 mov ecx,[edi+38]                  // pTechLevel
 test ecx,ecx
 jz _ExitMTL

 mov ebx,[iMinTech]
 cmp ebx,8
 jg _ExitMTL

 test ebx,ebx
 js _ExitMTL

 push eax
 mov eax,#1000
 imul ebx,eax
 pop eax

 cmp ebx,[ecx+eax*4]
 jle _ExitMTL

 mov [ecx+eax*4],ebx

_ExitMTL:
 pop ebx
 pop edx

 mov ecx,[edi+38]                  // Original code
 mov ecx,[ecx+eax*4]               // Original code
 jmp _BackMTL                      // Back to main code

//========================================= v2.2.0
// When a character detail is displayed (select the portrait)
_MonSelChar:
 mov ecx,[eax+58]

 mov [pSelPlayer],ecx
 mov [pSelOwner],eax

