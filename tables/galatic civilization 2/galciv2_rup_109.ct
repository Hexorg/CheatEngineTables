]]-- 
local GalCiv2_Table = { };
local game = "GalCiv2.exe";
GalCiv2_Table.bForm = false;
function IsGameLoaded(ge)
  local t = readInteger(ge);
  if (t == nil) then
    return false;
  end
  return true;
end
  
function AssertVersion()
	local sa1 = "GalCiv2.exe+07ebad"
    local ta1 = {0x8b,0xaf,0xf0,0x04,0x00,0x00};
	local sa2 = "galciv2.exe+18f0f2"
    local ta2 = {0x8b,0x96,0x04,0x05,0x00,0x00};
	local sa3 = "galciv2.exe+1ce76b"
    local ta3 = {0x89,0xb7,0x5c,0xd1,0x01,0x00};
	local sa4 = "galciv2.exe+167198"
    local ta4 = {0x29,0x86,0xf0,0x01,0x00,0x00};
	local sa5 = "galciv2.exe+165f6c"
    local ta5 = {0x89,0x86,0xe4,0x01,0x00,0x00};
	local sa6 = "galciv2.exe+217b24"
    local ta6 = {0x8b,0x46,0x60,0x8d,0x14,0x80};
	if (CheckArray(ta1,sa1) == false) then
	    return false; 
	end
	if (CheckArray(ta2,sa2) == false) then
	    return false; 
	end
	if (CheckArray(ta3,sa3) == false) then
	    return false; 
	end
	if (CheckArray(ta4,sa4) == false) then
	    return false; 
	end
	if (CheckArray(ta5,sa5) == false) then
	    return false; 
	end
	if (CheckArray(ta6,sa6) == false) then
	    return false; 
	end
    return true;	
end
function CheckArray(tb, gm)
    local sz,i,rm = 0,0,0;
    local address = " ";
   
    if(type(tb) ~= "table") then
        return false;
    end 
    sz = table.maxn(tb);
    if(sz == 0) then
        return false;
    end
    for i = 1, sz do
        address = gm.."+"..tostring(i-1);
        rm	= readBytes(address);
        if (tb[i] ~= rm ) then
            return false;
        end
    end
    return true;
end
function LoadScript()
  local te = getTableEntry("GalCiv2_V10_Script");
  if (te == nil) then
      return false;
  else
	  memrec_freeze(te);
	  return true;
  end
end
function GalCiv2_Table.Main()
    if(IsGameLoaded(game) == false) then
	    showMessage("Galactic Civilization is not loaded");
	    return false;
	end
    if(AssertVersion() == false) then
	    showMessage("Script not compliant with this version of the game");
	    return false;
	end;
	if(LoadScript() == true) then
	    showMessage("AA Script loaded successfully");
		GalCiv2_Table.bScriptEnable = true;
		return true;
	else
	    showMessage("AA Script was not found");
		return false;
	end
end
function InitButton( form, caption, x, y, w, h, func ) 
    local button = createButton( form ); 
    if( button == nil ) then 
        return nil; 
    end 
    control_setCaption( button, caption ); 
    control_setPosition( button, x, y ); 
    control_setSize( button, w, h ); 
    control_onClick( button, func ); 
    return button; 
end
function InitLabel( form, x, y, text ) 
    local label = createLabel( form ); 
    if( label == nil ) then 
        return nil; 
    end 
    
    control_setCaption( label, text ); 
    control_setPosition( label, x, y ); 
    return label; 
end 
function GalCiv2_Table.CreateForm()
    GalCiv2_Table.Form = createForm( true ); 
    form_centerScreen(GalCiv2_Table.Form);
	form_onClose(GalCiv2_Table.Form, GalCiv2_Table.OnFormClose);
-- Create buttons. 
    GalCiv2_Table.btnME = InitButton( GalCiv2_Table.Form, "Enable/Disable Unlimited Empire Treasure", 1, 30, 250, 30, GalCiv2_Table.OnMEClicked ); 
    GalCiv2_Table.btnMR = InitButton( GalCiv2_Table.Form, "Enable/Disable Research Tech in 1 Turn", 1, 65, 250, 30, GalCiv2_Table.OnMRClicked ); 
    GalCiv2_Table.btnMM = InitButton( GalCiv2_Table.Form, "Enable/Disable Unlimited MP for Sel Ship", 1, 100, 250, 30, GalCiv2_Table.OnMMClicked ); 
    GalCiv2_Table.btnGM = InitButton( GalCiv2_Table.Form, "Enable/Disable God Mode for Ship", 1, 135, 250, 30, GalCiv2_Table.OnGMClicked ); 
    GalCiv2_Table.btnMC = InitButton( GalCiv2_Table.Form, "Enable/Disable Colonists/Troops In Ship", 1, 170, 250, 30, GalCiv2_Table.OnMCClicked ); 
-- Create labels. 
   GalCiv2_Table.lbME = InitLabel( GalCiv2_Table.Form, 260, 38, "= ON "); 
   GalCiv2_Table.lbMR = InitLabel( GalCiv2_Table.Form, 260, 73, "= ON "); 
   GalCiv2_Table.lbMM = InitLabel( GalCiv2_Table.Form, 260, 108,"= ON "); 
   GalCiv2_Table.lbGM = InitLabel( GalCiv2_Table.Form, 260, 143,"= ON "); 
   GalCiv2_Table.lbMC = InitLabel( GalCiv2_Table.Form, 260, 178,"= ON "); 
-- Create info label. 
   GalCiv2_Table.lblInfo = InitLabel( GalCiv2_Table.Form, 5, 210,"AA and LUA scripts by Recifense (14-Jan-2011)"); 
   
-- Set form caption. 
    control_setCaption( GalCiv2_Table.Form, "Galatic Civilization 2 V1.0" ); 
    return true; 
end 
function GalCiv2_Table.OnMEClicked()
    local state   = readInteger("iEnableME");
	local caption = ""
	
	if (state == nil) then
	    caption = "= Error";
	elseif (state == 0)	then
	    writeInteger("iEnableME",1);
	    caption = "= ON";
	else
	    writeInteger("iEnableME",0);
	    caption = "= OFF";
	end
	
	control_setCaption(GalCiv2_Table.lbME,caption);  
    return true;    
end 
