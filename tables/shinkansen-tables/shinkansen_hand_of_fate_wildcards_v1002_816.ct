    local selectedAOB = stringlist_getString(resultsAOB, positionAOB)

    local script = 'label(' .. labelAOB .. ')\nregistersymbol(' .. labelAOB .. ')\n' .. selectedAOB .. ':\n' .. labelAOB .. ':'
    autoAssemble(script)

    object_destroy(resultsAOB)
    resultsAOB = nil
  end
end
