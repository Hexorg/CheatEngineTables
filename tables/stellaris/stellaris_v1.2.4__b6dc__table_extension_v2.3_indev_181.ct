_MonWormhole:
 push ebx

 cmp dword ptr [iEnableMFW],0
 je _ExitMWH                       // Jump if feature is disabled

 mov ebx,[ebp-04]
 test ebx,ebx
 jz _ExitMWH

 mov ebx,[ebx+04]
 test ebx,ebx
 jz _ExitMWH

 mov ebx,[ebx+000000a4]
 cmp ebx,[iOwnerID]
 jne _ExitMWH

 mov [esi+20],eax

_ExitMWH:
 pop ebx

 cmp eax,[esi+20]                  // Original code (chk prog)
 pop esi                           // Original code
 setle al                          // Original code
 jmp _BackMWH                      // Back to main code

//========================================= 124 new
// When constructing by ship constructor (ex.: wormhole)(%)
_MonBuildProg3:
 add [ebx+34],eax                  // Original code (upd % prog)

 cmp dword ptr [iEnableMBP],0
 je _ExitBP3                       // Jump if feature is disabled

 mov esi,[ebp+08]
 test esi,esi
 jz _ExitBP3

 mov eax,[esi+000000a4]
 cmp eax,[iOwnerID]
 jne _ExitBP3

 mov eax,#95000
 cmp eax,[ebx+34]
 js _ExitBP3

 mov [ebx+34],eax

_ExitBP3:
 pop edi                           // Original code
 pop esi                           // Original code
 jmp _BackBP3                      // Back to main code

//========================================= 124 new
// When terraforming a planet (days)
_MonPlanetTerraforming:
 cmp dword ptr [iEnableMTF],0
 je _ExitMTF                       // Jump if feature is disabled

 test eax,eax
 jz _ExitMTF

 mov eax,[eax+000000a4]
 cmp eax,[iOwnerID]
 jne _ExitMTF

 mov eax,[esi+0c]
 lea eax,[eax-0a]
 cmp eax,[esi+08]
 js _ExitMTF

 mov [esi+08],eax

_ExitMTF:
 inc [esi+08]                      // Original code (upd # days passed)
 mov eax,[esi+08]                  // Original code
 jmp _BackMTF                      // Back to main code

//========================================= 123 ok
// Check if it is a player's ship/Station (based on its fleet id)
// Input : EAX = FleetID
// Output: Z = found and NZ = Not Found
// Including renamed fleets (by TranscendPerfection)
_ChkPlayerFleet:
    push ebx
    push ecx

    mov ebx,[pPlayer]
    mov ecx,[ebx+000006a0]
    mov ebx,[ebx+0000069c]
    test ebx,ebx
    jz _ChkPF01

_ChkPF00:
    cmp ecx,ebx
    jle _ChkPF01

    cmp eax,[ebx]
    je _ChkPF04

    lea ebx,[ebx+04]
    jmp _ChkPF00
//--
_ChkPF01:
    mov ebx,[pPlayer]
    mov ecx,[ebx+00000690]
    mov ebx,[ebx+0000068c]
    test ebx,ebx
    jz _ChkPF03

_ChkPF02:
    cmp ecx,ebx
    jle _ChkPF03

    cmp eax,[ebx]
    je _ChkPF04

    lea ebx,[ebx+04]
    jmp _ChkPF02
//--
_ChkPF03:
    inc eax
    test eax,eax

_ChkPF04:
    pop ecx
    pop ebx
    ret

//========================================= 120
// Check if it is a player's troop (based on its id)
// Input : EAX = TroopID
// Output: Z = found and NZ = Not Found
_ChkPlayerTroop:
 push ebx
 push ecx

 mov ebx,[pPlayer]
 mov ecx,[ebx+000006b0]
 mov ebx,[ebx+000006ac]
 test ebx,ebx
 jz _ChkPT01

_ChkPT00:
 cmp ecx,ebx
 jle _ChkPF01

 cmp eax,[ebx]
 je _ChkPT02

 lea ebx,[ebx+04]
 jmp _ChkPT00
//--
_ChkPT01:
 inc eax
 test eax,eax

_ChkPT02:
 pop ecx
 pop ebx
 ret

