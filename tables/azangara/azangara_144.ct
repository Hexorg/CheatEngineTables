    <CheatEntry>
      <ID>22</ID>
      <Description>"Change cords with U, J, H, K keys"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat
alloc(newmem,2048) //2kb should be enough
label(returnhere)
label(originalcode)
label(exit)
globalalloc(cordmod,4)
label(down)
label(left)
label(right)


cordmod:
dd (float)0.1

newmem: //this is allocated memory, you have read,write,execute access
pushfd
pushad
push 'U'
call GetAsyncKeyState
shr ax,#15
cmp ax,1
popad
jne down
fld dword ptr [edi+00000128]
fadd dword ptr [cordmod]
fstp dword ptr [edi+00000128]


down:
pushad
push 'J'
call GetAsyncKeyState
shr ax,#15
cmp ax,1
popad
jne left
fld dword ptr [edi+00000128]
fsub dword ptr [cordmod]
fstp dword ptr [edi+00000128]

left:
pushad
push 'H'
call GetAsyncKeyState
shr ax,#15
cmp ax,1
popad
jne right
fld dword ptr [edi+00000124]
fsub dword ptr [cordmod]
fstp dword ptr [edi+00000124]

right:
pushad
push 'K'
call GetAsyncKeyState
shr ax,#15
cmp ax,1
popad
jne originalcode
fld dword ptr [edi+00000124]
fadd dword ptr [cordmod]
fstp dword ptr [edi+00000124]


originalcode:
popfd
fld dword ptr [edi+00000124]

exit:
jmp returnhere

"Azangara.exe"+5105E:
jmp newmem
nop
returnhere:


 
 
[DISABLE]
//code from here till the end of the code will be used to disable the cheat
dealloc(newmem)
"Azangara.exe"+5105E:
fld dword ptr [edi+00000124]
//Alt: db D9 87 24 01 00 00
</AssemblerScript>
    </CheatEntry>
