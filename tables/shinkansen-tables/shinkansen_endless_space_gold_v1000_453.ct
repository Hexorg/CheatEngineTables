registersymbol(Empire_update_enable)
registersymbol(Empire_set_enable)
registersymbol(Empire_researchset_enable)
registersymbol(Empire_dustset_enable)

alloc(Empire_basepointer, 4)
alloc(Empire_update_enable, 4)
alloc(Empire_set_enable, 4)
alloc(Empire_researchset_enable, 4)
alloc(Empire_dustset_enable, 4)


[DISABLE]
unregistersymbol(Empire_basepointer)
unregistersymbol(Empire_update_enable)
unregistersymbol(Empire_set_enable)
unregistersymbol(Empire_researchset_enable)
unregistersymbol(Empire_dustset_enable)

