<?xml version="1.0"?>
<CheatTable CheatEngineTableVersion="12">
  <Forms>
    <UDF1></UDF1>
  </Forms>
  <CheatEntries/>
  <UserdefinedSymbols/>
  <LuaScript>x, y = control_getSize( UDF1 )

form_show(UDF1)

function math.clamp( input, down, up )
if ( input &gt; up ) then input=up; end
if ( input &lt; down ) then input=down; end
return input;
end


function CETimer1Timer(sender)
xpos = math.clamp(math.random(1, x), 1, x - 50 )
ypos = math.clamp(math.random(1, y), 1, y - 15 )

control_setPosition( UDF1_CEImage1, xpos, ypos );
control_setPosition( UDF1_CEProgressbar1, xpos -20, ypos +33 );
end

UDF1_str = 50;
sc = 0;
rd = 1;
rdh = 50;
mon = 0;
hit = 2;
function CEImage1Click(sender)
mon = mon +math.random(5,10)
sca = math.random(5,20)
sc = sc +sca
UDF1_str = UDF1_str -hit
if ( UDF1_str &lt; 0 ) then
rd = rd +1
rdh = rdh +50;
control_setCaption( UDF1_CELabel2, " Round: "..rd )
UDF1_str = UDF1_str +rdh
mon = mon +100
else
progressbar_setPosition( UDF1_CEProgressbar1, UDF1_str )
control_setCaption( UDF1_CELabel3, "Enemy HP: "..UDF1_str)
control_setCaption( UDF1_CELabel1, "Score: "..sc );
control_setCaption( UDF1_CELabel4, "Money: "..mon)
control_setCaption( UDF1_CELabel5, "Damage: "..hit)
control_setCaption( UDF1_CELabel6, "Stun: "..stunr)
end
end

req = 500;
function CEButton1Click(sender)
if ( mon &lt; req ) then
showMessage( "Req Money "..req )
else
mon = mon -req
req = req +100
hit = hit +10
end
end

reqs = 200;
function CEButton2Click(sender)
if ( mon &lt; reqs ) then
showMessage( "Req Money "..reqs)
else
mon = mon -reqs
reqs = reqs +1
stunr = stunr +1
end
end

stunr = 0;
function stun()
if ( stunr &lt; 1 ) then
showMessage( "No Stun Item" )
else
stunr = stunr -1
timer_setEnabled( UDF1_CETimer1, false )
timer_setEnabled( UDF1_CETimer2, true )
end
end


function CETimer2Timer(sender)
timer_setEnabled( UDF1_CETimer1, true )
timer_setEnabled( UDF1_CETimer2, false )
end

createHotkey("stun", VK_F9 );
</LuaScript>
</CheatTable>
