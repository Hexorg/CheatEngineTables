//=========================================
// Declaration section
label(_MonHero)
label(_MonH00)
label(_BackMH)
label(_ExitMH)
label(_GodMode)
label(_GodM00)
label(_BackGM)
label(_ExitGM)
label(_MonMana)
label(_BackMM)
label(_ExitMM)
label(pHero)
label(iEnableGM)
label(iEnableMX)
label(iEnableMG)
label(iEnableMM)
label(iMinGold)

//=========================================
// Registering Symbols
registersymbol(MyCode)
registersymbol(pHero)
registersymbol(iEnableGM)
registersymbol(iEnableMX)
registersymbol(iEnableMG)
registersymbol(iEnableMM)
registersymbol(iMinGold)

//=========================================
MyCode:
//=========================================
_MonHero:
 push ecx
 mov [pHero],esi

 cmp dword ptr [iEnableMX],0
 je _MonH00                        // Jump if feature is disabled

 mov ecx,[esi+0000058c]
 cmp ecx,edi
 jge _MonH00

 lea ecx,[edi-1]
 mov [esi+0000058c],ecx            // Update XP

_MonH00:
 cmp dword ptr [iEnableMG],0
 je _ExitMH                        // Jump if feature is disabled

 mov ecx,[iMinGold]
 cmp ecx,[esi+00000588]
 jle _ExitMH

 mov [esi+00000588],ecx            // Update Gold

_ExitMH:
 pop ecx

 mov esi,[esi+00000110]            // Original code (rds level)
 jmp _BackMH                       // Back to main code

//=========================================
_GodMode:
 push eax

 cmp dword ptr [iEnableGM],0
 je _ExitGM                        // Jump if feature is disabled

 mov eax,[esi+00000704]
 test eax,eax
 jnz _GodM00

 mov eax,esi

_GodM00:
 cmp eax,[pHero]
 jne _ExitGM

 fst dword ptr [esi+00000558]      // HP = max

_ExitGM:
 pop eax

 fcomp dword ptr [esi+00000558]    // Original code (hp)
 jmp _BackGM                       // Back to main code

//=========================================
_MonMana:
 push eax

 fst dword ptr [esi+0000057C]      // Original code (upds mana)

 cmp esi,[pHero]
 jne _ExitMM

 cmp dword ptr [iEnableMM],0
 je _ExitMM                        // Jump if feature is disabled

 mov eax,[esp+08]
 mov [esi+0000057C],eax

_ExitMM:
 pop eax
 jmp _BackMM                       // Back to main code

//=========================================
 db '=======================================&gt;'
 db 'CE6.2 Script by Recifense 100612'
//=========================================
// Variables
iEnableGM:
 dd 0
iEnableMX:
 dd 0
iEnableMG:
 dd 0
iEnableMM:
 dd 0
pHero:
 dd MyCode
iMinGold:
 dd #62000

//=========================================
// Hacking Points
//LUDO+0038e9f7:
//LUDO+0038f2d7:
LUDO+00392cc7:
 jmp _MonHero
 nop
_BackMH:

//LUDO+0012a532:
//LUDO+00129e32:
LUDO+0012a4b2:
 jmp _GodMode
 nop
_BackGM:

//LUDO+001133ee:
//LUDO+00112c0e:
LUDO+00112dee:
