    <CheatEntry>
      <ID>102</ID>
      <Description>"Force Prefixes"</Description>
      <LastState Activated="0"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>define(force_prefix, Terraria.Item::Prefix+AE)
define(invalid_prefix, Terraria.Item::Prefix+2147) //because game will do stuff like assign unreal to no knockback weapons (like minishark), then repeat at random until it finds a valid prefix
define(invalid_prefix_ret, Terraria.Item::Prefix+214d)
define(legendary, Terraria.Item::Prefix+8ef)
define(legendary_ret, Terraria.Item::Prefix+8fa)
define(godly, Terraria.Item::Prefix+b83)
define(godly_ret, Terraria.Item::Prefix+b88)
define(unreal, Terraria.Item::Prefix+105d)
define(unreal_ret, Terraria.Item::Prefix+1068)
define(all_else, Terraria.Item::Prefix+1800)

[ENABLE]
assert(force_prefix, 75 02)
assert(invalid_prefix, 89 55 dc db 45 dc)  //this will avoid indefinite postponement, but will forfeit prefix selection as a result, 'cause the logic to hard-code every possible exception for optimal prefix is too much work.
assert(legendary, 0F 85 40 0F 00 00 bb 51 00 00 00)
assert(godly, 83 f8 0b 75 05)
assert(unreal, 0F 85 d2 07 00 00 BB)
assert(all_else, 6a 51 ba 3e)
alloc(mem, 2048)
label(flag)
label(invalid_prefix_mem)
label(legendary_mem)
label(godly_mem)
label(unreal_mem)

//------------------
//hack points
//------------------
force_prefix:
db eb

invalid_prefix:
jmp invalid_prefix_mem
nop

legendary:
jmp legendary_mem
nop

godly:
jmp godly_mem

unreal:
jmp unreal_mem
nop

all_else:
db 6a 41 ba 41  //41 = warding
//db 6a 48 ba 48  //48 = menacing

//------------------
//hack scripts
//------------------
mem:
flag:
dd 0

invalid_prefix_mem:
mov [ebp-24],edx
mov [flag],0
fild dword ptr [ebp-24]
jmp invalid_prefix_ret

legendary_mem:
cmp [flag],0 //wooden hammer, for example, can't be legendary
jne @f
mov eax,27
mov [flag],1
@@: //if random re-rolled legendary, even though flag was set
cmp eax,27
jne legendary_ret
mov ebx,51
jmp legendary_ret

godly_mem:
cmp [flag],0
jne godly_ret
add [flag],1
mov eax,b
jmp godly_ret

unreal_mem:
cmp [flag],0
jne @f
mov eax,23
mov [flag],1
@@: //if random re-rolled unreal, even though flag was set
cmp eax,23
jne unreal_ret
mov ebx,52
jmp unreal_ret

[DISABLE]
force_prefix:
db 75
invalid_prefix:
db 89 55 dc db 45 dc
legendary:
db 0F 85 40 0f 00 00
godly:
db 83 f8 0b 75 05
unreal:
db 0F 85 d2 07 00 00 BB
all_else:
db 6a 51 ba 3e
dealloc(mem)


</AssemblerScript>
