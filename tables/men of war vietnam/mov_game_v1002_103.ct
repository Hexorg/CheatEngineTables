[ENABLE]
//=========================================
// Check if script is compatible to this game version
// If false the script will not be loaded

assert(mov_game.exe+5a2b3b,d9 9e 98 00 00 00)
assert(mov_game.exe+4a1a70,d8 ae 98 00 00 00)
assert(mov_game.exe+3f17c0,8b 44 24 04 89 41 2c)
assert(mov_game.exe+5a2545,d9 00 d9 55 00)

//=========================================
alloc(MyCode,1024)
//=========================================
// Declaration section
label(_GodMode)
label(_BackGM)
label(_ExitGM)
label(_MonFuel)
label(_BackMF)
label(_ExitMF)
label(_MonAmmo)
label(_MonA0)
label(_MonA1)
