  <UserdefinedSymbols/>
  <LuaScript>LHandDelta = 0
RHandDelta = 0
WepData = '[["DATA.exe"+00FDEC00]+74]+3b0c'
WepTable = '[[["DATA.exe"+00FDEC00]+74]+6c]-26eb'
Weapons = nil

function WepGetter()
  -- if anticrash == blah then
    LHand = readInteger('[[["DATA.exe"+00F49020]+18]+2fc]+0')
    RHand = readInteger('[[["DATA.exe"+00F49020]+18]+2fc]+4')
    if Weapons == nil then
    Weapons = {}
      for i=0,1244 do
        local wepid = readInteger(getAddress(WepTable)+(i*8))
        local wepnum = readInteger(getAddress(WepTable)+(i*8)+4)
        table.insert(Weapons,{WeaponID = wepid,Index = wepnum})
      end
    end
    if LHand ~= LHandDelta then
      LHandDelta = LHand
      unregisterSymbol("LHIndex")
      for k,v in pairs(Weapons) do
        if v.WeaponID == round(LHand,-2) then
          registerSymbol("LHIndex",getAddress(WepData)+v.Index*0x110,true)
        end
      end
    end
    if RHand ~= RHandDelta then
      RHandDelta = RHand
      unregisterSymbol("RHIndex")
      for k,v in pairs(Weapons) do
        if v.WeaponID == round(RHand,-2) then
          registerSymbol("RHIndex",getAddress(WepData)+v.Index*0x110,true)
        end
      end
    end
end


if PTimer~=nil then
  timer_setEnabled(PTimer, false)
  object_destroy(PTimer);
  PTimer = nil;
end

PTimer = createTimer(nil)
timer_onTimer(PTimer, WepGetter)
timer_setInterval(PTimer,200)
timer_setEnabled(PTimer, false)

function round(num, idp)
  local mult = 10^(idp or 0)
  return math.floor(num * mult + 0.5) / mult
end

function WepDatEnable()
timer_setEnabled(PTimer, true)
end

function WepDatDisable()
timer_setEnabled(PTimer, false)
end
</LuaScript>
