MyCode:
//=========================================
// On main Screen
_MonHumanPlayer:
 push ebx
 push ecx

 mov ecx,[eax+000000A8]            // Original code (get pRes)

 mov [pPlayer],eax

 cmp dword ptr [iEnableMMR],0
 je _ExitMHP                       // Jump if feature is disabled

 mov edx,(float)6300.0

 mov ebx,[ecx]
 cmp edx,ebx
 cmovg ebx,edx
 mov [ecx],ebx

 mov ebx,[ecx+04]
 cmp edx,ebx
 cmovg ebx,edx
 mov [ecx+04],ebx

 mov ebx,[ecx+08]
 cmp edx,ebx
 cmovg ebx,edx
 mov [ecx+08],ebx

 mov ebx,[ecx+0c]
 cmp edx,ebx
 cmovg ebx,edx
 mov [ecx+0c],ebx

_ExitMHP:
 pop ecx
 pop ebx

 mov eax,[eax+000000A8]            // Original code (get pRes)
 jmp _BackMHP                      // Back to main code

//=========================================
_GodMode:
 push ecx

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov ecx,[esi+0c]
 cmp ecx,[pPlayer]
 jne _ExitGMD

 mov ecx,[esi+08]                  // Unit info
 movzx ecx,word ptr [ecx+2e]       // Get mHP

 cvtsi2ss xmm0,ecx                 // Convert INT to FLOAT

_ExitGMD:
 pop ecx

 movss [esi+30],xmm0               // Original code (rds fcHP)
 jmp _BackGMD                      // Back to main code

//=========================================
// During combat (Units)
_GodMode1:
 push eax

 cmp dword ptr [iEnableGMD],0
 je _ExitGM1                       // Jump if feature is disabled

 mov eax,[edi+0c]
 cmp eax,[pPlayer]
 jne _ExitGM1

 mov eax,[edi+08]                  // Unit info
 movzx eax,word ptr [eax+2e]       // Get mHP

 cvtsi2ss xmm1,eax                 // Convert INT to FLOAT

_ExitGM1:
 pop eax

 movss [edi+30],xmm1               // Original code (upds HP)
 jmp _BackGM1                      // Back to main code

//=========================================
