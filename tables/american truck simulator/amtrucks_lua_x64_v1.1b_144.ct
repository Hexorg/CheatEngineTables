ets2_mAlloc+128:
pMoneyStore1:
dd 0

ets2_mAlloc+130:
pMoneyStore2:
dd 0

ets2_mAlloc:
  cmp [pMoneyStore1],0
  je pGetMoney
push rax
push rbx
  mov rax,[pMoneyStore1]
  mov rbx,[pMoneyStore2]
  cmp rax,rbx
pop rbx
pop rax
  je pGetMoney
  jmp pSetMoney

pSetMoney:
  cvtsi2ss xmm2,[pMoneyStore1]
  divss xmm2,[mMultiplierFloat]
  cvttss2si rax,xmm2
  mov [pMoneyStore1],rax
  xorps xmm2,xmm2
  xor rax,rax
  jmp pUpdateMoney

pGetMoney:
  cvtsi2ss xmm2,[rdx+30]
  mulss xmm2,[mMultiplierFloat]
  cvttss2si rax,xmm2
  mov [pMoneyStore1],rax
  mov [pMoneyStore2],rax
  xorps xmm2,xmm2
  xor rax,rax
  jmp pOriginalCode

pUpdateMoney:
  mov rax,[pMoneyStore1]
  mov [rdx+30],rax
  jmp pGetMoney

pOriginalCode:
  readmem(ets2_mAOB+B,7)
jmp ets2_mExit

ets2_mAOB+B:
  jmp ets2_mAlloc
  db 90 90
ets2_mExit:


