--[[
- infinite backpack space
]]--
function infiniteBackPack(toggleState)
  local bpSpaceUsedAddress = addresslist_getMemoryRecordByDescription(AddressList, "Inventory Limit Check")
  local bpSizeValue = tonumber(memoryrecord_getValue(bpSpaceUsedAddress))
  local bpUsedSpaceAddress = addresslist_getMemoryRecordByDescription(AddressList, "Backpack Used Space")
  local bpReturnValue = tonumber(memoryrecord_getValue(bpUsedSpaceAddress))

  if toggleState == nil then
     if bpSizeValue == 1 then toggleState = false else toggleState = true end
  end

  if toggleState == true then
    memoryrecord_setValue(bpSpaceUsedAddress, 1)
  else
    memoryrecord_setValue(bpSpaceUsedAddress, bpReturnValue)
  end

  if toggleState == false then memoryrecord_unfreeze(bpSpaceUsedAddress) end
  if toggleState == true then memoryrecord_freeze(bpSpaceUsedAddress) end
end
--[[ -------------------------------------------------------------------- ]]--
function unfreezeAllMemoryRecords()
  local count = addresslist_getCount(AddressList)
  for i=0,count-1 do
    local record = addresslist_getMemoryRecord(AddressList, i)
    memoryrecord_unfreeze(record)
  end
end
