   if bEnabled == nil or bEnabled ~= 1 then return 0 end
   return 1
end

function TeleportSave()
  if getOpenedProcessID() == 0 and readInteger("pBasePointer") == nil then return end
  if TeleEnabled() == 0 then return end
  DebugLog("@Function: TeleportSave()")

  SavePos = readInteger("iTeleModSavePos")
  SaveLoc[SavePos] = { x = readFloat(getAddress(BaseAddressStringX)),
                       y = readFloat(getAddress(BaseAddressStringY)),
                       z = readFloat(getAddress(BaseAddressStringZ)) }

  DebugLog("-Saved Location: Pos = " .. SavePos ..
           " :: X = " .. SaveLoc[SavePos]['x'] ..
           " :: Y = " .. SaveLoc[SavePos]['y'] ..
           " :: Z = " .. SaveLoc[SavePos]['z'])
  beep()
end

function TeleportLoad()
   if getOpenedProcessID() == 0 and readInteger("pBasePointer") == nil then return end
   if TeleEnabled() == 0 or SaveLoc[SavePos] == nil then return end
   DebugLog("@Function: TeleportLoad()")

   SavePos = readInteger("iTeleModSavePos")
   writeFloat(getAddress(BaseAddressStringX), SaveLoc[SavePos]['x'])
   writeFloat(getAddress(BaseAddressStringY), SaveLoc[SavePos]['y'])
   writeFloat(getAddress(BaseAddressStringZ), SaveLoc[SavePos]['z'])

   DebugLog("-Loaded Location: Pos = " .. SavePos ..
           " :: X = " .. SaveLoc[SavePos]['x'] ..
           " :: Y = " .. SaveLoc[SavePos]['y'] ..
           " :: Z = " .. SaveLoc[SavePos]['z'])
   beep()
end

if hktelesave~=nil then object_destroy(hktelesave);hktelesave=nil;end
hktelesave = createHotkey("TeleportSave", VK_NUMPAD0)
if hkteleload~=nil then object_destroy(hkteleload);hkteleload=nil;end
hkteleload = createHotkey("TeleportLoad", VK_DECIMAL)
</LuaScript>
