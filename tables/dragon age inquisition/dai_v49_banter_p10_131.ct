      <AssemblerScript>[ENABLE]
alloc(memory,2048)
LuaCall(AOBExecScan("onSetHero", "48 8B 9E 00 11 00 00 48 8B AE 08 11 00 00"))
LuaCall(AOBExecScan("onSetActiveHero", "8B 47 40 B8 01 00 00 00 4D 29 C8 49 C1 F8 02"))
label(code)
label(return)
label(skip)
label(loop)
label(record)
label(code2)
label(return2)

label(pHeroBase)
registersymbol(pHeroBase)
label(pHeroBase1)
registersymbol(pHeroBase1)
label(pHeroBase2)
registersymbol(pHeroBase2)
label(pHeroBase3)
registersymbol(pHeroBase3)
label(pHeroBase4)
registersymbol(pHeroBase4)

LuaCall(labelParty_start())

onSetHero:
  jmp code // 14
return:

onSetActiveHero:
  jmp code2 // 14
  nop
return2:

memory:

// set hero
code:
  // run original code
  db 48 8B 9E 00 11 00 00 48 8B AE 08 11 00 00

  // set up
  push rax
  push rbx
  push rcx
  add rsi,1100
  mov rcx,0
  mov rax,pHeroBase

loop:
  // loop limiter
  inc rcx
  cmp rcx,10
  je skip

  add rax,8
  mov rbx,[rax]
  cmp rbx,rsi
  je skip
  cmp rbx,0
  je record
  jmp loop

record:
  // only record heroes: 700A302C 3FFFE005
  mov rbx,[rsi]
  mov ebx,[rbx+0]
  cmp ebx,700A302C
  jne skip
  mov rbx,[rsi]
  mov ebx,[rbx+4]
  cmp ebx,3FFFE005
  jne skip
  // record
  mov rbx,[rsi]
  mov [rax],rbx

skip:
  // clean up
  sub rsi,1100
  pop rcx
  pop rbx
  pop rax

  // return
  jmp return

// set active hero
code2:
  // record base active hero pointer
  mov [pHeroBase],rcx

  // run original code
  db 8B 47 40 B8 01 00 00 00 4D 29 C8 49 C1 F8 02

  // return
  jmp return2

pHeroBase:
  db 00 00 00 00 00 00 00 00
pHeroBase1:
  db 00 00 00 00 00 00 00 00
pHeroBase4:
  db 00 00 00 00 00 00 00 00
pHeroBase3:
  db 00 00 00 00 00 00 00 00
pHeroBase2:
  db 00 00 00 00 00 00 00 00

[DISABLE]
onSetHero:
  db 48 8B 9E 00 11 00 00 48 8B AE 08 11 00 00 // 14

onSetActiveHero:
  db 8B 47 40 B8 01 00 00 00 4D 29 C8 49 C1 F8 02 // 15

LuaCall(labelParty_stop())

unregistersymbol(onSetHero)
unregistersymbol(pHeroBase)
unregistersymbol(pHeroBase1)
unregistersymbol(pHeroBase2)
unregistersymbol(pHeroBase3)
unregistersymbol(pHeroBase4)
dealloc(memory)
