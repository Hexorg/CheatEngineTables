//=========================================
MyCode:
//=========================================
_MonPlayer:
 cmp byte ptr [ebx+000000e1],01
 jne _ExitMP

 mov [pPlayer],ebx

 mov cl,[ebx]
 mov [bPlayerID],cl

 cmp dword ptr [iEnableMR],0
 je _ExitMP                        // Jump if feature is disabled

 mov ecx,#5000
 cmp eax,5
 jle _MonP00

 mov ecx,#62000
 cmp eax,6
 jne _ExitMP

_MonP00:
 cmp ecx,[ebx+eax*4+0000009C]
 jle _ExitMP

 mov [ebx+eax*4+0000009C],ecx

_ExitMP:
 mov ecx,[ebx+eax*4+0000009C]      // Original code
 jmp _BackMP                       // Back to main code

//=========================================
_MonMovement:
 mov cl,[esi+22]
 cmp cl,[bPlayerID]
 jne _ExitMM

 mov [pUnit],esi

 cmp dword ptr [iEnableMM],0
 je _MonM00                        // Jump if feature is disabled

 mov ecx,[esi+49]
 mov [esi+4d],ecx

 xor ecx,ecx
 mov [ebp-08],ecx

_MonM00:
 cmp dword ptr [iEnableTS],0
 je _MonM01                        // Jump if feature is disabled

 mov ecx,[iMinSize]

 mov edx,[esi+000000AD]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000AD],edx

 mov edx,[esi+000000b1]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000b1],edx

 mov edx,[esi+000000b5]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000b5],edx

 mov edx,[esi+000000b9]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000b9],edx

 mov edx,[esi+000000bd]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000bd],edx

 mov edx,[esi+000000c1]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000c1],edx

 mov edx,[esi+000000c5]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+000000c5],edx

_MonM01:
 cmp dword ptr [iEnableMH],0
 je _ExitMM                        // Jump if feature is disabled

 mov ecx,#20
 xor edx,edx

 mov dl,[esi+00000476]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+00000476],dl

 mov dl,[esi+00000477]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+00000477],dl

 mov dl,[esi+00000478]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+00000478],dl

 mov dl,[esi+00000479]
 cmp ecx,edx
 cmovg edx,ecx

 mov [esi+00000479],dl

 lea ecx,[edx+edx*4]
 add ecx,ecx

 mov [esi+18],cx

_ExitMM:
 mov ecx,[ebp-08]                  // Original code
 mov edx,[esi+4D]                  // Original code
 jmp _BackMM                       // Back to main code

//=========================================
 db '========================================&gt;'
 db 'CE6.2 Script by Recifense 010313'
