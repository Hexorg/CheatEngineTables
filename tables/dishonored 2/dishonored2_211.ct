aobscanmodule( UpdateAmmo, Dishonored2.exe, 480359??488B13837A600475??83 )
registersymbol( UpdateAmmo )
label( UpdateAmmo_o )
registersymbol( UpdateAmmo_o )
alloc( AmmoHook, 256, Dishonored2.exe )
label( back )

AmmoHook:
UpdateAmmo_o:
readmem( UpdateAmmo, 7 )
push r8
mov r8,[dqPlayerPtr]
cmp [rcx+10],r8
pop r8
jne short UpdateAmmo_o
  cmp dword ptr [rdx+60],4  // update only ammo and its inventory; runes are 0x7
  jne short @f
    push rbx
    mov rbx,[rsp+30]
    mov r8d,[rbx+A4]        // get Max Clip
    mov [rbx+A0],r8d        // update Current Clip
    pop rbx
    xor r8d,r8d			    // setting parameter to 0 for function that updates inventory amount
    xor edi,edi
  @@:
  cmp dword ptr [rdx+60],7  // if runes
  jne short @f
// you can change this to a value different than to GAIN runes :)
//e.g.:
//  mov r8d,64 // 100 runes
    xor r8d,r8d
    xor edi,edi
@@:
jmp back

UpdateAmmo:
jmp AmmoHook
db 90 90
back:

[DISABLE]

UpdateAmmo:
readmem( UpdateAmmo_o, 7 )

dealloc( AmmoHook )
unregistersymbol( UpdateAmmo_o )
unregistersymbol( UpdateAmmo )
