      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]

// CE Version

{$lua}
  if(getCEVersion() &lt; 6.7) then
    ShowMessage('Please Update Cheat Engine To 6.7')
  end
{$asm}

// Selected Process

{$lua}
  if(process == nil) then
    ShowMessage('No Process Was Selected')
  elseif (process ~= "AnonymousME.exe") then
    ShowMessage('Expected Process = AnonymousME.exe')
  end
{$asm}
 
[DISABLE]
</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>58</ID>
          <Description>"[CharacterProperty] (Activate: Get Hit, Update: Get Hit)"</Description>
          <Options moHideChildren="1"/>
          <LastState/>
          <Color>000080</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>{ Game   : AnonymousME.exe
  Version: 
  Date   : 2017-10-16
  Author : Kalas

  This script does blah blah blah
}

[ENABLE]

aobscan(CharacterPropertyOnHit,F3 0F 11 6E 28 F3 0F 10 46)
alloc(newmem,$100,CharacterPropertyOnHit)

// CharacterProperty //

alloc(ptrCharacterProperty, 08)
alloc(CharacterProperty_Health, 08)
alloc(CharacterProperty_DamageScale, 08)

registersymbol(ptrCharacterProperty)
registersymbol(CharacterProperty_Health)
registersymbol(CharacterProperty_DamageScale)

ptrCharacterProperty:
  dd 0

CharacterProperty_Health:
  dd 0

CharacterProperty_DamageScale:
  dd 0

// ==================== //

label(code)
label(return)
label(PlayerCheck)

newmem:
  movss [rsi+28],xmm5

  cmp [rsi+24], 0 // PropertyID
  je PlayerCheck
  jmp code

PlayerCheck:
  mov [ptrCharacterProperty], rsi // Get Hit To Populate

  mov [rsi+60], (float)1 // Resets Value Back

  cmp dword ptr[CharacterProperty_Health], 0
  je @f
  fld [rsi+34]
  fstp [rsi+28]

@@:
  cmp dword ptr[CharacterProperty_DamageScale], 0
  je @f
  mov [rsi+60], (float)10

code:
  //movss [rsi+28],xmm5
  jmp return

CharacterPropertyOnHit:
  jmp newmem
return:
registersymbol(CharacterPropertyOnHit)

[DISABLE]

CharacterPropertyOnHit:
  db F3 0F 11 6E 28

unregistersymbol(CharacterPropertyOnHit)
dealloc(newmem)

// CharacterProperty //

unregistersymbol(ptrCharacterProperty)
unregistersymbol(CharacterProperty_Health)
unregistersymbol(CharacterProperty_DamageScale)

dealloc(ptrCharacterProperty)
dealloc(CharacterProperty_Health)
dealloc(CharacterProperty_DamageScale)
</AssemblerScript>
