//========================================= v1110 *
// On main screen
_MonPlayer:
 push ebx
 push ecx
 push esi

 mov esi,edi
 test esi,esi
 jz _ExitMP

 mov [pPlayer],esi

 mov eax,[esi+08]
 test eax,eax
 jz _MonP01

 mov eax,[eax+08]
 mov [pOwner],eax

 test eax,eax
 jz _MonP01

 cmp eax,[eax+000000f4]
 jne _MonP01

 cmp dword ptr [iEnableSR],0
 je _MonP00                        // Jump if feature is disabled

 xor ecx,ecx
 mov ebx,#70

 mov cl,[eax+00000108]             // Diplomacy (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+00000108],cl

 mov cl,[eax+00000109]             // Martial (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+00000109],cl

 mov cl,[eax+0000010a]             // Stewardship (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+0000010a],cl

 mov cl,[eax+0000010b]             // Intrigue (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+0000010b],cl

 mov cl,[eax+0000010c]             // Learning (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+0000010c],cl

//Other structure

 mov ebx,#1000
 mov ecx,[esi+00000094]            // Fertility (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000094],ecx

 mov ecx,[esi+00000098]            // Fertility (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+00000098],ecx

 mov ebx,#9900
 mov ecx,[esi+0000009c]            // Health (base)
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+0000009c],ecx

 mov ebx,#50000
 mov ecx,[esi+000000a4]            // Prestige
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+000000a4],ecx

 mov ecx,[esi+000000ac]            // Piety
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [esi+000000ac],ecx

// Other Structure

 mov ebx,[iDate]
 mov ecx,0006aef0
 sub ebx,ecx
 js _MonP00

 mov ecx,[eax+00000090]            // Age
 cmp ebx,ecx
 cmovg ecx,ebx
 mov [eax+00000090],ecx

_MonP00:
 cmp dword ptr [iEnableMDe],0
 je _MonP01                        // Jump if feature is disabled

 mov ecx,[eax+00000098]
 test ecx,ecx
 jz _MonP01

 mov ebx,[ecx+5c]
 test ebx,ebx
 js _MonP01

 mov [ecx+58],0
 mov [ecx+5c],0

_MonP01:
 cmp dword ptr [iEnableMG],0
 je _ExitMP                        // Jump if feature is disabled

 mov eax,[esi+000000b4]
 test eax,eax
 jnz _ExitMP

 mov eax,C1C0000
 cmp eax,[esi+000000b0]
 jle _ExitMP

 mov [esi+000000b0],eax

_ExitMP:
 pop esi
 pop ecx
 pop ebx

 lea eax,[edi+000000b0]            // Original code (pMoney)
 jmp _BackMP                       // Back to main code

//========================================= v2.0.2
// When changing date
_MonUpgrade:
 push esi

 mov esi,[ebp+04]
 cmp esi,UPNL
 jne _MonU00

 mov ecx,[edi+28]                  // Get pCapital
 test ecx,ecx
 jz _ExitMU

 mov ecx,[ecx+30]                  // Get pProvince
 test ecx,ecx
 jnz _MonU01
 jmp _ExitMU

_MonU00:
 cmp dword ptr [iEnableMUR],0
 je _ExitMU                        // Jump if feature is disabled

 mov ecx,[edi+20]                  // Get pProvince

 cmp esi,UPRB
 je _MonU01

 cmp esi,UPRX
 jne _ExitMU

 mov ecx,[edi+28]                  // Get pCapital
 test ecx,ecx
 jz _ExitMU

 mov ecx,[ecx+18]                  // Get pConqueror
 jmp _MonU02

_MonU01:
 test ecx,ecx
 jz _ExitMU

 mov ecx,[ecx+0000072C]            // Get pProvinceName
 test ecx,ecx
 jz _ExitMU

 mov ecx,[ecx+00000204]            // Get pOwner

_MonU02:
 cmp ecx,[pOwner]
 jne _ExitMU

 mov ecx,[eax]                     // Get current date *****

 cmp dword ptr [iEnableMU],0
 je _ExitMU                        // Jump if feature is disabled

 mov ecx,[eax]                     // Get current date
 mov [edi+14],ecx                  // Update finishing Date
 mov ecx,[edi+18]
 imul ecx,ecx,000003e8
 mov [edi+1c],ecx

_ExitMU:
 pop esi

 mov ecx,[eax]                     // Original code
 cmp ecx,[edi+14]                  // Original code
 jmp _BackMU                       // Back to main code

//========================================= v2.0.2
// On Selecting the Capital of a Province
_MonCapital:
 mov eax,[esi+20]

 mov [pCapital],eax

 mov eax,[eax+30]
 mov [pProvince],eax

 cmp dword ptr [iEnableRR],0
 je _ExitMC                        // Jump if feature is disabled

 mov eax,[eax+0000072C]
 test eax,eax
 jz _ExitMC

 mov eax,[eax+00000204]
 cmp eax,[pOwner]
 jne _ExitMC

 mov eax,[pCapital]
 mov [eax+64],0

 mov eax,[pProvince]
 mov [eax+00000310],0

_ExitMC:
 mov eax,[esi+20]                  // Original code
 mov eax,[eax+10]                  // Original code
 jmp _BackMC                       // Back to main code

//========================================= v2.0.2
// On Main Screen
_MonDate:
 mov edi,[eax+00000234]            // Original code (Get Date)

 mov [iDate],edi

_ExitMD:
 jmp _BackMD                       // Back to main code

//========================================= v2.0.2
// When changing date (construction)
_MonBuilding:
 cmp dword ptr [iEnableMB],0
 je _ExitMB                        // Jump if feature is disabled

 mov ecx,[edi+20]                  // Get pProvince
 test ecx,ecx
 jz _MonB00

 mov ecx,[ecx+0000072C]            // Get pProvinceName
 test ecx,ecx
 jz _MonB00

 mov ecx,[ecx+00000204]            // Get pOwner
 cmp ecx,[pOwner]
 je _MonB01

_MonB00:
 cmp dword ptr [iEnableMBR],0
 je _ExitMB                        // Jump if feature is disabled

 mov ecx,[edi+28]
 cmp ecx,[pOwner]
 jne _ExitMB

_MonB01:
 mov ecx,[eax]                     // Get current date
 mov [edi+14],ecx                  // Update finishing Date

_ExitMB:
 mov ecx,[eax]                     // Original code
 cmp ecx,[edi+14]                  // Original code
 jmp _BackMB                       // Back to main code

//========================================= v1102
// When changing date
_MonMovement:
 cmp dword ptr [iEnableMM],0
 je _ExitMM                        // Jump if feature is disabled

 mov eax,[ebx+00000124]            // Get pOwner
 cmp eax,[pOwner]
 jne _ExitMM

 mov eax,[ebp+08]
 inc eax
 mov [ebx+00000084],eax

_ExitMM:
 mov eax,[ebx+00000084]            // Original code
 jmp _BackMM                       // Back to main code

//========================================= v1102
// During combat
_GodMode:
 push esi

 mov esi,[ebp+08]
 test esi,esi
 jz _ExitGM

 lea esi,[esi-b0]
 mov esi,[esi+4c]                  // Get pOwner
 cmp esi,[pOwner]
 jne _GodM00

 cmp dword ptr [iEnableGM],0
 je _ExitGM                        // Jump if feature is disabled

 mov ecx,[ebx+20]
 mov [ebx],ecx

 xor eax,eax                       // Value2decrease = 0
 jmp _ExitGM

_GodM00:
 cmp dword ptr [iEnableMD],0
 je _ExitGM                        // Jump if feature is disabled

 shl eax,4                         // Damage * 16
 cmp ecx,eax
 cmovl eax,ecx

_ExitGM:
 pop esi

 sub ecx,eax                       // Original code
 mov [ebx],ecx                     // Original code
 mov ecx,[ebp-14]                  // Original code
 jmp _BackGM                       // Back to main code

//========================================= v1102
// When in combat
_MonMorale:
 push edi

 cmp dword ptr [iEnableMMo],0
 je _ExitMMo                       // Jump if feature is disabled

 mov edi,[eax+0c]                  // Get pOwner
 cmp edi,[pOwner]
 jne _ExitMMo

 mov edi,#1000
 mov [eax],edi

 xor ecx,ecx                       // value2decrease = 0

_ExitMMo:
 pop edi

 sub [eax],ecx                     // Original code
 cmp dword ptr [eax],00            // Original code
 jmp _BackMMo                      // Back to main code

//========================================= v1102
// Selected Province in Siege
// If a player's province, set value to 1 day
// If a Enemy's province, set value to maximum (Enable Assault)
_MonSiege:
 push edi

 add eax,edx                       // Original code

 cmp dword ptr [iEnableMS],0
 je _ExitMS                        // Jump if feature is disabled

 mov edi,[ebx+10]                  // Get pCapital
 test edi,edi
 jz _ExitMS

 mov edi,[edi+10]                  // Get pInfo
 test edi,edi
 jz _ExitMS

 mov edi,[edi+000001f8]            // Get pOwner
 cmp edi,[pOwner]
 jne _MonS00

 mov eax,1
 jmp _ExitMS
// Enemy Province's Capital
_MonS00:
 mov edi,[ebx+10]                  // Get pCapital

 mov edi,[edi+30]                  // Get pProvince
 test edi,edi
 jz _ExitMS

 mov edi,[edi+000006ec]            // Get ppArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi]                     // Get pArmy
 test edi,edi
 jz _ExitMS

 mov edi,[edi+00000124]            // Get pOwner
 cmp edi,[pOwner]
 jne _ExitMS

 mov [ebx+14],eax                  // Enable Assault

_ExitMS:
 pop edi

 cmp [ebx+14],eax                  // Original code
 jmp _BackMS                       // Back to main code

//========================================= v1102
// Army During Siege
_GodModeS:
 push esi
 push edi
 push ecx

 mov [eax+000000D4],ecx            // Original code

 cmp dword ptr [iEnableGM],0
 je _ExitGMS                       // Jump if feature is disabled

 mov esi,[eax+4C]                  // Get pOwner
 cmp esi,[pOwner]
 jne _ExitGMS

 pushfd
 cld

 lea esi,[eax+000000d8]
 lea edi,[eax+000000b8]
 mov ecx,8

 rep movsd

 popfd

 mov [eax+40],#1000                // Morale

_ExitGMS:
 pop ecx
 pop edi
 pop esi
 jmp _BackGMS                      // Back to main code

//========================================= v1102
// Looking at the Technology Screen
// ESI = province or demesne part, EDI = Index
_MonTechLevel:
 push edx
 push ebx

 cmp dword ptr [iEnableMTL],0
 je _ExitMTL                       // Jump if feature is disabled

 mov ebx,[edi+0000072c]            // Get pProvinceName
 test ebx,ebx
 jz _ExitMTL

 mov ebx,[ebx+00000204]            // Get pOwner
 cmp ebx,[pOwner]
 jne _ExitMTL

 mov ecx,[edi+30]                  // pTechLevel
 test ecx,ecx
 jz _ExitMTL

 mov ebx,[iMinTech]
 cmp ebx,8
 jg _ExitMTL

 test ebx,ebx
 js _ExitMTL

 push eax
 mov eax,#1000
 imul ebx,eax
 pop eax

 cmp ebx,[ecx+eax*4]
 jle _ExitMTL

 mov [ecx+eax*4],ebx

_ExitMTL:
 pop ebx
 pop edx

 mov ecx,[edi+30]                  // Original code
 mov ecx,[ecx+eax*4]               // Original code
 jmp _BackMTL                      // Back to main code

