dealloc(Empire_dustset_enable)

</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>110707</ID>
          <Description>"Empire Base Address Scan (To Activate: Load Game, To Update: Play)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(Empire_savedAOB)
label(Empire_returnhere)
label(Empire_originalcode)
label(Empire_exit)

label(Empire_update_exit)
label(Empire_set_exit)
label(Empire_researchset_exit)
label(Empire_dustset_exit)

registersymbol(Empire_savedAOB)

//0482382F - 8B C8                 - mov ecx,eax
//04823831 - 39 09                 - cmp [ecx],ecx
// ControlBanner:Update+14b
//04823833 - 8B 80 A0000000        - mov eax,[eax+000000A0]
//04823839 - 3D 03000000           - cmp eax,00000003
//0482383E - 0F84 01020000         - je 04823A45
//04823844 - 8B 45 08              - mov eax,[ebp+08]
//04823847 - 8B 80 8C000000        - mov eax,[eax+0000008C]
//0482384D - 83 EC 08              - sub esp,08
//04823850 - 57                    - push edi
aobscan(Empire_searchedAOB, 8B 80 A0 00 00 00 3D 03 00 00 00 * * * * * * 8B 45 08 8B 80 8C 00 00 00)

alloc(Empire_newmem, 2048)
alloc(Empire_temp1, 4)


Empire_newmem:
mov [Empire_temp1], ebx

// Player check
cmp dword ptr [eax+a0], #1
jne Empire_dustset_exit

cmp dword ptr [Empire_update_enable], #0
je Empire_update_exit
mov [Empire_basepointer], eax
Empire_update_exit:

cmp dword ptr [Empire_set_enable], #0
je Empire_set_exit
Empire_set_exit:

cmp dword ptr [Empire_researchset_enable], #0
je Empire_researchset_exit
cmp dword ptr [eax+44], #0
je Empire_researchset_exit
mov ebx, [eax+44]
cmp dword ptr [ebx+38], #0
je Empire_researchset_exit
mov ebx, [ebx+38]
mov [ebx+14], (float)9999999
Empire_researchset_exit:

cmp dword ptr [Empire_dustset_enable], #0
je Empire_dustset_exit
cmp dword ptr [eax+4c], #0
je Empire_dustset_exit
mov ebx, [eax+4c]
mov [ebx+40], (float)9999999
Empire_dustset_exit:

mov ebx, [Empire_temp1]


Empire_originalcode:
db 8B 80 A0 00 00 00
//mov eax,[eax+000000A0]


Empire_exit:
jmp Empire_returnhere


Empire_searchedAOB:
Empire_savedAOB:
jmp Empire_newmem
nop
Empire_returnhere:


[DISABLE]
Empire_savedAOB:
db 8B 80 A0 00 00 00
//mov eax,[eax+000000A0]

unregistersymbol(Empire_savedAOB)

dealloc(Empire_newmem)
dealloc(Empire_temp1)

</AssemblerScript>
