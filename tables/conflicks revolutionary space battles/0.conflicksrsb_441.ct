              <Color>80000008</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>define(islocal,"ShipProperties:get_IsOwnedByLocalPlayer")
define(actship,"ShipAction:get_ShipProperties")
define(actcost,"ShipAction:get_Cost")
define(movcost,"ShipMove:get_Cost")

[ENABLE]
assertNot(actcost,e9)
assertNot(movcost,e9)
globalalloc(_cost,$400)
_cost:
dd _cost,actcost,movcost
_cost+10:
dd 5

MemoryRecord(ConFlicks/32/cost/amount,Type=vtDword,Address=_cost+10)

_cost+20:
readmem(actcost,24)
_cost+40:
readmem(movcost,24)

label(commoncost)
// action cost
_cost+60:
pop   dword ptr [_cost+5c]
push  @f
ExReassemble(actcost,,,_act)
jmp   actcost+_act
@@:
push  dword ptr [_cost+5c]
jmp   short commoncost


// move cost
_cost+90:
pop   dword ptr [_cost+8c]
push  @f
ExReassemble(movcost,,,_mov)
jmp   movcost+_mov
@@:
push  dword ptr [_cost+8c]

commoncost:

push  ebp
mov   ebp,esp
push  ecx
push  eax
mov   ecx,[ebp+08]

sub   esp,0c
push  ecx
call  actship
add   esp,10

test  eax,eax
je    @f

sub   esp,0c
push  eax
call  islocal
add   esp,10

@@:
test  eax,eax
pop   eax
pop   ecx
pop   ebp
je    @f

sub   eax,[_cost+10]
test  eax,eax
jge   @f
xor   eax,eax
@@:
ret

actcost:
jmp   _cost+60
movcost:
jmp   _cost+90



[DISABLE]
actcost:
