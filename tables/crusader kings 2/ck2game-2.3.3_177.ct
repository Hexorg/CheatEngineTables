 cmp dword ptr [iEnableMCR],0
 je _ExitCCR                       // Jump if feature is disabled

 mov ecx,[pReligion]
 test ecx,ecx
 jz _ExitCCR

 mov [edx+00000800],ecx

_ExitCCR:
 pop ecx

 mov esi,[edx+000007FC]            // Original code
 jmp _BackCCR                      // Back to main code

//=========================================
// Procedure to update Perk Info
_UpdPerkInfo:
 push eax
 push ebx
 
 // Checking Perk List
 mov ebx,[PERK]
 test ebx,ebx
 jz _ExitUPI

 mov eax,[ebx]
 mov [imPerkIndex],eax

// Dealing with Any Perk Info

 mov eax,[iPerkIndex]
 cmp eax,[imPerkIndex]
 jg _ExitUPI

 test eax,eax
 jz _ExitUPI

 dec eax
 mov [iPerkID],eax

 mov ebx,[PERK]
 mov ebx,[ebx+0c]
 mov eax,[ebx+eax*4]
 mov [pPerk],eax

// Dealing with Human Player

 mov ebx,[pOwner]                  // Just for safety
 test ebx,ebx
 jz _ExitUPI

 mov eax,[ebx+000000fc]
 test eax,eax
 jz _ExitUPI

 mov eax,[ebx+00000100]
 test eax,eax
 jz _ExitUPI

 sub eax,[ebx+000000fc]
 shr eax,1
 mov [imPPerkIndex],eax

 mov eax,[iPPerkIndex]
 test eax,eax
 jz _ExitUPI

 cmp eax,[imPPerkIndex]
 jg _ExitUPI

 push eax
 push ebx
 push pPPerkID
 push pPlrPerk
 call _GetPerkPointer

// Dealing with Last Selected Character

 mov ebx,[pSelOwner]
 test ebx,ebx
 jz _ExitUPI

 mov eax,[ebx+000000fc]
 test eax,eax
 jz _ExitUPI

 mov eax,[ebx+00000100]
 test eax,eax
 jz _ExitUPI

 sub eax,[ebx+000000fc]
 shr eax,1
 mov [imSPerkIndex],eax

 mov eax,[iSPerkIndex]
 test eax,eax
 jz _ExitUPI

 cmp eax,[imSPerkIndex]
 jg _ExitUPI

 mov eax,[iSPerkIndex]
 test eax,eax
 jz _ExitUPI

 cmp eax,[imSPerkIndex]
 jg _ExitUPI

 push eax
 push ebx
 push pSPerkID
 push pSelPerk
 call _GetPerkPointer

_ExitUPI:
 pop ebx
 pop eax
 ret

//=========================================
// Procedure to Get Perk Pointer
_GetPerkPointer:
 push ebp
 mov ebp,esp

 push eax
 push ebx
 push ecx
 push edx

 mov ebx,[ebp+10]                  // Get pOwnerStr
 mov ecx,[ebp+14]                  // Get Index
 mov edx,[ebp+0C]                  // Get pPerkID

 dec  ecx                          // Adjust Index

 mov ebx,[ebx+000000fc]            // get pPerkIDList
 lea eax,[ebx+ecx*2]               // Point to ID
 mov [edx],eax

 movzx ecx,word ptr [eax]          // Get ID(Index)

 mov edx,[PERK]                    // Get pPerkList
 mov edx,[edx+0c]                  // Get PerkList

 mov eax,[edx+ecx*4]               // Get pPerk address
 mov ebx,[ebp+08]                  // Get pPerk
 mov [ebx],eax

_ExitGPP:
 pop edx
 pop ecx
 pop ebx
 pop eax

 pop ebp
 ret 0010

