    <CheatEntry>
      <ID>0</ID>
      <Description>"SaveCharPointers"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
alloc(SaveChar,2048)
alloc(pArray,80)

label(_SaveChar_Hook)
label(_SaveChar_Start)
label(_SaveChar_Back)
label(_SaveChar_Exit)
label(cCount)

registersymbol(_SaveChar_Hook)
registersymbol(SaveChar)
registersymbol(pArray)
registersymbol(cCount)
aobscan(aob_loadcharpointer,8BD88B430C8B4830)
// 8BD88B430C8B4830 Load Char Pointer
// 0043FFB1 - 8B D8                      - mov ebx,eax
// 0043FFB3 - 8B 43 0C                   - mov eax,[ebx+0C]
// 0043FFB6 - 8B 48 30                   - mov ecx,[eax+30]

//Game.exe+3FFB1:
aob_loadcharpointer:
_SaveChar_Hook:
 jmp _SaveChar_Start
_SaveChar_Back:

SaveChar:
_SaveChar_Start:
 mov edx,[edi+1C]
 mov [cCount], edx
 mov edx,eax
 mov eax, 4
 mul bl
 mov ebx,eax
 mov eax, edx
 mov edx,[edx+0C]
 mov edx,[edx+30]
 //mov ebx,[esp+18]
 mov [ebx+pArray],edx
_SaveChar_Exit:
 mov ebx, eax
 mov eax, [ebx+0C]
 jmp _SaveChar_Back

// Variables
cCount:
  dd 0

[DISABLE]
//Game.exe+3FFB1:
_SaveChar_Hook:
  mov ebx,eax
  mov eax,[ebx+0C]

dealloc(SaveChar)
dealloc(pArray)
unregistersymbol(SaveChar)
unregistersymbol(pArray)
unregistersymbol(cCount)
unregistersymbol(_SaveChar_Hook)


</AssemblerScript>
