alloc(TPORT,2048)

label(_TP_Coord_Start)
label(_TP_Coord_Back)
label(_TP_Coord_Hook)
label(_TP_Squad_Start)
label(_TP_Squad_Done)
label(_TP_Squad_Back)
label(_TP_Squad_Exit)
label(_TP_Squad_Hook)
