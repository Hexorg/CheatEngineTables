 jne _ExitMCP

 movss xmm1,[ebp+0c]
 movss [ebp+08],xmm1

_ExitMCP:
 pop eax

 movss xmm1,[ebp+08]               // Original code (rd cp)
 jmp _BackMCP                      // Back to main code

//=========================================
