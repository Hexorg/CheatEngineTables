      <AssemblerScript>[ENABLE]
aobscan(tiles_aob, c4 1d ?? ?? ?? ?? ?? ?? 34 19 ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? 00 00 00 00 00 00 00 00) //.net 4.5.1
//aobscan(tiles_aob, fc d4 ?? ?? ?? ?? ?? ?? 34 19 ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? 00 00 00 00 00 00 00 00)
/*
fc d4 ?? ?? ?? ?? ?? ?? 34 19 ?? ?? ?? ?? ?? ?? ?? ?? ?? ?? 00 00 00 00 00 00 00 00
ar_id      | size      | tile id   | x_max     |  y_max    | x_start   | y_start

The above scan is the most likely point of failure in the script.  It
depends on the mt ids for the array and tile to have the same last four
bytes as on my machine.  We could scan for the dimensions instead, but then we'd
need seperate scripts for each map size.
*/
label(tiles)
registersymbol(tiles)
tiles_aob:
tiles:

aobscan(timer_aob, DD 1D ?? ?? ?? ?? 80 3D ?? ?? ?? ?? 00 0F 85 ?? 04 00 00)
/*
The above scan picks up the static value type range
(clr!JIT_GetSharedGCThreadStaticBase), so we can pick up
the dungeon location to choose a reasonable y-value for our horizontal line
If you've got some code that hooks the timer, it could potentially interfere
without being obvious why it would for this reason.
*/
label(dungeon)
registersymbol(dungeon)
timer_aob+2:
dungeon:

alloc(mem,2048)
label(thread)
registersymbol(thread) //for debugging purposes
label(hor_loop)
label(ver_loop)
label(box)
registersymbol(box)
label(place_wood)
label(left_wall)
label(place_wood_wire)
label(place_wall)
label(place_wall_wire)
label(mid_wall1)
label(mid_wall2)
label(mid_wall3)
label(right_wall)
label(place_tp1)
label(place_tp2)
label(place_tp3)

mem:

thread:
//push #15000 //15 seconds
//call sleep //give us time to attach, find our label, and set a breakpoint

//the following chunk of code draws a green wire horizontally
//across the entire map at a height equal to the dungeon entrance.
mov ecx,[dungeon] //load the world size from the ill-named dungeon label
add ecx,1e0 //maxTilesX is at offset 1e0 (turns out, it's always 8401 regardless of map size)
mov ecx,[ecx] //dereference the pointer
sub ecx,1 //index w/ zero
mov eax,[dungeon]
add eax,214 //load eax w/ the height of the dungeon
mov eax,[eax]
lea edi,[tiles] //set our base to tiles
add edi,#24 //point to the first tile
mov ebx,[tiles+10] //y dimension of array (turns out it's invariant against map size)
hor_loop: //once for each tile in a horizontal line across the map
mov edx,ecx
imul edx,ebx
add edx,eax
shl edx,2 // offset of (ecx,eax) = (ecx*y_max+eax)*4bytes
add edx,edi //edx should now point a pointer to the tile at (ecx,eax)
mov edx,[edx] //dereference
add edx,c //offset c contains the wiring flag for green and blue
mov esi,[edx]
or esi,2 //set bit 2 = green wire
mov [edx],esi
loop hor_loop //dec ecx and repeat

//the following chunk of code draws a blue wire vertically
//bisecting the entire map about the default spawn/origin.
mov ecx,[dungeon] //load the world size from the ill-named dungeon label
add ecx,1e4 //maxTilesY is at offset 1e4
mov ecx,[ecx] //dereference the pointer
sub ecx,1 //index w/ zero
mov eax,[dungeon] //load the world size from the ill-named dungeon label
add eax,1e0 //maxTilesX is at offset 1e0
mov eax,[eax] //dereference the pointer
shr eax,1 //assume origin is about exactly in the middle of the map
sub eax,1 //index w/ zero
lea edi,[tiles] //set our base to tiles
add edi,#24 //point to the first tile
mov ebx,[tiles+10] //y dimension of array
ver_loop: //once for each tile in a horizontal line across the map
mov edx,eax
imul edx,ebx
add edx,ecx
shl edx,2 // offset of (ecx,eax) = (ecx*y_max+eax)*4bytes
add edx,edi //edx should now point a pointer to the tile at (ecx,eax)
mov edx,[edx] //dereference
add edx,c //offset c contains the wiring flag for green and blue
mov esi,[edx]
or esi,1 //set bit 1 = blue wire
mov [edx],esi
loop ver_loop //dec ecx and repeat

//the following chunk of code builds a little teleporter hutch
//down in hell, connected to the blue wire
//set ebx to about ~88% of the maximum depth:
box:
mov ecx,[dungeon] //load the world size from the ill-named dungeon label
add ecx,1e4 //maxTilesY is at offset 1e4
mov ecx,[ecx] //dereference the pointer
push eax
mov eax,ecx
mov edx,#88
imul eax,edx
cdq
mov ecx,#100
idiv ecx
mov esi,eax //esi now holds the y value of ~88% depth
pop eax
sub eax,1 //eax still holds the x pos of blue wire
/*
in summary, at this point:
eax = x (blue wire -1), esi = y (~88% down),
ebx = max_y (always 2401), edi = base of tiles[,]
*/
//draw 7 wood blocks in a row, just left of the blue wire
mov ecx,7
left_wall:
call place_wood
add esi,1 //move down one tile
loop left_wall
add eax,1 //move turtle to next column
sub esi,1 //move up one tile
call place_wood_wire
sub esi,1 //move up one tile
call place_tp1
sub esi,1 //move up one tile
mov ecx,4
mid_wall1: //draw the remaining interior background walls
call place_wall_wire
sub esi,1 //move up one tile
loop mid_wall1
call place_wood_wire //draw top wall
add eax,1 //move turtle to next column
call place_wood //draw top wall
add esi,1 //move down one tile
mov ecx,4
mid_wall2: //draw remaining interior background walls
call place_wall
add esi,1 //move down one tile
loop mid_wall2
call place_tp2
add esi,1 //move down one tile
call place_wood //draw floor
add eax,1 //move turtle to next column
call place_wood //draw floor
sub esi,1 //move up one tile
call place_tp3
sub esi,1 //move up one tile
mov ecx,4
mid_wall3: //draw the remaining interior background walls
call place_wall
sub esi,1 //move up one tile
loop mid_wall3
call place_wood //draw roof
add eax,1 //move turtle to next column
mov ecx,7
right_wall: //draw the last wall, enclosing our little teleport pod
call place_wood
add esi,1 //move down one tile
loop right_wall

ret //exit thread

//function transforms tile into a plain wood block
//pre: eax = x, esi = y, ebx = max_y, edi = base of tiles[,]
//post: edx points to the tile in question
place_wood:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],2400a2 //draw a wood wall
mov [edx+8],100001e
mov [edx+C],20000
ret

//function transforms tile into wood block w/ blue wire
place_wood_wire:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],2400a2 //draw a wood wall
mov [edx+8],100001e
mov [edx+C],20001
ret

//function transforms tile into wood wall
place_wall:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],FFFFFFFF //draw a wood wall
mov [edx+8],400
mov [edx+C],1380000
ret

//function transforms tile into wood wall w/ blue wire
place_wall_wire:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],FFFFFFFF //draw a wood wall
mov [edx+8],400
mov [edx+C],1380001
ret

//function transforms tile into left tp piece in front of wall w/ blue wire
place_tp1:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],0 //draw a teleporter's left tile on a wood wall background
mov [edx+8],10004eb
mov [edx+C],1240001 //include blue wire on terminal
ret

//function transforms tile into left tp piece in front of wall w/ blue wire
place_tp2:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],12 //draw a teleporter's middle tile on a wood wall background
mov [edx+8],10004eb
mov [edx+C],1240000
ret

//function transforms tile into left tp piece in front of wall w/ blue wire
place_tp3:
//calculate offset into tiles array:
mov edx,eax
imul edx,ebx
add edx,esi
shl edx,2
add edx,edi
mov edx,[edx] //now, edx points to the tile
mov [edx+4],24 //draw a teleporter's middle tile on a wood wall background
mov [edx+8],10004eb
mov [edx+C],1240000
ret


