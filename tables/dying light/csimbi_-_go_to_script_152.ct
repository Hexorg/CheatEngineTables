cApplyPreset:
lea rax,[rax+rax*2]
push rbx                 // Workaround for lea rax,[pLocationPresets+rax*4]
mov rbx,pLocationPresets
lea rax,[rbx+rax*4]
pop rbx
movss xmm0,[rax]
movss xmm1,[rax+4]
movss xmm2,[rax+8]
ret
