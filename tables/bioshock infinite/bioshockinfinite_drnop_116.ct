mov [eax],(float)1.0
pop eax
push C8
call Sleep
jmp KeyHandlerThread

ToggleSloMo3:
push eax
mov eax,[dwSloMo]
mov [eax],(float)1.5
pop eax
push C8
call Sleep
jmp KeyHandlerThread

Toggle:
xor [Toggler],1
push 96
call Sleep
jmp KeyHandlerThread

ExitKeyHandler:
ret

Toggler:
dd 1

KeyHandlerOff:
dd 0

