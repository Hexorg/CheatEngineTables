          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>// Window resolution override hack for ClaDun X2
// CE 6.2 Script by HenryEx

[ENABLE]
alloc(Resolution,2048)
registersymbol(iOrigH)
registersymbol(iOrigW)
registersymbol(iNewH)
registersymbol(iNewW)
registersymbol(bMul)
label(_Resolution2)
label(_Resolution3)
label(_Resolution4)
label(_Resolution5)
label(iOrigH)
label(iOrigW)
label(iNewH)
label(iNewW)
label(bMul)
label(_BackR1)
label(_BackR2)
label(_BackR3)
label(_BackR4)
label(_R1Code)
label(_R2Code)
label(_R3Code)
label(_R4Code)
label(_R5Code)
label(_R5Exit)

//------------------------------
// Hacking Points

// Displayed Resolution 1
"CladunX2.exe"+90AE:
 jmp Resolution
 nop
_BackR1:

// Displayed Resolution 2
"CladunX2.exe"+9109:
 jmp _Resolution2
 nop
_BackR2:

// Window clipping?
"CladunX2.exe"+24C64:
 jmp _Resolution3
 nop
_BackR3:

// Window clipping?
"CladunX2.exe"+24C9A:
 jmp _Resolution4
 nop
 nop
_BackR4:

// Switching sizes
"CladunX2.exe"+18A1:
 jmp _Resolution5
 nop
 nop
 nop

//------------------------------
// Code Section
Resolution:
// Something something rendered screen space
// This is after the sizes were loaded into registers
// ecx = screen size, edx = height, eax = width
 push ebx                       // Back up register
 xor ebx,ebx                    // Null it

 mov [iOrigH],edx               // Read original values for display
 mov [iOrigW],eax

 cmp ecx,1                      // Is enlarge screen size?
 jne _R1Code                    // If not, abort
 mov bl,[bMul]                  // Get current multiplier
 test bl,bl                     // If Multiplier is 0, skip
 jz _R1Code
 cmp bl,4                       // If Multiplier &gt; 4, skip
 ja _R1Code

// mov edx,[iNewH]
// mov eax,[iNewW]
 imul edx,ebx,#272              // multiply height
 imul eax,ebx,#480              // multiply width

_R1Code:
 pop ebx                        // Restore register
 push edx                       // Original code
 push eax
 push 00
 push 00
 jmp _BackR1

_Resolution2:
// Something something rendered screen space again
// This is after the sizes were loaded into registers
// edx = screen size, ecx = height, eax = width
 push ebx                       // Back up register
 xor ebx,ebx                    // Null it

 cmp edx,1                      // Is enlarge screen size?
 jne _R2Code                    // If not, abort
 mov bl,[bMul]                  // Get current multiplier
 test bl,bl                     // If Multiplier is 0, skip
 jz _R2Code
 cmp bl,4                       // If Multiplier &gt; 4, skip
 ja _R2Code

// mov ecx,[iNewH]
// mov eax,[iNewW]
 imul ecx,ebx,#272              // multiply height
 imul eax,ebx,#480              // multiply width

_R2Code:
 pop ebx                        // Restore register
 push ecx                       // Original code
 push eax
 push 00
 push 00
 jmp _BackR2

_Resolution3:
// This probably(?) does mouse-to-screen adjustments
// ebx is XOR'd, edi holds the width, ecx = screen size
 cmp ecx,1                      // Is enlarge screen size?
 jne _R3Code                    // If not, abort
 mov dl,[bMul]                  // Get current multiplier
 test dl,dl                     // If Multiplier is 0, skip
 jz _R3Code
 cmp dl,4                       // If Multiplier &gt; 4, skip
 ja _R3Code

 imul edi,edx,#480              // multiply width

_R3Code:
 xor edx,edx                    // Original code
 div edi
 mov edi,eax
 jmp _BackR3

_Resolution4:
// This probably(?) does mouse-to-screen adjustments
// ebx is XOR'd, ecx holds the height
 mov edx,[CladunX2.exe+17E9D8]  // Get screen size again
 cmp edx,1                      // Is enlarge screen size?
 jne _R4Code                    // If not, abort
 mov dl,[bMul]                  // Get current multiplier
 test dl,dl                     // If Multiplier is 0, skip
 jz _R4Code
 cmp dl,4                       // If Multiplier &gt; 4, skip
 ja _R4Code

 imul edi,edx,#272              // multiply height

_R4Code:
 xor edx,edx                    // Original code
 div ecx
 mov [esi+18],eax
 jmp _BackR4

_Resolution5:
// Gets activated when switching sizes
// Sends size to window border render (?)
// edi = old size, ecx = new size, eax = decremented size
 mov al,[bMul]                  // Get current multiplier
 test al,al                     // If Multiplier is 0, skip
 jz _R5Code
 cmp al,4                       // If Multiplier &gt; 4, skip
 ja _R5Code

 push ebx                       // Backup register, esp increased
 imul ebx,eax,#480              // multiply width
 mov [esp+1C],ebx               // store updated width
 imul ebx,eax,#272              // multiply height
 mov [esp+18],ebx               // store updated height
 pop ebx                        // Restore register^, esp decreased
 jmp _R5Exit

_R5Code:
 mov [esp+18],000003C0          // Original code
 mov [esp+14],00000220

_R5Exit:
 xor eax,eax                    // Make sure eax is 0 again
 jmp CladunX2.exe+18CA

//------------------------------
// Variables
iOrigH:
 dd 0
iOrigW:
 dd 0
iNewH:
 dd 0
iNewW:
 dd 0
bMul:
 db 0


[DISABLE]
"CladunX2.exe"+90AE:
 push edx
 push eax
 push 00
 push 00
// db 52 50 6A 00 6A 00

"CladunX2.exe"+9109:
 push ecx
 push eax
 push 00
 push 00
// db 51 50 6A 00 6A 00

"CladunX2.exe"+24C64:
 xor edx,edx
 div edi
 mov edi,eax
// db 33 D2 F7 F7 8B F8

"CladunX2.exe"+24C9A:
 xor edx,edx
 div ecx
 mov [esi+18],eax
// db 33 D2 F7 F1 89 46 18

"CladunX2.exe"+18A1:
 mov [esp+18],000003C0
// db C7 44 24 18 C0 03 00 00

unregistersymbol(iOrigH)
unregistersymbol(iOrigW)
unregistersymbol(iNewH)
unregistersymbol(iNewW)
unregistersymbol(bMul)
dealloc(Resolution)
</AssemblerScript>
