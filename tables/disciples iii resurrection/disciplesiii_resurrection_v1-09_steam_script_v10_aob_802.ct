 jmp _BackMPR                   // Back to main code

//========================================= 1.09
_MonArmyMP:
 push edx

 cmp dword ptr [iEnableMAM],0
 je _ExitMAM                    // Jump if feature is disabled

 mov edx,[ecx+0000ba18]         // Get pointer to faction
 mov edx,[edx+1]                // Get Faction name

 cmp edx,[aFaction]             // Is it a player's Army?
 jne _ExitMAM                   // Jump if false

 mov eax,[ecx+0000bba0]         // Get current MP

_ExitMAM:
 pop edx

 mov [ecx+0000bba0],eax         // Original code
 jmp _BackMAM                   // Back to main code

//========================================= 1.09
_GodMode:
 push ecx

 mov [pUnit],esi                // Save pointer for debugging

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                    // Jump if feature is disabled

 mov ecx,[esi+0000ba18]         // Get pointer to faction
 mov ecx,[ecx+1]                // Get Faction name

 cmp ecx,[aFaction]             // Is it a player's Army?
 jne _ExitGMD                   // Jump if false

 mov edx,[esi+18]               // Get current HP

_ExitGMD:
 pop ecx

 test eax,eax                   // Original code
 mov [esi+18],edx               // Original code
 jmp _BackGMD                   // Back to main code

