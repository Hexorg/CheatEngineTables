function RevertAndDisable()
  if getOpenedProcessID() == 0 then return end
  local defaultValues = {200, 100, 10, 5, 1, 0.1, 0.01}
  local defaultValues2 = {0.085, 0.07, 0.06, 0.05}

  for i=1,#ID do
    writeFloat('['..ID[i]..']', defaultValues[i])
    unregisterSymbol(ID[i])
  end

  for i=1,#DropsSymbols do
    writeFloat('['..DropsSymbols[i]..']', defaultValues2[i])
    unregisterSymbol(DropsSymbols[i])
  end

end

function presetSetValues(memrec)
  if getOpenedProcessID() == 0 then return end

  for description,_ in pairs(Presets) do
    if presetMemoryRecords[description] ~= memrec then
      memoryrecord_unfreeze(presetMemoryRecords[description])
    end
  end

  local whichmemrec = memoryrecord_getDescription(memrec)

  for i=1,#ID do
    writeFloat('['..ID[i]..']', Presets[whichmemrec][i])
  end

  return true
end

presetMemoryRecords ={}
for description,_ in pairs(Presets) do
 if presetMemoryRecords[description] == nil then
  presetMemoryRecords[description] = addresslist_getMemoryRecordByDescription(getAddressList(),description)
  memoryrecord_onActivate(presetMemoryRecords[description] ,presetSetValues)
 end
end

