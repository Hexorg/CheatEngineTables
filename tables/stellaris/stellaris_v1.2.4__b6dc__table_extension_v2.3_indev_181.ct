//========================================= 120 OK
// At main game screen
_MonHumanPlayer:
 push eax
 push ebx
 push edx

 mov [pPlayer],esi
 mov ecx,[esi+08]
 mov [iOwnerID],ecx
 mov ecx,[esi+0000010c]
