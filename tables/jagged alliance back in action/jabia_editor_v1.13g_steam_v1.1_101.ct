  MP_enabled.Checked = MP_entry.Active;
  MP_enabled.OnClick = EVENT_MP_setSlots;
  EVENT_MP_setSlots(); -- call at least once to get proper state at form creation
--
  MP_slots.Top = SPACING + MP_enabled.Height + SPACING;
  MP_slots.Left = SPACING;
  MP_slots.Height = 300;
  MP_slots.Width = LEFT_WIDTH;
  MP_slots.OnClick = EVENT_MP_updateItems;
--
  MP_updateBtn.Top = SPACING;
  MP_updateBtn.Left = SPACING + LEFT_WIDTH + SPACING + 50;
  MP_updateBtn.Height = MP_enabled.Height;
  MP_updateBtn.Width = MP_form.Width - SPACING - MP_updateBtn.Left; -- fill up the rest
  MP_updateBtn.Caption = "Update Pockets";
  MP_updateBtn.Enabled = false;
  MP_updateBtn.OnClick = EVENT_MP_updatePocket;
--
  MP_items.Top = MP_slots.Top;
  MP_items.Left = SPACING + LEFT_WIDTH + SPACING;
  MP_items.Height = MP_slots.Height;
  MP_items.Width = RIGHT_WIDTH;
  MP_items.OnClick = EVENT_MP_itemSelected;
--
  MP_form.Height = SPACING + math.max(MP_enabled.Height, MP_updateBtn.Height)
                   + SPACING + math.max(MP_slots.Height, MP_items.Height) + SPACING;

  -- 2. disable sender (button) and reenable on OnClose event
  sender.Enabled = false;
  MP_form.OnClose = function()
    sender.Enabled = true;
    MP_form.destroy();
  end;

  -- 3. get to work
  MP_form.show();
end;

function initializeComponents()
  loadMercsBtn.Top = SPACING;
  loadMercsBtn.Left = SPACING;
  loadMercsBtn.Height = 25;
  loadMercsBtn.Width = LEFT_WIDTH;
  loadMercsBtn.Caption = "Load Mercs";
  loadMercsBtn.OnClick = EVENT_readMercNames;
--
  mercListBox.Top = SPACING + loadMercsBtn.Height + SPACING;
  mercListBox.Left = SPACING;
  mercListBox.Height = 96;
  mercListBox.Width = LEFT_WIDTH;
--
  slotBox.Top = SPACING + loadMercsBtn.Height + SPACING +
                mercListBox.Height + SPACING;
  slotBox.Left = SPACING;
  slotBox.Height = 425;
  slotBox.Width = LEFT_WIDTH;
  slotBox.OnClick = EVENT_updateItemSelection;
--
  performBtn.Top = SPACING;
  performBtn.Left = SPACING + math.max(loadMercsBtn.Width,
                                       mercListBox.Width,
                                       slotBox.Width) + SPACING;
  performBtn.Height = 25;
  performBtn.Width = (RIGHT_WIDTH - SPACING) / 2;
  performBtn.Caption = "Update Inventory";
  performBtn.Enabled = false;
  performBtn.OnClick = EVENT_performUpdate;
--
  mpBtn.Top = SPACING;
  mpBtn.Left = performBtn.Left + performBtn.Width + SPACING;
  mpBtn.Height = performBtn.Height;
  mpBtn.Width = (RIGHT_WIDTH - SPACING) / 2;
  mpBtn.Caption = "Magic Pockets";
  mpBtn.Enabled = true;
  mpBtn.OnClick = EVENT_showMagicPockets;
--
  itemBox.Top = SPACING + performBtn.Height + SPACING;
  itemBox.Left = performBtn.Left;
  itemBox.Height = mercListBox.Height + SPACING + slotBox.Height;
  itemBox.Width = RIGHT_WIDTH;
  itemBox.OnClick = EVENT_itemSelected;
--
  GUI.Caption = "JABiA Editor v" .. EDITOR_VERSION ..
                " by Gr4veDigg0r";
  GUI.Height = SPACING + loadMercsBtn.Height + SPACING +
               mercListBox.Height + SPACING +  slotBox.Height + SPACING;
  GUI.Width = performBtn.Left + math.max(performBtn.Width,
                                         itemBox.Width) + SPACING;
  GUI.OnClose = function()
    unhideMainCEwindow();
    GUI.destroy();
  end;
--
  GUI.show();
end;

function waitForGame()
  local waitForm = createForm(false);
  local waitLabel = createLabel(waitForm);
  waitLabel.Caption = "waiting for process to spawn..."
  waitLabel.Top = SPACING;
  waitLabel.Left = SPACING;
  waitForm.Width = SPACING + waitLabel.Width*2.5 + SPACING;
  waitForm.Height = SPACING + waitLabel.Height + SPACING;
  waitForm.Caption = "";
  waitForm.centerScreen();
  waitForm.show();
  waitForm.OnClose = function()
    waitForm.destroy();
  end;

  local t = createTimer(nil);
  t.setInterval(250); -- milliseconds
  t.OnTimer = function()
    if getProcessIDFromProcessName(JABIA_EXE) ~= nil then
      t.destroy();
      waitForm.destroy();
      openProcess(JABIA_EXE);
      initializeComponents();
    end;
  end;
end;




--------------------------------------------------------------------------------
-- Script Startup
--------------------------------------------------------------------------------

hideAllCEWindows();
waitForGame();


