 je _ExitMTC                       // Jump if feature is disabled

 mov eax,[ebx+00000210]
 cmp eax,[iOwnerID]
 jne _ExitMTC

 mov eax,[ebx+000001E4]
 mov ecx,[ebx+000001E8]
 cmp eax,ecx
 jns _ExitMTC

 mov ecx,[eax]
 test ecx,ecx
 jz _ExitMTC

 mov eax,[ecx+48]
 test eax,eax
 jz _ExitMTC

 mov dword ptr [ecx+4c],1

_ExitMTC:
 mov eax,[ebx+000001E4]            // Original code (get pList)
 jmp _BackMTC                      // Back to main code

//========================================= 120 OK
