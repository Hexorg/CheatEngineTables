_GETCHA:
  PUSH EBP
  MOV EBP,ESP
  PUSHAD
  MOV EDI,[EBP+10]
  MOV EAX,[EBP+14]
  CMP [EDI],EAX
  JE SHORT _GETCHA_EXIT

  MOV EBX,2

_GETCHA_LOOP2:
  MOV ESI,[EBP+0C]
  MOV ECX,[EBP+08]
  XOR EDI,EDI
  DEC EDI

_GETCHA_LOOP1:
  CMP DWORD PTR [ESI],0
  JNE SHORT _GETCHA_CHECKADDRESS
  PUSH [EBP+1C]
  PUSH ECX
  CALL _CHECKCHA
  TEST AL,AL
  JE SHORT _GETCHA_EXIT
  MOV EDX,[EBP+18]
  TEST EDX,EDX
  JZ SHORT _GETCHA_LOOP1_SKIP0
  PUSH EBX
  MOV EBX,ECX
  CALL EDX
  POP EBX
  TEST AL,AL
  JZ SHORT _GETCHA_EXIT

_GETCHA_LOOP1_SKIP0:
  MOV EAX,[EBP+10]
  MOV [ESI],ECX
  INC [EAX]
  JMP SHORT _GETCHA_EXIT

_GETCHA_CHECKADDRESS:
  CMP [ESI],ECX
  JE SHORT _GETCHA_EXIT
  MOV EDX,[ESI]
  PUSH 00
  PUSH EDX
  CALL _CHECKCHA
  TEST AL,AL
  JE SHORT _GETCHA_CHECKADDRESS_CLEARCHA
  JNE SHORT _GETCHA_NEXTADDRESS

_GETCHA_CHECKADDRESS_CLEARCHA:
  PUSHAD
  MOV EAX,[EBP+10]
  MOV EBX,[EAX]
  MOV [ESI],0
  INC EDI
  INC EBX
  SUB EBX,EDI
  DEC EBX
  PUSH 00
  PUSH EBX
  PUSH ESI
  CALL _COPY
  DEC [EAX]
  POPAD

_GETCHA_NEXTADDRESS:
  MOV EAX,[EBP+10]
  INC EDI
  ADD ESI,4
  CMP EDI,[EAX]
  JNE SHORT _GETCHA_LOOP1
  DEC EBX
  JNZ SHORT _GETCHA_LOOP2
  JMP SHORT _GETCHA_LOOP1

_GETCHA_EXIT:
  POPAD
  POP EBP
  RET 0018

//==================================================
DB INT3
//==================================================

_CHECKCHA:
  PUSH EBP
  MOV EBP,ESP
  PUSHAD
  XOR EAX,EAX
  MOV ESI,[EBP+08]
  TEST ESI,ESI
  JZ SHORT _CHECKCHA_FALSE
  MOV EAX,[ESI+00000004]
  SHR EAX,1C
  CMP AL,01
  SETE AL
  CMP DWORD PTR [ESI+00000008],0
  SETE AH
  TEST AH,AL
  JE SHORT _CHECKCHA_FALSE
  CMP BYTE PTR [EBP+0C],01
  JNE SHORT _CHECKCHA_SKIP0
  CMP BYTE PTR [ESI+00000014],01
  JE SHORT _CHECKCHA_FALSE

_CHECKCHA_SKIP0:
  PUSHAD
  PUSH 04
  PUSH [ESI+00000028]
  CALL KERNEL32.ISBADREADPTR
  TEST AL,AL
  POPAD
  JNE SHORT _CHECKCHA_FALSE
  MOV ECX,[ESI+00000028]
  MOV ECX,[ECX+0000001C]
  XOR EAX,EAX
  CMP DWORD PTR [ECX+00000010],0
  SETE AL
  SHL EAX,08
  CMP DWORD PTR [ECX+00000014],0
  SETE AL
  SHL EAX,08
  CMP DWORD PTR [ECX+00000018],0
  SETE AL
  TEST EAX,00010101
  JNE SHORT _CHECKCHA_FALSE
  CMP DWORD PTR [ESI+000002D4],000003E7
  SETE AL
  CMP DWORD PTR [ESI+000002D8],000003E7
  SETE AH
  TEST AH,AL
  JNE SHORT _CHECKCHA_FALSE
  CMP DWORD PTR [ESI+000002D4],0
  JZ SHORT _CHECKCHA_FALSE

_CHECKCHA_TRUE:
  POPAD
  POP EBP
  MOV AL,01
  RET 0008

_CHECKCHA_FALSE:
  POPAD
  POP EBP
  XOR AL,AL
  RET 0008

