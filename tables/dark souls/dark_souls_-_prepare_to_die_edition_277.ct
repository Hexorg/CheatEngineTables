_ONHIT:
  PUSHAD
  MOV EDI,[BASE]
  MOV EDI,[EDI]
  TEST EDI,EDI
  JZ _ONHIT_TRUE
  MOV EDI,[EDI+0000003C]
  TEST EDI,EDI
  JZ _ONHIT_TRUE
  MOV ECX,[EAX+0000012C]
  MOV EDX,[ECX]
  MOV EDX,[EDX+00000008]
  CALL EDX
  CMP EAX,03
  JA _ONHIT_TRUE
  JMP DWORD PTR [EAX*4+ONHIT.PJUMP]

_ONHIT2:
  POPAD
  PUSHAD
  MOV ECX,[EAX+00000130]
  MOV EDX,[ECX]
  MOV EDX,[EDX+00000008]
  CALL EDX
  CMP EAX,01
  JNE _ONHIT_TRUE
  POPAD
  PUSHAD
  MOV ESI,EAX
  MOV ECX,[EAX+0000012C]
  MOV EBX,[EAX+00000130]
  CMP ECX,EBX
  JE _ONHIT_TRUE
  MOV EDX,[EBX]
  MOV EDX,[EDX+000000A4]
  MOV ECX,EBX
  CALL EDX
  TEST AL,AL
  JE SHORT _ONHIT2_ONEHITKILL
  CMP BYTE PTR [PLAYER.GODMODE.ENABLE],01
  JNE _ONHIT_TRUE
  JMP _ONHIT_FALSE

_ONHIT2_ONEHITKILL:
  MOV ECX,[ESI+0000012C]
  MOV EDX,[ECX]
  MOV EDX,[EDX+000000A4]
  CALL EDX
  TEST AL,AL
  JE _ONHIT_TRUE
  CMP BYTE PTR [NPC.ONEHITKILL.ENABLE],01
  JNE _ONHIT_TRUE
  OR BYTE PTR [EBX+000001FF],20
  JMP _ONHIT_TRUE

_ONHIT3:
  POPAD
  PUSHAD
  MOV ESI,EAX
  MOV ECX,[EAX+00000130]
  PUSH EAX
  PUSH 00
  PUSH ECX
  CALL _CHECKCHA
  TEST AL,AL
  POP EAX
  JE _ONHIT_TRUE
  MOV EDX,[ECX]
  MOV EDX,[EDX+000000A4]
  CALL EDX
  TEST AL,AL
  JE SHORT _ONHIT3_ONEHITKILL
  CMP BYTE PTR [PLAYER.GODMODE.ENABLE],01
  JNE _ONHIT_TRUE
  JMP _ONHIT_FALSE

_ONHIT3_ONEHITKILL:
  MOV ECX,[ESI+0000012C]
  MOV EBX,[ESI+00000130]
  MOV EDX,[ECX]
  MOV EDX,[EDX+000000A4]
  CALL EDX
  TEST AL,AL
  JE _ONHIT_TRUE
  CMP BYTE PTR [NPC.ONEHITKILL.ENABLE],01
  JNE _ONHIT_TRUE
  OR BYTE PTR [EBX+000001FF],20
  JMP _ONHIT_TRUE

_ONHIT4:
  POPAD
  PUSHAD
  MOV ECX,[EAX+00000130]
  MOV EDX,[ECX]
  MOV EDX,[EDX+00000008]
  CALL EDX
  CMP EAX,01
  JNE _ONHIT_TRUE
  POPAD
  PUSHAD
  MOV ESI,EAX
  MOV EBX,[EAX+00000130]
  MOV EDX,[EBX]
  MOV EDX,[EDX+000000A4]
  MOV ECX,EBX
  CALL EDX
  TEST AL,AL
  JE SHORT _ONHIT4_ONEHITKILL
  MOV EAX,[ESI+0000012C]
  MOV EDX,[EAX+0000008C]
  CALL GETCHAHITBYOBJ
  CMP EAX,[ESI+00000130]
  JE _ONHIT_TRUE
  CMP BYTE PTR [PLAYER.GODMODE.ENABLE],01
  JNE _ONHIT_TRUE
  JMP _ONHIT_FALSE

_ONHIT4_ONEHITKILL:
  MOV EAX,[ESI+0000012C]
  MOV EDX,[EAX+0000008C]
  CALL GETCHAHITBYOBJ
  CMP EAX,[ESI+00000130]
  JE SHORT _ONHIT_TRUE
  MOV EBX,[ESI+00000130]
  MOV ECX,EAX
  MOV EDX,[ECX]
  MOV EDX,[EDX+000000A4]
  CALL EDX
  TEST AL,AL
  JE SHORT _ONHIT_TRUE
  CMP BYTE PTR [NPC.ONEHITKILL.ENABLE],01
  JNE SHORT _ONHIT_TRUE
  OR BYTE PTR [EBX+000001FF],20

_ONHIT_TRUE:
  POPAD
  SUB ESP,08
  PUSH EBX
  PUSH ESI
  JMP _ONHIT_RET

_ONHIT_FALSE:
  POPAD
  XOR AL,AL
  ret

ONHIT.PJUMP:
  DD _ONHIT_TRUE
  DD _ONHIT2
  DD _ONHIT3
  DD _ONHIT4

