--------------------------------------------------------------------------------
-- helper functions
--------------------------------------------------------------------------------

function fill(text, list, comp)
  local a = {};
  for key, value in pairs(list) do table.insert(a, key) end;
  table.sort(a, comp);
  for key, value in ipairs(a) do
------------------------------------------
    text = text .. value .. "\n";
------------------------------------------
  end;
  return text;
end;

slotSort = function(a, b)
-- comparison predicate for strings, must return boolean
-- if one of two strings starts with "Item" it shall be greater
-- if BOTH of them do, give it alphabetical odering
  local A = string.sub(a, 1, 4) == "Item";
  local B = string.sub(b, 1, 4) == "Item";

  if (A and not B) then return false; end;
  if (B and not A) then return true; end;
  return a &lt; b;
end;

function fillSelection(target, list)
  local text = "";
  target.clear();

  if list == ALL_DATA then
	-- browse a list of 11 lists
	text = "      --- Offhand Items ---\n";
	text = fill(text, OFFHND_DATA);
	text = text .. "      --- Head Dresses ---\n";
	text = fill(text, HEAD_DATA);
	text = text .. "      --- Facial Gear ---\n";
	text = fill(text, FACE_DATA);
	text = text .. "      --- Outerwear ---\n";
	text = fill(text, TORSO_DATA);
	text = text .. "      --- Vests ---\n";
	text = fill(text, VEST_DATA);
	text = text .. "      --- Shoes ---\n";
	text = fill(text, SHOES_DATA);
	text = text .. "      --- Pants ---\n";
	text = fill(text, PANTS_DATA);
	text = text .. "      --- Ammunition ---\n";
	text = fill(text, AMMO_DATA);
	text = text .. "      --- Attachments ---\n";
	text = fill(text, ATTCH_DATA);
	text = text .. "      --- Food and Edibles ---\n";
	text = fill(text, FOOD_DATA);
	text = text .. "      --- Collectible Items ---\n";
	text = fill(text, COLL_DATA);
  else
	text = fill(text, list);
  end;
  target.Items.Text = text;
end




--------------------------------------------------------------------------------
-- game I/O functions
--------------------------------------------------------------------------------

function getMercAddr(base, num)
  -- num == number, 0 .. 5
  for i = 1, num do
      base = MERC_LIST_PRE .. base .. MERC_LIST_INC;
  end
  return MERC_DATA_PRE .. base .. MERC_DATA_BASE;
end

function getMercName(base, num)
  return readString(getMercAddr(base, num) .. MERC_NAME_OFFS);
end

function getNumMercs(base, num)
  -- starting at MERC_BASE, check
  c = 0; -- start at first merc, one MUST exist, and get names until name is NIL
  while getMercName(base, c) ~= nil do
    c = c +1;
  end;
  return c;
end

function setMercItem(base, num, slot, id, dura, amount, uses)
  local targetH = getMercAddr(base, num) .. "+" .. slot.ah;
  local targetL = getMercAddr(base, num) .. "+" .. slot.al;

  writeInteger(targetH, (uses*65536)+dura);  -- uses + durability
  writeInteger(targetL, (amount*65536)+id);  -- amount + id
end




--------------------------------------------------------------------------------
-- GUI functions
--------------------------------------------------------------------------------

function EVENT_readMercNames(sender)
  -- determine amount of mercs in squad?
  performBtn.Enabled = false;
  local numMercs = getNumMercs(MERCS_BASE);
  local textOutput = "";

  for i = 0, (numMercs-1) do
      local name = getMercName(MERCS_BASE, i);
      textOutput = textOutput .. name .. "\n";
  end;

  mercListBox.Items.Text = textOutput;
  mercListBox.setItemIndex(0);
  slotBox.Items.Text = fill("", ITM_SLOTS, slotSort);
end

-- deprecated, now part of EVENT_readMercNames
function EVENT_updateSlotSelection(sender)
  slotBox.Items.Text = fill("", ITM_SLOTS, slotSort);
end

local oldSlot = {};
function EVENT_updateItemSelection(sender)
  local selectedSlot = ITM_SLOTS[slotBox.Items[slotBox.getItemIndex()]];
  if (selectedSlot.l == oldSlot.l) then
    -- do not refill when the type of item list stays the same
    return;
  end;
  oldSlot = selectedSlot;
  fillSelection(itemBox, selectedSlot.l);
  performBtn.Enabled = false;
end;

function EVENT_itemSelected(sender)
  local selected = sender.getItemIndex();
  -- one can safely assume that no item list is empty
  if string.sub(sender.items[selected], 1, 1) == " " then
    sender.setItemIndex(selected +1);
  end
  performBtn.Enabled = true;
end

function EVENT_performUpdate(sender)
  -- assume everything has been properly selected
  -- 1. assemble a proper adress record
  local num = mercListBox.getItemIndex();
  local slot = ITM_SLOTS[slotBox.Items[slotBox.getItemIndex()]];

  -- 2. gather all necessary data
  -- the target is actually 2x4 bytes in size divided in 4x2 bytes parts
  local list = slot.l;

  -- default to empty item
  local uses = 0;
  local dura = 0;
  local amount = 0;
  local id = 65535;

  if list == ALL_DATA then
    -- one text is part of 11 possible lists ...
    local selectedItem = itemBox.Items[itemBox.getItemIndex()];
    local done = false;
	for key in pairs(ALL_DATA) do
      for subKey in pairs(ALL_DATA[key]) do
        if subKey == selectedItem then
          -- we found the proper list in "key"
          list = ALL_DATA[key];
          done = true;
          break;
        end;
      end;

      if done then break; end;
	end;
  end;

  -- depending on the currently selected item itself
  local data = list[itemBox.Items[itemBox.getItemIndex()]];


  if list == WPN_DATA then
    amount = 1;
    id = data[1];
    dura = data[2];
  elseif list == OFFHND_DATA then
	id = data.id;
	amount = 1;
	uses = data.uses;
  elseif list == AMMO_DATA then
	amount = data.amount;
	id = data.id;
  else
    -- anything else has durability 100 (10) regardless of type
    amount = 1;
    dura = 100;
    id = data;
  end;

  -- 3. write the adress record
  setMercItem(MERCS_BASE, num, slot, id, dura, amount, uses);
end

