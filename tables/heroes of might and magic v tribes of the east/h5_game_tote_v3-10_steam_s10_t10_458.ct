//=========================================
MyCode:
//=========================================
// At Main game screen
_MonHumanPlayer:
 push edx
 mov edx,eax

 call dword ptr [edx+00000184]     // Original code (chk player)
 test al,al
 je _ExitMHP

 push eax
 mov [pPlayer],ecx
 mov eax,[ecx+000001d0]
 mov [pMainHero],eax
 mov eax,[ecx+78]
 mov [iID],eax
 pop eax

 cmp dword ptr [iEnableMPR],0
 je _ExitMHP                       // Jump if feature is disabled

 push eax
 push ebx

 mov eax,ctMMON

 mov ebx,[ecx+54]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+54],ebx

 mov eax,ctMRES

 mov ebx,[ecx+3c]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+3c],ebx

 mov ebx,[ecx+40]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+40],ebx

 mov ebx,[ecx+44]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+44],ebx

 mov ebx,[ecx+48]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+48],ebx

 mov ebx,[ecx+4c]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+4c],ebx

 mov ebx,[ecx+50]
 cmp ebx,eax
 cmovs ebx,eax
 mov [ecx+50],ebx

 pop ebx
 pop eax

_ExitMHP:
 pop edx
 jmp _BackMHP                      // Back to main code

//=========================================
// When moving
_MonArmyMovement:
 push ebx
 push edx
 push esi

 mov ebx,[ecx+00000128]
 cmp ebx,[pPlayer]
 jne _ExitMAM

 mov [pHero],ecx

 cmp dword ptr [iEnableMAM],0
 je _MonAM00                       // Jump if feature is disabled

 mov eax,[ecx+00000130]

_MonAM00:
 cmp dword ptr [iEnableMTS],0
 je _ExitMAM                       // Jump if feature is disabled

 mov esi,[ecx+00000110]
 test esi,esi
 jz _ExitMAM

 mov ebx,ctMTSZ

_MonAM01:
 mov edx,[esi]
 test edx,edx
 jz _ExitMAM

 lea esi,[esi+04]

 cmp ebx,[edx+20]
 js _MonAM01

 mov [edx+20],ebx
 jmp _MonAM01
//--
_ExitMAM:
 pop esi
 pop edx
 pop ebx

 mov [ecx+0000012c],eax            // Original code (upd MP)
 jmp _BackMAM                      // Back to main code

//=========================================
// When cursor is over the castle
_MonCastleTroop:
 push ebx
 push ecx
 push edx
 push esi

 mov edx,eax

 mov ebx,[edx+08]
 cmp ebx,[iID]
 jne _ExitMCT

 mov [pCastle],edx

 cmp dword ptr [iEnableMCB],0
 je _MonCT00                       // Jump if feature is disabled

 mov dword ptr [edx+000000b0],1

_MonCT00:
 cmp dword ptr [iEnableMCT],0
 je _ExitMCT                       // Jump if feature is disabled

 mov esi,[edx+74]
 test esi,esi
 jz _ExitMCT

 mov ebx,ctMTSZ

_MonCT01:
 mov ecx,[esi]
 test ecx,ecx
 jz _ExitMCT

 lea esi,[esi+04]

 cmp ebx,[ecx+20]
 js _MonCT01

 mov [ecx+20],ebx
 jmp _MonCT01
//--
_ExitMCT:
 pop esi
 pop edx
 pop ecx
 pop ebx

 mov eax,[eax+74]                  // Original code (rd pTL)
 mov ebp,[eax+ecx*4]               // Original code
 jmp _BackMCT                      // Back to main code

//=========================================
 db '===========================&gt;'
 db 'CE6.3 Script by Recifense 030214'
//=========================================
// Variables
iEnableMPR:
 dd 0
iEnableMAM:
 dd 0
iEnableMTS:
 dd 0
iEnableMCT:
 dd 0
iEnableMCB:
 dd 0
iID:
 dd #99
pPlayer:
 dd MyCode
pMainHero:
 dd MyCode
pHero:
 dd 0
pCastle:
 dd 0

//=========================================
// Hacking Points
MHPR:
 jmp _MonHumanPlayer
 nop
_BackMHP:

MOAM:
 jmp _MonArmyMovement
 nop
_BackMAM:

MOCT:
 jmp _MonCastleTroop
 nop
_BackMCT:

//=========================================
// Script for Restoring Original Codes
[DISABLE]
MHPR:
// call dword ptr [eax+00000184]
 db ff 90 84 01 00 00

MOAM:
// mov [ecx+0000012c],eax
 db 89 81 2c 01 00 00

MOCT:
// mov eax,[eax+74]
// mov ebp,[eax+ecx*4]
