<CheatTable CheatEngineTableVersion="16">
  <CheatEntries>
    <CheatEntry>
      <ID>16</ID>
      <Description>"Infinite Health + Infinite Money"</Description>
      <Options moHideChildren="1"/>
      <Color>FF0000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>//Risk Of Rain v1.00
//Infinite Health + Infinite Money
//Steve Andrew

[ENABLE]

alloc(MultiScript,2048)
label(SetHealth)
label(SetMoney)
label(DoNormal)
label(munny1)
label(hp1)
label(enablehp)
label(enablehp2)
label(enablemon)
label(setmaxhp)
label(hp2)



registersymbol(enablehp)
registersymbol(enablehp2)
registersymbol(enablemon)
registersymbol(hp1)
registersymbol(hp2)
registersymbol(munny1)


MultiScript:
fistp qword [edx]
/*-----------------------------------------------------*/

     cmp [esp+10], c2 //commando
     je SetHealth
     cmp [esp+10], c4 //acrid
     je SetHealth
     cmp [esp+10], c5 //enforcer
     je SetHealth
     cmp [esp+10], c9 //bandit
     je SetHealth
     cmp [esp+10], cb //sniper
     je SetHealth
     cmp [esp+10], cc //engineer
     je SetHealth
     cmp [esp+10], cf //huntress
     je SetHealth


/*-------------------------------------------------------*/

     cmp [esp+10], c4 //commando
     je setmaxhp
     cmp [esp+10], c6 //acrid
     je setmaxhp
     cmp [esp+10], c7 //enforcer
     je setmaxhp
     cmp [esp+10], cb //bandit
     je setmaxhp
     cmp [esp+10], cd //sniper
     je setmaxhp
     cmp [esp+10], ce //engineer
     je setmaxhp
     cmp [esp+10], d1 //huntress
     je setmaxhp

/*-------------------------------------------------------*/

   cmp [esp+10],27
   je SetMoney

jmp DoNormal

SetHealth:
cmp dword ptr [enablehp],0
    je DoNormal
    cmp [esp+0c], 186d2 // Current HP check
    jne DoNormal
        fstp st(0)
        fild qword [hp1]
        jmp DoNormal

setmaxhp:

cmp dword ptr [enablehp2],0
    je DoNormal
    cmp [esp+0c], 18790 // Max HP check
    jne DoNormal
        fstp st(0)
        fild qword [hp2]
        jmp DoNormal


SetMoney:
    cmp dword ptr [enablemon],0
        je DoNormal
            cmp [esp+0c],187d6 //double check its actually money
            jne DoNormal
            fstp st(0)
            fild qword [munny1]



DoNormal:
fistp qword [ecx+edx]
ret

enablemon:
dd 0
enablehp:
dd 0
enablehp2:
dd 0


hp1:
dq (double)9999

hp2:
dq (double)9999

munny1:
dq (double)999999999


"RoR v1.0.0 Humble.exe"+4544:
jmp MultiScript



[DISABLE]

"RoR v1.0.0 Humble.exe"+4544:
fistp qword [edx]
fistp qword [ecx+edx]

dealloc(MultiScript)
unregistersymbol(hp1)
unregistersymbol(hp2)
unregistersymbol(munny1)
unregistersymbol(enablehp)
unregistersymbol(enablehp2)
unregistersymbol(enablemon)



</AssemblerScript>
