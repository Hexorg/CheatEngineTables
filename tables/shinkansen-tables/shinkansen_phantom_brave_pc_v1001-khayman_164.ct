//Phantom Brave PC.exe+4FFC1 - A3 54002900           - mov ["Phantom Brave PC.exe"+170054],eax
//Phantom Brave PC.exe+4FFC6 - 8B 0D B0693F00        - mov ecx,["Phantom Brave PC.exe"+2D69B0]
// Accesses Time Played
//Phantom Brave PC.exe+4FFCC - FE 41 0C              - inc byte ptr [ecx+0C]
//Phantom Brave PC.exe+4FFCF - 80 79 0C 3C           - cmp byte ptr [ecx+0C],3C
//Phantom Brave PC.exe+4FFD3 - 72 31                 - jb "Phantom Brave PC.exe"+50006
//Phantom Brave PC.exe+4FFD5 - FE 41 0B              - inc byte ptr [ecx+0B]
aobscan(playerstats_searchedAOB, FE 41 0C 80 79 0C 3C 72 31)

alloc(playerstats_newmem, 2048, playerstats_searchedAOB)
alloc(playerstats_temp1, 4, playerstats_searchedAOB)


playerstats_newmem:
mov [playerstats_temp1], ebx

cmp dword ptr [playerstats_update_enable], #0
je playerstats_update_exit
mov [playerstats_basepointer], ecx
mov ebx, [playerstats_character_value]
add ebx, #736
add ebx, ecx
mov [playerstats_character_basepointer], ebx
playerstats_update_exit:

cmp dword ptr [playerstats_set_enable], #0
je playerstats_set_exit
playerstats_set_exit:

cmp dword ptr [playerstats_borset_enable], #0
je playerstats_borset_exit
mov [ecx+2c0], #999999999
playerstats_borset_exit:

mov ebx, [playerstats_temp1]


playerstats_originalcode:
db FE 41 0C 80 79 0C 3C
//inc byte ptr [ecx+0C]
//cmp byte ptr [ecx+0C],3C


playerstats_exit:
jmp playerstats_returnhere


playerstats_searchedAOB:
playerstats_savedAOB:
jmp playerstats_newmem
nop
nop
playerstats_returnhere:


[DISABLE]
playerstats_savedAOB:
db FE 41 0C 80 79 0C 3C
//inc byte ptr [ecx+0C]
//cmp byte ptr [ecx+0C],3C

unregistersymbol(playerstats_savedAOB)

dealloc(playerstats_newmem)
dealloc(playerstats_temp1)

