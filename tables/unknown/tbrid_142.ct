//when entering this function eax=&amp;PlayerData
GetSpeedBonus:   //speed bonus is now total xp/1000 = (PM01 + PM02 + PM03) xp/1000
push ecx         //save ecx, may not be needed
call GetTotalXp
//now lets' do eax=TotalXp/1000
xor edx,edx
mov ecx,SPEED_DIVISOR
div ecx
pop ecx
retn

