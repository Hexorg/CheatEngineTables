   memscan_firstScan(EnScan,soExactValue,vtDword,"",EnBase,"0","0x00000000","0x70000000","",fsmAligned,"4",false,false,true,false)
   memscan_waitTillDone(EnScan)
   EnResult = createFoundList(EnScan)
