assert(FHPR,8b 90 20 09 00 00 85 d2 8b b8 a8 0a 00 00 ?? ?? 8b 88 24 09 00 00)
assert(GDMD,f3 0f 11 96 90 00 00 00 e8 ?? ?? ?? ?? f3 0f 10 44 24 50 83 c4 18)
assert(MOPR,8b 4b 18 8b 53 1c 89 4c 24 34 89 54 24 38 33 c0 8d 4b 08 ?? ?? 8d)

