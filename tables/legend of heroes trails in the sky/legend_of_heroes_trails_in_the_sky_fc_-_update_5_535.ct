function readShortint(address)
  return bAnd(0xFFFF, readInteger(address) )
end

function writeShortint(address,value)
  local Int = readInteger(address)
  writeInteger(address,value+bAnd(0xFFFF0000,Int))
end

function FormShow(sender)
  if ItemBase == nil then
    local Scan = AOBScan(ItemBaseAOB)
    ItemBase = Scan[0]
    ItemBase = getAddress("[" .. ItemBase .. "+1]")
    Scan = nil
  end
end

function BUpdateListClick(sender)
  ItemSwap_LItems:clear()
  local ICount = nil
  local List = ItemSwap_LItems:getItems()
  for i=0,300 do
      local Max = readShortint(ItemBase+i*4)
      if Max == 0 then ICount = i-1 break end
  end
  for i=0,ICount+5 do
    local IName = List:Add()
    local CIIndex = readShortint(ItemBase+i*4)
    for k,v in pairs(ItemTable) do
      if v.ItemID == CIIndex then
        IName:setCaption(v.ItemName)
        break
      end
    end
    local INameS = IName:getSubItems()
    INameS:add(readShortint(ItemBase+i*4+2))
  end
end

function BQuantAllClick(sender)
  local Quant = tonumber(ItemSwap_EQuant.Text)
  if Quant == nil then showMessage("Please enter a valid number.") end
  for i = 0, ItemSwap_LItems.Items.Count-1 do
    writeBytes(ItemBase+i*4+2,Quant)
  end
  BUpdateListClick()
end

function BQuantSelClick(sender)
  local Quant = tonumber(ItemSwap_EQuant.Text)
  local Index = ItemSwap_LItems:getItemIndex()
  if Index == -1 then showMessage("Please select an item first.") return end
  if Quant == nil then showMessage("Please enter a valid number.") return end
  writeBytes(ItemBase+Index*4+2,Quant)
  BUpdateListClick()
end

function BSwapItemClick(sender)
  local Index = ItemSwap_LItems:getItemIndex()
  if Index == -1 then showMessage("Please select an item first.") return end
  --ItemTable[ItemSwap_CBItems.ItemIndex+1].ItemName
  local CBIndex = ItemSwap_CBItems.ItemIndex+1
  writeShortint(ItemBase+Index*4,ItemTable[CBIndex].ItemID)
  BUpdateListClick()
end


