====================================
1) Click "Search player structure" and wait 5 seconds, then expand Hearts, Items, etc...
2) Click "Search current floor" will show the current floor number, edit floor number and beat current floor, then you will be teleported to that floor.
3) Click "The Lost unlocker!" and go die/suicide, you will unlock The Lost character.
4) Click "Pedestal Script" then find a pedestal, pick the item you want to pick up in place of the item you see on the pedestal by editing the "Next Item" field. You can also build up a queue of items, that can be picke up from pedestals by using the "Add Item"/"Add Items" scripts.
5) Click "Search Stats" to edit your scores like death counts, mom kills, eden tokens, etc... You must ennter to the stats menu! Then click the script
6) In the Pill Effects section you can edit which pill effects are in the pool for the current run. Values that appear to have two effects (e.g. Range Down/Up) have the negative effect starting out but change to the positive one when you pick up an item which prevents negative pill effects (e.g. PhD)
</Comments>
  <LuaScript>function setup()
  local card_pill_ids = {52, 187, 185}
  local attack_style_ids = {136, 143, 142, 141, 140, 139, 138, 137, 144,
                            145, 146, 147, 148, 149, 150, 151, 165, 127,
                            126, 125, 124, 123, 166, 121, 128, 129, 130,
                            131, 132, 133, 134, 135}
  local invulnerability_ids = {252, 253}
  local seeds_filename = "seeds.txt"

  enableMTM,disableMTM = setup_MoveToMouse()
  makeBitsToggle(card_pill_ids)
  makeBitsToggle(attack_style_ids)
  makeBitsToggle(invulnerability_ids)
  enableSL,disableSL = setup_SeedLog(seeds_filename)
  pedestalScripts()
end





function setup_MoveToMouse()
  function wasdKeys(s,a,w,d)
    if w then keyDown(VK_W) else keyUp(VK_W) end
    if a then keyDown(VK_A) else keyUp(VK_A) end
    if s then keyDown(VK_S) else keyUp(VK_S) end
    if d then keyDown(VK_D) else keyUp(VK_D) end
  end

  function moveToMouse()
    local sx = 2*readFloat("[pPlayer]+35c")
    local sy = 2*readFloat("[pPlayer]+360")
    local mx, my = getMousePos()
    local vx = mx-sx
    local vy = my-sy-30

    local len = math.sqrt(vx*vx+vy*vy)
    if len &lt; 20 then
      wasdKeys(false, false, false, false)
      return
    end

    local phi = math.atan2(vy, vx)
    local quad = 4*phi/math.pi + 0.5
    if quad &lt; 0 then quad = quad + 8 end

    if     quad &lt; 1 then wasdKeys(false, false, false,  true)
    elseif quad &lt; 2 then wasdKeys( true, false, false,  true)
    elseif quad &lt; 3 then wasdKeys( true, false, false, false)
    elseif quad &lt; 4 then wasdKeys( true,  true, false, false)
    elseif quad &lt; 5 then wasdKeys(false,  true, false, false)
    elseif quad &lt; 6 then wasdKeys(false,  true,  true, false)
    elseif quad &lt; 7 then wasdKeys(false, false,  true, false)
    else                 wasdKeys(false, false,  true,  true) end

  end

  local mtm_timer = createTimer(nil, false)
  mtm_timer:setInterval(50)
  mtm_timer:setOnTimer(moveToMouse)

  local enableMTM = function()
    mtm_timer:setEnabled(true)
  end

  local disableMTM = function()
    mtm_timer:setEnabled(false)
    wasdKeys(false, false, false, false)
  end

  return enableMTM, disableMTM
end




function makeToggleable(mr)
  local closure = function()
    if mr.Value == "1" then
      mr.Value = "0"
    else
      mr.Value = "1"
    end
  end
  return closure
end

function makeBitsToggle(toggle_ids)
  local al = getAddressList()
  for i,id in ipairs(toggle_ids) do
    local mr = al:getMemoryRecordByID(id)
    mr.OnActivate = makeToggleable(mr)
  end
end



function setup_SeedLog(filename)
  local old_seed = ""
  function isSeed(s)
    if s:len() ~= 9 then
      return false
    elseif s:byte(5) ~= 32 then
      return false
    elseif s:sub(1,4):match("%W") then
      return false
    elseif s:sub(6,9):match("%W") then
      return false
    end
    return true
  end
  function logSeed()
    local seed = readString("[isaac-ng.exe+21E18C]+2ed44", 10)
    if seed == old_seed then
      return
    end
    if isSeed(seed) then
      local f = io.open(filename, "a")
      f:write(seed, "\n")
      f:close()
    end
    old_seed = seed
  end

  local sl_timer = createTimer(nil, false)
  sl_timer:setInterval(1000)
  sl_timer:setOnTimer(logSeed)

  local enableSL = function()
    sl_timer:setEnabled(true)
  end

  local disableSL = function()
    sl_timer:setEnabled(false)
    old_seed = ""
  end

  return enableSL, disableSL
end


function pedestalScripts()

  local al = getAddressList()
  local mr = al:getMemoryRecordByID(221)

  function joinList(list)
    if #list == 0 then
      return ""
    end
    local string = tostring(list[1])
    for i = 2,#list do
      string = string .. " " .. tostring(list[i])
    end
    return string
  end

  function splitList(text)
    local list = {}; local pos = 1; local delimiter = " "
    while 1 do
      local first, last = string.find(text, delimiter, pos)
      if first then
        table.insert(list, tonumber(string.sub(text, pos, first-1)))
        pos = last+1
      else
        table.insert(list, tonumber(string.sub(text, pos)))
        break
      end
    end
    return list
  end

  function pedestal_getQueue()
    local queue = {}
    local base = getAddress("pe_queue")
    local i = 1
    while true do
      local v = readInteger(base)
      if v == 0 then
        break
      end
      queue[i] = v
      i = i+1
      base = base+4
    end
    return queue
  end

  function pedestal_setQueue(queue)
    local base = getAddress("pe_queue")
    for i=1,#queue do
      writeInteger(base, queue[i])
      base = base + 4
    end
    writeInteger(base,0)
  end

  function pedestal_addItem()
    if not mr.Active then
      return false
    end
    local id = readInteger("pe_addid")
    if id == 0 then
      return false
    end
    local queue = pedestal_getQueue()
    queue[#queue+1] = id
    pedestal_setQueue(queue)
    writeInteger("pe_queue_len", readInteger("pe_queue_len")+1)
    return false
  end

  function pedestal_addItems()
    if not mr.Active then
      return false
    end
    local queue = pedestal_getQueue()
    local news = inputQuery("Add Items to Pedestal Queue", "queue:", joinList(queue))
    if news == nil then
      return false
    end
    local q2 = splitList(news)
    pedestal_setQueue(q2)
    return false
  end

  function pedestal_clearQueue()
    writeInteger("pe_queue", 0)
    writeInteger("pe_queue_len", 0)
  end

  al:getMemoryRecordByID(227).OnActivate = pedestal_addItem
  al:getMemoryRecordByID(228).OnActivate = pedestal_addItems
  al:getMemoryRecordByID(229).OnActivate = pedestal_clearQueue
  makeBitsToggle({224})
end


setup()
