 mov eax,[esi+0c]
 test eax,eax
 jz _ExitMPP

 mov eax,[eax+18]
 cmp eax,[pPlayer]
 jne _ExitMPP

 movss [esi+68],xmm0

_ExitMPP:
 pop eax

 movss xmm1,[esi+68]               // Original code (rd pp)
 jmp _BackMPP                      // Back to main code

