---------- item base scanner ----------

function getItemBase(itemBaseName, pItem)
  -- result of the scan
  local result = nil

  -- validate selected item
  local pLastItemChanged = readInteger(pItem)
  if (pLastItemChanged == 0) then
    showMessage("Item editor values are not populated. A valid item must be selected to use this script.")
    return result
  end

  -- validate target item
  if (item == "") then
    showMessage("No target item specified. Select the item you want to scan for first.")
    return result
  end

  -- set up
  local scan = createMemScan()
  local pStart = getAddress("[["..pItem.."]+10]") - 0x10000000
  local pEnd = pStart + 0x20000000

  -- scan
  memscan_firstScan(scan, soExactValue, vtString, 0, itemBaseName, nil, pStart, pEnd, "+W-X", 1, 1, true, false, false, true)
  memscan_waitTillDone(scan)

  -- find correct result
  local results = createFoundList(scan)
  foundlist_initialize(results)
  local count = foundlist_getCount(results)
  local index = 0
  local address = memscan_getOnlyResult(scan)
  while (index &lt; count) do
    -- fetch result
    local address = foundlist_getAddress(results, index)
    local name = readString(address, 1024)
    if (name == itemBaseName) then
      break
    end
    index = index + 1
  end

  -- parse result
  local address = tonumber(foundlist_getAddress(results, index), 16)
  if (address ~= nil) then
    -- find local pointer
    local pointerscan = createMemScan()
    memscan_returnOnlyOneResult(pointerscan, true)
    memscan_firstScan(pointerscan, soExactValue, 3, 0, address, nil, address - 0x1000, address, "+W-X", 1, 1, false, false, false, false)
    memscan_waitTillDone(pointerscan)
    local pointer = memscan_getOnlyResult(pointerscan)

    -- register label
    if (pointer ~= nil) then
      result = pointer - 0x10
    end

    -- clean up
    object_destroy(pointerscan)
  end

  -- clean up
  object_destroy(scan)

  -- result the result
  return result
end

function scan_itemBase(destination, item)
  local pItemBase = getItemBase(item, "pLastItemChanged")
  if (pItemBase == nil) then
    showMessage("Could not find item " .. item)
  else
    writeInteger(getAddress(destination), pItemBase)
  end
end

---------- item base editor ----------

itemBaseEditor_pItem = nil

function itemBaseEditor_show(pItem)
  itemBaseEditor_pItem = pItem
  itemBaseEditor.cancel.OnClick = itemBaseEditor_onCancel
  itemBaseEditor.submit.OnClick = itemBaseEditor_onSubmit
  itemBaseEditor.value.OnKeyPress = itemBaseEditor_onKeyPress
  itemBaseEditor.show()
end

function itemBaseEditor_hide()
  itemBaseEditor.hide()
end

function itemBaseEditor_onCancel()
  itemBaseEditor.hide()
end

function itemBaseEditor_onSubmit()
  if (itemBaseEditor_pItem == nil) then
    itemBaseEditor_pItem = "pLastItemChanged"
  end
  local itemBaseName = itemBaseEditor.value.Text
  local pItemBase = getItemBase(itemBaseName, itemBaseEditor_pItem)
  if (pItemBase == nil) then
    showMessage("Could not find item "..itemBaseName)
  else
    local pItemBasePointer = getAddress("["..itemBaseEditor_pItem.."]+10")
    local pOldItemBase = getAddress("[["..itemBaseEditor_pItem.."]+10]")
    local pItemDisplayString = getAddress("["..itemBaseEditor_pItem.."]+30")
    local pItemBaseDisplayString = pItemBase + 0x1c
    local pDisplayString = readInteger(pItemBaseDisplayString)
    showMessage('Changing item base at '..dectohex(pItemBasePointer)..' from '..dectohex(pOldItemBase).." to "..dectohex(pItemBase))
    -- showMessage('Changing item display string at '..dectohex(pItemDisplayString)..' to the one at '..dectohex(pItemBaseDisplayString)..' : '..dectohex(pDisplayString))
    writeInteger(pItemBasePointer, pItemBase)
    writeInteger(pItemDisplayString, pDisplayString)
    itemBaseEditor.hide()
  end
end

function itemBaseEditor_onKeyPress(sender, key)
  if (string.byte(key) == 13) then
    itemBaseEditor_onSubmit()
  else
    return key
  end
end

itemBaseEditor.hide()

---------- combat stats scanner ----------

function printCombatStats()
  -- set up
  local scan = createMemScan()
  local pStart = 0x00000000
  local pEnd = 0xFFFFFFFF
  local sPrefix = "DA3/Combat/Stats/"

  -- scan
  memscan_firstScan(scan, soExactValue, vtString, 0, sPrefix, nil, pStart, pEnd, "+W-X", 1, 1, true, false, false, true)
  memscan_waitTillDone(scan)

  -- get results
  local results = createFoundList(scan)
  foundlist_initialize(results)
  local count = foundlist_getCount(results)
  local index = 0
  while (index &lt; count) do
    -- fetch result
    local address = getAddress(foundlist_getAddress(results, index))
    local label = readString(address)

    -- find local pointer
    local pointerscan = createMemScan()
    memscan_returnOnlyOneResult(pointerscan, true)
    memscan_firstScan(pointerscan, soExactValue, 3, 0, address, nil, address - 0x1000, address, "+W-X", 1, 1, false, false, false, false)
    memscan_waitTillDone(pointerscan)
    local pointerresult = memscan_getOnlyResult(pointerscan)

    -- register label
    if (pointerresult ~= nil) then
      pointerresult = pointerresult - 0x10
      print(pointerresult .. ": " .. label)
    end

    -- clean up
    object_destroy(pointerscan)

    index = index + 1
  end

  -- clean up
  object_destroy(scan)
end

