newInvul:
  cmp dword ptr [pBase],0 // check if pBase is loaded
  jng codeInvul
  push eax // player check...
  mov eax,dword ptr [pBase]
  mov eax,dword ptr [eax+10]
  cmp dword ptr [ebp+0C],eax
  pop eax // ...player check
  jne codeInvul
  cmp byte ptr [bInvuln],1 // cheat check
  jne codeInvul
  cmp byte ptr [edx+14],00 // cmp edx,edx // fake compare
  jmp returnInvul
codeInvul:
  cmp ecx,esi
  ja "Game.exe"+25B392
  cmp byte ptr [edx+14],00
  jmp returnInvul


newHealth:
  cmp dword ptr [pBase],0 // check if pBase is loaded
  jng codeHealth
  push eax // player check
  mov eax,dword ptr [pBase]
  mov eax,dword ptr [eax+10]
  cmp edi,eax
  pop eax // ...player check
  jne enemyHealth
  cmp byte ptr [bHealth],1 // cheat check
  jne codeHealth
  fstp dword ptr [esi]
  fld dword ptr [edi+000001A8] // load max health
  jmp codeHealth
enemyHealth:
  cmp byte ptr [bEnemy],1 // cheat check
  jne codeHealth
  cmp dword ptr [edi+000001AC],(float)1 // check if health &lt;= 1
  jle codeHealth
  fstp dword ptr [esi]
  fld1 // load 1.0
  jmp codeHealth
codeHealth:
  fstp dword ptr [esi]
  mov edx,[edi+7C]
  jmp returnHealth


newStamina:
  cmp byte ptr [bStamina],1 // cheat check
  jne codeStamina
  fstp dword ptr [ecx+00000148]
  fld dword ptr [ecx+00000150] // load max stamina
  jmp codeStamina
codeStamina:
  fst dword ptr [ecx+00000148] // store current stamina
  jmp returnStamina


newIntenso_1:
