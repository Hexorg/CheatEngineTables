      <AssemblerScript>[Enable]

alloc(ne_1,128,RGSS102E.dll) //Name Editor
aobscanmodule(ne_aob1,RGSS102E.dll,8A 08 40 84 C9 * * 2B C2 89 44 24 30)
registersymbol(ne_aob1)
label(returnhere_ne_1)

label(findNamePointer_status) //Find Name Pointer
registersymbol(findNamePointer_status)
label(currentName_1) //Current Name (Letters 1 - 8)
registersymbol(currentName_1)
label(currentName_2) //Current (Letters 9 - 12)
registersymbol(currentName_2)
globalalloc(p_isName,4) //Name Pointer

label(ne_orig) //Original Code

//--------------------------------------------------//
ne_1:
cmp [findNamePointer_status],1
jne ne_orig
push ebx
mov ebx,[currentName_1]
cmp [eax],ebx
pop ebx
jne ne_orig
mov [p_isName],eax
mov cl,[eax]
inc eax
test cl,cl
mov [findNamePointer_status],0
jmp returnhere_ne_1

findNamePointer_status:
dd 0

currentName_1:
dq 0

currentName_2:
dd 0
//--------------------------------------------------//
ne_orig:
mov cl,[eax]
inc eax
test cl,cl
jmp returnhere_ne_1
//--------------------------------------------------//

ne_aob1:
jmp ne_1
returnhere_ne_1:

[Disable]

ne_aob1:
db 8A 08 40 84 C9
unregistersymbol(ne_aob1)
dealloc(ne_1)

unregistersymbol(findNamePointer_status)
unregistersymbol(currentName_1)
unregistersymbol(currentName_2)
unregistersymbol(p_isName)
