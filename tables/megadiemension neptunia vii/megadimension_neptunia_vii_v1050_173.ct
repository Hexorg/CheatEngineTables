          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(itemsstats_savedAOB)
label(itemsstats_returnhere)
label(itemsstats_originalcode)

label(itemsstats_savedAOB2)
label(itemsstats_returnhere2)
label(itemsstats_originalcode2)

label(itemsstats_update_exit)
label(itemsstats_add_exit)
label(itemsstats_set_exit)

