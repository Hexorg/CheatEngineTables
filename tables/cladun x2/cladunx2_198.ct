    <CheatEntry>
      <ID>116</ID>
      <Description>"Gate Manipulation"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>// Gate manipulation in Cladun X2
// by HenryEx

[ENABLE]
alloc(Gates,1024)

// Declaration section
registersymbol(Gates)
label(_GatesMon)
label(_GatesItem)
label(_GatesTitle)
label(_GatesWarp)
label(_ExitG1)
label(_ExitG2)
label(_ExitG3)
label(_ExitG4)
label(_BackG1)
label(_BackG2)
label(_BackG3)
label(_BackG4)

//=========================================
// Hacking Points
"CladunX2.exe"+6B578:
 jmp _GatesMon
 db 90 90 90
_BackG1:

"CladunX2.exe"+6B776:
 jmp _GatesItem
 db 90 90 90
_BackG2:

"CladunX2.exe"+6B7DB:
 jmp _GatesTitle
 db 90 90 90
_BackG3:

"CladunX2.exe"+6B8F3:
 jmp _GatesWarp
 db 90 90
_BackG4:

//=========================================
// Variables
Gates:
 dd 0


//=========================================
// Monster Level modifier! Keep it low
// eax = new lvl, ebx = pointer, ecx = free?
_GatesMon:
 cmp byte ptr [Gates],00      // check if feature is active
 jz _ExitG1                   // if not, skip this

 test byte ptr [Gates],80   // is activation signed (negative)?
 jns +7                       // If not, skip the next line
 cmp [ebx+0C],eax             // check again, but this time for...
 jge +7                       // ... smaller new mod
 jmp short _ExitG1            // apply the greater new mod

 cmp [ebx+0C],eax             // check new mlvl-mod against current mod
 jge short _ExitG1            // if current &gt;= new, exit as normal
 mov eax,[ebx+0C]             // if new mod was greater, overwrite with current

_ExitG1:
 cmp byte ptr [esp+13],00     // Original code
 mov [ebx+0C],eax             // Original code
 jmp _BackG1                  // Back to game

//=========================================
// Item Drop modifier! Keep it high
// eax = new mod, ebx = pointer, ecx = free?
_GatesItem:
 call CladunX2.exe+6B1A0      // Original code

 cmp byte ptr [Gates+1],00    // check if feature is active
 jz _ExitG2                   // if not, skip this

 test byte ptr [Gates+1],80   // is activation signed (negative)?
 jns +2                       // If not, skip the next line
 neg eax                      // invert the mod's sign ( + -&gt; - )

 cmp [ebx+10],eax             // check new item-mod against current mod
 jle _ExitG2                  // if current =&lt; new, exit as normal
 mov eax,[ebx+10]             // if new mod was smaller, overwrite with current

_ExitG2:
 mov [ebx+10],eax             // Original code
 jmp _BackG2                  // Back to game

//=========================================
// Rare Title modifier! Keep it high
// eax = new mod, ebx = pointer, ecx = free?
_GatesTitle:
 call CladunX2.exe+6B1A0      // Original code

 cmp byte ptr [Gates+2],00    // check if feature is active
 jz _ExitG3                   // if not, skip this

 test byte ptr [Gates+2],80   // is activation signed (negative)?
 jns +2                       // If not, skip the next line
 neg eax                      // invert the mod's sign ( + -&gt; - )

 cmp [ebx+14],eax             // check new rare-mod against current mod
 jle _ExitG3                  // if current =&lt; new, exit as normal
 mov eax,[ebx+14]             // if new mod was smaller, overwrite with current

_ExitG3:
 mov [ebx+14],eax             // Original code
 jmp _BackG3                  // Back to game

//=========================================
// Floor warp modifier! Keep it high?
// eax = soon-to-be new mod, ebx = pointer
_GatesWarp:
 mov eax,[eax]                // Original code (gets new mod)

 cmp byte ptr [Gates+3],00    // check if feature is active
 jz _ExitG4                   // if not, skip this

 test byte ptr [Gates+3],80   // is activation signed (negative)?
 jns +2                       // If not, skip the next line
 neg eax                      // invert the mod's sign ( + -&gt; - )

 cmp [ebx+18],eax             // check new warp-mod against current mod
 jle _ExitG4                  // if current =&lt; new, exit as normal
 mov eax,[ebx+18]             // if new mod was smaller, overwrite with current

_ExitG4:
 cmp byte ptr [esp+13],00     // Original code
 jmp _BackG4                  // Back to game


[DISABLE]
"CladunX2.exe"+6B578:
 cmp byte ptr [esp+13],00
 mov [ebx+0C],eax
// db 80 7C 24 13 00 89 43 0C

"CladunX2.exe"+6B776:
 call CladunX2.exe+6B1A0
 mov [ebx+10],eax
// db E8 25 FA FF FF 89 43 10

"CladunX2.exe"+6B7DB:
 call CladunX2.exe+6B1A0
 mov [ebx+14],eax
// db E8 C0 F9 FF FF 89 43 14

"CladunX2.exe"+6B8F3:
 cmp byte ptr [esp+13],00
 mov eax,[eax]
// db 80 7C 24 13 00 8B 00

unregistersymbol(Gates)
dealloc(Gates)
</AssemblerScript>
