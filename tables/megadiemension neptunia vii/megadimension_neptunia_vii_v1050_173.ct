aobscanmodule(charstats_searchedAOB,NeptuniaVII.exe,8B 02 8B 52 04 5F 5E 5B) // should be unique

alloc(charstats_newmem, 2048)

charstats_newmem:
  cmp dword ptr [charstats_update_enable], #0
  je charstats_update_exit
  mov [charstats_basepointer], esi
  mov [charstats_basepointer2], edx
charstats_update_exit:

  cmp dword ptr [charstats_set_enable], #0
  je charstats_set_exit
  mov dword ptr [esi+20],#99999 //HP
  mov dword ptr [esi+28],#9999 //SP
  mov dword ptr [esi+2C],#9999 //STR
  mov dword ptr [esi+30],#9999 //VIT
  mov dword ptr [esi+34],#9999 //INT
  mov dword ptr [esi+38],#9999 //MEN
  mov dword ptr [esi+3C],#9999 //AGI
  mov dword ptr [esi+40],#9999 //TEC
  mov dword ptr [esi+44],#9999 //LUC
  mov dword ptr [esi+48],#100 //MOV
charstats_set_exit:

  push eax

  cmp dword ptr [charstats_spset_enable], #0
  je charstats_spset_exit
  mov eax, [esi+28]
  mov dword ptr [edx+10], eax
charstats_spset_exit:

  cmp dword ptr [charstats_hpset_enable], #0
  je charstats_hpset_exit
  mov eax, [esi+20]
  mov dword ptr [edx], eax
charstats_hpset_exit:

  pop eax

  cmp dword ptr [charstats_hddset_enable], #0
  je charstats_hddset_exit
  push edx
  call cpu_check
  cmp edx,#0
  je charstats_hddset_end
  cmp edx,#1
  je charstats_hddset_next
  or [esi+1],0x09
  jmp charstats_hddset_end
charstats_hddset_next:
  or [esi+1],0x19
charstats_hddset_end:
  pop edx
charstats_hddset_exit:

charstats_originalcode:
  mov eax,[edx]
  mov edx,[edx+04]

charstats_exit:
  jmp charstats_returnhere

cpu_check:
  cmp word ptr [esi+4],0x1 //Neptune
  je cpu_check_main
  cmp word ptr [esi+4],0x2 //Noire
  je cpu_check_main
  cmp word ptr [esi+4],0x3 //Blanc
  je cpu_check_main
  cmp word ptr [esi+4],0x4 //Vert
  je cpu_check_main
  cmp word ptr [esi+4],0x5 //Nepgear
  je cpu_check_side
  cmp word ptr [esi+4],0x6 //Uni
  je cpu_check_side
  cmp word ptr [esi+4],0x7 //Rom
  je cpu_check_side
  cmp word ptr [esi+4],0x8 //Ram
  je cpu_check_side
  cmp word ptr [esi+4],0xD //S-Sha
  je cpu_check_side
  cmp word ptr [esi+4],0xE //B-Sha
  je cpu_check_side
  cmp word ptr [esi+4],0xF //C-Sha
  je cpu_check_side
  cmp word ptr [esi+4],0x10 //K-Sha
  je cpu_check_side
  mov edx,#0
  ret
cpu_check_main:
  mov edx,#1
  ret
cpu_check_side:
  mov edx,#2
  ret

charstats_searchedAOB:
charstats_savedAOB:
  jmp charstats_newmem
charstats_returnhere:

[DISABLE]
charstats_savedAOB:
  db 8B 02 8B 52 04

unregistersymbol(charstats_savedAOB)

dealloc(charstats_newmem)
