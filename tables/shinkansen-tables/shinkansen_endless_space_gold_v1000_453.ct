          <Description>"StarSystem Base Address Scan (To Activate: View System, To Update: View System)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(StarSystem_savedAOB)
label(StarSystem_returnhere)
label(StarSystem_originalcode)
label(StarSystem_exit)

label(StarSystem_update_exit)
label(StarSystem_set_exit)

registersymbol(StarSystem_savedAOB)

//219EB34C - 83 EC 04              - sub esp,04
//219EB34F - 8B 7D 08              - mov edi,[ebp+08]
// StarSystem:get_CurrentOwnership+a
//219EB352 - 8B 47 58              - mov eax,[edi+58]
//219EB355 - 85 C0                 - test eax,eax
//219EB357 - 75 07                 - jne 219EB360
//219EB359 - D9EE                  - fldz
//219EB35B - E9 58000000           - jmp 219EB3B8
//219EB360 - 8B 47 1C              - mov eax,[edi+1C]
aobscan(StarSystem_searchedAOB, D9 87 B8 00 00 00 D9 5D CC D9 45 CC)

alloc(StarSystem_newmem, 2048)
alloc(StarSystem_temp1, 4)


StarSystem_newmem:
mov [StarSystem_temp1], eax

// Player check
cmp dword ptr [edi+58], #0
je StarSystem_set_exit
mov eax, [edi+58]
cmp dword ptr [eax+a0], #1
jne StarSystem_set_exit

cmp dword ptr [StarSystem_update_enable], #0
je StarSystem_update_exit
mov [StarSystem_basepointer], edi
StarSystem_update_exit:

cmp dword ptr [StarSystem_set_enable], #0
je StarSystem_set_exit
mov [edi+b8], (float)9999999
StarSystem_set_exit:

mov eax, [StarSystem_temp1]


StarSystem_originalcode:
db D9 87 B8 00 00 00
//fld dword ptr [edi+000000B8]


StarSystem_exit:
jmp StarSystem_returnhere


StarSystem_searchedAOB:
StarSystem_savedAOB:
jmp StarSystem_newmem
nop
StarSystem_returnhere:


[DISABLE]
StarSystem_savedAOB:
db D9 87 B8 00 00 00
//fld dword ptr [edi+000000B8]

unregistersymbol(StarSystem_savedAOB)

dealloc(StarSystem_newmem)
dealloc(StarSystem_temp1)

