--[[
- Loot Splosion Advance
]]--
LootResults = nil
LootCount = nil

function LootSplosionAdvance()
  if CheckEnabled() == false then return end

  DebugLog("@Function: LootSplosionAdvance()")
  if GetBaseTables() == false then return end
  if BaseItemPoolList == nil then
    DebugLog("-Scanning for the base item pool list")
    for i=0,9000 do
      if GetString(readInteger(BaseEntityTable+(i*4)),90) == "Default__ItemPoolListDefinition" then
        BaseItemPoolList = readInteger(readInteger(BaseEntityTable+(i*4)))
        DebugLog("-Base item pool list found")
        break
      end
    end
  end
  if BaseItemPoolList == nil then
    DebugLog("!Error: Base item pool list not found")
    return
  end
  if LootResults == nil or LootCount == nil then
    DebugLog("-Scanning base item list pools")
    LootResults = ScanForItDword(BaseItemPoolList)
    LootCount = foundlist_getCount(LootResults) - 1
    DebugLog("-Scan Results: Found " .. LootCount .. " items")
  end
  local Finder = 0
  DebugLog("-Scanning for the base item list tables")
  for i=1,LootCount do
    local address = foundlist_getAddress(LootResults,i)
    if GetString(getAddress(address), 90) == "StandardEnemyGunsAndGear" then
      DebugLog("-StandardEnemyGunsAndGear Found")
      Finder = Finder + 1
      registerSymbol("StandardEnemy",address,true)
    elseif GetString(getAddress(address), 90) == "ChubbyEnemyGunsAndGear" then
      DebugLog("-ChubbyEnemyGunsAndGear Found")
      Finder = Finder + 1
      registerSymbol("ChubbyEnemy",address,true)
    elseif GetString(getAddress(address), 90) == "BadassEnemyGunsAndGear" then
      DebugLog("-BadassEnemyGunsAndGear Found")
      Finder = Finder + 1
      registerSymbol("BadAssEnemy",address,true)
    elseif GetString(getAddress(address), 90) == "LootMidgetLoot" then
      DebugLog("-LootMidgetLoot Found")
      Finder = Finder + 1
      registerSymbol("LootMidgetEnemy",address,true)
    end
    if Finder &gt;= 4 then break end
  end
  DebugLog("-Finished retrieving base item list pools")
  beep()
end

function SploAdvanceDisable()
  DebugLog("@Function: SploAdvanceDisable()")
  unregisterSymbol("StandardEnemy")
  unregisterSymbol("ChubbyEnemy")
  unregisterSymbol("BadAssEnemy")
  unregisterSymbol("LootMidgetEnemy")
end

