// pBase is required for Health cheats!!!
newBase: // access health
  mov [pBase],esi // read here
  jmp codeBase
codeBase:
  fld dword ptr [ecx+000001AC]
  jmp returnBase

//newBase: // access stamina
  //mov [pBase],edx // read here
