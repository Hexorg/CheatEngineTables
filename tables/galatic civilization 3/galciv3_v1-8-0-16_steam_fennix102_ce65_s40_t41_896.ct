MyCode:
//========================================= 1.8.0.16 ok
// On the main screen
_MonHumanPlayer:
 push rax
 push rbx
 push rcx

 mov rcx,rcx

 mov rbx,[rcx+08]

 mov eax,[rbx+34]
 test eax,eax
 jnz _ExitMHP

 mov [pPlayer],rbx

 mov eax,[rbx+08]
 mov [iID],eax

 mov eax,[rbx+44]
 mov [iID2],eax

 cmp dword ptr [iEnableMPR],0
 je _ExitMHP                       // Jump if feature is disabled

 mov eax,ctMCRE
 cmp eax,[rcx+00022b64]
 js _ExitMHP

 mov [rcx+00022b64],eax

_ExitMHP:
 pop rcx
 pop rbx
 pop rax

 mov eax,[rax+rcx+00022B64]        // Original code (get Credits)
 jmp _BackMHP                      // Back to main code

//========================================= 1.8.0.16 ok
// Unit Under Cursor/Selected
_MonSelUnit:
 push rbx

 lea rbx,[rcx-28]
 mov [pUnit],rbx

 mov ebx,[rbx+000000f0]
 cmp ebx,[iID]
 jne _ExitMSU

 cmp dword ptr [iEnableGMD],0
 je _MonSU00                       // Jump if feature is disabled

 mov ebx,[rcx+00000344]
 mov [rcx+00000468],ebx

_MonSU00:
 cmp dword ptr [iEnableMSS],0
 je _MonSU01                       // Jump if feature is disabled

 mov ebx,[rcx+00000478]
 test ebx,ebx
 jz _MonSU01

 mov ebx,ctMMSS
 cmp ebx,[rcx+00000478]
 js _MonSU01

 mov [rcx+00000478],ebx

_MonSU01:
 cmp dword ptr [iEnableMMP],0
 je _ExitMSU                       // Jump if feature is disabled

 mov ebx,[rcx+0000038c]
 mov [rcx+00000474],ebx

_ExitMSU:
 pop rbx

 mov eax,[rax+rcx+00000468]        // Original code (get cur HP)
 jmp _BackMSU                      // Back to main code

//========================================= 1.8.0.16 ok
// On Selected Planet Detail screen
_MonSelPlanet:
 mov rcx,[r12+08]                  // Original code (get pPlanetBase)

 mov [pPlanet],rcx

_ExitMSP:
 jmp _BackMSP                      // Back to main code

//========================================= 1.7.0.16
// During Turn
_MonResearchProg:
 cmp dword ptr [iEnableMRP],0
 je _ExitMRP                       // Jump if feature is disabled

 lea r8,[r15-78]
 cmp r8,[pPlayer]
 jne _ExitMRP

 mov edx,[rax]
 mov r8d,[rbx+08]
 cmp r8d,edx
 jns _ExitMRP

 sub edx,r8d
 inc edx
 mov [rsi],edx

_ExitMRP:
 mov r8d,[rdi+08]                  // Original code (get res prog)
 mov edx,[rsi]                     // Original code
 jmp _BackMRP                      // Back to main code

//========================================= 1.8.0.16 ok
// At main screen (planet)
_MonConstProg:
 push rdx

 test rbx,rbx
 jz _ExitMCP

 cmp rbx,r15
 jne _ExitMCP

 cmp eax,[rsi+08]
 jle _ExitMCP

 cmp dword ptr [iEnableMCP],0
 je _ExitMCP                       // Jump if feature is disabled

 test r13,r13                      // pPopulation
 jz _ExitMCP

 mov rdx,[rbx]                     // Get pPopBase
 test rdx,rdx
 jz _ExitMCP

 mov edx,[rdx+38]
 cmp edx,[iID]
 jne _ExitMCP

 lea rdx,[rax-64]
 mov [rsi+08],edx

_ExitMCP:
 pop rdx

 sub eax,[rsi+08]                  // Original code (cur prog)
 mov [rsp+30],00000000             // Original code
 jmp _BackMCP                      // Back to main code

//========================================= v1.8.0.16
// During turn
_MonUnitProd:
 cmp dword ptr [iEnableMUP],0
 je _ExitMUP                       // Jump if feature is disabled

 mov rax,[r15]
 test rax,rax
 jz _ExitMUP

 mov eax,[rax+00000238]
 cmp eax,[iID]
 jne _ExitMUP

 mov [r14+10],ecx

_ExitMUP:
 mov eax,[r14+10]                  // Original code (Get Prog)
 add eax,ebx                       // Original code (Inc Prog)
 jmp _BackMUP                      // Back to main code

//========================================= 1.8.0.16
// During Race Customization (On changing points)
_MonRaceCustomization:
 push rbx

 cmp dword ptr [iEnableMRC],0
 je _ExitMRC                       // Jump if feature is disabled

 mov dword ptr [rax+00000414],#46
 mov dword ptr [rax+00000418],#17

_ExitMRC:
 pop rbx

 mov eax,[rax+00000414]            // Original code (Get max traits' points)
 jmp _BackMRC                      // Back to main code

//========================================= 1.8.0.16
// Shipyard under cursor or Last Selected
_MonSelShipyard:
 mov rcx,[r13+00]                  // Original code (get pSY)
 mov [pSY],rcx

 cmp dword ptr [iEnableMMP],0
 je _ExitMSS                       // Jump if feature is disabled

 mov edx,[rcx+00000238]
 cmp edx,[iID]
 jne _ExitMSS

 mov rdx,#1000
 cmp edx,[rcx+00009a28]
 js _ExitMSS

 mov [rcx+00009a28],edx

_ExitMSS:
 mov edx,00000001                  // Original code
 jmp _BackMSS                      // Back to main code

//========================================= 1.8.0.16
// Starbase at the detailed screen
_MonSelStarbase:
 mov rdx,[rsi+20]
 mov [pSB],rdx

 cmp dword ptr [iEnableMSB],0
 je _ExitMSB                       // Jump if feature is disabled

 mov edx,[rdx+00000238]
 cmp edx,[iID]
 jne _ExitMSB

 mov edx,[rsi+00002fb4]
 mov [rsi+00002fb0],edx

_ExitMSB:
 mov rax,[rsi+18]                  // Original code
 lea rdx,[rsp+30]                  // Original code
 jmp _BackMSB                      // Back to main code

