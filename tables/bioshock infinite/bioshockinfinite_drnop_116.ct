nop
continue1:

// ---------------------------------------

weaponsHandler1:
// new code

// just for debugging purposes..
// save current weapon struct base
mov [currWeaponPtr], esi

// save current weapon ID
mov ax, [esi+00000DA4]
mov [currWeaponID], ax

// save current weapon max. ammo
// s.a. 07BC
mov ax, [esi+000007EC]
mov [currWeaponMax], ax

// here we go..
mov eax, 14  // set # of bullets

// originalcode:
mov [esi+000007C4],eax

// weaponsHandler1_Exit:
jmp continue1

currWeaponPtr:
dd 0
currWeaponID:
dw 0
currWeaponMax:
dw 0

// -----------------------------------
[DISABLE]
