    <CheatEntry>
      <ID>66970</ID>
      <Description>"Stats Base Address Scan Initialization (Activate This First)"</Description>
      <Options moHideChildren="1"/>
      <Color>800000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat

registersymbol(statsupdate_enable)
registersymbol(statsset_enable)
registersymbol(status_basepointer)
registersymbol(battlestats_basepointer)
registersymbol(position_basepointer)

alloc(statsupdate_enable,4)
alloc(statsset_enable,4)
alloc(status_basepointer,4)
alloc(battlestats_basepointer,4)
alloc(position_basepointer,4)


[DISABLE]

unregistersymbol(statsupdate_enable)
unregistersymbol(statsset_enable)
unregistersymbol(status_basepointer)
unregistersymbol(battlestats_basepointer)
unregistersymbol(position_basepointer)


dealloc(statsupdate_enable)
dealloc(statsset_enable)
dealloc(status_basepointer)
dealloc(battlestats_basepointer)
dealloc(position_basepointer)
</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>66971</ID>
          <Description>"Stats Base Address Scan (To Update: Play Game)"</Description>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
//code from here to '[DISABLE]' will be used to enable the cheat
label(statsscan_savedAOB)
label(statsscan_returnhere)
label(statsscan_originalcode)
label(statsscan_exit)

label(statsset_entry)
label(statsset_exit)

registersymbol(statsscan_savedAOB)

aobscan(searchedAOB,74 2A D9 EE D8 9E 64 06 00 00 DF E0 F6 C4 01 75 1B)

alloc(statsscan_newmem,2048) //2kb should be enough
alloc(eax_temp,4)
alloc(health_temp,4)
alloc(stamina_temp,4)


statsscan_newmem:
mov [eax_temp], eax
mov eax, ["BladeKitten.exe"+007233B0]
cmp dword ptr [statsupdate_enable], #1
jne statsset_entry
mov [status_basepointer], eax
mov eax, [eax+4c]
mov [battlestats_basepointer], eax
mov eax, [eax+2b8]
mov [position_basepointer], eax


statsset_entry:
cmp dword ptr [statsset_enable], #1
jne statsset_exit
mov eax, [status_basepointer]
mov [eax+30c], #0
//mov [eax+33c], #0
mov eax, [battlestats_basepointer]
mov [eax+658], (float)999999
mov [eax+664], (float)999999

statsset_exit:
mov eax, [eax_temp]

statsscan_originalcode:
db D8 9E 64 06 00 00
//fcomp dword ptr [esi+00000664]


statsscan_exit:
jmp statsscan_returnhere


searchedAOB + 4:
statsscan_savedAOB:
jmp statsscan_newmem
nop
statsscan_returnhere:


[DISABLE]
statsscan_savedAOB:
db D8 9E 64 06 00 00
//fcomp dword ptr [esi+00000664]

unregistersymbol(statsscan_savedAOB)

dealloc(statsscan_newmem)
dealloc(eax_temp,4)
dealloc(health_temp,4)
dealloc(stamina_temp,4)

</AssemblerScript>
