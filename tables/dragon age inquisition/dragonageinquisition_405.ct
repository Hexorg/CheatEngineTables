---------- combat stats scanner ----------

function scan_combatStats(unregister)
  -- set up
  local scan = createMemScan()
  local pStart = 0x00000000
  local pEnd = 0xFFFFFFFF
  local sPrefix = "DA3/Combat/Stats/" -- /stat_, /Stat_

  -- scan
  memscan_firstScan(scan, soExactValue, vtString, 0, sPrefix, nil, pStart, pEnd, "+W-X", 1, 1, true, false, false, true)
  memscan_waitTillDone(scan)

  -- get results
  local results = createFoundList(scan)
  foundlist_initialize(results)
  local count = foundlist_getCount(results)
  local index = 0
  while (index &lt; count) do
    -- fetch result
    local address = foundlist_getAddress(results, index)
    local rawaddress = getAddress(address)
    local noprefix = rawaddress + 0x11
    local label = readString(noprefix)

    -- ignore long labels, they are likely false positives
    if (string.len(label) &lt; 50) then
      -- if we want to unregister all labels
      if (unregister ~= nil) then
        -- unregister label
        autoAssemble([[
          unregistersymbol(pCombatStats_]]..label..[[)
        ]])
      else
        -- only register this label if it's not already registered
        errorOnLookupFailure(false)
        local alreadyfound = getAddress("pCombatStats_"..label)
        errorOnLookupFailure(true)
        if (alreadyfound == 0) then
          -- find local pointer
          local pointerscan = createMemScan()
          memscan_returnOnlyOneResult(pointerscan, true)
          memscan_firstScan(pointerscan, soExactValue, 3, 0, rawaddress, nil, rawaddress - 0x1000, rawaddress, "+W-X", 1, 1, false, false, false, false)
          memscan_waitTillDone(pointerscan)
          local pointerresult = memscan_getOnlyResult(pointerscan)

          -- register label
          if (pointerresult ~= nil) then
            pointerresult = pointerresult - 0x10
            -- print("pCombatStats_"..label, address, pointerresult)
            changeLabel("pCombatStats_"..label, pointerresult)
          end

          -- clean up
          object_destroy(pointerscan)
        end
      end
    end

    index = index + 1
  end

  -- clean up
  object_destroy(scan)
end

