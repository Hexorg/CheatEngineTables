je Player_set_exit
mov [eax+30], (float)9999999
mov [eax+34], (float)9999999
mov [eax+38], (float)9999999
cmp dword ptr [eax+20], #0
je Player_set_exit
mov ebx, [eax+20]
mov [ebx+228], (float)400
Player_set_exit:

mov ebx, [Player_temp1]


Player_originalcode:
db D9 40 34 D9 5D F0
//fld dword ptr [eax+34]
//fstp dword ptr [ebp-10]


Player_exit:
jmp Player_returnhere


Player_searchedAOB:
Player_savedAOB:
jmp Player_newmem
nop
