label(pShipShield)

//=========================================
// Registering Symbols
registersymbol(MyCode)
registersymbol(iEnableMMM)
registersymbol(iEnableMSD)
registersymbol(iEnableGMD)
registersymbol(iEnableMSM)
registersymbol(iMinMoney)
registersymbol(pPlayer)
registersymbol(pShipHull)
registersymbol(pShipShield)

//=========================================
MyCode:
//=========================================
// When Money is Visible
_MonHumanPlayer:
 mov [pPlayer],ecx

 cmp dword ptr [iEnableMMM],0
 je _ExitMHP                       // Jump if feature is disabled

 mov eax,[ecx+14]
 test eax,eax
 jnz _ExitMHP

 mov eax,[iMinMoney]
 cmp eax,[ecx+10]
 js _ExitMHP

 mov [ecx+10],eax

_ExitMHP:
 mov eax,[ecx+10]                  // Original code (rds Money L)
 mov edx,[ecx+14]                  // Original code (rds Money H)
 jmp _BackMHP                      // Back to main code

//=========================================
// Called all the time
// EDI = pHull
_MonPlayerShip:
 push eax

 mov eax,[edi+2c]
 test eax,eax
 jnz _ExitMPS

 mov ecx,[edi+000002DC]            // Get pShield

 mov [pShipHull],edi
 mov [pShipShield],ecx

 cmp dword ptr [iEnableMSD],0
 je _MonPS00                       // Jump if feature is disabled

 mov eax,[ecx+44]
 mov [ecx+68],eax

_MonPS00:
 cmp dword ptr [iEnableGMD],0
 je _ExitMPS                       // Jump if feature is disabled

 mov eax,[edi+0000029c]
 mov [edi+000002b0],eax

_ExitMPS:
 pop eax

 mov ecx,[edi+000002DC]            // Original code (rds pShield)
 jmp _BackMPS                      // Back to main code

//=========================================
// Called when shield or turbo bust is used
// EDI = pShield
_MonShipShield:
 push eax

 cmp edi,[pShipShield]
 jne _MonSS00

 cmp dword ptr [iEnableMSD],0
 je _ExitMSS                       // Jump if feature is disabled

 fstp st(0)
 fld dword ptr [edi+44]
 jmp _ExitMSS

_MonSS00:
 cmp ebp,[pShipHull]
 jne _ExitMSS

 cmp dword ptr [iEnableGMD],0
 je _ExitMSS                       // Jump if feature is disabled

 fstp st(0)
 fld dword ptr [ebp+0000029c]

_ExitMSS:
 pop eax

 movsd xmm0,[esp+08]               // Original code
 jmp _BackMSS                      // Back to main code

//=========================================
// Called when missile is shot
_MonShipMissile:
 mov edi,[ebp+1c]
 cmp edi,[pShipHull]
 jne _ExitMSM

 cmp dword ptr [iEnableMSM],0
 je _ExitMSM                       // Jump if feature is disabled

 xor edi,edi
 mov [ecx],edi

_ExitMSM:
 mov ecx,[ecx]                     // Original code
 sub [edx+14],ecx                  // Original code (upd missile)
 jmp _BackMSM                      // Back to main code

//=========================================
 db '====================================&gt;'
 db 'CE6.3 Script by Recifense 052214'
//=========================================
// Variables
iEnableMMM:
 dd 0
iEnableMSD:
 dd 0
iEnableGMD:
 dd 0
iEnableMSM:
 dd 0
iMinMoney:
 dd ctMMMY
pPlayer:
 dd 0
pShipHull:
 dd MyCode
pShipShield:
 dd MyCode

//=========================================
// Hacking Points
MHPR:
 jmp _MonHumanPlayer
 nop
_BackMHP:

MPSP:
 jmp _MonPlayerShip
 nop
_BackMPS:

MSSD:
 jmp _MonShipShield
 nop
_BackMSS:

MSME:
 jmp _MonShipMissile
_BackMSM:

//=========================================
// Script for Restoring Original Codes
[DISABLE]
MHPR:
// mov eax,[ecx+10]
// mov edx,[ecx+14]
