MyCode:
//========================================= 1.427 ok
// Main game screen
_FindHumanPlayer:
 mov [pPlayer],eax                 // XP Base
 movzx ebx,byte ptr [eax+28]
 mov [iID],ebx

 cmp dword ptr [iEnableMPR],0
 je _ExitFHP                       // Jump if feature is disabled

 mov ebx,ctMMON
 cmp ebx,[eax+000004ac]
 js _FindHP0

 mov [eax+000004ac],ebx

_FindHP0:
 mov ebx,ctMMAN
 cmp ebx,[eax+000004dc]
 js _ExitFHP

 mov [eax+000004dc],ebx

_ExitFHP:
 mov eax,[eax+08]                  // Original code
 cmp dword ptr [eax+00000140],00   // Original code
 jmp _BackFHP                      // Back to main code

//========================================= 1.549 ok
// When Moving
_MonMovePoints:
 mov edx,[esi+00000238]
 cmp edx,[pPlayer]
 jne _ExitMMP

 cmp dword ptr [iEnableMMP],0
 je _MonMP00                       // Jump if feature is disabled

 mov edx,[esi+0000024c]
 mov [esi+00000418],edx
 xor eax,eax

_MonMP00:
 cmp dword ptr [iEnableGMD],0
 je _MonMP01                       // Jump if feature is disabled

 mov edx,[esi+00000250]
 mov [esi+0000041c],edx

_MonMP01:
 cmp dword ptr [iEnableMCU],0
 je _ExitMMP                       // Jump if feature is disabled

 mov edx,[esi+00000290]
 mov [esi+00000494],edx

_ExitMMP:
 sub [esi+00000418],eax            // Original code (upd MP)
 jmp _BackMMP                      // Back to main code

//========================================= 1.549 ok
// During Turn
_MonResearchProg:
 cmp ebx,[pPlayer]
 jne _ExitMRP

 cmp dword ptr [iEnableMRP],0
 je _ExitMRP                       // Jump if feature is disabled

 mov ecx,[esi+00000150]
 test ecx,ecx
 jz _ExitMRP

 add edi,[ecx+00000098]

_ExitMRP:
 add [esi+00000158],edi            // Original code (upd RP)
 jmp _BackMRP                      // Back to main code

//========================================= 1.549 ok
// On Selecting unit in Battle
_MonMovePointsB:
 push ecx

 mov ecx,eax

 mov edx,[ecx+0000023c]
 test edx,edx
 jz _ExitMPB

 mov edx,[edx+00000428]
 cmp edx,[pPlayer]
 jne _ExitMPB

 cmp dword ptr [iEnableMPB],0
 je _MonMPB0                       // Jump if feature is disabled

 mov edx,[ecx+0000024c]
 mov [ecx+00000418],edx            // recover AC

 mov byte ptr [ecx+0000082c],00    // Enable Act Again

_MonMPB0:
 cmp dword ptr [iEnableGMD],0
 je _MonMPB1                       // Jump if feature is disabled

 mov edx,[ecx+00000250]
 mov [ecx+0000041c],edx

_MonMPB1:
 cmp dword ptr [iEnableMCU],0
 je _ExitMPB                       // Jump if feature is disabled

 mov edx,[ecx+00000290]
 mov [ecx+00000494],edx

_ExitMPB:
 pop ecx

 mov edx,[eax+00000418]            // Original code (upd MP)
 jmp _BackMPB                      // Back to main code

//========================================= 1.549 ok
// When in battle
_GodMode:
 push edx

 mov edx,[esi+0000023c]
 test edx,edx
 jz _ExitGMD

 mov edx,[edx+00000428]
 cmp edx,[pPlayer]
 jne _ExitGMD

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov edx,[esi+00000250]
 mov [esi+0000041C],edx
 xor eax,eax

_ExitGMD:
 pop edx

 cmp eax,[esi+0000041C]            // Original code (rd HP)
 jmp _BackGMD                      // Back to main code

//========================================= 1.549 ok
// When City is Selected
_MonSelCity:
 push edx

 mov [pCity],eax

 mov edx,[eax+00000314]
 test edx,edx
 jz _ExitMSC

 mov edx,[edx+0c]
 cmp edx,[pPlayer]
 jne _ExitMSC

 cmp dword ptr [iEnableMCP],0
 je _ExitMSC                       // Jump if feature is disabled

 mov edx,ctMMCP
 cmp edx,[eax+00000848]
 js _ExitMSC

 mov [eax+00000848],edx

_ExitMSC:
 pop edx

 mov ecx,[eax+00000848]            // Original code (rd POP)
 jmp _BackMSC                      // Back to main code

//========================================= 1.549 ok
// During Turn
_MonProdProg:
 mov eax,[edi+08]
 test eax,eax
 jz _ExitMPP

 mov eax,[eax+08]
 test eax,eax
 jz _ExitMPP

 mov eax,[eax+00000314]
 test eax,eax
 jz _ExitMPP

 mov eax,[eax+0c]
 cmp eax,[pPlayer]
 jne _ExitMPP

 cmp dword ptr [iEnableMPP],0
 je _ExitMPP                       // Jump if feature is disabled

 mov [edi+28],ecx

_ExitMPP:
 mov eax,[edi+28]                  // Original code (rd Prog)
 cmp eax,ecx                       // Original code
 jmp _BackMPP                      // Back to main code

//========================================= 1.549 ok
// When in battle
_MonCastingUsed:
 push edx

 mov edx,[eax+00000428]
 cmp edx,[pPlayer]
 jne _ExitMCU

 cmp dword ptr [iEnableMCU],0
 je _MonCU00                       // Jump if feature is disabled

 mov edx,[eax+0000039c]
 mov [eax+00000398],edx

 mov edx,[eax+000003a4]
 cmp edx,[eax+000003a0]
 js _MonCU00

 mov [eax+000003a0],edx

_MonCU00:
 cmp dword ptr [iEnableMCC],0
 je _ExitMCU                       // Jump if feature is disabled

 mov byte ptr [eax+000003dc],00

_ExitMCU:
 pop edx

 cmp byte ptr [eax+000003dd],00    // Original code (rd flag)
 jmp _BackMCU                      // Back to main code

//========================================= 1.549 ok
// When unit detail is displayed
_MonUnitXP:
 push ecx
 push edx

 mov edx,[ebx+10]                  // Get pXP

 mov ecx,[edx+10]
 cmp ecx,[pPlayer]
 jne _ExitMUX

 mov [pUnit],edx

 cmp dword ptr [iEnableMUX],0
 je _ExitMUX                       // Jump if feature is disabled

 cmp edi,[edx+00000270]
 jle _ExitMUX

 dec edi
 mov [edx+00000270],edi

 inc edi

_ExitMUX:
 pop edx
 pop ecx

 mov ecx,[esp+44]                  // Original code
 mov edi,[esp+48]                  // Original code
 jmp _BackMUX                      // Back to main code

//========================================= 1.549 ok
// During Turn
_MonProdProg1:
 call edx                          // Original code

 mov ebp,[esi+08]
 test ebp,ebp
 jz _ExitPP1

 mov ebp,[ebp+08]
 test ebp,ebp
 jz _ExitPP1

 mov ebp,[ebp+00000314]
 test ebp,ebp
 jz _ExitPP1

 mov ebp,[ebp+0c]
 cmp ebp,[pPlayer]
 jne _ExitPP1

 cmp dword ptr [iEnableMPP],0
 je _ExitPP1                       // Jump if feature is disabled

 mov [esi+28],eax

_ExitPP1:
 cmp [esi+28],eax                  // Original code (rd Prog)
 jmp _BackPP1                      // Back to main code

