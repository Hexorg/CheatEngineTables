//046C3CD4 - C1 E1 02              - shl ecx,02
aobscan(SaveGame_searchedAOB, 50 8B 45 B8 52 51 50 39 00)

alloc(SaveGame_newmem, 2048, SaveGame_searchedAOB)
alloc(SaveGame_temp1, 4, SaveGame_searchedAOB)


SaveGame_newmem:
mov [SaveGame_temp1], ebx

mov ebx, [SaveGame_savedAOB-4]
sub ebx, 4

cmp dword ptr [SaveGame_update_enable], #0
je SaveGame_update_exit
mov [SaveGame_basepointer], ebx
SaveGame_update_exit:

cmp dword ptr [SaveGame_set_enable], #0
je SaveGame_set_exit
SaveGame_set_exit:

mov ebx, [SaveGame_temp1]


SaveGame_originalcode:
db 50 8B 45 B8 52
//push eax
//mov eax,[ebp-48]
//push edx


SaveGame_exit:
