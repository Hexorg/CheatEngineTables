      <AssemblerScript>{ Game   : isaac-ng.exe
  Version: v1.041
  Date   : 29/12/2014
  Author : AikonCWD

  This script search player structure and fill pointers
  Also provides the option to use "Starter Rush Pack"
}




[ENABLE]

aobscanmodule(FindStructure,isaac-ng.exe,8B B8 A0 0B 00 00)
alloc(newmem,$1000)

label(code)
label(return)

globalalloc(pHearts,4)
globalalloc(pStarterPack,4)

newmem:

code:
  mov edi,[eax+00000BA0]
  mov [pHearts],eax            //Get the base address of player structure
  cmp [pStarterPack],1         //Check if StarterRushPack script is active
  jne return
  mov [eax+b50],10
  mov [eax+b54],10
  mov [eax+b5c],8
  mov [eax+b60],3
  mov [eax+eb0],9
  mov [eax+b6c],1
  mov [eax+2f98],1
  mov [eax+2f94],5
  mov [eax+cc8],125
  mov [eax+ccc],6
  mov [eax+d64],8
  mov [eax+b74],63
  mov [eax+b70],63
  mov [eax+b68],63
  jmp return

FindStructure:
  jmp code
  nop
return:
  registersymbol(FindStructure)

[DISABLE]

FindStructure:
  db 8B B8 A0 0B 00 00

unregistersymbol(FindStructure)
dealloc(newmem)

