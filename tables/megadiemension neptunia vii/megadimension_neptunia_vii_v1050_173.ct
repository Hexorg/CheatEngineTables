          <AssemblerScript>[ENABLE]
label(partystats_savedAOB)
label(partystats_returnhere)
label(partystats_originalcode)
label(partystats_exit)

label(partystats_update_exit)
label(partystatscredit_set_exit)
label(partystatsexdrive_set_exit)
label(partystats_scoutset_return)
label(partystats_scoutset_skip)
label(partystats_scoutset_exit)
label(partystats_scoutlevelset_return)
label(partystats_scoutlevelset_skip)
label(partystats_scoutlevelset_exit)

registersymbol(partystats_savedAOB)

aobscanmodule(partystats_searchedAOB,NeptuniaVII.exe,0F B7 B7 A0 0F 01 00) // should be unique

alloc(partystats_newmem, 2048)

partystats_newmem:

  cmp dword ptr [partystats_update_enable], #0
  je partystats_update_exit
  mov [partystats_basepointer], edi
partystats_update_exit:

  cmp dword ptr [partystats_creditset_enable], #0
  je partystatscredit_set_exit
  mov [edi+10FA8], #99999999
partystatscredit_set_exit:

  cmp dword ptr [partystats_exdriveset_enable], #0
  je partystatsexdrive_set_exit
  mov word ptr [edi+10FA0], #500
  mov word ptr [edi+10FA2], #4
partystatsexdrive_set_exit:

  push eax
  push ebx
  mov ebx,[edi+5E954] //Number of Scouts
  imul ebx,ebx,0x10
  lea ebx,[ebx+edi+5E958] //End Address
  cmp dword ptr [partystats_scoutset_enable], #0
  je partystats_scoutset_exit
  lea eax,[edi+5E958] //Start Address
partystats_scoutset_return:
  cmp dword ptr [eax+C],#0
  je partystats_scoutset_skip
  mov dword ptr [eax+C],#0
partystats_scoutset_skip:
  add eax,0x10
  cmp eax,ebx
  jb partystats_scoutset_return
partystats_scoutset_exit:

  cmp dword ptr [partystats_scoutlevelset_enable], #0
  je partystats_scoutlevelset_exit
  lea eax,[edi+5E958] //Start Address
partystats_scoutlevelset_return:
  cmp byte ptr [eax+1],#5
  jae partystats_scoutlevelset_skip
  mov byte ptr [eax+1],#5 //Level
  mov byte ptr [eax+4],#10 //EnergyMate Consumed
partystats_scoutlevelset_skip:
  add eax,0x10
  cmp eax,ebx
  jb partystats_scoutlevelset_return
partystats_scoutlevelset_exit:

  pop ebx
  pop eax

partystats_originalcode:
  movzx esi,word ptr [edi+00010FA0]

partystats_exit:
  jmp partystats_returnhere

partystats_searchedAOB:
partystats_savedAOB:
  jmp partystats_newmem
  nop
  nop
partystats_returnhere:

[DISABLE]

partystats_savedAOB:
  db 0F B7 B7 A0 0F 01 00

unregistersymbol(partystats_savedAOB)

dealloc(partystats_newmem)

