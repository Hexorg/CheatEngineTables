NUM 5 - forward
NUM 2 - backward
NUM 1 - strafe left
NUM 3 - strafe right
NUM 7 - Elev up
NUM 9 - Elev down

press right mouse button to speed up freecam
}

[ENABLE]
{$lua}
  if syntaxcheck or cameraCallStubFound then return end
  cameraCallStubFound=
  autoAssemble([[aobscanmodule(cameraCallStub,witcher3.exe,E8 * * * * 48 8B 44 24 58 48 83 C4 48 C3)
                 registerSymbol(cameraCallStub)]])
  dis=createDisassembler()
  dis.disassemble('cameraCallStub')
  unregisterSymbol('cameraCallStub')
  unregisterSymbol('CamPos')
  registerSymbol('CamPos',dis.LastDisassembleData.parameterValue,true)
  dis.destroy()
{$asm}



alloc(newmem,$1000,witcher3.exe)

aobscanmodule(AdvancedFreeCam,witcher3.exe,E8 8B 42 3C 89 41 EC 8B 42 40 89 41 F0) // should be unique
registersymbol(AdvancedFreeCam)

label(return)
label(return2)


label(normVector)
label(FreeCam)
label(epilog)
label(subGetAsyncKeyState)
label(factor)

label(followNormalCam)
registersymbol(followNormalCam)

alloc(normalCamPrevPos,16)

newmem:
  lea rax,[rdx+40]
  mov [normVector],rax

  mov eax,[rdx+40]
  mov [rcx-10],eax
  jmp return




FreeCam:
cmp [rcx+5c],4
je @f

mov eax,[rdx]     //orig
mov [rcx],eax     //orig
mov eax,[rdx+04]  //orig
jmp return2

@@:
movups xmm0,[rdx] // new cam position calc by game engine
test [followNormalCam],01
jz @f
movaps xmm1,xmm0
subps xmm1,[normalCamPrevPos]
addps xmm1,[rcx]
movaps [rcx],xmm1
@@:
movups [normalCamPrevPos],xmm0

push r10
push r11
push r12
push r13
push r14
push r15
push r8
push r9
push rax
push rbp
push rbx
push rcx
push rdi
push rdx
push rsi

//pushed again, because it has needed address
push rcx

  xor r10,r10
  xorps xmm15,xmm15

  mov ecx,#101 //FORWARD
  mov r11,1
  call subGetAsyncKeyState
  mov ecx,#98 //BACKWARD
  mov r11,2
  call subGetAsyncKeyState
  mov ecx,#97 //LEFT
  mov r11,4
  call subGetAsyncKeyState
  mov ecx,#99 //RIGHT
  mov r11,8
  call subGetAsyncKeyState

  mov ecx,#103 //UP
  mov r11,10
  call subGetAsyncKeyState
  mov ecx,#105 //DOWN
  mov r11,20
  call subGetAsyncKeyState

  mov ecx,#2 //Faster (right mouse button)
  mov r11,40
  call subGetAsyncKeyState

  pop rcx // rcx has cam pos address

  test r10,r10
  jz epilog


  mov rax,[normVector]
  test rax,rax
  jz epilog

  movups xmm0,[rax]

  test r10,01  // forward
  jz +4
  addps xmm15,xmm0

  test r10,02  // backward
  jz +4
  subps xmm15,xmm0


  shufps xmm0,xmm0,21 //swap x and y
  movq xmm1,xmm0      // only x and y
  subss xmm1,xmm0
  subss xmm1,xmm0

  test r10,04  // left
  jz +4
  addps xmm15,xmm1

  test r10,08  // right
  jz +4
  subps xmm15,xmm1

  // move in Z axis
  mov eax,(float)1
  movd xmm0,eax
  shufps xmm0,xmm0,45

  test r10,10  // up
  jz +4
  addps xmm15,xmm0

  test r10,20  // down
  jz +4
  subps xmm15,xmm0

  // xmm15 is a sum of vectors

  movaps xmm0,xmm15
  xorps xmm1,xmm1
  mulps xmm15,xmm15
  movss xmm1,xmm15
  shufps xmm15,xmm15,21
  addss xmm1,xmm15
  movhlps xmm15,xmm15
  addss xmm1,xmm15
  sqrtss xmm1,xmm1 // xmm1 contains vector length


  xorps xmm15,xmm15
  comiss xmm1,xmm15 // deal with "division by zero"
  je epilog

  shufps xmm1,xmm1,00
  divps xmm0,xmm1   // vector is now normalized


  movss xmm1,[factor]
  shufps xmm1,xmm1,00
  mulps xmm0,xmm1


  test r10,40  // faster (3times faster)
  jz @f
  movaps xmm1,xmm0
  addps xmm0,xmm1
  addps xmm0,xmm1
@@:

  // move camera
  movups xmm1,[rcx]
  addps xmm1,xmm0
  movq [rcx],xmm1
  movhlps xmm1,xmm1
  movss [rcx+8],xmm1

epilog:
pop rsi
pop rdx
pop rdi
pop rcx
pop rbx
pop rbp
pop rax
pop r9
pop r8
pop r15
pop r14
pop r13
pop r12
pop r11
pop r10
mov rax,rcx
ret




subGetAsyncKeyState:
  push r10
  push r11

  sub rsp,08
  call GetAsyncKeyState
  add rsp,08

  pop r11
  pop r10
  test ax,8000

  jz short @f
    or r10,r11
  @@:
  ret

normVector:
dq 0

factor:
dd (float)CameraSpeed

followNormalCam:
dd 0

normalCamPrevPos:
dd 0 0 0 0

AdvancedFreeCam+07:
  jmp newmem
  nop
return:


CamPos:
  jmp FreeCam
  nop
  nop
return2:

aobscanmodule(angle1,witcher3.exe,F3 A4 48 8B 44 24 50 C6) // should be unique
registersymbol(angle1)
alloc(newmemAngle1,$1000,angle1)

label(returnAngle1)

newmemAngle1:
  test [followNormalCam],01
  jnz @f
  repe movsb
  @@:
  mov rax,[rsp+50]
  jmp returnAngle1

angle1:
  jmp newmemAngle1
  nop
  nop
returnAngle1:

aobscanmodule(angle2,witcher3.exe,F3 A4 48 8B 44 24 40 48 8B 4C 24 48 0F) // should be unique
registersymbol(angle2)
alloc(newmemAngle2,$1000,angle2)

label(returnAngle2)

newmemAngle2:
  test [followNormalCam],01
  jnz @f
  repe movsb
  @@:
  mov rax,[rsp+40]
  jmp returnAngle2

angle2:
  jmp newmemAngle2
  nop
  nop
returnAngle2:


