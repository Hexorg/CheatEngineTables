label(TP_meh)

registersymbol(TPORT)
registersymbol(Coords)
registersymbol(iTP)
registersymbol(TP_meh)
registersymbol(_TP_Coord_Hook)
registersymbol(_TP_Squad_Hook)

aobscan(aob_mapmouse,8B8BCC0200008B8930030000384108)
aobscan(aob_squadselected,538B5C2408558B6834)

// Calculation of mouse-map coordinates
// [EBX+384] == x
// [EBX+388] == y
// 0043479B - 8B 8B CC020000             - mov ecx,[ebx+000002CC]
// 004347A1 - 8B 89 30030000             - mov ecx,[ecx+00000330]
// 004347A7 - 38 41 08                   - cmp [ecx+08],al
// 8B8BCC0200008B8930030000384108
//Game.exe+3479B:  // Grab location of mouse coords from function that calcs them
aob_mapmouse:
_TP_Coord_Hook:
  jmp _TP_Coord_Start
  nop
_TP_Coord_Back:

// Squad selected on map screen: ESI is SquadOffset+98
// Something at Squad+7C when there's an order
// 0043B370 - 53                         - push ebx
// 0043B371 - 8B 5C 24 08                - mov ebx,[esp+08]
// 0043B375 - 55                         - push ebp
// 0043B376 - 8B 68 34                   - mov ebp,[eax+34]
// 538B5C2408558B6834
//Game.exe+3B370:
aob_squadselected:
_TP_Squad_Hook:
  jmp _TP_Squad_Start
_TP_Squad_Back:

TPORT:
_TP_Coord_Start: // Capture Mouse Map Coord location
  lea ecx, [ebx+00000384]
  mov dword ptr [Coords], ecx
  mov ecx,[ebx+000002CC]
  jmp _TP_Coord_Back

_TP_Squad_Start: // Hook function only run when squad selected in map screen.
  mov ecx, dword ptr [ESI-7C]
  mov dword ptr [TP_meh], ecx
  mov ecx, dword ptr [ESI-78]
  mov dword ptr [TP_meh+4], ecx
  //
  cmp dword ptr [ESI-1C],0 // Don't proceed if there's an order
  jne _TP_Squad_Exit
  cmp dword ptr [iTP], 0
//  je _TP_Squad_Exit
  //
  pushad
  pushfd
  push '0'
  call GetAsyncKeyState
//  shr ax,#15
  and ax,1
  cmp ax,1
  jne _TP_Squad_Done
  //
  mov dword ptr [iTP], 0
  mov ecx, dword ptr [Coords]
  mov ecx, [ecx]
  mov dword ptr [ESI-7C], ecx
  mov ecx, dword ptr [Coords]
  mov ecx, [ecx+4]
  mov dword ptr [ESI-78], ecx
_TP_Squad_Done:
  popfd
  popad
_TP_Squad_Exit:
  push ebx
  mov ebx,[esp+08]
  jmp _TP_Squad_Back


// Variables
Coords:
  dd 0
iTP:
  dd 0
TP_meh:
  dd 0
  dd 0

[DISABLE]
//Game.exe+3479B:
_TP_Coord_Hook:
  mov ecx,[ebx+000002CC]
  mov ecx,[ecx+00000330]
  cmp [ecx+08],al
//Game.exe+3B370:
_TP_Squad_Hook:
  push ebx
  mov ebx,[esp+08]
  push ebp
  mov ebp,[eax+34]


dealloc(TPORT)
unregistersymbol(TPORT)
unregistersymbol(Coords)
unregistersymbol(iTP)
unregistersymbol(TP_meh)
unregistersymbol(_TP_Coord_Hook)
unregistersymbol(_TP_Squad_Hook)

</AssemblerScript>
