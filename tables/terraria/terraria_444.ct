    <CheatEntry>
      <ID>140</ID>
      <Description>"fullbright"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
aobscan(lightcolor_aob, 2B 41 10 3B 41 08 0F 83 12 02 00 00 2B 51 14 3B 51 0C 0F 83 06 02 00 00 0F AF 41 0C 03 C2 8D 54 81 18)
/*
ecx holds the address of the color2[][]  (red spectrum magnitudes) in the ds.
The address immediately after contains *colorg2 (greem light brightness),
and finally *colorb2.  When this instruction executes, eax and esi both
hold x and edi and edx both hold y. The address below in the second fld
instruction is the lightcolor.  Our objective is to set the lightcolor
and color2[x][y] etc to something large.
*/
label(lightcolor)
registersymbol(lightcolor)
alloc(mymem, 1024)
registersymbol(mymem)
label(mycode)
label(done)

lightcolor_aob:
lightcolor:
jmp mycode
nop

mymem:
dd 0 // **ppcolor2
dd 0 // *lightcolor
mycode:
push eax
mov eax,[lightcolor-4]
mov [mymem],eax //save **color2 so we can get other two array pointers
mov eax,[lightcolor+26]
mov [mymem+4],eax //save *lightcolor
mov [eax], (float)255.0 //stuff lightcolor w/ something large.
pop eax

//the following code navigates the multidimensional
//array to get the tile we're lighting:
sub eax,[ecx+10]
cmp eax,[ecx+08]
jae done //out of bounds?
sub edx,[ecx+14]
cmp edx,[ecx+0C]
jae done //out of bounds?
imul eax,[ecx+0C]
add eax,edx
lea edx,[ecx+eax*4+18]
//now, edx points to the rcolor of the tile:
mov [edx],(float)255.0 //stuff it w/ something large.

//now, repeat for green/colorg2:
mov ecx,[mymem]
add ecx,4 //load address of *colorg2 into ecx
mov ecx,[ecx]
mov eax, esi //load x into eax
mov edx, edi //load y into edx
//the following code navigates the multidimensional
//array to get the tile we're lighting:
sub eax,[ecx+10]
cmp eax,[ecx+08]
jae done //out of bounds?
sub edx,[ecx+14]
cmp edx,[ecx+0C]
jae done //out of bounds?
imul eax,[ecx+0C]
add eax,edx
lea edx,[ecx+eax*4+18]
//now, edx points to the gcolor of the tile:
mov [edx],(float)255.0 //stuff it w/ something large.

//now, repeat for blue/colorb2:
mov ecx,[mymem]
add ecx,8 //load address of *colorb2 into ecx
mov ecx,[ecx]
mov eax, esi //load x into eax
mov edx, edi //load y into edx
//the following code navigates the multidimensional
//array to get the tile we're lighting:
sub eax,[ecx+10]
cmp eax,[ecx+08]
jae done //out of bounds?
sub edx,[ecx+14]
cmp edx,[ecx+0C]
jae done //out of bounds?
imul eax,[ecx+0C]
add eax,edx
lea edx,[ecx+eax*4+18]
//now, edx points to the bcolor of the tile:
mov [edx],(float)255.0 //stuff it w/ something large.


done:
//function epilogue, short-circuit the rest of the function
lea esp,[ebp-08]
pop esi
pop edi
pop ebp
ret

[DISABLE]
lightcolor:
db 2b 41 10 3b 41 08

</AssemblerScript>
