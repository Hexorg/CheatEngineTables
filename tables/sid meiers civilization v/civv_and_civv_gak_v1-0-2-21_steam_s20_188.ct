 cmp dword ptr [ecx+000005bc],2 // Player's unit? (*)
 jne _ExitGM

 cmp dword ptr [iEnableGM],0
 je _GodM00                     // Jump if feature is disabled

 mov [pUnit],ecx

 xor ebp,ebp                    // Negative HP = 0  (*)
 mov [esp+00000084],ebp         // Value2Decrease = 0 (*)
 mov [ecx+000000c0],ebp         // NegHP = 0 (*)

