MyCode:
//=========================================
_MonPlayer:
 mov [pPlayer],ecx

 cmp dword ptr [iEnableMR],0
 je _MonP00                        // Jump if feature is disabled

 mov eax,[iMinRes]
 cmp eax,[ecx+4c]
 jle _MonP00

 mov [ecx+4c],eax

_MonP00:
 cmp dword ptr [iEnableMPP],0
 je _MonP01                        // Jump if feature is disabled

 mov eax,#32
 cmp eax,[ecx+00000264]
 jle _MonP01

 mov [ecx+00000264],eax

_MonP01:
 cmp dword ptr [iEnableMCP],0
 je _ExitMP                        // Jump if feature is disabled

 xor eax,eax
 mov [ecx+38],eax

_ExitMP:
 mov eax,[ecx+00000264]            // Original code (rds points)
 jmp _BackMP                       // Back to main code

//=========================================
_GodMode:
 push ecx                          // Original code

 mov [pUnit],ecx

 mov eax,(float)1.0
 cmp eax,[ecx+10]
 je _ExitGM

 mov eax,(float)999999.0
 cmp eax,[ecx+10]
 je _ExitGM

 cmp dword ptr [iEnableGM],0
 je _ExitGM                        // Jump if feature is disabled

 lea eax,[ecx-10]
 mov eax,[eax+08]
 mov eax,[eax+0000023c]
 mov eax,[eax+04]
 mov eax,[eax+08]
 cmp eax,[pPlayer]
 jne _ExitGM

 mov eax,[esp+08]
 test eax,eax
 jns _ExitGM                       // jump if the value is positive

 and eax,7fffffff
 cmp eax,[ecx+10]
 je _ExitGM

 xor eax,eax
 mov [esp+08],eax                  // value2decrease = 0

_ExitGM:
 fld dword ptr [esp+08]            // Original code
 jmp _BackGM                       // Back to main code

//=========================================
_MonSel:
 mov [pSel],esi                    // Get pHP
 mov esi,[esi+00000200]            // Original code

_ExitMS:
 jmp _BackMS                       // Back to main code

