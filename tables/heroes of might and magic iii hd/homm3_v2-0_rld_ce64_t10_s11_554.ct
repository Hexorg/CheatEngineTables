//=========================================
MyCode:
//=========================================
// on entering/leaving a castle, on closing army detail
_MonHumanPlayer:
 cmp byte ptr [ebx+000000da],1
 jne _ExitMHP

 mov [pPlayer],ebx

 movzx ecx,byte ptr [ebx]
 mov [iID],ecx

 cmp dword ptr [iEnableMMR],0
 je _ExitMHP                       // Jump if feature is disabled

 mov ecx,ctMRES
 cmp eax,6
 js _MonHP00

 mov ecx,ctMGLD

_MonHP00:
 cmp ecx,[ebx+eax*4+00000094]
 js _ExitMHP

 mov [ebx+eax*4+00000094],ecx

_ExitMHP:
 push [ebx+eax*4+00000094]         // Original code (get Res Amount)
 jmp _BackMHP                      // Back to main code

//=========================================
// On moving an Army
_MonMovePoints:
 push ebx
 push ecx
 push edx

 mov [pArmy],edi

 movzx eax,[edi+1e]
 cmp eax,[iID]
 jne _ExitMMP

 cmp dword ptr [iEnableMMP],0
 je _MonMP00                       // Jump if feature is disabled

 mov eax,[edi+45]
 mov [edi+49],eax

 xor eax,eax
 mov [ebp-14],eax

_MonMP00:
 cmp dword ptr [iEnableMTS],0
 je _MonMP01                       // Jump if feature is disabled

 mov eax,ctSIZE

 mov ebx,[edi+000000a9]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000a9],ebx

 mov ebx,[edi+000000ad]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000ad],ebx

 mov ebx,[edi+000000b1]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000b1],ebx

 mov ebx,[edi+000000b5]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000b5],ebx

 mov ebx,[edi+000000b9]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000b9],ebx

 mov ebx,[edi+000000bd]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000bd],ebx

 mov ebx,[edi+000000c1]
 cmp ebx,eax
 cmovs ebx,eax
 mov [edi+000000c1],ebx

_MonMP01:
 cmp dword ptr [iEnableMHA],0
 je _ExitMMP                       // Jump if feature is disabled

 mov ecx,ctATTR
 xor edx,edx

 mov dl,[edi+00000446]
 cmp ecx,edx
 cmovg edx,ecx

 mov [edi+00000446],dl

 mov dl,[edi+00000447]
 cmp ecx,edx
 cmovg edx,ecx

 mov [edi+00000447],dl

 mov dl,[edi+00000448]
 cmp ecx,edx
 cmovg edx,ecx

 mov [edi+00000448],dl

 mov dl,[edi+00000449]
 cmp ecx,edx
 cmovg edx,ecx

 mov [edi+00000449],dl

 lea ecx,[edx+edx*4]
 add ecx,ecx

 mov [edi+18],cx

_ExitMMP:
 pop edx
 pop ecx
 pop ebx

 mov eax,[ebp-14]                  // Original code
 sub [edi+49],eax                  // Original code (set MP)
 jmp _BackMMP                      // Back to main code

//=========================================
// During Combat
_GodMode:
 push eax

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 mov eax,[ecx+000000e0]
 cmp eax,[iBattleID]
 jne _ExitGMD

 mov esi,[ecx+60]
 mov [ecx+4c],esi

 xor esi,esi

_ExitGMD:
 pop eax

 sub [ecx+4C],esi                  // Original code (set SIZE)
 cmp dword ptr [ecx+4C],00         // Original code
 jmp _BackGMD                      // Back to main code

//=========================================
// On other ways
_MonMovePoints2:
 movzx eax,[edi+1e]
 cmp eax,[iID]
 jne _ExitMP2

 cmp dword ptr [iEnableMMP],0
 je _ExitMP2                       // Jump if feature is disabled

 mov eax,[edi+45]
 mov [edi+49],eax

 xor eax,eax
 mov [ebp+1c],eax

_ExitMP2:
 mov eax,[ebp+1C]                  // Original code
 sub [edi+49],eax                  // Original code (set MP)
 jmp _BackMP2                      // Back to main code

//=========================================
// On Leaving a Ship
_MonMovePoints3:
 push eax

 mov [edx+49],00000000             // Original code (set MP)

 movzx eax,[edx+1e]
 cmp eax,[iID]
 jne _ExitMP3

 cmp dword ptr [iEnableMMP],0
 je _ExitMP3                       // Jump if feature is disabled

 mov eax,[edx+45]
 mov [edx+49],eax

_ExitMP3:
 pop eax
 jmp _BackMP3                      // Back to main code

//=========================================
// On getting onto a Ship
_MonMovePoints4:
 push eax

 mov [ecx+49],00000000             // Original code (set MP)

 movzx eax,[ecx+1e]
 cmp eax,[iID]
 jne _ExitMP4

 cmp dword ptr [iEnableMMP],0
 je _ExitMP4                       // Jump if feature is disabled

 mov eax,[ecx+45]
 mov [ecx+49],eax

_ExitMP4:
 pop eax
 jmp _BackMP4                      // Back to main code

//=========================================
// On Battle Start
_MonBeforeBattle:
 push eax

 mov eax,[ebp-0c]
 test eax,eax
 jz _ExitMBB

 movzx eax,[eax+1e]
 cmp eax,[iID]
 mov eax,ebx
 je _MonBB00

 mov eax,1
 xor eax,ebx

_MonBB00:
 mov [iBattleID],eax

_ExitMBB:
 pop eax

 push [edx+esi*4+1C]               // Original code (Size)
 push [edx+esi*4]                  // Original code (Type)
 jmp _BackMBB                      // Back to main code

