  db F3 A4 48 8B 44 24 50
angle2:
  db F3 A4 48 8B 44 24 40
unregistersymbol(angle1)
unregistersymbol(angle2)
dealloc(newmemAngle1)
dealloc(newmemAngle2)


AdvancedFreeCam+07:
  db 8B 42 40 89 41 F0

CamPos:
  db 8B 02 89 01 8B 42 04

//unregistersymbol(CamPos)
unregistersymbol(AdvancedFreeCam)
unregistersymbol(followNormalCam)

dealloc(newmem)
dealloc(normalCamPrevPos)

