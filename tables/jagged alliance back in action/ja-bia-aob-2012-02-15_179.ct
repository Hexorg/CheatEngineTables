// Trade - Copy Inv Function
//Game.exe+11AC70 - 53                  - push ebx
//Game.exe+11AC71 - 55                  - push ebp
//Game.exe+11AC72 - 56                  - push esi
//Game.exe+11AC73 - 57                  - push edi
//Game.exe+11AC74 - 0FB7 51 10          - movzx edx,word ptr [ecx+10]
//Game.exe+11AC78 - 66 89 50 10         - mov [eax+10],dx
//Game.exe+11AC7C - 0FB7 51 12          - movzx edx,word ptr [ecx+12]
//Game.exe+11AC80 - 66 89 50 12         - mov [eax+12],dx
//Game.exe+11AC84 - 0FB7 51 14          - movzx edx,word ptr [ecx+14]
//Game.exe+11AC88 - 66 89 50 14         - mov [eax+14],dx

// Militia Start Trade
//Game.exe+1D9EE - D9 9D 28030000       - fstp dword ptr [ebp+00000328]
//Game.exe+1D9F4 - 8B 8D 18030000       - mov ecx,[ebp+00000318]
//Game.exe+1D9FA - 50                   - push eax
//Game.exe+1D9FB - 51                   - push ecx
//Game.exe+1D9FC - E8 FFDE0000          - call Game.exe+2B900
//Game.exe+1DA01 - 8B 55 74             - mov edx,[ebp+74]       // This is the EIP on Stack that we want to look for from the "Setup trade function"
//Game.exe+1DA04 - 8B 8D D8000000       - mov ecx,[ebp+000000D8]
//Game.exe+1DA0A - 6A 01                - push 01
//Game.exe+1DA0C - 89 9D DC000000       - mov [ebp+000000DC],ebx
//Game.exe+1DA12 - C6 85 2C030000 01    - mov byte ptr [ebp+0000032C],01
//Game.exe+1DA19 - 8B 82 C8000000       - mov eax,[edx+000000C8]
// 8B55748B8DD80000006A01899DDC000000 (+0)
aobscan(aob_militia_trade_start,8B55748B8DD80000006A01899DDC000000)

// Militia End Trade
//Game.exe+1DC92 - 8B B6 18030000       - mov esi,[esi+00000318]
//Game.exe+1DC98 - 8B 46 18             - mov eax,[esi+18]
//Game.exe+1DC9B - 3B C5                - cmp eax,ebp
//Game.exe+1DC9D - 74 08                - je Game.exe+1DCA7
//Game.exe+1DC9F - 8D 4E 1C             - lea ecx,[esi+1C]
//Game.exe+1DCA2 - E8 C9CF0F00          - call Game.exe+11AC70 // This is the call to Copy Inv to write back the changes
//Game.exe+1DCA7 - 5F                   - pop edi
//Game.exe+1DCA8 - 89 6E 18             - mov [esi+18],ebp
// 8BB6180300008B46183BC574088D4E1CE8 (JE +B) (Call +10) (Pop +15)
aobscan(aob_militia_trade_end,8BB6180300008B46183BC574088D4E1CE8)

// SetUp Trade Function
//Game.exe+2B954 - 8D 8D 74010000       - lea ecx,[ebp+00000174] // ECX is the address of the inv to pass to the copy trade function
//Game.exe+2B95A - 8D 43 1C             - lea eax,[ebx+1C]
//Game.exe+2B95D - 89 4B 18             - mov [ebx+18],ecx
//Game.exe+2B960 - E8 0BF30E00          - call Game.exe+11AC70
// 8D8D740100008D431C894B18  (+0)
aobscan(aob_setup_trade,8D8D740100008D431C894B18)

// Start TeamMate Trade
//Game.exe+1B61D - 8D B3 D4000000       - lea esi,[ebx+000000D4]
//Game.exe+1B623 - 50                   - push eax
//Game.exe+1B624 - 51                   - push ecx
//Game.exe+1B625 - E8 D6020100          - call Game.exe+2B900 // Load Active Char Inventory
//Game.exe+1B62A - 8B 54 24 14          - mov edx,[esp+14]
//Game.exe+1B62E - 8B 83 D8000000       - mov eax,[ebx+000000D8]
//Game.exe+1B634 - 52                   - push edx
//Game.exe+1B635 - 50                   - push eax
//Game.exe+1B636 - E8 C5020100          - call Game.exe+2B900 // Load Targetted Char Inventory
//Game.exe+1B63B - BF 02000000          - mov edi,00000002    // This is the EIP we'd look for in 'SetUpTrade' to pass a different inventory
//Game.exe+1B640 - 8B 0E                - mov ecx,[esi]
//Game.exe+1B642 - 51                   - push ecx
// 5250E8xxxxxxxxBF020000008B0E (+7)
aobscan(aob_teammate_trade_start,5250E8xxxxxxxxBF020000008B0E)

// Finish TeamMate Trade
//Game.exe+1B683 - 8D B9 D4000000       - lea edi,[ecx+000000D4]
//Game.exe+1B689 - BB 02000000          - mov ebx,00000002
//Game.exe+1B68E - 8B FF                - mov edi,edi
//Game.exe+1B690 - 8B 37                - mov esi,[edi]
//Game.exe+1B692 - 8B 46 18             - mov eax,[esi+18]
//Game.exe+1B695 - 85 C0                - test eax,eax
//Game.exe+1B697 - 74 08                - je Game.exe+1B6A1
//Game.exe+1B699 - 8D 4E 1C             - lea ecx,[esi+1C]
//Game.exe+1B69C - E8 CFF50F00          - call Game.exe+11AC70
//Game.exe+1B6A1 - 83 C7 04             - add edi,04
