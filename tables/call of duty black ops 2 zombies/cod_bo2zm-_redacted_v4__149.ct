    <CheatEntry>
      <ID>46</ID>
      <Description>"Call of Duty BlackOps 2 - Zombie Mode"</Description>
      <LastState Activated="0"/>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{
========================================================
 Game Title    : Call of Duty BlackOps 2 Zombie Mode - (Redacted Client v4)
 Proces Name   : t6zm.exe
 CE Version    : 6.4
 Author        : XByte07
 Features:
 - Infinite Health
 - Infinite Point
 - Infinite Ammo
 - Infinite Ammo in Clip &amp; Grenade
 - One Hit Kill
 - No Recoil
 - Rapid Fire
========================================================
}
[ENABLE]
aobscanmodule(Health,t6zm.exe,29 B7 A8 01 00 00)
aobscanmodule(Point,t6zm.exe,89 B3 C8 55 00 00 * * * 1B)
aobscanmodule(Ammo,t6zm.exe,01 08 79 06 C7 00 00 00 00 00)
aobscanmodule(AmmoinClip,t6zm.exe,89 10 8B C2 C3 8B)
aobscanmodule(OHK,t6zm.exe,29 B0 A8 01 00 00)
aobscanmodule(NoRecoil,t6zm.exe,74 05 C6 44 24 10 00 66)
aobscanmodule(RapidFire,t6zm.exe,89 03 80 BF 34 06 00 00 00)

alloc(mem,2048)

label(_Health)
label(Back_Health)
label(Exit_Health)
label(Flag_Health)
registersymbol(Health)
registersymbol(Flag_Health)

label(_Point)
label(Back_Point)
label(Exit_Point)
label(Flag_Point)
registersymbol(Point)
registersymbol(Flag_Point)

label(_Ammo)
label(Back_Ammo)
label(Exit_Ammo)
label(Flag_Ammo)
registersymbol(Ammo)
registersymbol(Flag_Ammo)

label(_AmmoinClip)
label(Back_AmmoinClip)
label(Exit_AmmoinClip)
label(Flag_AmmoinClip)
registersymbol(AmmoinClip)
registersymbol(Flag_AmmoinClip)

label(_OHK)
label(Back_OHK)
label(Exit_OHK)
label(Flag_OHK)
registersymbol(OHK)
registersymbol(Flag_OHK)

label(_NoRecoil)
label(Back_NoRecoil)
label(Exit_NoRecoil)
label(Flag_NoRecoil)
registersymbol(NoRecoil)
registersymbol(Flag_NoRecoil)

label(_RapidFire)
label(Back_RapidFire)
label(Exit_RapidFire)
label(Flag_RapidFire)
registersymbol(RapidFire)
registersymbol(Flag_RapidFire)

mem:
Flag_Health:
dd 0
Flag_Point:
dd 0
Flag_Ammo:
dd 0
Flag_AmmoinClip:
dd 0
Flag_OHK:
dd 0
Flag_NoRecoil:
dd 0
Flag_RapidFire:
dd 0

// Infinite Health
_Health:
cmp [Flag_Health],1
jne Back_Health
mov [edi+000001A8],#999999
mov [edi+000001AC],#999999
jmp Exit_Health

Back_Health:
sub [edi+000001A8],esi
jmp Exit_Health

// Infinite Point
_Point:
cmp [Flag_Point],1
jne Back_Point
mov [ebx+000055C8],#100000
jmp Exit_Point

Back_Point:
mov [ebx+000055C8],esi
jmp Exit_Point

// Infinite Ammo
_Ammo:
cmp [Flag_Ammo],1
jne Back_Ammo
mov [eax],#999
jmp Exit_Ammo

Back_Ammo:
add [eax],ecx
jns t6zm.exe+1E2778
mov [eax],00000000
jmp Exit_Ammo

// Infinite Ammo in Clip &amp; Grenade
_AmmoinClip:
cmp [Flag_AmmoinClip],1
jne Back_AmmoinClip
db 90 90
mov eax,edx
ret
jmp Exit_AmmoinClip

Back_AmmoinClip:
mov [eax],edx
mov eax,edx
ret
jmp Exit_AmmoinClip

// One Hit Kill
_OHK:
cmp [Flag_OHK],1
jne Back_OHK
sub [eax+000001A8],#999999
jmp Exit_OHK

Back_OHK:
sub [eax+000001A8],esi
jmp Exit_OHK

// No Recoil
_NoRecoil:
cmp [Flag_NoRecoil],1
jne Back_NoRecoil
db 90 90
mov byte ptr [esp+10],00
jmp Exit_NoRecoil

Back_NoRecoil:
je t6zm.exe+AA86A
mov byte ptr [esp+10],00
jmp Exit_NoRecoil

// Rapid Fire
_RapidFire:
cmp [Flag_RapidFire],1
jne Back_RapidFire
db 90 90
jmp Exit_RapidFire

Back_RapidFire:
mov [ebx],eax
cmp byte ptr [edi+00000634],00
jmp Exit_RapidFire


Health:
jmp _Health
nop

Exit_Health:

Point:
jmp _Point
nop

Exit_Point:

Ammo:
jmp _Ammo
nop
nop
nop
nop
nop

Exit_Ammo:

AmmoinClip:
jmp _AmmoinClip

Exit_AmmoinClip:

OHK:
jmp _OHK
nop

Exit_OHK:

NoRecoil:
jmp _NoRecoil
nop
nop

Exit_NoRecoil:

RapidFire:
jmp _RapidFire
nop
nop
nop
nop

Exit_RapidFire:


[DISABLE]
dealloc(mem)
Health:
db 29 B7 A8 01 00 00
Point:
db 89 B3 C8 55 00 00
Ammo:
db 01 08 79 06 C7 00 00 00 00 00
AmmoinClip:
db 89 10 8B C2 C3 8B
OHK:
db 29 B0 A8 01 00 00
NoRecoil:
db 74 05 C6 44 24 10 00 66
RapidFire:
db 89 03 80 BF 34 06 00 00 00
unregistersymbol(Health)
unregistersymbol(Flag_Health)
unregistersymbol(Point)
unregistersymbol(Flag_Point)
unregistersymbol(Ammo)
unregistersymbol(Flag_Ammo)
unregistersymbol(AmmoinClip)
unregistersymbol(Flag_AmmoinClip)
unregistersymbol(OHK)
unregistersymbol(Flag_OHK)
unregistersymbol(NoRecoil)
unregistersymbol(Flag_NoRecoil)
unregistersymbol(RapidFire)
unregistersymbol(Flag_RapidFire)
</AssemblerScript>
