 je _ExitMML                       // Jump if feature is disabled

 cmp edi,[pPlayer]
 jne _ExitMML

 mov eax,#30000
 cmp eax,[ebx+ecx*8+04]
 js _ExitMML

 mov [ebx+ecx*8+04],eax            // Additional # of leaders (30)

_ExitMML:
 cmp byte ptr [edx+000026D1],00    // Original code
 jmp _BackMML                      // Back to main code

//========================================= 124 new
