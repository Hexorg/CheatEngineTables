 jz _ChkTP01

 cmp ecx,edx
 jns _ChkTP01

_ChkTP00:
 cmp ebx,[ecx]
 je _ExitCTP

 add ecx,4
 cmp ecx,edx
 js _ChkTP00

_ChkTP01:
 mov ecx,1
 test ecx,ecx

_ExitCTP:
 pop edx
 pop ecx
 pop eax
 pop ebx

 ret 0004

