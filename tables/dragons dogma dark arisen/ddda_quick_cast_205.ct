dd (float)500
emptybar:
dd 0

quickcast:
  comiss xmm0, [emptybar]
  jz next
  movss xmm0, [fullbar]

next:
  movss [ebx+00000140],xmm0
  jmp return

UpdateCastTimeAOB:
  jmp quickcast
  nop
  nop
  nop
return:
registersymbol(UpdateCastTimeAOB)

[DISABLE]

