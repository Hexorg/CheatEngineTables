 mov ecx,[ecx+1c]                  // Get ppTown
 mov ecx,[ecx+30]                  // Get pTown

 movzx ecx,byte ptr [ecx+45]       // Get ID

 cmp ecx,[iID]
 jne _ExitTUE

 mov ecx,[ebx+28]
 mov [ebx+3C],ecx                  // complete production

_ExitTUE:
 pop ecx

 jmp JTUE                          // Original code (rd POP)
 jmp _BackTUE                      // Back to main code (template)

