je short lblBuilderInstant
cmp dword ptr [bEnableBuildSpeedMod],1
jne short lblBuilderSkip
fild dword ptr [ebp-18]
fmul dword ptr [fBuildSpeedMod]
fistp dword ptr [ebp-18]
lblBuilderSkip:
