 cmp dword ptr [iEnableMIN],0
 je _ExitMIN                       // Jump if feature is disabled

 mov eax,7
 mov ebx,[ecx+08]
 cmp ebx,eax
 jle _MonIN00

 mov eax,#27
 cmp ebx,eax
 jle _MonIN00

 mov eax,#47
 cmp ebx,eax
 jge _ExitMIN

_MonIN00:
 mov [ecx+08],eax

_ExitMIN:
 pop ebx
 pop eax

