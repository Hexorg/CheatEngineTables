hookSaleSetup:
  jmp codeSale
  nop
_hookSaleSetup:

hookBuySetup:
  jmp codeBuy
  nop
_hookBuySetup:


newmem:
//variables
iPatience:
  dd 0


//functions

codeSelectCustomer:
  mov [recettear.exe+6F0B570],ecx //original code

  //Compute Custumer Patience
  pushad
  pushfd

  call recettear.exe+60F16
  mov [iPatience],eax

  popfd
  popad
  jmp _hookSelectCustomer


codeSale:
  inc [recettear.exe+6F0B584]
  pushad
  pushfd
  //if patience limit is reached ...
  mov eax,[recettear.exe+6F0B584]
  cmp eax,[iPatience]
  je saleFinalOffer
saleHaggleOffer:
  mov eax,[recettear.exe+6F0B580]
  dec eax
  mov [recettear.exe+1C6BB8],eax
  jmp saleCleanUp
saleFinalOffer:
  //assigns to the most extreme accepted value
  mov eax,[recettear.exe+6F0B574]
  mov [recettear.exe+1C6BB8],eax
saleCleanUp:
  popfd
  popad
  jmp _hookSaleSetup


codeBuy:
  inc [recettear.exe+6F0B584]
  pushad
  pushfd
  //if patience limit is reached ...
  mov eax,[recettear.exe+6F0B584]
  cmp eax,[iPatience]
  je buyFinalOffer
buyHaggleOffer:
  mov eax,[recettear.exe+6F0B580]
  inc eax
  mov [recettear.exe+1C6BB8],eax
  jmp buyCleanUp
buyFinalOffer:
  //assigns to the most extreme accepted value
  mov eax,[recettear.exe+6F0B574]
  mov [recettear.exe+1C6BB8],eax
buyCleanUp:
  popfd
  popad
  jmp _hookBuySetup




