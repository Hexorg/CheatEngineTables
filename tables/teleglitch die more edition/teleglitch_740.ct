    <CheatEntry>
      <ID>11</ID>
      <Description>"Script: Player Health"</Description>
      <LastState Activated="1" />
      <Color>FF0000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{ Game   : Teleglitch.exe
  Version: 
  Date   : 2015-10-02
  Author : Jason

  All credit to Shinkansen for finding this injection point.

  EAX+00BC: (float) Current Health
  EAX+00C0: (float) Current Armor
  EAX+00C4: (float) Max Health (+Armor?)
  EAX+00FC: (int) 1 for invulnerable, 0 for not
}

[ENABLE]

globalalloc(globals,$100000)

aobscanmodule(INJECT_HEALTH,teleglitch.exe,D9 80 BC 00 00 00) // should be unique
alloc(newmem,$1000)

label(code)
label(skip)
label(skip2)
label(return)

newmem:

code:
  // make sure we're using the player
  cmp [eax+00000014],79616c70 // "play", string is "player"
  jne skip
  mov [globals+1000],eax
  cmp [globals+2000],1
  jne skip2
  fld dword ptr [eax+000000C4] // load max health (+armor?  100.0)
  fst dword ptr [eax+000000BC] // save current health
  fstp dword ptr [eax+000000C0] // save current armor
  mov dword ptr [eax+000000FC],1 // 'invulnerable flag' I think
  jmp skip

skip2:
  mov dword ptr [eax+000000FC],0 // 'invulnerable flag' needs to be disabled
skip:
  fld dword ptr [eax+000000BC]
  jmp return

INJECT_HEALTH:
  jmp code
  nop
return:
registersymbol(INJECT_HEALTH)

[DISABLE]

INJECT_HEALTH:
  db D9 80 BC 00 00 00

unregistersymbol(INJECT_HEALTH)
dealloc(newmem)

