jne originalcode
cmp dword ptr [ecx+64],00000000
jne originalcode
cmp dword ptr [ecx+60],00000000
jne originalcode

mov [pAC],ebx
mov ecx,[ebx]
sub ecx,eax
cmp ecx,[minAC]
jge originalcode
