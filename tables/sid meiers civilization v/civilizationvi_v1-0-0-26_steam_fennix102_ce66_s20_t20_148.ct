 jne _ExitMSC

 cmp dword ptr [iEnableCMP],0
 je _ExitMSC                       // Jump if feature is disabled

 mov ecx,#2
 cmp [rax+000001E8],ecx
 jns _ExitMSC

 mov [rax+000001E8],ecx

_ExitMSC:
 mov ecx,[rax+000001E8]            // Original code (get Pop)
 jmp _BackMSC                      // Back to main code

