function OtherEditor_OnShow(sender)
  local scan = nil

  if base_mira == nil then
    scan = AOBScan(AOB_mira)
    base_mira = getAddress(scan[0]) - 80
    base_bp = base_mira + 4
    base_sepithEarth = base_mira + 8
    base_sepithWater = base_mira + 12
    base_sepithFire = base_mira + 16
    base_sepithWind = base_mira + 20
    base_sepithTime = base_mira + 24
    base_sepithSpace = base_mira + 28
    base_sepithMirage = base_mira + 32
    scan = nil
  end

  if base_charEstelle == nil then
    scan = AOBScan(AOB_charEstelle)
    --base_charEstelle = getAddress(scan[0]) - 352
    base_charEstelle = base_mira - 7716
    base_charJoshua = base_charEstelle + 52
    base_charScherazard = base_charEstelle + 104
    base_charOliver = base_charEstelle + 156
    base_charKloe = base_charEstelle + 208
    base_charAgate = base_charEstelle + 260
    base_charTita = base_charEstelle + 312
    base_charZane = base_charEstelle + 364
    scan = nil
  end

  OtherEditor_btnRefreshClick()
end

