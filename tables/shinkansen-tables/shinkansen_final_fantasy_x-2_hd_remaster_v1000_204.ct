label(playerstats_gilset_exit)
label(playerstats_partyset_exit)

registersymbol(playerstats_savedAOB)

//FFX-2.exe+32721D - F6 C4 41              - test ah,41
//FFX-2.exe+327220 - 7A 3A                 - jp FFX-2.exe+32725C
// Accesses Play Time
//FFX-2.exe+327222 - 8B 86 BC000000        - mov eax,[esi+000000BC]
//FFX-2.exe+327228 - 85 C0                 - test eax,eax
//FFX-2.exe+32722A - 79 0C                 - jns FFX-2.exe+327238
//FFX-2.exe+32722C - C7 86 BC000000 00000000 - mov [esi+000000BC],00000000
//FFX-2.exe+327236 - EB 1A                 - jmp FFX-2.exe+327252
aobscan(playerstats_searchedAOB, 8B 86 BC 00 00 00 85 C0 79 0C)

alloc(playerstats_newmem, 2048, playerstats_searchedAOB)


playerstats_newmem:
cmp dword ptr [playerstats_update_enable], #0
je playerstats_update_exit
mov [playerstats_basepointer], esi
playerstats_update_exit:

cmp dword ptr [playerstats_set_enable], #0
je playerstats_set_exit
playerstats_set_exit:

cmp dword ptr [playerstats_timeset_enable], #0
je playerstats_timeset_exit
mov [esi+bc], #0
playerstats_timeset_exit:

cmp dword ptr [playerstats_gilset_enable], #0
je playerstats_gilset_exit
mov [esi+77d8], #999999999
playerstats_gilset_exit:

cmp dword ptr [playerstats_partyset_enable], #0
je playerstats_partyset_exit
playerstats_partyset_exit:


playerstats_originalcode:
db 8B 86 BC 00 00 00
//mov eax,[esi+000000BC]


playerstats_exit:
jmp playerstats_returnhere


playerstats_searchedAOB:
playerstats_savedAOB:
jmp playerstats_newmem
nop
playerstats_returnhere:


[DISABLE]
playerstats_savedAOB:
db 8B 86 BC 00 00 00
//mov eax,[esi+000000BC]

unregistersymbol(playerstats_savedAOB)

dealloc(playerstats_newmem)

