function FindItemDisplayString()
  local pItem = readInteger(getAddress("pLastItemChanged"))
  local pDisplayString = pItem + 0x30
  local b1 = dectohex(readBytes(pDisplayString + 0, 1))
  local b2 = dectohex(readBytes(pDisplayString + 1, 1))
  local b3 = dectohex(readBytes(pDisplayString + 2, 1))
  local b4 = dectohex(readBytes(pDisplayString + 3, 1))
  local aob = b1.." "..b2.." "..b3.." "..b4.." ?? ".." ?? ".." ?? ".." ?? ".." ?? ".." "..b2.." "..b3.." "..b4
  local results = AOBScan(aob)
  local count = 0
  local result = 0

  if (results ~= nil) then
    count = stringlist_getCount(results)
    if (count &gt; 0) then
      result = stringlist_getString(results, 0)
    end
  end

  autoAssemble([[
    label(pDisplayStringMap)
    registersymbol(pDisplayStringMap)
    ]]..result..[[:
    pDisplayStringMap:
  ]])

  object_destroy(results)
  results = nil
end

