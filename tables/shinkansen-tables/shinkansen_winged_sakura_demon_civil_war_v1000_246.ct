      <ID>114096</ID>
      <Description>"LootTable Base Address Scan Initialization (Activate This First)"</Description>
      <Options moHideChildren="1"/>
      <LastState Activated="0"/>
      <Color>800000</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
registersymbol(LootTable_basepointer)
registersymbol(LootTable_update_enable)
registersymbol(LootTable_set_enable)
registersymbol(LootTable_lootset_enable)

alloc(LootTable_basepointer, 4)
alloc(LootTable_update_enable, 4)
alloc(LootTable_set_enable, 4)
alloc(LootTable_lootset_enable, 4)


[DISABLE]
unregistersymbol(LootTable_basepointer)
unregistersymbol(LootTable_update_enable)
unregistersymbol(LootTable_set_enable)
unregistersymbol(LootTable_lootset_enable)

dealloc(LootTable_basepointer)
dealloc(LootTable_update_enable)
dealloc(LootTable_set_enable)
dealloc(LootTable_lootset_enable)

</AssemblerScript>
      <CheatEntries>
        <CheatEntry>
          <ID>114097</ID>
          <Description>"LootTable Base Address Scan (To Activate: Receive Loot, To Update: Receive Loot)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(LootTable_savedAOB)
label(LootTable_returnhere)
label(LootTable_originalcode)
label(LootTable_exit)

label(LootTable_update_exit)
label(LootTable_set_exit)
label(LootTable_lootset_exit)

registersymbol(LootTable_savedAOB)

//050FB373 - 83 EC 08              - sub esp,08
//050FB376 - 8B 45 08              - mov eax,[ebp+08]
// LootTable:Update+9
//050FB379 - 8B 40 2C              - mov eax,[eax+2C]
//050FB37C - 85 C0                 - test eax,eax
//050FB37E - 74 16                 - je 050FB396
//050FB380 - 8B 45 08              - mov eax,[ebp+08]
//050FB383 - 8B 40 2C              - mov eax,[eax+2C]
//050FB386 - 83 EC 0C              - sub esp,0C
aobscan(LootTable_searchedAOB, D9 45 FC D9 45 08 DF F1)

alloc(LootTable_newmem, 2048, LootTable_searchedAOB)
alloc(LootTable_temp1, 4, LootTable_searchedAOB)
alloc(LootTable_temp2, 4, LootTable_searchedAOB)
alloc(LootTable_temp3, 4, LootTable_searchedAOB)
alloc(LootTable_temp4, 4, LootTable_searchedAOB)


LootTable_newmem:
mov [LootTable_temp1], eax
mov [LootTable_temp2], ebx
mov [LootTable_temp3], ecx
mov [LootTable_temp4], edx

cmp dword ptr [LootTable_update_enable], #0
je LootTable_update_exit
LootTable_update_exit:

cmp dword ptr [LootTable_set_enable], #0
je LootTable_set_exit
LootTable_set_exit:

cmp dword ptr [LootTable_lootset_enable], #0
je LootTable_lootset_exit
mov [ebp-4], (float)0
LootTable_lootset_exit:

mov eax, [LootTable_temp1]
mov ebx, [LootTable_temp2]
mov ecx, [LootTable_temp3]
mov edx, [LootTable_temp4]


LootTable_originalcode:
db D9 45 FC D9 45 08
//fld dword ptr [ebp-04]
//fld dword ptr [ebp+08]


LootTable_exit:
jmp LootTable_returnhere


LootTable_searchedAOB:
LootTable_savedAOB:
jmp LootTable_newmem
nop
LootTable_returnhere:


[DISABLE]
LootTable_savedAOB:
db D9 45 FC D9 45 08
//fld dword ptr [ebp-04]
//fld dword ptr [ebp+08]

unregistersymbol(LootTable_savedAOB)

dealloc(LootTable_newmem)
dealloc(LootTable_temp1)
dealloc(LootTable_temp2)
dealloc(LootTable_temp3)
dealloc(LootTable_temp4)

</AssemblerScript>
