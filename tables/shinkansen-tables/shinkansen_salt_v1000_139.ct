je PlayerMovement_dodgeset_exit
mov [edi+6c], (float)60
mov [edi+70], (float)100
mov [edi+74], (float)0
PlayerMovement_dodgeset_exit:


PlayerMovement_originalcode:
db D9 5F 7C 8B 47 1C
//fstp dword ptr [edi+7C]
//mov eax,[edi+1C]


PlayerMovement_exit:
jmp PlayerMovement_returnhere


PlayerMovement_searchedAOB:
PlayerMovement_savedAOB:
jmp PlayerMovement_newmem
nop
PlayerMovement_returnhere:


