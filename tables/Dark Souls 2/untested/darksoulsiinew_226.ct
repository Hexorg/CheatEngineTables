function getItemName(index)
  local base = readPointer("DarkSoulsII.exe+160DC90")
  --base = readPointer("7FFEDED80A0+100+1D8")
  base = readPointer(base + 0x100 + 0x1D8)
  local maxIndex = readInteger(base+0xC)
  --print(maxIndex)
  local addr = base + index * 4
  local itemId = readInteger(addr+0x20)
  local my_eax = readInteger(addr+0x1C)
  local my_ecx = readInteger(base+0x14)
  my_eax = my_eax - readInteger(addr+0x20)
  my_ecx = my_ecx + base
  my_eax = my_eax + readInteger(addr+0x24)
  my_eax = readInteger(my_ecx + my_eax * 4)
  local itemName = readString(base + my_eax, 500, true)
  print(itemName .. " = " .. itemId)
  --print(itemId)
  --print(my_eax)
  --print(string.format("%02X",my_ecx))
  --print(string.format("%02X",addr))
end
