  <LuaScript>--[[Rarity Editor]]--

Snippet =
 {
   ScanForIt = function (scanThis)
     local newscanObj = createMemScan()

     memscan_returnOnlyOneResult(newscanObj,true)
     memscan_firstScan(newscanObj, soExactValue, vtGrouped, rtTruncated,
                       scanThis,nil,"00000000","7fffffff","+W*X-C",fsmAligned,
                       "4" ,false ,false, false, false);
     memscan_waitTillDone(newscanObj)

     local RESULT = memscan_getOnlyResult(newscanObj)
     object_destroy(newscanObj)

     if RESULT==nil then return nil end
     return RESULT+32
   end,

   updateMemoryRecordAddress = function (registeredSymbol,scanThis)
     local address = Snippet.ScanForIt(scanThis)
     if address==nil then return end
     writeInteger(registeredSymbol,address)
   end
 }

RarityChances =
 {
  ID_2C = "4:44 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_2E = "4:46 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_30 = "4:48 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_31 = "4:49 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_32 = "4:50 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_33 = "4:51 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216",
  ID_34 = "4:52 w:20 4:0 4:1 4:* 4:0 4:0 4:1065353216 4:1065353216"
 }

function GetRarityChancesMemoryRecords()
  if getOpenedProcessID() == 0 then return end
  local timer = createTimer(nil,true)
  timer_onTimer(timer,GetRarityChancesMemoryRecordsDelayed)
end

function GetRarityChancesMemoryRecordsDelayed(timer)
  object_destroy(timer)

local script =[[
globalalloc(Rarity,1024)
label(ID_2C)
label(ID_2E)
label(ID_30)
label(ID_31)
label(ID_32)
label(ID_33)
label(ID_34)
registersymbol(ID_2C)
registersymbol(ID_2E)
registersymbol(ID_30)
registersymbol(ID_31)
registersymbol(ID_32)
registersymbol(ID_33)
registersymbol(ID_34)

Rarity:
ID_2C:
dd 0
ID_2E:
dd 0
ID_30:
dd 0
ID_31:
dd 0
ID_32:
dd 0
ID_33:
dd 0
ID_34:
dd 0
]]

  autoAssemble(script)

  local memrec = addresslist_getMemoryRecordByDescription(getAddressList(),"Item Rarity/Chances - Enable")
  memoryrecord_setDescription(memrec,"Scanning.... wait")
  processMessages()


  pause()
  for symbol,groupscancommand in pairs(RarityChances) do
    Snippet.updateMemoryRecordAddress(symbol,groupscancommand)
  end
  unpause()

  memoryrecord_setDescription(memrec,"Item Rarity/Chances - Enable")
end

function RevertAndDisable()
  if getOpenedProcessID() == 0 then return end
  local ID = {'ID_2C','ID_2E','ID_30','ID_31','ID_32','ID_33','ID_34'}
  local defaultValues = {200, 100, 10, 5, 1, 0.1, 0.01}
  for i=1,#ID do
    writeFloat('['..ID[i]..']', defaultValues[i])
    unregisterSymbol(ID[i])
  end
end

function presetSetValues(memrec)
  if getOpenedProcessID() == 0 then return end

  if memrec~=setOriginal      then memoryrecord_unfreeze(setOriginal)      end
  if memrec~=setPreset1Orange then memoryrecord_unfreeze(setPreset1Orange) end
  if memrec~=setPreset2       then memoryrecord_unfreeze(setPreset2)       end
  if memrec~=setPreset3       then memoryrecord_unfreeze(setPreset3)       end
  if memrec~=setPreset4       then memoryrecord_unfreeze(setPreset4)       end

  local ID = {'ID_2C','ID_2E','ID_30','ID_31','ID_32','ID_33','ID_34'}
  local case =
   {
    ["Revert All (Normal)"]  = {200, 100,  10,    5,    1,  0.1,  0.01},
    ["Preset 1 (Legendary)"] = {0.001, 0.001, 0.001,  0.001,  0.001,  0.001,  10000},
    ["Preset 2 (Purple)"]    = {0.001, 0.001, 0.001,  0.001,  0.001,  10000,  0.001},
    ["Preset 3 (Blue)"]      = {0.001, 0.001, 0.001,  0.001,  10000,  0.001,  0.001},
    ["Preset 4 (Eridium)"] = {0.001, 0.001,  0.001,    10000,    0.001,  0.001,  0.001}
   }

  local whichmemrec = memoryrecord_getDescription(memrec)

  for i=1,#ID do
    writeFloat('['..ID[i]..']', case[whichmemrec][i])
  end

  return true
end

setOriginal      = addresslist_getMemoryRecordByDescription(getAddressList(),"Revert All (Normal)")
setPreset1Orange = addresslist_getMemoryRecordByDescription(getAddressList(),"Preset 1 (Legendary)")
setPreset2       = addresslist_getMemoryRecordByDescription(getAddressList(),"Preset 2 (Purple)")
setPreset3       = addresslist_getMemoryRecordByDescription(getAddressList(),"Preset 3 (Blue)")
setPreset4       = addresslist_getMemoryRecordByDescription(getAddressList(),"Preset 4 (Eridium)")
memoryrecord_onActivate(setOriginal      ,presetSetValues)
memoryrecord_onActivate(setPreset1Orange ,presetSetValues)
memoryrecord_onActivate(setPreset2       ,presetSetValues)
memoryrecord_onActivate(setPreset3       ,presetSetValues)
memoryrecord_onActivate(setPreset4       ,presetSetValues)

