      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{ by HenryEx
  for The Binding of Isaac: Afterbirth

  finds the base floor (and a whole lot of other stuff) pointer
  hopefully version-independent
  floor pointer location as of 20.11.2015: isaac-ng.exe+2E2634
  stat pointer location as of 20.11.2015: isaac-ng.exe+2E417C
}

[ENABLE]
aobscanmodule(AOB_pFloor,isaac-ng.exe,D9 5D * 8B 45 08 D9 00 D8 45 * D9 5D)
aobscanmodule(AOB_pStats,isaac-ng.exe,8B 1D * * * * 8B 83 * * * * 8B C8 81 E1 01 00 00 80)
alloc(pFloor,256)
alloc(pStats,256)
registersymbol(AOB_pFloor)
registersymbol(pFloor)
registersymbol(AOB_pStats)
registersymbol(pStats)
label(_BackF)
label(_BackS)
label(_ExitF)
label(_ExitS)


AOB_pFloor+3:
 jmp pFloor+C
_BackF:

AOB_pStats+E:
 jmp pStats+10
 nop
_BackS:


pFloor:
 dd 0  // floor pointer
 dd 0  // item to display
 dd 0  // cur. item addr

// Dumping Code
 push ebx
 mov [pFloor],eax
 cmp byte ptr [eax],0       // is in-game?
 jz short _ExitF

// Do item display stuff
 mov ebx,[pFloor+4]         // get current item ID
 cmp ebx,0
 je +8                      // skip next 2 lines
 cmp ebx,#441               // max. item id
 jbe +A                     // skip next 3 lines

 xor ebx,ebx
 mov [pFloor+8],ebx         // null pointer
 jmp short _ExitF

 mov eax,[eax+B7D8]
 mov eax,[eax]
 lea ebx,[eax+ebx*4+1DA4]   // get item addr
 mov [pFloor+8],ebx         // store addr of current item

_ExitF:
 pop ebx
 mov eax,[ebp+08]
 fld dword ptr [eax]
 jmp _BackF


pStats:
 dd 0  // stats pointer
 dd 0  // secret to unlock
 dd 0  // cur. secret addr
 dd 0  // secrets active?

// Dumping Code
 mov [pStats],ebx
 push eax
 cmp byte ptr [pStats+C],0  // do secrets?
 je short _ExitS

// Do secrets code

 mov eax,[pStats+4]
 cmp eax,0
 je +9                      // skip next 2 lines
 cmp eax,#276               // max. secret num
 jbe +A                     // skip next 3 lines

 xor eax,eax
 mov [pStats+8],eax
 jmp short _ExitS

 lea eax,[ebx+eax+4C]       // 4 bytes
 mov [pStats+8],eax         // store addr of current secret
 cmp byte ptr [pStats+D],1
 jne short _ExitS           // skip next 2 lines

 mov byte ptr [eax],1
 mov byte ptr [pStats+D],0

_ExitS:
 pop eax
 and ecx,80000001
 jmp _BackS

[DISABLE]

AOB_pFloor+3:
 mov eax,[ebp+08]
 fld dword ptr [eax]
// db 8B 45 08 D9 00

AOB_pStats+E:
 and ecx,80000001
// db 81 E1 01 00 00 80

unregistersymbol(AOB_pFloor)
unregistersymbol(pFloor)
unregistersymbol(AOB_pStats)
unregistersymbol(pStats)
dealloc(pFloor)
dealloc(pStats)

