_GodMode:
 mov dword ptr [aLast],'_GMD'

 mov [pDebug],rbx

 cmp dword ptr [iEnableGMD],0
 je _ExitGMD                       // Jump if feature is disabled

 lea rax,[rcx-00000a48]
