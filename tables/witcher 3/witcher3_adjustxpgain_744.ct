      <AssemblerScript>{
Daijobu @ Cheatengine.org
For: Witcher3 v1.05 (GOG)

WARNING: BUG
Causes you to gain as much Skill Points as you set your EXP gain to ONLY when gaining a level.
}

[ENABLE]
{Seriously, 3 identical functions?
This WILL break if updated}
aobscanmodule(w3_pExpgain_aob,witcher3.exe,48 89 5C 24 08 56 57 41 56 48 83 EC 20 48 8B 42 30 C7 44 24 48 00 00 00 00 49 8B F0 0F B6 08 48 FF C0 4C 8D 35 B7 7F FC 00 48 89 42 30 8B C1 48 8B 0A 4C 8D 44 24 48 48 8B FA 41 FF 14 C6 48 8B 05 A3 05 FD 00 C7 44 24 50 00 00 00 00 48 85 C0 48 8D 5C 24 48 4C 8D 44 24 50 48 0F 45 D8 48 8B 47 30 48 8B D7 0F B6 08 48 FF C0 48 89 47 30 8B C1 48 8B 0F 41 FF 14 C6 48 FF 47 30 8B 44 24 50 01 03 8B 03 48 85 F6 74 02 89 06 48 8B 5C 24 40 48 83 C4 20 41 5E 5F 5E C3)
registersymbol(w3_pExpgain_aob)
{Offset: 7C}
alloc(w3_pExpgain_mem,128,witcher3.exe)

registersymbol(w3_pExpgain_ptr)
label(w3_pExpgain_ptr)

label(originalcode)
label(exit)

{-- Start --}
w3_pExpgain_mem+64:
w3_pExpgain_ptr:
dd 19 //4-byte

{-- Allocated Memory --}
w3_pExpgain_mem:
push rax //Store
mov rax,r9 //r9 is at any time 4 short of r8.
add rax,4 //Add 4 to r9 to make it identical to r8.
cmp r8,rax //Compare r8 and r9. If identical, yay! Otherwise, meh.
jne originalcode
pop rax //Restore
mov eax,[w3_pExpgain_ptr] //Dump our variable into eax.
add [rbx],eax //Add to be gained to total.
mov eax,[rbx] //Add new total to eax, later on gets move into [rsi].
jmp exit

{-- Original Code --}
originalcode:
pop rax //Restore
mov eax,[rsp+50] //[rsp+50] contains exp to be gained and other functions, such as inventory.
add [rbx],eax //Add to be gained to total.
mov eax,[rbx] //Add new total to eax, later on gets move into [rsi].
jmp exit

{-- Jump --}
w3_pExpgain_aob+7C: //"witcher3.exe"+17F821C:
jmp w3_pExpgain_mem
db 90 90 90
exit:

[DISABLE]
dealloc(w3_pExpgain_mem)
w3_pExpgain_aob+7C: //"witcher3.exe"+17F821C:
mov eax,[rsp+50]
add [rbx],eax
mov eax,[rbx]
//Alt: db 8B 44 24 50 01 03 8B 03
unregistersymbol(w3_pExpgain_ptr)
unregistersymbol(w3_pExpgain_aob)

