je PlayerMovement_jumpset_exit
mov eax, [eax+14]
mov [eax+c], (float)7.5
PlayerMovement_jumpset_exit:

cmp dword ptr [PlayerMovement_movementset_enable], #0
je PlayerMovement_movementset_exit
