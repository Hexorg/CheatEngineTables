MyCode:
//=========================================
_MonPlayer:
 cmp dword ptr [esi+00000724],0
 jne _ExitMP

 cmp dword ptr [esi+000005f4],0
 jne _MonP0

 mov [pPlayer],esi

 lea edi,[esi+000002f0]
 mov [pHP],edi

 mov edi,[esi+000007ec]
 mov [pCommon],edi

 cmp dword ptr [iEnableCD],0
 je _MonP1                         // Jump if feature is disabled

