        <CheatEntry>
          <ID>162</ID>
          <Description>"All Jobs Max"</Description>
          <Color>80000008</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>// Every Main Character has all Jobs at Level 5
// All Subs have all Jobs at their max of Level 4
// by HenryEx

[ENABLE]
alloc(MCJobs,1024)

// Declaration section
registersymbol(SubJobs)
label(SubJobs)
label(_JobLoop)
label(_JobLoop2)
label(_ExitMCJ)
label(_ExitSJ)

//=========================================
// Hacking Points
"CladunX2.exe"+3D3EF:
 jmp MCJobs
 nop
 nop
 nop
_ExitMCJ:

"CladunX2.exe"+3F12B:
 jmp SubJobs
_ExitSJ:


MCJobs:
//=========================================
// Code section:
// esi = address to current (main) char, edi = current Job
// eax = address to Job Skill database after the routine call
// ecx = free, edx = job effect (to be)
 xor edi,edi                  // set to Job 0 to start the loop

_JobLoop:
 inc edi                      // Job number + 1
 push edi                     // push current job as argument for routine
 call CladunX2.exe+2ADA0      // routine stores cur. job pointer in eax
 test eax,eax                 // Original code, check pointer validity
 je _ExitMCJ+15               // Original code, abort and go back if 0 pointer

 mov dx,[eax+0A]              // get Cur. Job Lvl 5 effect
 add [esi+edi*2+00000B18],dx  // store effect in Char Data
 add byte ptr [esi+edi+00000B31],05  // store Job Lvl 5 in Char Data
 cmp edi,#10                  // If this is the last job...
 jb _JobLoop                  // ... get back to game code

 mov eax,edi                  // do this cause O.C. did something similar
 jmp _ExitMCJ+E               // jmp to "CladunX2.exe"+3D405


SubJobs:
//=========================================
// esp+14 (before push / after routine) = address to current (sub) char
// ebx = supposed Job Lvl -1 (0-4), ebp/ecx = initial/to-be current Job
// eax = address to Job Skill database after the routine call
 xor ebp,ebp                  // set to Job 0 to start the loop

_JobLoop2:
 inc ebp                      // Job number + 1
 push ebp                     // push current job as argument for routine
 call CladunX2.exe+2ADA0      // routine stores cur. job pointer in eax
 test eax,eax                 // Original code, check pointer validity
 je _ExitSJ+1D                // Original code, abort and go back if 0 pointer

 mov bl,3                     // Set Job Lvl to 4 (cosmetic for now)
 mov dx,[eax+08]              // get Cur. Job Lvl 4 effect
 mov eax,[esp+14]             // get pointer to Sub Char Data
 add [eax+ebp*2+00000B18],dx  // store effect in Char Data
 lea eax,[ebp+eax+00000B31]   // eax = address to Char Data JobLvl
 inc bl                       // Original code, correct JobLvl (cosmetic)
 add byte ptr [eax],04        // store Job Lvl 4 in Char Data
 cmp ebp,#10                  // If this is the last job...
 jb _JobLoop2                 // ... get back to game code

 mov ecx,ebp                  // do this cause O.C. did something similar
 jmp _ExitSJ+1D               // jmp to "CladunX2.exe"+3F14D

[DISABLE]
"CladunX2.exe"+3D3EF:
 add [esi+edi*2+00000B18],dx
// db 66 01 94 7E 18 0B 00 00

"CladunX2.exe"+3F12B:
 mov dx,[eax+ebx*2+02]
// db 66 8B 54 58 02

unregistersymbol(SubJobs)
dealloc(MCJobs)
</AssemblerScript>
