_MonMinimumSRes:
 push edi

 mov ecx,[esi+00000C30]         // Original code

 cmp dword ptr [iEnableMMSR],0
 je _ExitMMSR                   // Jump if feature is disabled

 cmp esi,[pPlayer]
 jne _ExitMMSR

 mov edi,[ecx+ebx*4]
