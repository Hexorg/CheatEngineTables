GetTotalXp:
cmp eax,TBRID.exe+e3cc4
jb GetTotalXp_BadGuys
cmp eax,TBRID.exe+E3D44
ja GetTotalXp_BadGuys
  push edx
  push ecx         //save ecx, may not be needed
  mov edx,dword [eax+4]
  shr edx,#18               //now edx=pm01 xp
  mov ecx,dword [eax+C]
  and ecx, 3FFF            //now ecx=pm02 xp
  add edx,ecx              //now edx=pm01+pm02 xp
  mov ecx,dword [eax+C]
  shr ecx,E
  and ecx, 3FFF            //now ecx=pm03 xp
  add edx,ecx              //now edx=pm01+pm02+pm03 xp
  mov eax,edx
  pop ecx
  pop edx
retn
GetTotalXp_BadGuys:
xor eax,eax
retn

GetTotalXpEcxForm:
