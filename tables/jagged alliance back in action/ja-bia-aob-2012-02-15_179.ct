    <CheatEntry>
      <ID>7</ID>
      <Description>"Equipment Max Dura/Ammo/Charge when viewed."</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
alloc(EquipView,2048)

// Declarations
label(_EquipView_Start)
label(_EquipView_Back)
label(_EquipView_Exit)
label(_EquipView_Hook)
label(_mainhand)
label(_ammo)
label(_not_mainhand)
label(_not_ammo)
label(_not_clothing)
label(_offhand)

registersymbol(EquipView)
registersymbol(_EquipView_Hook)

aobscan(aob_equipview,668995740100000FB74704)

// Hack points
// Display item routine: player icons, inventory, loot, etc.
// 0042CD6A - 66 89 95 74010000          - mov [ebp+00000174],dx
// 0042CD71 - 0FB7 47 04                 - movzx eax,word ptr [edi+04]
// 668995740100000FB74704
// Game.exe+2CD6A:
aob_equipview:
_EquipView_Hook:
  jmp _EquipView_Start
  nop
  nop
_EquipView_Back:

EquipView:
_EquipView_Start:
 cmp word ptr [edi], FF
 je _EquipView_Exit
 cmp dword ptr [esi], 0 // Weapon
 jne _not_mainhand
_mainhand:
 // Look for mainhand durability and max rounds
 movzx eax, word ptr [esi+40] // Max Durability
 mov word ptr [edi+04], ax // Current Durability
 jmp _EquipView_Exit
_not_mainhand:
 cmp dword ptr [esi], 1 // Ammo
 jne _not_ammo
_ammo:
 cmp word ptr [ebp+170], FF
 je _EquipView_Exit
 // Otherwise it is a "loaded" stack where the not FF value is max rounds
 movzx eax, word ptr [ebp+170]
 mov word ptr [edi+02], ax
_not_ammo:
 cmp dword ptr [esi], 2
 jne _not_clothing
 mov eax, 64 // Set Clothing to Max Durability (100)
 mov word ptr [edi+04], ax
 jmp _EquipView_Exit
_not_clothing:
 cmp dword ptr [esi], 3
 jne _EquipView_Exit
_offhand:
 movzx eax, word ptr [esi+3C] // Max Charges
 mov word ptr [edi+06], ax
_EquipView_Exit:
 mov [ebp+00000174],dx
 jmp _EquipView_Back

// Original
[DISABLE]
//Game.exe+2CD6A:
_EquipView_Hook:
 mov [ebp+00000174],dx

dealloc(EquipView)
unregistersymbol(EquipView)
unregistersymbol(_EquipView_Hook)

</AssemblerScript>
