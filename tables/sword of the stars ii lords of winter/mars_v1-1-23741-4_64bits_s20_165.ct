 cmp dword ptr [r15],0
 je _ExitMSP                       // Jump if feature is disabled

 mov r9,[rax+00000ba0]             // Get pointer to current values
 test r9,r9
 jz _ExitMSP                       // Jump if it is null

 mov ecx,(float)10.0
 cmp ecx,[r9]
 jge _ExitMSP

 mov [r9],ecx

_ExitMSP:
