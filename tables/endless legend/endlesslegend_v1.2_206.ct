    mov ebp, esp

    cmp [n_city], 0
    je @o

    call clear_player_assets
    mov eax, a_city
    @@:
    mov ecx, [i_city]
    cmp ecx, [n_city]
    jl @f
    mov [i_city], 0
    jmp @b

    @@:
    lea ebx, [eax+ecx*4]
    cmp [ebx], 0
    jz @o
    push [i_city]
    push [ebx]
    call refresh_city
    add esp, 4
    pop [i_city]
    jmp @o

    @o:
    mov esp, ebp
    pop ebp
    ret

