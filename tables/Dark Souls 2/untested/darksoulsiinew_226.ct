{$lua}
FLIGHT_ENABLED = true

FLIGHT_INCREMENT = 1.0
FLIGHT_DECREMENT = -1.0

FLIGHT_OFFSET_X_GHOST = 0x1A0
FLIGHT_OFFSET_Y_GHOST = 0x1A8
FLIGHT_OFFSET_Z_GHOST = 0x1A4
FLIGHT_OFFSET_X_OTHER = 0x1B0
FLIGHT_OFFSET_Y_OTHER = 0x1B8
FLIGHT_OFFSET_Z_OTHER = 0x1B4
FLIGHT_OFFSET_X_MODEL = 0x1C0
FLIGHT_OFFSET_Y_MODEL = 0x1C8
FLIGHT_OFFSET_Z_MODEL = 0x1C4

if flight_getPointer == nil then
  function flight_getPointer()
    local addr = readPointer("DarkSoulsII.exe+01682E10")
    addr = readPointer(addr + 0x110)
    addr = readPointer(addr + 0x40)
    addr = readPointer(addr + 0x1D0)
    addr = readPointer(addr + 0x20)
    return addr
  end
end

if flight_set == nil then
  function flight_set(addr, offset1, offset2)
    local value = readFloat(addr + offset2)
    writeFloat(addr + offset1, value)
  end
end

if flight_reset == nil then
  function flight_reset()
    local addr = flight_getPointer()
    flight_set(addr, FLIGHT_OFFSET_X_GHOST, FLIGHT_OFFSET_X_MODEL)
    flight_set(addr, FLIGHT_OFFSET_Y_GHOST, FLIGHT_OFFSET_Y_MODEL)
    flight_set(addr, FLIGHT_OFFSET_Z_GHOST, FLIGHT_OFFSET_Z_MODEL)
  end
end

