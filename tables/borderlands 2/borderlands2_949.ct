--[[
Mob spawn point finder and Vermi setter

Start CE, attach it to b2, execute script
Hit insert to generate a list of the enemy dens
Home will reset the kill counter
Page up will increase max amount that can spawn
Page down will decrease max amount that can spawn
End will search for and swap out the adult varkid for vermi
hitting page up after all enemies are killed will cause only 1 more to spawn.
]]--

BaseAddressPointerString = '[[["Borderlands2.exe"+01600EC0]+358]+1E4]+0'

memscanusedbefore = false
ObjScan = createMemScan()
ObjResult = createFoundList(ObjScan)

BaseAddress = 0
Count = 0
CurrentObj = 0
AdultVarkidAddress = 0

function DenFind()

   BaseAddress = getAddress(BaseAddressPointerString)
   if BaseAddress==0 then
     addtolog("Couldn't find any Enemy Spawns")
     return
   end

   if (memscanusedbefore==true) then foundlist_deinitialize(ObjResult) end
   memscanusedbefore=true

   memscan_firstScan(ObjScan,soExactValue,vtDword,"",BaseAddress,
                     "0","0x00000000","0x70000000",'+W-X-C',fsmAligned,"4",false,false,false,false)
   memscan_waitTillDone(ObjScan)
   foundlist_initialize(ObjResult)
   Count = foundlist_getCount(ObjResult) - 1
   addtolog("Found " .. Count .. " Enemy Spawns")
   beep()
end

createHotkey("DenFind",45)

function KillReset()
  if BaseAddress == 0 then return end

  for i=1,Count do
    local offset = foundlist_getAddress(ObjResult,i)
      local address = getAddress(offset .. "+84")
      writeInteger(address,0)
  end
  addtolog("Kill counter reset")
  beep()
end

function MaxUp()
  if BaseAddress == 0 then return end

  for i=1,Count do
    local offset = foundlist_getAddress(ObjResult,i)
      local address = getAddress(offset .. "+60")
      local CurVal = readInteger(address)
      if CurVal~=nil then writeInteger(address,CurVal + 1) end
  end
  addtolog("Spawn max increased")
  beep()
end

function MaxDown()
  if BaseAddress == 0 then return end

  for i=1,Count do
    local offset = foundlist_getAddress(ObjResult,i)
      local address = getAddress(offset .. "+60")
      local CurVal = readInteger(address)
      if CurVal~=nil and CurVal&gt;1 then writeInteger(address,CurVal - 1) end
  end
  addtolog("Spawn max decreased")
  beep()
end

createHotkey("KillReset",36)
createHotkey("MaxUp",33)
createHotkey("MaxDown",34)


function getAdultVarkidAddress()
  for i=3,Count-2 do
    local offset = foundlist_getAddress(ObjResult,i)
    if readInteger(getAddress("[" .. offset .. "+24]+44")) == 2 then
      if readInteger(getAddress("[[" .. offset .. "+24]+40]+2c")) == 990057071 then
         return "[[" .. offset .. "+24]+40]+0"
      end
    end
  end
  return nil
end

memscanAdultVarkid = false
ObjScan2 = createMemScan()
VarResult = createFoundList(ObjScan2)

function VarkidSwap()
   if BaseAddress == 0 then return end

   local AdultVarkidAddress = getAdultVarkidAddress()

   if AdultVarkidAddress==nil then
     addtolog("AdultVarkid not Found")
     return
   end

   addtolog("AdultVarkidAddress = " .. AdultVarkidAddress)

   local Varkid = readInteger('[' .. AdultVarkidAddress .. ']+0')

   if (memscanAdultVarkid==true) then foundlist_deinitialize(VarResult) end
   memscanAdultVarkid=true

   memscan_firstScan(ObjScan2,soExactValue,vtDword,"",Varkid,
                     "0","0x00000000","0x70000000",'+W-X-C',fsmAligned,"4",false,false,false,false)
   memscan_waitTillDone(ObjScan2)
   foundlist_initialize(VarResult)

   local VarCount = foundlist_getCount(VarResult) - 1
   addtolog("Found " .. VarCount .. " Enemy Spawns")

   for i=1,VarCount do
      local offset = foundlist_getAddress(VarResult,i)
        if readString(getAddress("[[[" .. offset .. "+AC]+44]+4]+0"),10,true) == "Vermi" then
          writeInteger(getAddress(AdultVarkidAddress),getAddress(offset))
          addtolog("Adult Varkids Replaced with Vermi")
        end
   end
   beep()
end
