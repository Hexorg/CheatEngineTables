      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>{ Game   : isaac-ng.exe
  Version: 
  Date   : 2017-02-19
  Author : Akizman

  This script finds the player's base address
  Pointers from CTAB+ table
}

[ENABLE]

aobscanmodule(PLAYER,isaac-ng.exe,3B B9 18 1B 00 00) // should be unique
alloc(newmem,$1000)
alloc(Pbase,4)

label(code)
label(return)

registersymbol(Pbase)

newmem:

code:
  mov [Pbase],ecx
  cmp edi,[ecx+00001B18]
  jmp return

PLAYER:
  jmp newmem
  nop
return:
registersymbol(PLAYER)

[DISABLE]

PLAYER:
  db 3B B9 18 1B 00 00

unregistersymbol(Pbase)
unregistersymbol(PLAYER)
dealloc(newmem)
dealloc(Pbase)

</AssemblerScript>
