registersymbol(idscan_savedAOB)

aobscan(searchedAOB,8B 00 F6 40 05 03 74 0D 8B 4D 08)

alloc(idscan_newmem,2048) //2kb should be enough


searchedAOB:
idscan_savedAOB:
jmp idscan_newmem
nop
idscan_returnhere:


idscan_newmem:

// Money ID match "idEr"
cmp dword ptr [eax+12C], #1917150313
jne moneyidscan_exit
mov [moneyid_basepointer], eax
mov eax, [eax]
mov [money1_id], eax
mov eax, [moneyid_basepointer]
mov eax, [eax+4]
mov [money2_id], eax
mov eax, [moneyid_basepointer]
moneyidscan_exit:

// Mandate ID match "CurE"
