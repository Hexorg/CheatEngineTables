//=========================================
// Unregistering Symbols
unregistersymbol(MyCode)
unregistersymbol(iEnableGLD)
unregistersymbol(iEnablePMA)
unregistersymbol(iEnablePHP)
unregistersymbol(pPlayer)

//=========================================
dealloc(MyCode)
