       cntValue = readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+38]+20]+C")
       if cntValue == nil then
          print("    Current Solution: None")
       else
          s=""
          for _p=1, cntValue do
            _p= 16 + 4*(_p-1)
            if _p == 16 then
               s=s..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+64]+8]+10")
            else
               s=s..", "..readInteger("[[[[[[[bGSM]+18]+C]+8]+"..string.format("%X",__o).."]+64]+8]+"..string.format("%X",_p))
            end
          end
          print("    Current Solution:   "..s)
       end
     end
  end
