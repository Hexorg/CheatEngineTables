aobscan(aob_damage,8BD189542448DB442448)
// 00515161 - 8B D1                      - mov edx,ecx
// 00515163 - 89 54 24 48                - mov [esp+48],edx
// 00515167 - DB 44 24 48                - fild dword ptr [esp+48]
// 8BD189542448DB442448

// HumanPlayer   = 0
// HumanMilitia  = 1
// Deidranna     = 2 (default)
// HillBillies	 = 3
// KillTarget    = 4
// VillagePeople = 5

// Hack points
//Game.exe+115161:
aob_damage:
_Damage_Hook:
  jmp _Damage_Start
  nop
_Damage_Back:

Damage:
_Damage_Start:
 cmp dword ptr [ebp+0000028C], 0
 je _Friendly
 cmp dword ptr [ebp+0000028C], 1
 je _Friendly
 cmp dword ptr [ebp+0000028C], 5
 je _Friendly
 jmp _Enemy
_Friendly:
 cmp dword ptr[iEnableGM], 1
 jne _Damage_Exit
 mov ecx, 0
 jmp _Damage_Exit
_Enemy:
 cmp dword ptr[iEnableOH], 1
 jne _Damage_Exit
 fld dword ptr[ebp+294]
 fiadd dword ptr[iEnableOH]
 fistp dword ptr[meh]
 mov ecx, dword ptr[meh]
_Damage_Exit:
 mov edx,ecx
 mov [esp+48],edx
 jmp _Damage_Back

// Variables
iEnableGM:
 dd 1
iEnableOH:
 dd 1
meh:
 dd 1

// Original
[DISABLE]
//Game.exe+115161:
_Damage_Hook:
 mov edx,ecx
 mov [esp+48],edx
 fild dword ptr[esp+48]

dealloc(Damage)
unregistersymbol(Damage)
unregistersymbol(iEnableGM)
unregistersymbol(iEnableOH)
unregistersymbol(_Damage_Hook)

</AssemblerScript>
