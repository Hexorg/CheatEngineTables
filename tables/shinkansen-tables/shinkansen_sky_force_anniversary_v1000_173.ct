      <CheatEntries>
        <CheatEntry>
          <ID>107166</ID>
          <Description>"WeaponPlayer Base Address Scan (To Activate: Fire Weapon, To Update: Fire Weapon)"</Description>
          <LastState Activated="0"/>
          <Color>804000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
label(WeaponPlayer_savedAOB)
label(WeaponPlayer_returnhere)
label(WeaponPlayer_originalcode)
label(WeaponPlayer_exit)

label(WeaponPlayer_update_exit)
label(WeaponPlayer_set_exit)
label(WeaponPlayer_firerateset_exit)
label(WeaponPlayer_damagemultiplierset_exit)

registersymbol(WeaponPlayer_savedAOB)

//046E6C3C - 83 EC 14              - sub esp,14
//046E6C3F - 8B 7D 08              - mov edi,[ebp+08]
// WeaponPlayer:LateUpdate+a
//046E6C42 - D9 47 34              - fld dword ptr [edi+34]
//046E6C45 - DD 5D F0              - fstp qword ptr [ebp-10]
//046E6C48 - E8 83AAFDFF           - call 046C16D0
//046E6C4D - DD 45 F0              - fld qword ptr [ebp-10]
//046E6C50 - D9C9                  - fxch st(1)
//046E6C52 - DEC1                  - faddp
aobscan(WeaponPlayer_searchedAOB, D9 47 34 DD 5D F0 * * * * * DD 45 F0)

alloc(WeaponPlayer_newmem, 2048, WeaponPlayer_searchedAOB)


WeaponPlayer_newmem:
cmp dword ptr [WeaponPlayer_update_enable], #0
je WeaponPlayer_update_exit
mov [WeaponPlayer_basepointer], edi
WeaponPlayer_update_exit:

cmp dword ptr [WeaponPlayer_set_enable], #0
je WeaponPlayer_set_exit
WeaponPlayer_set_exit:

cmp dword ptr [WeaponPlayer_firerateset_enable], #0
je WeaponPlayer_firerateset_exit
mov [edi+30], (float)0
WeaponPlayer_firerateset_exit:

cmp dword ptr [WeaponPlayer_damagemultiplierset_enable], #0
je WeaponPlayer_damagemultiplierset_exit
mov [edi+38], (float)9999999
WeaponPlayer_damagemultiplierset_exit:


WeaponPlayer_originalcode:
db D9 47 34 DD 5D F0
//fld dword ptr [edi+34]

WeaponPlayer_exit:
jmp WeaponPlayer_returnhere


WeaponPlayer_searchedAOB:
WeaponPlayer_savedAOB:
jmp WeaponPlayer_newmem
nop
WeaponPlayer_returnhere:


[DISABLE]
WeaponPlayer_savedAOB:
db D9 47 34 DD 5D F0
//fld dword ptr [edi+34]

unregistersymbol(WeaponPlayer_savedAOB)

dealloc(WeaponPlayer_newmem)

</AssemblerScript>
